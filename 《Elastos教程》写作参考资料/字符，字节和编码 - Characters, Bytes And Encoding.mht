function formatstr( str )
{
    if( str =3D=3D null ) return "";

    var str2 =3D str;

    str2 =3D str2.replace( /&/g   , "&amp;"    );
    str2 =3D str2.replace( /</g   , "&lt;"    );
    str2 =3D str2.replace( />/g   , "&gt;"    );
    str2 =3D str2.replace( /\r\n?|\n\r?/g , "<br>"  );

    str2 =3D str2.replace( /\t/g, "&nbsp; &nbsp; " );
    str2 =3D str2.replace( /\x20\x20/g, "&nbsp; " );
    str2 =3D str2.replace( /\x20\x20/g, " &nbsp;" );
    str2 =3D str2.replace( /(^|<br>)\x20/g, "$1&nbsp;");

    return str2;
}

