<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_e/methodmanual.xsl"?><manpage>
<volume>DMA Routines</volume>
<interface>DmaChannel</interface><method> RequestTransfer </method>
    <description>
        <para>
            Requests and sets a DMA transfer (between memories).
        </para>
    </description>
    <syntax>
        <return_val> ECODE </return_val>
        <parameters>
            <api_param>
                <param_type> virtaddr_t </param_type>
                <param_name> vaDestBuffer </param_name>
                <param_note>
                    <para>
                        Target address (virtual address) of DMA transfer.
                    </para>
                </param_note>
            </api_param>

			<api_param>
                <param_type> virtaddr_t </param_type>
                <param_name> vaSrcBuffer </param_name>
                <param_note>
                    <para>
                        DMA transfer memory source address (virtual address).
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_type> uint32_t </param_type>
                <param_name> uLength </param_name>
                <param_note>
                    <para>
                        Data transfer length, in bytes.
                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

	<return>
	    <table>
	    <th>
		<td>Return Value</td>
		<td>Hex Value</td>
		<td>Description</td>
	    </th>
	    <tr>
		<td>E_NOTIMPL</td>
		<td>0x80004001</td>
		<td>This method is not implemented yet.</td>
	    </tr>
	</table>
    </return>

	<remarks>
		<para>
            This method requests and sets a DMA transfer between memories. This method is not implemented yet.
		</para>
	</remarks>

    <requirements>
        <system>
            <system_name>
				Elastos 2.0 or later.
			</system_name>
        </system>
    </requirements>
</manpage>