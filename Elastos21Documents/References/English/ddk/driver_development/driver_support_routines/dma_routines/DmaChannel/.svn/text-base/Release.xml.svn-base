<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_e/methodmanual.xsl"?><manpage>
<volume>DMA Routines</volume>
<interface>DmaChannel</interface><method> Release </method>
	<description>
		<para>
			Releases ocupied DMA channel.
		</para>
	</description>
	<syntax>
		<return_val> void </return_val>
		<NoArguments/>
	</syntax>
	
	<return>
		<NoReturnValue/>
	</return>
	
	<remarks>
		<para>
			After DMA channel is used, occupied DMA channel is released by using this method.
		</para>
	</remarks>
	
	<requirements>
		<system>
			<system_name>
				Elastos 2.0 or later.
			</system_name>
		</system>
	</requirements>
</manpage>