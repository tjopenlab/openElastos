<manpage>
	<sample url="dma_routines\FreeDmaPage">FreeDmaPage</sample>
	<volume>DMA Routines</volume>
	<title>FreeDmaPage</title>
	<routine_idx> ../../dma_routines.htm </routine_idx>
	
	<description>
		<para>Releases an allocated DMA memory page. </para>
	</description>
	
	<syntax>
		<return_val>void </return_val>
		<parameters>
			<api_param>
				<param_type> virtaddr_t </param_type>
				<param_name> va </param_name>
				<param_note>
					<para>Specifies released DMA memory page address (virtual address) </para>
				</param_note>
			</api_param>
		</parameters>
	</syntax>
	
	<return>
		<NOReturnValue/>
	</return>
	
	<remarks>
		<para>This function releases a DMA memory page allocated by <link url="AllocDmaPage.htm">AllocDmaPage</link> function. Attempts to release invalid memory pages (Pages that are not allocated by <link url="AllocDmaPage.htm">AllocDmaPage</link> ) will cause unpredictable results.</para>
	</remarks>
	
	<requirements>
		<system>
			<system_name>
				Elastos 2.0 or later
			</system_name>
		</system>
		<headerfile>
			<file_name>dma.h</file_name>
		</headerfile>
		
	</requirements>
	
	<references>
		<reference>
			<ref_name>AllocDmaPage</ref_name>
			<ref_url>AllocDmaPage</ref_url>
		</reference>
		<reference>
			<ref_name>AllocDmaPages</ref_name>
			<ref_url>AllocDmaPages</ref_url>
		</reference>
		<reference>
			<ref_name>FreeDmaPages</ref_name>
			<ref_url>FreeDmaPages</ref_url>
		</reference>
	</references>
	
</manpage>