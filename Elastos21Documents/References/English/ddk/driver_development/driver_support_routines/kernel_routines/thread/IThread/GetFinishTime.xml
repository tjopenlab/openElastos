    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_e/methodmanual.xsl"?><manpage>
<volume>Thread and Scheduling</volume>
<interface>IThread</interface><method> GetFinishTime </method>
    <description>
        <para>
        	Gets the time, when the associated thread finished execution.
        </para>
    </description>
    <syntax>
        <return_val> ECODE </return_val>
        <parameters>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> EzTime *  </param_type>
                <param_name> pTime </param_name>
                <param_note>
                    <para>
                    	Pointer to EzTime, used to get the exit time of the thread.
                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

	<return>
	<table>
	    <th>
		<td>Return Value </td>
		<td>Hex Value</td>
		<td>Description</td>
	    </th>
	    <tr>
		<td>S_OK</td>
		<td>0x00000000</td>
		<td>Function succeeds in getting the finish time</td>
	    </tr>
	    <tr>
		<td>E_INVALIDARG</td>
		<td>0x80070057</td>
		<td>Invalid argument(s)</td>
	    </tr>
	    <tr>
		<td>E_FAIL</td>
		<td>0x80004005</td>
		<td>Thread is not exited</td>
	    </tr>
	</table>
    </return>

	<remarks>
		<para>
	        This method is used to get the system time when a thread exits.
		    If the thread is not exited, the method return E_FAIL.
			If pTime is NULL, the method return E_INVALIDARG.
		</para>
	</remarks>

    <requirements>
        <system>
            <system_name>
				Elastos 2.0 or later.
			</system_name>
        </system>
    </requirements>
</manpage>