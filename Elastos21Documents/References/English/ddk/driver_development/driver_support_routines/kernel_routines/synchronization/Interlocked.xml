<?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="../../../../../xsl/xsl_e/interfaceman.xsl"?>    <manpage>
    <volume>Synchronization</volume>
    <interface> Interlocked </interface>
    <description>
        <para>

        </para>
    </description>
    <methods>
<self>
    <method>
        <method_name>InterlockedExchangeAdd</method_name>
        <method_url>Interlocked\InterlockedExchangeAdd</method_url>
    <m_description>
        Performs an atomic addition of an increment value to an addend variable.
    </m_description>
    </method>
    <method>
        <method_name>InterlockedExchange</method_name>
        <method_url>Interlocked\InterlockedExchange</method_url>
    <m_description>
        Atomically exchanges a pair of values.
    </m_description>
    </method>
    <method>
        <method_name>InterlockedExchangePointer</method_name>
        <method_url>Interlocked\InterlockedExchangePointer</method_url>
    <m_description>
        Atomically exchanges a pair of values.
    </m_description>
    </method>

    <method>
        <method_name>InterlockedCompareExchange</method_name>
        <method_url>Interlocked\InterlockedCompareExchange</method_url>
    <m_description>
        Performs an atomic comparison of the specified values and exchanges the values, based on the outcome of the comparison.
    </m_description>
    </method>

    <method>
        <method_name>InterlockedCompareExchangePointer</method_name>
        <method_url>Interlocked\InterlockedCompareExchangePointer</method_url>
    <m_description>
        Performs an atomic comparison of the specified values and exchange of the values, based on the outcome of the comparison.
    </m_description>
    </method>

    <method>
        <method_name>InterlockedIncrement</method_name>
        <method_url>Interlocked\InterlockedIncrement</method_url>
    <m_description>
        Atomically increments the value of the specified variable by one and checks the resulting value.
    </m_description>
    </method>

    <method>
        <method_name>InterlockedDecrement</method_name>
        <method_url>Interlocked\InterlockedDecrement</method_url>
    <m_description>
        Atomically decrements the value of the specified variable  by one and checks the resulting value.
    </m_description>
    </method>

</self>
    </methods>
    <requirements>
        <system>
            <system_name>
				Elastos2.0 or later.
			</system_name>
        </system>
        <headerfile>
            <file_name> ddk.h </file_name>
        </headerfile>
    </requirements>
</manpage>

