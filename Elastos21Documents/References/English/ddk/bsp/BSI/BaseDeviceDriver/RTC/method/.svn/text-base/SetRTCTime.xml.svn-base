<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_e/apimanual.xsl"?>

<manpage>
    <volume>Real Time Clock Method</volume>
    <title>SetRTCTime</title>
    <routine_idx> ../method.htm </routine_idx>

    <description>
        <para>Sets real time clock.</para>
    </description>

    <syntax>
        <return_val>void</return_val>
        <parameters>
            <api_param>
                <param_type> const RTCTime *</param_type>
                <param_name> pRTCTime </param_name>
                <param_note>
                    <para>Pointer to the real time clock time data, which is needed to be set, see <link url="../data_structure/RTCTime.htm">RTCTime</link> structure. This parameter cannot be NULL.</para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

	<return>
	    <NoReturnValue/>
    </return>

    <remarks>
        <para>This method is used to set real time clock.</para>
    </remarks>

    <requirements>
        <system>
            <system_name>
                Elastos 2.0 or later.
            </system_name>
        </system>
        <headerfile>
            <file_name> ddk.h </file_name>
        </headerfile>
    </requirements>
    <references>
        <reference>
            <ref_name>GetRTCTime</ref_name>
            <ref_url>GetRTCTime</ref_url>
        </reference>
    </references>

</manpage>
 