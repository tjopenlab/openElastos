<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_e/apimanual.xsl"?>

<manpage>
    <volume>Timer Method</volume>
    <title>GetPITTime</title>
    <routine_idx> ../method.htm </routine_idx>

    <description>
        <para>Gets the time between the last interrupt request that is sent and the current time of timer.</para>
    </description>

    <syntax_nopara>
        <return_val>uint_t</return_val>
        <NoArguments/>
    </syntax_nopara>

	<return>
	    <para>The return value is the time from last time the timer sent interrupt request to the current time, in nanoseconds.</para>
    </return>

    <remarks>
        <para>This method is used to get the time differnce between the last time the timer sent interrupt request, till the current time in nanoseconds.</para>
    </remarks>

    <requirements>
        <system>
            <system_name>
                Elastos 2.0 or later.
            </system_name>
        </system>
        <headerfile>
            <file_name> ddk.h </file_name>
        </headerfile>
    </requirements>

</manpage>
 