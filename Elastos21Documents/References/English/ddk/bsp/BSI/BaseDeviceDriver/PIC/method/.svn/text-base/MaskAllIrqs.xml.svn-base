<manpage>
    <volume>Interrupt Controller Method</volume>
    <title>MaskAllIrqs</title>
    <routine_idx> ../method.htm </routine_idx>

    <description>
        <para>Masks or activates all interrupt requests.</para>
    </description>

    <syntax>
        <return_val>void</return_val>
        <parameters>
            <api_param>
                <param_type> bool_t</param_type>
                <param_name> bEnable </param_name>
                <param_note>
                    <para>Specifies mask or activate all interrupt requests.If TRUE, then activate all interrupt requests.If FALSE,then mask all interrupt requests.</para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

	<return>
	    <NoReturnValue/>
    </return>

    <remarks>
        <para>This method is used to mask or activate all interrupt requests.</para>
    </remarks>

    <requirements>
        <system>
            <system_name>
                Elastos 2.0 or later.
            </system_name>
        </system>
        <headerfile>
            <file_name> ddk.h </file_name>
        </headerfile>
    </requirements>
    <references>
        <reference>
            <ref_name>AcknowledgeIrq</ref_name>
            <ref_url>AcknowledgeIrq</ref_url>
        </reference>
        <reference>
            <ref_name>MaskIrqs</ref_name>
            <ref_url>MaskIrqs</ref_url>
        </reference>
        <reference>
            <ref_name>GetIrqs</ref_name>
            <ref_url>GetIrqs</ref_url>
        </reference>
    </references>

</manpage>
  