<manpage>
    <volume>Data Enumeration Value </volume>
    <types>ComPortParity</types>

    <description>
        <para> Serial port communication data parity check method.</para>
    </description>

    <enum>
        <enum_member>
            <mem_name>ComPortParity_No</mem_name>
            <mem_value>0</mem_value>
            <mem_description>No data parity check.</mem_description>
        </enum_member>
        <enum_member>
            <mem_name>ComPortParity_Odd</mem_name>
            <mem_value>1</mem_value>
            <mem_description>Odd parity check.</mem_description>
        </enum_member>
        <enum_member>
            <mem_name>ComPortParity_Even </mem_name>
            <mem_value>2</mem_value>
            <mem_description>Even parity check.</mem_description>
        </enum_member>
        <enum_member>
            <mem_name>ComPortParity_Mark </mem_name>
            <mem_value>3</mem_value>
            <mem_description>Mark check.</mem_description>
        </enum_member>
        <enum_member>
            <mem_name>ComPortParity_Space</mem_name>
            <mem_value>4</mem_value>
            <mem_description>Space check.</mem_description>
        </enum_member>
        <enum_member>
            <mem_name>ComPortParity_Default</mem_name>
            <mem_value>ComPortParity_No</mem_value>
            <mem_description>By de-fault, no data check.</mem_description>
        </enum_member>
    </enum>
</manpage>

