 <?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_e/apimanual.xsl"?>

<manpage>
    <volume>Kernel Console Method</volume>
    <title>WriteConsole</title>
    <routine_idx> ../method.htm </routine_idx>

    <description>
        <para>Output information to kernel console.</para>
    </description>

    <syntax>
        <return_val>void</return_val>
        <parameters>
            <api_param>
                <param_type> const char *</param_type>
                <param_name> szMessage </param_name>
                <param_note>
                    <para>Specified string information, output to kernel console.</para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

	<return>
	    <NoReturnValue/>
    </return>

    <remarks>
        <para>This method is used to output specified information to kernel console.</para>
    </remarks>

    <requirements>
        <system>
            <system_name>
                Elastos 2.0 or later.
            </system_name>
        </system>
        <headerfile>
            <file_name> ddk.h </file_name>
        </headerfile>
    </requirements>

</manpage>
 