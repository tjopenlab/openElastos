<manpage>
    <sample url="practical_routines\Inl">Inl</sample>
    <volume>Common Library</volume>
    <title>Inl</title>
    <routine_idx> ../common_library.htm </routine_idx>

    <description> 
    	<para>
    		Inputs 4 bytes of data from the specified 32-bit I/O port.
    	</para>
    </description>

    <syntax>
        <return_val>uint32_t</return_val>
        <parameters>
            <api_param>
            	<param_inout>[in]</param_inout>
                <param_type> ioport_t </param_type>
                <param_name> port </param_name>
                <param_note>
                    <para>
                    	The address of specified 32-bit I/O port.
                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>
    <return>
	    <para>
	    	4 bytes of input data.
	    </para>
    </return>


    <remarks>
    	<para>
    		This function inputs 4 bytes of data from specified 32-bit I/O port.
    	</para>
    </remarks>

    <requirements>
        <system>
            <system_name>
                Elastos 2.0 or later. 
            </system_name>
        </system>
        <headerfile>
            <file_name> ddk.h </file_name>
        </headerfile>
    </requirements>

