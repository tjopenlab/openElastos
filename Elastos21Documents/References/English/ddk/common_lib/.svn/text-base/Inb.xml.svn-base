<manpage>
    <sample url="practical_routines\Inb">Inb</sample>
    <volume>Common Library</volume>
    <title>Inb</title>
    <routine_idx> ../common_library.htm </routine_idx>

    <description>
        <para>
        	Inputs one byte of data from the specifed 8-bit I/O port.
        </para>
    </description>

    <syntax>
        <return_val>uint8_t</return_val>
        <parameters>
            <api_param>
            	<param_inout>[in]</param_inout>
                <param_type> ioport_t </param_type>
                <param_name> port </param_name>
                <param_note>
                    <para>
                    	The address of the specified 8-bit I/O port.
                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>
    <return>
	    <para>
	    	One byte of input data.
	    </para>
	</return>

    <remarks>
    	<para>
    		This function inputs one byte of data from specified 8-bit I/O port.
    	</para>
    </remarks>

    <requirements>
        <system>
            <system_name>
                Elastos 2.0 or later. 
            </system_name>
        </system>
        <headerfile>
            <file_name> ddk.h </file_name>
        </headerfile>
    </requirements>

