<manpage>
    <sample url="practical_routines\Outw">Outw</sample>
    <volume>Common Library</volume>
    <title>Outw</title>
    <routine_idx> ../common_library.htm </routine_idx>

    <description>
        <para>
        	Outputs two bytes of data to the specified 16-bit I/O port. 
        </para>
    </description>

    <syntax>
        <return_val>void</return_val>

        <parameters>
            <api_param>
            	<param_inout>[in]</param_inout>
                <param_type> ioport_t </param_type>
                <param_name> port </param_name>
                <param_note>
                    <para>
                    	The address of the specified 16-bit I/O port.
                    </para>
                </param_note>
            </api_param>
        </parameters>

        <parameters>
            <api_param>
            	<param_inout>[out]</param_inout>
                <param_type> uint16_t </param_type>
                <param_name> u16Data </param_name>
                <param_note>
                    <para>
                    	The two bytes of output data.
                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>
    <return>
	    <NoReturnValue/>
	</return>

    <remarks>
    </remarks>

    <requirements>
        <system>
            <system_name>
                Elastos 2.0 or later.
            </system_name>
        </system>
        <headerfile>
            <file_name> ddk.h </file_name>
        </headerfile>
    </requirements>

