<manpage>
    <sample url="practical_routines\Insl">Insl</sample>
    <volume>Common Library</volume>
    <title>Insl</title>
    <routine_idx> ../common_library.htm </routine_idx>

    <description>
        <para>
        	Inputs data with specified size from the specifed 32-bit I/O port.
        </para>
    </description>

    <syntax>
        <return_val>void</return_val>
        <parameters>
            <api_param>
            	<param_inout>[in]</param_inout>
                <param_type> ioport_t </param_type>
                <param_name> port </param_name>
                <param_note>
                    <para>
                    	The address of the specified 32-bit I/O port.
                    </para>
                </param_note>
            </api_param>
        </parameters>

        <parameters>
            <api_param>
            	<param_inout>[out]</param_inout>
                <param_type> void *  </param_type>
                <param_name> buffer </param_name>
                <param_note>
                    <para>
                    	The starting address of the buffer, which inputs the data.
                    </para>
                </param_note>
            </api_param>
        </parameters>

        <parameters>
            <api_param>
            	<param_inout>[in]</param_inout>
                <param_type> size_t </param_type>
                <param_name> size </param_name>
                <param_note>
                    <para>
                    	The size of the data to input, the unit is byte.
                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>
    
    <return>
	    <NoReturnValue/>
	</return>


    <remarks>
          <para>
	          This function outputs data with specified size to the specified 32-bit I/O port, 4 bytes of data each time,
	          until the specified size of data has been output. The behavior of this function is not defined if the 
	          specified size of the output data is not exactly divisible by 4.
          </para>
    </remarks>
      
    <requirements>
        <system>
            <system_name>
                Elastos 2.0 or later. 
            </system_name>
        </system>
        <headerfile>
            <file_name> ddk.h </file_name>
        </headerfile>
    </requirements>

