<manpage>
    <sample url="practical_routines\Insb">Insb</sample>
    <volume>Common Library</volume>
    <title>Insb</title>
    <routine_idx> ../common_library.htm </routine_idx>

    <description>
        <para>
        	Inputs data with specified size from specified 8-bit I/O port.
        </para>
    </description>

    <syntax>
        <return_val>void</return_val>
        <parameters>
            <api_param>
            	<param_inout>[in]</param_inout>
                <param_type> ioport_t </param_type>
                <param_name> port </param_name>
                <param_note>
                    <para>
                    	The address of the specified 8-bit I/O port.
                    </para>
                </param_note>
            </api_param>
        </parameters>

        <parameters>
            <api_param>
            	<param_inout>[out]</param_inout>
                <param_type> void * </param_type>
                <param_name> buffer </param_name>
                <param_note>
                    <para>
                    	The starting address of the buffer, which inputs the data. 
                    </para>
                </param_note>
            </api_param>
        </parameters>

        <parameters>
            <api_param>
            	<param_inout>[in]</param_inout>
                <param_type> size_t </param_type>
                <param_name> size </param_name>
                <param_note>
                    <para>
                    	The size of the data to input, the unit is bytes.
                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>
    
    <return>
	    <NoReturnValue/>
	</return>

	<remarks>
        <para>
        	This function inputs data with specified size from specified 8-bit I/O port, one byte at a time, 
        	until specified size of data is input.
        </para>
    </remarks>

    <requirements>
        <system>
            <system_name>
                Elastos 2.0 or later. 
            </system_name>
        </system>
        <headerfile>
            <file_name> ddk.h </file_name>
        </headerfile>
    </requirements>

