<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_e/shellmanual.xsl"?>

<manpage>
    <title> vi </title>
    <volume>Common Elastos Tools</volume>

    <description>
        <para> Text editor. Please reference vi related documentation for details.</para>
    </description>

    <syntax>
        <cmd_name> VI </cmd_name>
        <cmd_param> filename </cmd_param>
    </syntax>
    <parameters>
    <param>
        <param_name>  filename  </param_name>
        <param_note>File name of file to be edited</param_note>
    </param>
    </parameters>

    <examples>
    <example>
        <eg_line>vi file.txt  </eg_line>
        <eg_note> Edits file file.txt .If this file does not exist then creates it.</eg_note>
    </example>
    </examples>
</manpage>