<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_e/shellmanual.xsl"?>

<manpage>
    <title> tee </title>
    <volume>Common Elastos Tools</volume>

    <description>
        <para> Displays the output of a program and copies it into a file.</para>
    </description>

    <syntax>
        <cmd_name>TEE</cmd_name>
        <cmd_param>[-?] [-a] OutputFileName(s)...</cmd_param>
    </syntax>

    <parameters>
    <param>
        <param_name>-?</param_name>
        <param_note> Displays help information </param_note>
    </param>
    <param>
        <param_name>-a</param_name>
        <param_note>Adds the output to the end of the output file instead of writing over it.</param_note>
    </param>
    <param>
        <param_name>OutputFileName</param_name>
        <param_note>Name of specified output file.If this file does not exists, then creats it</param_note>
    </param>
    </parameters>

    <examples>
    <example>
        <eg_line>type file1.txt | tee file2.txt</eg_line>
        <eg_note>This displays the standard output of file1.txt at the workstation, and at the same time saves a copy of file1.txt in the file file2.txt .</eg_note>
    </example>
    </examples>

    <references>
    <reference>
        <ref_name>head</ref_name>
        <ref_url>head</ref_url>
    </reference>
    <reference>
        <ref_name>tail</ref_name>
        <ref_url>tail</ref_url>
    </reference>
    </references>
</manpage>