<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_e/shellmanual.xsl"?>

<manpage>
    <title> regcom </title>
    <volume>Common Elastos Tools</volume>

    <description>
        <para>Registers component. </para>
    </description>

    <syntax>
        <cmd_name> REGCOM </cmd_name>
        <cmd_param> [-?] [/i][/l][/rs][lr][/rc remote_server_name][/u] dllname</cmd_param>
    </syntax>
    <parameters>
    <param>
        <param_name> -? </param_name>
        <param_note> Displays help information </param_note>
    </param>
    <param>
        <param_name> /i </param_name>
        <param_note> Registers component as server internal process</param_note>
    </param>
    <param>
        <param_name> /l
        </param_name>
        <param_note>Registers component as server local process</param_note>
    </param>
    <param>
        <param_name>/rs  </param_name>
        <param_note> Registers remote component information to server</param_note>
    </param>
    <param>
        <param_name> /lr </param_name>
        <param_note> Registers component to local or remote server</param_note>
    </param>
    <param>
        <param_name> /rc remote_server_name</param_name>
        <param_note> Registers remote component information to client machine</param_note>
    </param>
    <param>
        <param_name> /u </param_name>
        <param_note> Cancel component registration</param_note>
    </param>
    <param>
        <param_name> dllname </param_name>
        <param_note> Component name</param_note>
    </param>
    </parameters>

    <examples>
    <example>
        <eg_line>regcom helloworld.dll </eg_line>
        <eg_note> Registers component helloworld.dll to local machine.</eg_note>
    </example>
    </examples>

</manpage>