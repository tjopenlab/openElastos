<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_e/shellmanual.xsl"?>

<manpage>
    <title> cat </title>
    <volume>Common Elastos Tools</volume>

    <description>
        <para>Displays file content.</para>
    </description>

    <syntax>
        <cmd_name> CAT </cmd_name>
        <cmd_param> source1[source2...] </cmd_param>
    </syntax>
    <parameters>
    <param>
        <param_name>  source1 </param_name>
        <param_note> Source file name</param_note>
    </param>
    <param>
        <param_name>  source2 </param_name>
        <param_note> Source file name</param_note>
    </param>
    </parameters>

    <examples>
    <example>
        <eg_line>cat file.txt</eg_line>
        <eg_note> Displays the content of file file.txt .</eg_note>
    </example>
    </examples>

</manpage>