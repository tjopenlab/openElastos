<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_e/methodmanual.xsl"?>

<manpage>
    <sample url="sdk\eztypes\EzByteBuf\AllocByteBuf">alloc</sample>
    <volume> Common Data Structure </volume>
    <interface> EzByteBuf </interface>
    <method> AllocByteBuf </method>

    <description>
        <para> Allocates memory blocks of specified size for the variable EzByteBuf.</para>
    </description>

    <syntax>
        <return_val>static EzByteBuf</return_val>
        <parameters>
        <api_param>
            <param_type>UINT</param_type>
            <param_name> cb </param_name>
            <param_note>
                <para> The number of bytes to be allocated.  </para>
            </param_note>
        </api_param>
        </parameters>
    </syntax>

    <return>
        <para>If the allocation is successful, returns a pointer to the variable EzByteBuf; otherwise returns EZBYTEBUF_NULL.</para>
    </return>

    <remarks>
        <para>The memory blocks allocated by this function need releasing by function <b><link url="FreeByteBuf.htm">FreeByteBuf</link></b>.</para>
    </remarks>

<example></example>



    <references>
        <reference>
            <ref_name> AllocByteBufLen </ref_name>
            <ref_url> AllocByteBufLen </ref_url>
        </reference>
    </references>

</manpage>