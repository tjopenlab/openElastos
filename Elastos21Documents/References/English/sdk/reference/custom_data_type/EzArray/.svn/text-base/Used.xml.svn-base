<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_e/methodmanual.xsl"?>

<manpage>
    <sample url="datatypes\EzArray\Used">Used</sample>
    <volume>  Common Data Structures  </volume>
    <interface> EzArray </interface>
    <method>Used </method>

    <description>
        <para>
        	Gets the count of the used <b>EzArray</b> elements, the unit is sizeof(T).
        </para>
    </description>
    <syntax>
        <return_val>int</return_val>
        <NoArguments/>
    </syntax>

    <return>
        <para>
        	Returns the count of the used elements, the unit is sizeof(T).
        </para>
    </return>

    <remarks>
        <para> 
        	Before calling <link url="Append.htm">Append</link>, the data will be appended to 
        	the index specified by <b>Used</b>, and the value of the <b>Used</b> will be modified after 
        	<link url="Append.htm">Append</link> operation.
        </para>
        <para></para>
    </remarks>

    <example><pre><![CDATA[
#include <EzArray.h>
#include <stdio.h>

int __cdecl main()
{
    EzArray<int> myArray = EzArray<int>::CreateObject(4);

    for (int i = 0; i < myArray.GetLength(); i++){
        myArray[i] = i;
        printf("myArray[%d] = %d\n", i, myArray[i]);
    }
    printf("The used size of myArray is : %d.\n", myArray.Used());

    myArray.Dispose();

    return 0;
}
]]></pre></example>

    <result>
    <![CDATA[
myArray[0] = 0
myArray[1] = 1
myArray[2] = 2
myArray[3] = 3
The used size of myArray is : 4.
]]>
    </result>

    <references>
        <reference>
            <ref_name> SetUsed </ref_name>
            <ref_url> SetUsed </ref_url>
        </reference>
    </references>

</manpage>