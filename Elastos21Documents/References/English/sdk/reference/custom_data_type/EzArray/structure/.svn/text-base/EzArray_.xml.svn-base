            <ref_name> Copy Constructor (Deep copy) </ref_name>
            <ref_url> EzArrayCopy_ </ref_url>
        </reference>
        <reference>
            <ref_name> Constructor (Does not allocate memory) </ref_name>
            <ref_url> EzArray </ref_url>
        </reference>
    </references>

</manpage>