<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_e/structuremanual.xsl"?>

<manpage>
    <volume> Common Data Structure </volume>
    <class> EzStrBuf </class>
    <function> EzStrBuf(const EzStrBuf&amp; buf) </function>

    <description>
        <para> Constructor of EzStrBuf class, which is used to construct an EzStrBuf object.</para>
    </description>

    <syntax>
        <apiname>EzStrBuf</apiname>
        <parameters>
        <api_param>
            <param_type>const EzStrBuf&amp; </param_type>
            <param_name> buf </param_name>
            <param_note>
                <para> A reference of EzStrBuf object </para>
            </param_note>
        </api_param>
        </parameters>
    </syntax>

    <remarks>
        <para>This constructor only creates pointer to EzStrBuf object. You can call <link url="../AllocStrBuf.htm">AllocStrBuf</link> method to allocate memory for this object. </para>
    </remarks>

    <references>
        <reference>
            <ref_name> Constructor function (Allocates memory) </ref_name>
            <ref_url> EzStrBuf_ </ref_url>
        </reference>
    </references>
</manpage>