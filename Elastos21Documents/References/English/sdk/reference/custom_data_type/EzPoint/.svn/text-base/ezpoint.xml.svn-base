<?xml version = "1.0" encoding = "GB2312"?>
<?xml:stylesheet type = "text/xsl" href = "/xsl/xsl_e/methodmanual.xsl"?>

<manpage>
    <volume> Common data structure </volume>
    <interface> EzPoint </interface>
    <method> EzPoint </method>

    <description>
        <para> EzPoint's constructor function, which is used to construct an EzPoint object. </para>
    </description>
    <return>
      <para> Returns the constructed EzPoint object. </para>
    </return>
    <syntax>
        <return_val> EzPoint </return_val>
        <parameters>
        <api_param>

            <param_type> long </param_type>
            <param_name> x = 0 </param_name>
            <param_rem></param_rem>
            <param_note>
                <para> Parameter of X-axis.  </para>
            </param_note>
        </api_param>
        <api_param>

            <param_type> long </param_type>
            <param_name> y = 0 </param_name>
            <param_rem></param_rem>
            <param_note>
                <para> Parameter of Y-axis.</para>
            </param_note>
        </api_param>

        </parameters>
    </syntax>
  <syntax>
        <return_val> EzPoint </return_val>
        <parameters>
        <api_param>

            <param_type> EzPoint&amp; </param_type>
            <param_name> ezPointRef </param_name>
            <param_rem>  </param_rem>
            <param_note>
                <para> Reference to existing data of EzPoint type. </para>
            </param_note>
        </api_param>

        </parameters>
    </syntax>

   <example><pre><![CDATA[
    EzPoint thePoint;
    EzPoint thatPoint(1, 2);
    EzPoint thisPoint(thatPoint);
]]></pre></example>

</manpage>