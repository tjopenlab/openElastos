<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_e/structuremanual.xsl"?>

<manpage>
    <volume> Common Data Structure </volume>
    <class> EzWCharBuf </class>
    <function> EzWCharBuf(const EzWCharBuf&amp; buf) </function>

    <description>
        <para>Constructor of an EzWCharBuf object to construct an EzWCharBuf object.</para>
    </description>

    <syntax>
        <apiname>EzWCharBuf</apiname>
        <parameters>
        <api_param>
            <param_type>const EzWCharBuf&amp; </param_type>
            <param_name> buf </param_name>
            <param_note>
                <para> A reference to the EzWCharBuf object</para>
            </param_note>
        </api_param>
        </parameters>
    </syntax>

    <remarks>
        <para>This constructor only creates a pointer to EzWCharBuf. You can call <link url="../AllocWCharBuf.htm">AllocWCharBuf</link> to allocate memory blocks for this object.</para>
    </remarks>

    <references>
        <reference>
            <ref_name> Constructor (Allocates memory) </ref_name>
            <ref_url> EzWCharBuf_ </ref_url>
        </reference>
    </references>
</manpage>