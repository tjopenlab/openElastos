<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_e/methodmanual.xsl"?>

<manpage>
    <volume> Common Data Structure </volume>
    <interface> EzWCharBuf </interface>
    <method> EzWCharBuf </method>

    <description>
        <para>Constructor of a EzWCharBuf object to construct a EzWCharBuf object.</para>
    </description>

    <syntax>
        <parameters>
        <api_param>
            <param_type>const EzWCharBuf&amp; </param_type>
            <param_name> buf </param_name>
            <param_note>
                <para> A reference to the EzWChar object.</para>
            </param_note>
        </api_param>
        </parameters>
    </syntax>

    <remarks>
        <para>This constructor only creates a pointer to EzWCharBuf. One can call <link url="AllocWCharBuf.htm">AllocWCharBuf</link> to allocate memory blocks for this object.</para>
    </remarks>

    <references>
        <reference>
            <ref_name> Constructor (Allocating Memory Space) </ref_name>
            <ref_url> EzWCharBuf_ </ref_url>
        </reference>
    </references>
</manpage>