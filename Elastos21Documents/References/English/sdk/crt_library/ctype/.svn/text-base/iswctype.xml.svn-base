<?xml version="1.0" encoding="GB2312"?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_e/manual.xsl"?>

<manpage>
    <sample url="sdk\operating_system\crt\libc\ctype\iswctype">iswctype</sample>
    <title>iswctype</title>
    <volume>C Run-Time Library</volume>
    <routine_idx> ctype_idx.htm </routine_idx>

    <description>
        <para>Tests c for the property specified by the desc argument.</para>
    </description>

    <syntax>
        <return_val>int</return_val>
        <func_name>iswctype</func_name>
        <parameters>
            <func_param>
                <param_type>wint_t</param_type>
                <param_name>c</param_name>
                <param_type> wctype_t</param_type>
                <param_name>desc</param_name>
                <param_note>
                    <para>input wint_t,wctype_t</para>
                </param_note>
            </func_param>
        </parameters>
    </syntax>

    <return>
        <para>iswctype returns a nonzero value if c has the property specified by desc  or 0 if it does not </para>
    </return>

    <requirements>
        <routine>
            <routine_name>iswctype</routine_name>
            <headerfile>ctype.h</headerfile>
            <compatibility>ANSI, Windows 2000, Elastos 2.0</compatibility>
        </routine>
    </requirements>

    <example></example>



</manpage>