<manpage>
    <sample url="sdk\operating_system\crt\libc\direct\_getdcwd">_getdcwd</sample>
    <title>_getdcwd,_wgetdcwd </title>
    <volume>C Run-Time Library</volume>
    <routine_idx> dir_idx.htm </routine_idx>

    <description>
        <para>Gets the current working directory.</para>
    </description>

    <syntax>
        <return_val>char*</return_val>
        <func_name>_getdcwd</func_name>
        <parameters>
            <func_param>
                <param_type>int</param_type>
                <param_name>drive</param_name>
                <param_note>
                    <para>Disk drive.</para>
                </param_note>
            </func_param>
            <func_param>
                <param_type>char *</param_type>
                <param_name>Buffer</param_name>
                <param_note>
                    <para>Storage location for the path </para>
                </param_note>
            </func_param>
            <func_param>
                <param_type>int</param_type>
                <param_name>maxlen</param_name>
                <param_note>
                    <para>Maximum length of the path in characters: char for _getdcwd and wchar_t for __getdcwd. </para>
                </param_note>
            </func_param>
        </parameters>
    </syntax>
    <syntax>
        <return_val>wchar_t*</return_val>
        <func_name>_wgetcwd</func_name>
        <parameters>
            <func_param>
                <param_type>wchar_t*</param_type>
                <param_name>buffer</param_name>
            </func_param>
            <func_param>
                <param_type>int</param_type>
                <param_name>maxlen</param_name>
            </func_param>
        </parameters>
    </syntax>

    <return>
        <para> </para>
    </return>

    <remarks>
        <para> </para>
    </remarks>

    <requirements>
        <routine>
            <routine_name>_chdir</routine_name>
            <headerfile>direct.h</headerfile>
            <compatibility>Windows 2000, Elastos 2.0</compatibility>
        </routine>
        <routine>
            <routine_name>_getcwd</routine_name>
            <headerfile>direct.h</headerfile>
            <compatibility>Windows 2000, Elastos 2.0</compatibility>
        </routine>
        <routine>
            <routine_name>_getdrive</routine_name>
            <headerfile>direct.h</headerfile>
            <compatibility>Windows 2000, Elastos 2.0</compatibility>
        </routine>
        <routine>
            <routine_name>_mkdir</routine_name>
            <headerfile>direct.h</headerfile>
            <compatibility>Windows 2000, Elastos 2.0</compatibility>
        </routine>
        <routine>
            <routine_name>_rmdir</routine_name>
            <headerfile>direct.h</headerfile>
            <compatibility>Windows 2000, Elastos 2.0</compatibility>
        </routine>
    </requirements>

    <example></example>



</manpage>