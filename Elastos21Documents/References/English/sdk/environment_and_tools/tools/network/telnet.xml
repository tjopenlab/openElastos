<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_e/shellmanual.xsl"?>

<manpage>
    <title> telnet </title>
    <volume>Network Commands</volume>

    <description>
        <para> Remote login tool.</para>
    </description>

    <syntax>
        <cmd_name> telnet </cmd_name>
        <cmd_param>[host_ip|host_domain_name][port]</cmd_param>
    </syntax>

    <parameters>
        <param>
            <param_name> [host-ip|host_domain_name]</param_name>
            <param_note> Host IP address or host name. </param_note>
        </param>
        <param>
            <param_name>[port]</param_name>
            <param_note> Port number, i.e. various Internet application address.</param_note>
        </param>
    </parameters>

    <examples>
        <example>
            <eg_line>telnet 166.111.8.238</eg_line>
        </example>
        <example>
            <eg_line>telnet bbs.tsinghua.edu.cn</eg_line>
        </example>
    </examples>

</manpage>