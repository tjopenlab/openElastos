<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_e/shellmanual.xsl"?>

<manpage>
    <title> Del </title>
    <volume>
    	Commands Supported by Shell
    </volume>

    <description>
        <para>
        	Deletes one or more files.
        </para>
    </description>

    <syntax>
        <cmd_name> DEL </cmd_name>
        <cmd_param>
        	[-p] [-f] [-s] [-a[attributes]] names
        </cmd_param>
    </syntax>

    <parameters>
        <param>
            <param_name>  [-p] </param_name>
            <param_note>
            	Prompts for confirmation before deleting each file.
            </param_note>
        </param>
        <param>
            <param_name>  [-f] </param_name>
            <param_note>
            	Force deleting of read-only files. 
            </param_note>
        </param>
        <param>
            <param_name>[-s]</param_name>
            <param_note>
            	Deletes specified files from all subdirectories.
            </param_note>
        </param>
        <param>
            <param_name>[-a[attributes]]</param_name>
            <param_note>
            	Selects files to delete based on attributes. [attributes], File attribute. <i>r</i>, 
            	Means read-only files.
            </param_note>
        </param>
        <param>
            <param_name>names</param_name>
            <param_note>
            	Specifies a list of one or more files or directories. Wildcard characters can be used to 
            	delete multiple files. If a directory is specified, all files within the directory will be 
            	deleted.
            </param_note>
        </param>
    </parameters>

</manpage>