<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_e/shellmanual.xsl"?>

<manpage>
    <title> COPY </title>
    <volume>Commands Supported by Shell</volume>

    <description>
        <para> Copies one or more files to another location.</para>
    </description>

    <syntax>
        <cmd_name> Copy </cmd_name>
        <cmd_param>[-y] source [ source[ ...]] [destination]</cmd_param>
    </syntax>
    <parameters>
        <param>
            <param_name>  [-y] </param_name>
            <param_note>
            	Prompts for confirmation whether you want to overwrite an existing destination file.
            </param_note>
        </param>
        <param>
            <param_name>  source [ source[ ...]] </param_name>
            <param_note>
            	Specifies the file or files to be copied.
            </param_note>
        </param>
        <param>
            <param_name>[destination]</param_name>
            <param_note>
            	Specifies the directory and/or filename for the new file(s).
            </param_note>
        </param>
    </parameters>

</manpage>