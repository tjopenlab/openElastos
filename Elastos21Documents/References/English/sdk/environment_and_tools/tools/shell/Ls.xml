<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_e/shellmanual.xsl"?>

<manpage>
    <title> Ls </title>
    <volume>Commands Supported by Shell</volume>

    <description>
        <para> Displays specified file or path listing.</para>
    </description>

    <syntax>
        <cmd_name> LS </cmd_name>
        <cmd_param>[files]</cmd_param>
    </syntax>

    <parameters>
        <param>
            <param_name>[files]</param_name>
            <param_note>Specifies file</param_note>
        </param>
    </parameters>

</manpage>