<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_e/shellmanual.xsl"?>

<manpage>
    <title> debug </title>
    <volume>Shell Supported Commands</volume>

    <description>
        <para> Makes the program enter debug mode as soon as it starts running.</para>
    </description>

    <syntax>
        <cmd_name> debug </cmd_name>
        <cmd_param> program [progargs]</cmd_param>
    </syntax>

    <parameters>
        <param>
            <param_name>program</param_name>
            <param_note>Specified program name</param_note>
        </param>
        <param>
            <param_name>[progargs]</param_name>
            <param_note>Program parameter</param_note>
        </param>
    </parameters>
</manpage>