<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_e/shellmanual.xsl"?>

<manpage>
    <title> Type </title>
    <volume>Commands Supported by Shell</volume>

    <description>
        <para> Displays the file content.</para>
    </description>

    <syntax>
        <cmd_name> TYPE </cmd_name>
        <cmd_param>[path]filename</cmd_param>
    </syntax>

    <parameters>
        <param>
            <param_name>[path]</param_name>
            <param_note>Specified directory path.</param_note>
        </param>
        <param>
            <param_name>filename</param_name>
            <param_note>Specified file name.</param_note>
        </param>
    </parameters>
</manpage>