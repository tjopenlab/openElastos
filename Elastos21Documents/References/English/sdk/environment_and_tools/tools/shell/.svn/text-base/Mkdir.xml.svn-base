<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_e/shellmanual.xsl"?>

<manpage>
    <title> Mkdir </title>
    <volume>Commands Supported by Shell</volume>

    <description>
        <para> Creates specified directory.</para>
    </description>

    <syntax>
        <cmd_name> mkdir </cmd_name>
        <cmd_param>path</cmd_param>
    </syntax>

    <parameters>
        <param>
            <param_name>path</param_name>
            <param_note>Specifies directory path.</param_note>
        </param>
    </parameters>

</manpage>