<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_e/apimanual.xsl"?>

<manpage>
    <sample url="system_lib\EzSetTime"> EzSetSystemTime  </sample>
    <volume>System API</volume>
    <title>EzSetSystemTime</title>
    <routine_idx> ..\system_api_idx.htm </routine_idx>

    <description>
        <para>
        	The <b>EzSetSystemTime</b> function sets the current UTC (Co-ordinated Universal Time).
        </para>
    </description>

    <syntax>
        <return_val>ECODE</return_val>
        <parameters>
            <api_param>
                <param_type> EzTime </param_type>
                <param_name> time </param_name>
                <param_note>
                    <para>
                    	System time to set.
                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

    <return>
        
        <table>
            <th>
                <td>Return Value</td><td>Description</td>
            </th>
            <tr>
                <td>S_OK</td>
                <td>Function succeeds in setting the current UTC.</td>
            </tr>
            <tr>
                <td>E_INVALIDARG</td>
                <td>Invalid argument(s)</td>
            </tr>
        </table>
        <para>See <link url="/sdk/hresult.htm">ECODE</link> related help for more details.</para>
    </return>

    <remarks>
        <para>
        	The <b>EzSetSystemTime</b> function saves the time to set. The time is represented by the number of 
        	seconds from 00:00:00 Jan. 1st, 1970 till now. 
        </para>
    </remarks>

    <requirements>
        <system>
            <system_name>
                Windows 2000 or later; Elastos 2.0 or later.
            </system_name>
        </system>
        <headerfile>
            <file_name> elastos.h </file_name>
        </headerfile>
        <library>
            <lib_name> elastos.lib </lib_name>
        </library>
    </requirements>

</manpage>
