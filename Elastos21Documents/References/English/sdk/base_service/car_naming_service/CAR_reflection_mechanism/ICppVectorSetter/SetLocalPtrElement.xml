    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_e/methodmanual.xsl"?>
    <manpage>
    <volume>ICppVectorSetter Interface  Functions</volume>
     <interface>ICppVectorSetter</interface><method>SetLocalPtrElement</method>
    <description>
        <para>
  Set element value of LocalPtr type
        </para>
    </description>
    <syntax>
       <return_val> ECODE </return_val>
     <parameters>
        <api_param>
        <param_inout>[in]</param_inout>
        <param_type>Int32 </param_type>
        <param_name>index</param_name>
        <param_note>
        <pa>
        The input array index number 
        </pa>
        </param_note>
        </api_param>
      </parameters>
     <parameters>
        <api_param>
        <param_inout>[in]</param_inout>
        <param_type>LocalPtr </param_type>
        <param_name>value</param_name>
        <param_note>
        <pa>
        The input value of LocalPtr type 
        </pa>
        </param_note>
        </api_param>
      </parameters>
     </syntax>


	<return>
		<table>
		<th>
		<td>Return value</td><td>Description</td>
		</th>
		<tr>
		<td>NOERROR</td><td> Succeeds in setting  element value of LocalPtrtypey </td>
		 </tr>
		 <tr>
		<td>E_INVALID_ARGUMENT</td><td>Invalid argument(s)</td>
		 </tr>
		 </table>   
		    <para>See <link url="..\..\..\..\reference\basic_data_type\hresult.htm">ECODE</link> for related help.</para>
    </return>

    <requirements>
        <system>
            <system_name>
				Windows 2000 or later; Elastos 2.0 or later.
	    	</system_name>
        </system>
    </requirements>
</manpage>