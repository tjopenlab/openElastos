    <manpage>
    <volume>IClassInfo Interface Functions </volume>
     <interface>IClassInfo</interface><method>GetCallbackInterfaceCount </method>
    <description>
        <para>
     Gets   callback Interface count
        </para>
    </description>
    
    <syntax>
       <return_val> ECODE </return_val>
     <parameters>
        <api_param>
        <param_inout>[out]</param_inout>
        <param_type>Int32 *</param_type>
        <param_name>count</param_name>
        <param_note>
        <pa>
      Retrieves callback Interface count
        </pa>
        </param_note>
        </api_param>
      </parameters>
     </syntax>


	<return>
		<table>
		<th>
		<td>Return value</td><td>Description</td>
		</th>
		<tr>
		<td>NOERROR</td><td>Succeeds in   getting callback Interface count </td>
		 </tr>
		 <tr>
		<td>E_INVALID_ARGUMENT</td><td>Invalid argument(s).</td>
		 </tr>
		 </table>   
		    <para>See <link url="..\..\..\..\reference\basic_data_type\hresult.htm">ECODE</link> for related help.</para>
    </return>

    <requirements>
        <system>
            <system_name>
				Windows 2000 or later; Elastos 2.0 or later.
	    	</system_name>
        </system>
    </requirements>
</manpage>