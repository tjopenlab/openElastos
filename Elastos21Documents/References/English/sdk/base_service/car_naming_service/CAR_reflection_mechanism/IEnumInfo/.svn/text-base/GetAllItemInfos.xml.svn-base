    <manpage>
    <volume>IEnumInfo Interface Functions</volume>
     <interface>IEnumInfo</interface><method>GetAllItemInfos</method>
    <description>
        <para>
        Gets all  items information
        </para>
    </description>
    <syntax>
       <return_val> ECODE </return_val>
     <parameters>
        <api_param>
        <param_inout>[out]</param_inout>
        <param_type>ArrayOf<IObject *></param_type>
        <param_name>itemInfos</param_name>
        <param_note>
        <pa>
       if the function succeeds ,all  items information list is returned
        </pa>
        </param_note>
        </api_param>
      </parameters>
     </syntax>


	<return>
   <table>
   <thead>
   <td>return value</td> <td>Description</td>
   </thead>
   <tr>
   <td>NOERROR</td> <td>Succeeds in getting all  items information</td>
   </tr>
    <tr>
   <td>E_INVALID_ARGUMENT</td> <td>Invalid argument(s)</td>
   </tr>
   </table>
   		    <para>See <link url="..\..\..\..\reference\basic_data_type\hresult.htm">ECODE</link> for related help.</para>
   </return>

    <requirements>
        <system>
            <system_name>
				 Elastos 2.0 or later.
	    	</system_name>
        </system>
    </requirements>
</manpage>