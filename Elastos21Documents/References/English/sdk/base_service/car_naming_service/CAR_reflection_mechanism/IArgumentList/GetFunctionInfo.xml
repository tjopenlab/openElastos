    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_e/methodmanual.xsl"?><manpage>
    <volume>IArgumentList Interface  Functions</volume>
     <interface>IArgumentList</interface><method>GetFunctionInfo</method>
    <description>
        <para>
        Acquires the method interface
        </para>
    </description>
    <syntax>
      <return_val> ECODE </return_val>
      <parameters>
        <api_param>
         <param_inout>[out]</param_inout>
         <param_type>IFunctionInfo **</param_type>
         <param_name>functionInfo</param_name>
         <param_note>
          <para>
          returns  pointer to IFunctionInfo type
          </para>
         </param_note>
        </api_param>
      </parameters>
    </syntax>

	<return>
		<table>
		<th>
		<td>Return value</td><td>Description</td>
		</th>
		<tr>
		<td>NOERROR</td><td>Succeeds in acquiring  the method interface</td>
		 </tr>
		 <tr>
		<td>E_INVALID_ARGUMENT</td><td>The functionInfo is null </td>
		 </tr>
		 </table>   
		    <para>See <link url="..\..\..\..\reference\basic_data_type\hresult.htm">ECODE</link> for related help.</para>
    </return>

	<remarks>
	<para></para>
	</remarks>

    <requirements>
        <system>
            <system_name>
				Windows 2000 or later; Elastos 2.0 or later.
	    	   </system_name>
        </system>
    </requirements>
</manpage>

    
    
    
