    <manpage>
    <volume>IArgumentList Interface  Functions </volume>
     <interface>IArgumentList</interface><method> IArgumentList:: SetInInt32Arg </method>
    <description>
        <para>
<!--        Acquires the method interface -->
        </para>
    </description>
    <syntax>
         
        <return_val> ECODE </return_val>
      <parameters>
        <api_param>
        <para_inout>[in]</para_inout>
        <para_type>Int32</para_type>
        <para_name>index</para_name>
        <para_note>
        <para>
       index number
        </para>
        </para_note>
        </api_param>
      </parameters>
      <parameters>
        <api_param>
        <para_inout>[in]</para_inout>
        <para_type>Int32</para_type>
        <para_name>value</para_name>
        <para_note>
        <para>
        Set  Int32 type value
        </para>
        </para_note>
        </api_param>
      </parameters>
       </syntax>

	<return>
		<table>
		<th>
		<td>Return value</td><td>Description</td>
		</th>
		<tr>
		<td>NOERROR</td><td>Succeeds in acquiring  the method interface</td>
		 </tr>
		 <tr>
		<td>E_INVALID_ARGUMENT</td><td>The input parameter serial number exceeds the  number of method parameter or the data type of the seted parameter  mismatches that of the corresponding method </td>
		 </tr>
		  <tr>
		<td>E_INVALID_OPERATION</td><td>Fail to create parameter list or set Int32 type value</td>
		 </tr>
		 </table>   
		    <para>See <link url="..\..\..\..\reference\basic_data_type\hresult.htm">ECODE</link> for related help.</para>
    </return>

	<remarks>
	</remarks>

    <requirements>
        <system>
            <system_name>
				Windows 2000 or later; Elastos 2.0 or later.
	    	</system_name>
        </system>
    </requirements>
</manpage>