    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_e/methodmanual.xsl"?>
    <manpage>
    <volume>IModuleInfo Interface Functions </volume>
     <interface>IModuleInfo</interface><method>GetAllEnumInfos</method>
    <description>
        <para>
        Acquires  all  Enumeration information
        </para>
    </description>
    <syntax>
       <return_val> ECODE </return_val>
      <parameters>
        <api_param>
        <param_inout>[out]</param_inout>
        <param_type>ArrayOf<IObject *></param_type>
        <param_name>enumInfos</param_name>
        <param_note>
        <pa>
     The outputting all  Enumeration information
        </pa>
        </param_note>
        </api_param>
      </parameters>
     </syntax>


	<return>
		<table>
		<th>
		<td>Return value</td><td>Description</td>
		</th>
		<tr>
		<td>NOERROR</td><td> Succeeds in  getting  all  Enumeration information</td>
		 </tr>
		 <tr>
		<td>E_INVALID_ARGUMENT</td><td>Invalid argument(s)</td>
		 </tr>
		 </table>
		    <para>See <link url="..\..\..\..\reference\basic_data_type\hresult.htm">ECODE</link> for related help.</para>
    </return>

    <requirements>
        <system>
            <system_name>
				Windows 2000 or later; Elastos 2.0 or later.
	    	</system_name>
        </system>
    </requirements>
</manpage>