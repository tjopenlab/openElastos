    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_e/methodmanual.xsl"?><manpage>
<volume>System COM Interface </volume>
<interface>IReaderWriterLock</interface><method> ReaderUnlock </method>
    <description>
        <para>
         	Releases the acquired reader lock.
        </para>
    </description>
    <syntax>
        <return_val> ECODE </return_val>
    </syntax>

	<return>	
	<table>
	    <th>
		<td>Return Value </td>
		<td>Hex Value</td>
		<td>Description</td>
	    </th>
	    <tr>
		<td>NOERROR </td>
		<td>0x00000000</td>
		<td>Function succeeds in releasing the acquired reader lock.</td>
	    </tr>
	    <tr>
		<td>E_NOT_READER </td>
		<td>Function call failed. The calling thread has not already aquired the ReaderWriterLock object as a reader</td>
	   </tr>
	</table>
        <para>See <link url="..\..\..\..\reference\basic_data_type\hresult.htm">ECODE</link> for related help.</para>   
    </return>

	<remarks>
		<para>
			If the calling thread has not acquired the ReaderWriterLock object as a reader, the function will fail.
		</para>
	</remarks>

    <requirements>
        <system>
            <system_name>
		Windows 2000 or later; Elastos 2.0 or later.
	    </system_name>
        </system>
    </requirements>
</manpage>