    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_e/methodmanual.xsl"?><manpage>
<volume>System COM Interface</volume>
<interface>IProcess</interface><method> GetStartTime </method>
    <description>
        <para>
			Gets the time when the associated process started. 
        </para>
    </description>
    <syntax>
        <return_val> ECODE </return_val>
        <parameters>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> SystemTime *   </param_type>
                <param_name> *pTime </param_name>
                <param_note>
                    <para>
						Pointer to the starting time.
                    </para>
                </param_note>
            </api_param>       	    
			
        </parameters>
    </syntax>
	
	<return>
	<table>            
	    <th>            
		<td>Return Value </td>            
		<td>Hex Value</td>
		<td>Description</td>            
	    </th>            
	    <tr>            
		<td>NOERROR</td>            
		<td>Succeed in getting the time when the associated process started. </td>            
	    </tr>
	    <tr>            
		<td>E_INVALID_ARGUMENT</td>            
		<td>Invalid argument(s)</td>            
	    </tr> 
	    <tr>            
		<td>E_PROCESS_NOT_STARTED</td>            
		<td>Process hasn't started</td>            
	    </tr>                                    	    
	</table>            	
    </return>

	<remarks>
		<para>
			Returns the time when the associated process started. The start pointer is the time, a process uses the 
			CPU resources, in milliseconds.
		</para>		
		
	</remarks>

    <requirements>
        <system>
            <system_name>
				Windows 2000 or later; Elastos 2.0 or later.            
			</system_name>
        </system>
    </requirements>
</manpage>