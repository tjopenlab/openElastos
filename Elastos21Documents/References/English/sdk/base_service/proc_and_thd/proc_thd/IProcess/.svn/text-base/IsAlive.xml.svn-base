<volume>System COM Interface</volume>
<interface>IProcess</interface><method> IsAlive </method>
    <description>
        <para>
			Checks whether the process is running or not.
        </para>
    </description>
    <syntax>
        <return_val> ECODE </return_val>
        <parameters>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type>Boolean *</param_type>
                <param_name> pIsAlive  </param_name>
                <param_note>
                    <para>
						An array of threads currently running in the associated process.
                    </para>
                </param_note>
            </api_param>       			
			    
        </parameters>
    </syntax>
	
	<return>
	<table>            
	    <th>            
		<td>Return Value</td>            
		<td>Hex Value</td>
		<td>Description</td>            
	    </th>            
	    <tr>            
		<td>NOERROR</td>            
		<td></td>            
	    </tr>
	     <tr>            
		<td>E_INVALID_ARGUMENT</td>            
		<td>Invalid argument</td>            
	    </tr>                	    
	</table>
				    <para>See <link url="../../../../reference/basic_data_type/hresult.htm">ECODE</link> for related help.</para>           	            	
    </return>

	<remarks>
		<para>
			Not implemented.
		</para>		
	</remarks>	

    <requirements>
        <system>
            <system_name>
				Windows 2000 or later; Elastos 2.0 or later.            
			</system_name>
        </system>
    </requirements>
</manpage>