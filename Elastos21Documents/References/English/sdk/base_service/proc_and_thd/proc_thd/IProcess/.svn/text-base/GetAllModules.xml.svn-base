    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_e/methodmanual.xsl"?><manpage>
<volume>System COM Interface</volume>
<interface>IProcess</interface><method> GetModules </method>
    <description>
        <para>
				Get the modules which have been loaded by the associated process. 
        </para>
    </description>
    <syntax>
        <return_val> ECODE </return_val>
        <parameters>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> IModuleEnumerator  </param_type>
                <param_name> **ppModules </param_name>
                <param_note>
                    <para>
							An array of modules which  have been loaded by the  process.
                    </para>
                </param_note>
            </api_param>       			
			    
        </parameters>
    </syntax>
	
	<return>
	<table>            
	    <th>            
		<td>Return Value</td>            
		<td>Hex Value</td>
		<td>Description</td>            
	    </th>            
	    <tr>            
		<td>E_NOTIMPL</td>            
		<td>0x80004001</td>
		<td>Not implemented</td>            
	    </tr>               
	    
	</table>            	
    </return>

	<remarks>
		<para>
			A process module represents a .dll or .exe loaded into a particular process. A process will have a main 
			module, which indicates the executable file that is used to start the process, For example, a normal 
			process can have an ". exe" module or ". dll" modules.
		</para>		
	</remarks>

    <requirements>
        <system>
            <system_name>
				Windows 2000 or later; Elastos 2.0 or later.            
			</system_name>
        </system>
    </requirements>
</manpage>