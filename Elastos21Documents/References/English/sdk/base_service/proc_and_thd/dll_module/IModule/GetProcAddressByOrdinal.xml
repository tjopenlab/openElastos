    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_e/methodmanual.xsl"?><manpage>
<volume>System COM Interface</volume>
<interface>IModule</interface>
<method> GetProcAddressByOrdinal </method>
    <description>
        <para>
         	Gets the address of an exported function by ordinal value.
        </para>
    </description>
    <syntax>
        <return_val> ECODE </return_val>
			<parameters>
				<api_param>
					<param_inout>[in]</param_inout>
					<param_type> Int32 </param_type>
					<param_name>ordinal </param_name>
					<param_note>
						<para>
							Specifies the ordinal value of the function.
						</para>
					</param_note>
				</api_param>
				<api_param>
					<param_inout>[out]</param_inout>
					<param_type> ADDRESS *  </param_type>
					<param_name> pAddress </param_name>
					<param_note>
						<para>
							If the function succeeds, the address of the funtion is returned.
						</para>
					</param_note>
				</api_param>
			</parameters>
    </syntax>

	<return>
	<table>
	    <th>
			<td>Return Value</td>
			<td>Description</td>
	    </th>
	    <tr>
			<td>NOERROR</td>
			<td>Function succeeds in getting the address of an exported function by ordinal value.</td>
	    </tr>
	    <tr>
			<td>E_INVALID_ARGUMENT</td>
			<td>Invalid argument(s)</td>
	    </tr>
	    <tr>
			<td>E_DOES_NOT_EXIST</td>
			<td>The specified function is not found</td>
	    </tr>
	</table>
      	 <para>See <link url="../../../../reference/basic_data_type/hresult.htm">ECODE</link> for related help.</para> 
    </return>

	<remarks>
		<para>
			If the DLL does not export the function with the specified ordinal value, the method returns E_DOES_NOT_EXIST
		</para>
	</remarks>

    <requirements>
        <system>
            <system_name>
				Windows 2000 or later; Elastos 2.0 or later.
			</system_name>
        </system>
    </requirements>
</manpage>