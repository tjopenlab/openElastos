    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_e/methodmanual.xsl"?><manpage>
<volume>System COM Interface</volume>
<interface>IModule</interface>
<method> GetBaseAddress </method>
    <description>
        <para>
         	Gets the base address of the module.
        </para>
    </description>
    <syntax>
        <return_val> ECODE </return_val>
		<parameters>
			<api_param>
				<param_inout>[out]</param_inout>
				<param_type> ADDRESS *  </param_type>
				<param_name> pBase </param_name>
				<param_note>
					<para>
						If the function succeeds, the base address of the module is returned.
					</para>
				</param_note>
			</api_param>
		</parameters>
    </syntax>

	<return>
	<table>
	    <th>
			<td>Return Value</td>
			<td>Hex Value</td>
			<td>Description</td>
	    </th>
	    <tr>
			<td>NOERROR</td>
			<td>0x00000000</td>
			<td>Function succeeds in getting the base address of the module.</td>
	    </tr>
	    <tr>
		    <td>E_INVALID_ARGUMENT</td>
			<td>0x80070057</td>
			<td>Invalid argument(s)</td>
	    </tr>
	</table>
   	   <para>See <link url="../../../../reference/basic_data_type/hresult.htm">ECODE</link> for related help.</para>
    </return>

	<remarks>
		<para>
        		The loaded address of the module is also called the base address of the module. It is the 
        		virtual address of the module in the process memory space. If <i>pAddress</i> is NULL, the method 
        		returns E_INVALIDARG.
		</para>
	</remarks>

    <requirements>
        <system>
            <system_name>
				Windows 2000 or later; Elastos 2.0 or later.
	    	</system_name>
        </system>
    </requirements>
</manpage>