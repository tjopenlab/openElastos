    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_e/methodmanual.xsl"?><manpage>
<volume>System COM Interface</volume>
<interface>IModule</interface>
<method> GetResourceSection </method>
    <description>
        <para>
         	Gets the address of the resource section.
        </para>
    </description>
    <syntax>
        <return_val> ECODE </return_val>
			<parameters>
				<api_param>
					<param_inout>[out]</param_inout>
					<param_type> Address *  </param_type>
					<param_name> pAddress </param_name>
					<param_note>
						<para>
							If the function succeeds, the address of the resource section is returned.
						</para>
					</param_note>
				</api_param>
			</parameters>
    </syntax>

	<return>
	<table>
	    <th>
			<td>Return Value</td>
			<td>Description</td>
	    </th>
	    <tr>
			<td>NOERROR</td>
			<td>Function succeeds in getting the address of the resource section.</td>
	    </tr>
	    <tr>
			<td>E_INVALID_ARGUMENT</td>
			<td>Invalid argument(s)</td>
	    </tr>
	    <tr>
			<td>E_DOES_NOT_EXIST</td>
			<td>The specified resource section is not found</td>
	    </tr>
	</table>
         	 <para>See <link url="../../../../reference/basic_data_type/hresult.htm">ECODE</link> for related help.</para>  
    </return>


    <requirements>
        <system>
            <system_name>
				Windows 2000 or later; Elastos 2.0 or later.
			</system_name>
        </system>
    </requirements>
</manpage>