    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_e/methodmanual.xsl"?><manpage>
<volume>System COM Interface</volume>
<interface>IModule</interface>
<method> FindResource  </method>
    <description>
        <para>
         Obtains  memory address and memory size of specified resource in module.
        </para>
    </description>
    <syntax>
        <return_val> ECODE </return_val>
		<parameters>
			<api_param>
				<param_inout>[in]</param_inout>
				<param_type> WString  </param_type>
				<param_name> wsName </param_name>
				<param_note>
					<para>
					
					</para>
				</param_note>
			</api_param>
		<api_param>
				<param_inout>[in]</param_inout>
				<param_type> WString  </param_type>
				<param_name> wsType </param_name>
				<param_note>
					<para>
						
					</para>
				</param_note>
			</api_param>
			<api_param>
				<param_inout>[in]</param_inout>
				<param_type> Address *  </param_type>
				<param_name> pAddress </param_name>
				<param_note>
					<para>
						
					</para>
				</param_note>
			</api_param>
			<api_param>
				<param_inout>[in]</param_inout>
				<param_type> MemorySize *  </param_type>
				<param_name> pSize</param_name>
				<param_note>
					<para>
						
					</para>
				</param_note>
			</api_param>
		</parameters>
    </syntax>

	<return>
	<table>
	    <th>
			<td>Return Value</td>
			<td>Hex Value</td>
			<td>Description</td>
	    </th>
	    <tr>
			<td>NOERROR</td>
			<td>0x00000000</td>
			<td>Function succeeds in getting the base address of the module.</td>
	    </tr>
	    <tr>
		    <td>E_INVALID_ARGUMENT</td>
			<td>0x80070057</td>
			<td>Invalid argument(s)</td>
	    </tr>
	    <tr>
		    <td>E_DOES_NOT_EXISTED</td>
			<td>No result</td>
	    </tr>
	</table>
	        <para>See <link url="../../../../reference/basic_data_type/hresult.htm">ECODE</link> for related help.</para>
    </return>

	<remarks>
		<para>
        	
		</para>
	</remarks>

    <requirements>
        <system>
            <system_name>
				Windows 2000 or later; Elastos 2.0 or later.
	    	</system_name>
        </system>
    </requirements>
</manpage>