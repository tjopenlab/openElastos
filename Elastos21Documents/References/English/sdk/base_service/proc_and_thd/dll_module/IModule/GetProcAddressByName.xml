    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_e/methodmanual.xsl"?><manpage>
<volume>System COM Interface</volume>
<interface>IModule</interface>
<method> GetProcAddressByName </method>
    <description>
        <para>
         	Gets the starting address of an exported function by its name.
        </para>
    </description>
    <syntax>
        <return_val> ECODE </return_val>
			<parameters>
				<api_param>
					<param_inout>[in]</param_inout>
					<param_type> AString </param_type>
					<param_name> esName </param_name>
					<param_note>
						<para>
							Name of the specified  function,which can not be null
						</para>
					</param_note>
				</api_param>
				<api_param>
					<param_inout>[out]</param_inout>
					<param_type> Address *  </param_type>
					<param_name> pAddress </param_name>
					<param_note>
						<para>
							The address pointer used to get the address of the begin function,and it can not be null
						</para>
					</param_note>
				</api_param>
			</parameters>
    </syntax>

	<return>
	<table>
	    <th>
			<td>Return Value </td>
			<td>Description</td>
	    </th>
	    <tr>
			<td>NOERROR</td>
			<td>Function succeeds in getting the starting address of an exported function by its name.</td>
	    </tr>
	    <tr>
			<td>E_INVALID_ARGUMENT</td>
			<td>Invalid argument(s)</td>
	    </tr>
	    <tr>
			<td>E_DOES_NOT_EXIST</td>
			<td>The specified function is not found</td>
	    </tr>
	</table>
  	  <para>See <link url="../../../../reference/basic_data_type/hresult.htm">ECODE</link> for related help.</para>  
    </return>

	<remarks>
		<para>
			The specified function name must be the same as the function name exported by the DLL, otherwise,
			E_DOES_NOT_EXIST will be returned. 
		</para>
	</remarks>

    <requirements>
        <system>
            <system_name>
				Windows 2000 or later; Elastos 2.0 or later
			</system_name>
        </system>
    </requirements>
</manpage>