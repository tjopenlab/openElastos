<?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_e/interfaceman.xsl"?>
<manpage>
    <volume>System COM Interface</volume>
    <interface> IModule </interface>
    <description>
        <para>
			Module object interface.
        </para>
    </description>
    <methods>
<self>
    <method>
        <method_name>GetBaseAddress</method_name>
        <method_url>IModule\GetBaseAddress</method_url>
    <m_description>
		Gets the base address of the module.
    </m_description>
    </method>
    <method>
        <method_name>GetEntryPointAddress</method_name>
        <method_url>IModule\GetEntryPointAddress</method_url>
    <m_description>
		Gets the entry point address of the module.
    </m_description>
    </method>
    <method>
        <method_name>GetMemorySize</method_name>
        <method_url>IModule\GetMemorySize</method_url>
    <m_description>
		Gets the memory size needed for the module.
    </m_description>
    </method>
    <method>
        <method_name>GetName</method_name>
        <method_url>IModule\GetName</method_url>
    <m_description>
		Gets the name of the module.
    </m_description>
    </method>
    <method>
        <method_name>GetFullPath</method_name>
        <method_url>IModule\GetFullPath</method_url>
    <m_description>
		Gets the full path name of the module.
    </m_description>
    </method>
    <method>
        <method_name>GetProcAddressByName</method_name>
        <method_url>IModule\GetProcAddressByName</method_url>
    <m_description>
		Gets the starting address of an exported function by name.
    </m_description>
    </method>
    <method>
        <method_name>GetProcAddressByOrdinal</method_name>
        <method_url>IModule\GetProcAddressByOrdinal</method_url>
    <m_description>
		Gets the starting address of an exported function by ordinal value.
    </m_description>
    </method>

	<method>
        <method_name>GetResourceSection</method_name>
        <method_url>IModule\GetResourceSection</method_url>
    <m_description>
		Gets the starting address of the resource section.
    </m_description>
    </method>

    <method>
        <method_name>GetSectionByName</method_name>
        <method_url>IModule\GetSectionByName</method_url>
    <m_description>
		Gets the starting address of a section by name.
    </m_description>
    </method>

</self>
    </methods>
    <requirements>
        <system>
            <system_name>
				Windows 2000 or later; Elastos 2.0 or later.
			</system_name>
        </system>
    </requirements>
</manpage>