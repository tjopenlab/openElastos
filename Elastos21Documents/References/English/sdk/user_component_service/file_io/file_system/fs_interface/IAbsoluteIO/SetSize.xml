    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_e/methodmanual.xsl"?><manpage>
<volume>I/O interface</volume>
<interface>IAbsoluteIO</interface><method> SetSize </method>
    <description>
        <para>
       		Sets the size of the file object.
        </para>
    </description>
    <syntax>
        <return_val> ECODE </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> UINT </param_type>
                <param_name> size </param_name>
                <param_note>
                    <para>
						The size of the file to be set.
                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

	<return>
	    <table>
	        <th>
		        <td>Return Value </td>
		        <td>Description</td>
	        </th>
	        <tr>
		        <td>S_OK</td>
		        <td>Function succeeds in setting the size of the file object.</td>
	        </tr>
	        <tr>
		        <td>ECODE_FROM_ERRNO(EACCES)</td>
		        <td>The file object is read-only.</td>
	        </tr>
	    </table>
	    <para>See <link url="../../../../../reference/basic_data_type/hresult.htm">ECODE</link> for related help.</para>
    </return>

	<remarks>
		<para>
		</para>
	</remarks>

    <requirements>
        <system>
            <system_name>
				Windows 2000 or later; Elastos 2.0 or later.
	    </system_name>
        </system>
    </requirements>
</manpage>