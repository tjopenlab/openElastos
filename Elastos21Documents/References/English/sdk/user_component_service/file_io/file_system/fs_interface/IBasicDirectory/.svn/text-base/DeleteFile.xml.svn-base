    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_e/methodmanual.xsl"?><manpage>
<volume>Directory Object Interface</volume>
<interface>IBasicDirectory</interface><method> DeleteFile </method>
    <description>
        <para>
        	Deletes a file in current directory object.
        </para>
    </description>
    <syntax>
        <return_val> ECODE </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> EzStr </param_type>
                <param_name> name </param_name>
                <param_note>
                    <para>
						Name of the file to be deleted. <i>name</i> must be the file name and should not 
						include path separator '/'.
                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

	<return>
	    <table>
	        <th>
		        <td>Return Value </td>
		        <td>Description</td>
	        </th>
	        <tr>
		        <td>S_OK</td>
		        <td>Function succeeds in deleting a file.</td>
	        </tr>
	        <tr>
		        <td>E_INVALIDARG</td>
		        <td>Parameter name is NULL.</td>
	        </tr>
	        <tr>
		        <td>ECODE_FROM_ERRNO(ENOENT)</td>
		        <td>File object does not exist.</td>
	        </tr>
	        <tr>
	    	    <td>ECODE_FROM_ERRNO(EBUSY)</td>
	    	    <td>File object is in use.</td>
	        </tr>
	    </table>
	    <para>See <link url="../../../../../reference/basic_data_type/hresult.htm">ECODE</link> for related help.</para>
    </return>

	<remarks>
		<para>
		</para>
	</remarks>

    <requirements>
        <system>
            <system_name>
				Windows 2000 or later; Elastos 2.0 or later.</system_name>
        </system>
    </requirements>
</manpage>