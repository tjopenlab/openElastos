    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_e/methodmanual.xsl"?><manpage>
<volume>Directory Object Interface</volume>
<interface>IBasicDirectory</interface><method> CreateDirectory </method>
    <description>
        <para>
        	Creates a subdirectory under the current directory object.
        </para>
    </description>
    <syntax>
        <return_val> ECODE </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> EzStr </param_type>
                <param_name> name </param_name>
                <param_note>
                    <para>
						The name of the subdirectory to be created. <i>name</i> must be the directory name and 
						should not include path separator'/'.
                    </para>
                </param_note>
            </api_param>
        </parameters>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> enum FileAccessMode </param_type>
                <param_name> mode </param_name>
                <param_note>
                    <para>
						The access mode of the subdirectory created:
                            FileAccessMode_Read means the file is readable;
        	                FileAccessMode_Write means the file is writable;
                            FileAccessMode_ReadWrite means the file is readable and writable.
                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

	<return>
	    <table>
	        <th>
		        <td>Return Value </td>
		        <td>Description</td>
	        </th>
	        <tr>
		        <td>S_OK</td>
		        <td>Function succeeds in creating new directory.</td>
	        </tr>
	        <tr>
		        <td>E_INVALIDARG</td>
		        <td>Parameter name is NULL, or the value of parameter mode is illegal.</td>
	        </tr>
	        <tr>
		        <td>ECODE_FROM_ERRNO(ENOENT)</td>
		        <td>Directory pointed to by directory pointer does not exist.</td>
	        </tr>
	        <tr>
	    	    <td>ECODE_FROM_ERRNO(EEXIST)</td>
	    	    <td>File or subdirectory with the same name exists.</td>
	        </tr>
	        <tr>
	    	    <td>ECODE_FROM_ERRNO(EINVAL)</td>
	    	    <td>There is(are) illegal character(s) in the subdirectory name.</td>
	        </tr>
	    </table>
	    <para>See <link url="../../../../../reference/basic_data_type/hresult.htm">ECODE</link> for related help.</para>
    </return>

	<remarks>
		<para>
		</para>
	</remarks>

    <requirements>
        <system>
            <system_name>
				Windows 2000 or later; Elastos 2.0 or later.
			</system_name>
        </system>
    </requirements>
</manpage>