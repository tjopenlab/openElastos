<self>
    <method>
        <method_name>Current</method_name>
        <method_url>IEzStrEnumerator\Current</method_url>
    <m_description>
		Gets the EzStr object copy to which EzStr object enumerator is pointing.
    </m_description>
    </method>
    <method>
        <method_name>MoveNext</method_name>
        <method_url>IEzStrEnumerator\MoveNext</method_url>
    <m_description>
		Makes the EzStr object enumerator point to the next object in EzStr object set.
    </m_description>
    </method>
    <method>
        <method_name>Reset</method_name>
        <method_url>IEzStrEnumerator\Reset</method_url>
    <m_description>
		Resets EzStr object enumerator, and makes it point before the first element in EzStr object set.
    </m_description>
    </method>
</self>
