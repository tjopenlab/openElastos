    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_e/interfaceman.xsl"?>    <manpage>
    <volume>File System Interface</volume>
    <interface> IBasicFileSystem </interface>
    <description>
        <para>
            File system object interface.
        </para>
    </description>
    <methods>
<self>
    <method>
        <method_name>GetRoot</method_name>
        <method_url>IBasicFileSystem\GetRoot</method_url>
    <m_description>
		Gets the root directory of file system.
    </m_description>
    </method>
    <method>
        <method_name>Lookup</method_name>
        <method_url>IBasicFileSystem\Lookup</method_url>
    <m_description>
		Looks up file object or directory object according to the given path which starts from the root directory.
    </m_description>
    </method>
    <method>
        <method_name>Sync</method_name>
        <method_url>IBasicFileSystem\Sync</method_url>
    <m_description>		
		Writes all the data in file system memory into pernament storage. 
    </m_description>
    </method>
    <method>
        <method_name>GetFileSystemStat</method_name>
        <method_url>IBasicFileSystem\GetFileSystemStat</method_url>
    <m_description>
        Writes the information in file system to the buffer.
    </m_description>
    </method>
</self>
    </methods>
    <requirements>
        <system>
            <system_name>
				Windows 2000 or later£»Elastos 2.0 or later.
			</system_name>
        </system>
    </requirements>
</manpage>