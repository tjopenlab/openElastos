    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_e/methodmanual.xsl"?><manpage>
<volume>Virtual File System Object Interface.</volume>
<interface>IVfsFileSystem</interface><method> Unmount </method>
    <description>
        <para>
        	Unmount a file system.
        </para>
    </description>
    <syntax>
        <return_val> ECODE </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> EzStr </param_type>
                <param_name> dir </param_name>
                <param_note>
                    <para>
						 The file system root directory path that is to be unmounted.
                    </para>
                </param_note>
            </api_param>
        </parameters>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> UINT </param_type>
                <param_name> flags </param_name>
                <param_note>
                    <para>
						Unmout flags.
                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

	<return>
	    <table>
	        <th>
		        <td>Return Value </td>
		        <td>Description</td>
	        </th>
	        <tr>
		        <td>S_OK</td>
		        <td>Function succeeds in unmounting.</td>
	        </tr>
	        <tr>
		        <td>E_INVALIDARG</td>
		        <td>Parameter dir is NULL.</td>
	        </tr>
	        <tr>
		        <td>ECODE_FROM_ERRNO(EINVAL)</td>
		        <td>Unmount object is root file system.</td>
	        </tr>
	    </table>
	    <para>See <link url="../../../../../reference/basic_data_type/hresult.htm">ECODE</link> for related help.</para>
    </return>

	<remarks>
		<para>
		</para>
	</remarks>

    <requirements>
        <system>
            <system_name>
				Windows 2000 or later; Elastos 2.0 or later.
			</system_name>
        </system>
    </requirements>
</manpage>