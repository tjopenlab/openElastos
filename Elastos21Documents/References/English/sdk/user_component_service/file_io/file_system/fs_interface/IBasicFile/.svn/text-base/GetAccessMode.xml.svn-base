    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_e/methodmanual.xsl"?><manpage>
<volume>File Object Interface</volume>
<interface>IBasicFile</interface><method> GetAccessMode </method>
    <description>
        <para>
       		Gets the access mode of the current object. 
        </para>
    </description>
    <syntax>
        <return_val> ECODE </return_val>
        <parameters>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> enum FileAccessMode * </param_type>
                <param_name> pMode </param_name>
                <param_note>
                    <para>
						Gained file access mode:
                    </para>
					<para>
						    FileAccessMode_Read means file is readable;
					</para>
					<para>
                            FileAccessMode_Write means file is writable;
                    </para>
                    <para>
                            FileAccessMode_ReadWrite means file is readable and writable.
                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

	<return>
	    <table>
	        <th>
		        <td>Return Value </td>
		        <td>Description</td>
	        </th>
	        <tr>
		        <td>S_OK</td>
		        <td>Function succeeds in getting the access mode.</td>
	        </tr>
	        <tr>
		        <td>E_INVALIDARG</td>
		        <td>Parameter pMode is NULL.</td>
	        </tr>
	        <tr>
		        <td>E_FAIL</td>
		        <td>Abnormal read/write mode.</td>
	        </tr>
	    </table>
	    <para>See <link url="../../../../../reference/basic_data_type/hresult.htm">ECODE</link> related help.</para>
    </return>

	<remarks>
		<para>
		</para>
	</remarks>

    <requirements>
        <system>
            <system_name>
				Windows 2000 or later; Elastos 2.0 or later
			</system_name>
        </system>
    </requirements>
</manpage>