    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_e/methodmanual.xsl"?><manpage>
<volume>File Object Interface</volume>
<interface>IBasicFile</interface><method> CheckAccessMode </method>
    <description>
        <para>
        	Checks if the current object has specified access mode.
        </para>
    </description>
    <syntax>
        <return_val> ECODE </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> enum FileAccessMode </param_type>
                <param_name> mode </param_name>
                <param_note>
                    <para>
						File access mode to be checked;
				    </para>
					<para>
						    FileAccessMode_Read means the file is readable;
					</para>
					<para>
                            FileAccessMode_Write means the file is writable;
                    </para>
                    <para>
                            FileAccessMode_ReadWrite means file is readable and writable.
                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

	<return>
	    <table>
	        <th>
		        <td>Return Value </td>
		        <td>Description</td>
	        </th>
	        <tr>
		        <td>S_OK</td>
		        <td>Function succeeds in checking the specified access mode.</td>
	        </tr>
	        <tr>
		        <td>S_FALSE</td>
		        <td>Function succeeds in checking but object does not have the specified access mode.</td>
	        </tr>
	        <tr>
		        <td>E_INVALIDARG</td>
		        <td>Invalid mode.</td>
	        </tr>
	    </table>
    </return>

	<remarks>
		<para>
		</para>
	</remarks>

    <requirements>
        <system>
            <system_name>
				Windows 2000 or later; Elastos 2.0 or later
			</system_name>
        </system>
    </requirements>
</manpage>