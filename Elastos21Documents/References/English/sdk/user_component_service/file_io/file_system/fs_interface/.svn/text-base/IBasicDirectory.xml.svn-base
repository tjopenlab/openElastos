    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_e/interfaceman.xsl"?>    <manpage>
    <volume>File System Interface</volume>
    <interface> Directory Object Interface:IBasicDirectory </interface>
    <description>
        <para>

        </para>
    </description>
    <methods>
<self>
    <method>
        <method_name>Lookup</method_name>
        <method_url>IBasicDirectory\Lookup</method_url>
    <m_description>
		Looks up a file or subdirectory in current directory.
    </m_description>
    </method>
    <method>
        <method_name>CreateFile</method_name>
        <method_url>IBasicDirectory\CreateFile</method_url>
    <m_description>
		Creates a file in current directory object.
    </m_description>
    </method>
    <method>
        <method_name>DeleteFile</method_name>
        <method_url>IBasicDirectory\DeleteFile</method_url>
    <m_description>
		Deletes a file in current directory object.
    </m_description>
    </method>
    <method>
        <method_name>Rename</method_name>
        <method_url>IBasicDirectory\Rename</method_url>
    <m_description>
        Renames the file or subdirectory in current directory, or move them to another directory.
    </m_description>
    </method>
    <method>
        <method_name>CreateDirectory</method_name>
        <method_url>IBasicDirectory\CreateDirectory</method_url>
    <m_description>
        Creats a subdirectory in current directory object.
    </m_description>
    </method>
    <method>
        <method_name>DeleteDirectory</method_name>
        <method_url>IBasicDirectory\DeleteDirectory</method_url>
    <m_description>
       Deletes a subdirectory in current directory object.
    </m_description>
    </method>
    <method>
        <method_name>GetDirEntries</method_name>
        <method_url>IBasicDirectory\GetDirEntries</method_url>
    <m_description>
        Reads all the subdirectories and file items of the current directory object.
    </m_description>
    </method>
</self>
    </methods>
    <requirements>
        <system>
            <system_name>
