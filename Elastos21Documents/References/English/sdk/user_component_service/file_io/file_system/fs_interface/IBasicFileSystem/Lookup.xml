    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_e/methodmanual.xsl"?><manpage>
<volume>File System Object Interface</volume>
<interface>IBasicFileSystem</interface><method> Lookup </method>
    <description>
        <para>
        	Search for the file object or directory object according to given path name, which 
        	starts from root directory.
        </para>
    </description>
    <syntax>
        <return_val> ECODE </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> EzStr </param_type>
                <param_name> pathname </param_name>
                <param_note>
                    <para>
						The path name of the searching object.
                    </para>
                </param_note>
            </api_param>
        </parameters>
        <parameters>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> IBasicFile** </param_type>
                <param_name> piBasicFile </param_name>
                <param_note>
                    <para>
						The file object or directory object.
                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

	<return>
	    <table>
	        <th>
		        <td>Return Value </td>
		        <td>Description</td>
	        </th>
	        <tr>
		        <td>S_OK</td>
		        <td>Function succeeds in look up.</td>
	        </tr>
	        <tr>
		        <td>E_INVALIDARG</td>
		        <td>Pathname is NULL or piBasicFile is NULL</td>
	        </tr>
	        <tr>
		        <td>E_NOINTERFACE</td>
		        <td>Interface lookup fails.</td>
	        </tr>
	        <tr>
	    	    <td>E_OUTOFMEMORY</td>
	    	    <td>Memory is not enough, directory or file object creation fails.</td>
	        </tr>
	        <tr>
		        <td>E_FAIL</td>
		        <td>Root file system does not exist or cant be found.</td>
	        </tr>
	        <tr>
	    	    <td>ECODE_FROM_ERRNO(ENOENT)</td>
	    	    <td>File or directory reprensented by pathname does not exist.</td>
	        </tr>
	    </table>
	    <para>See <link url="../../../../../reference/basic_data_type/hresult.htm">ECODE</link> for related help.</para>
    </return>

	<remarks>
		<para>
			Release <i>piBasicFile</i> by calling piBasicFile->Release(); to avoid memory leak.
		</para>
	</remarks>

    <requirements>
        <system>
            <system_name>
				Windows 2000 or later; Elastos 2.0 or later.
			</system_name>
        </system>
    </requirements>
</manpage>