    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_e/methodmanual.xsl"?><manpage>
<volume>File Object Interface</volume>
<interface>IBasicFile</interface><method> GetType </method>
    <description>
        <para>
	        Gets the file type of the current object.
	        If the current object is a common file, <i>pType</i> will be FileType_RegularFile£»
	        If current object is a directory, <i>pType</i> will be FileType_Directory.
        </para>
    </description>
    <syntax>
        <return_val> ECODE </return_val>
        <parameters>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> enum FileType * </param_type>
                <param_name> pType </param_name>
                <param_note>
                    <para>
						Type of the file.
                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

	<return>
	    <table>
	        <th>
		        <td>Return Value </td>
		        <td>Description</td>
	        </th>
	        <tr>
		        <td>S_OK</td>
		        <td>Funciton succeeds in getting the type of the file.</td>
	        </tr>
	        <tr>
		        <td>E_INVALIDARG</td>
		        <td>Parameter pType is NULL.</td>
	        </tr>
	        <tr>
		        <td>E_FAIL</td>
		        <td>Invalid file type.</td>
	        </tr>
	    </table>
	    <para>See <link url="../../../../../reference/basic_data_type/hresult.htm">ECODE</link> for related help.</para>
    </return>

	<remarks>
		<para>
		</para>
	</remarks>

    <requirements>
        <system>
            <system_name>
				Windows 2000 or later; Elastos 2.0 or later
			</system_name>
        </system>
    </requirements>
</manpage>