    <methods>
<self>
    <method>
        <method_name>Read</method_name>
        <method_url>IAbsoluteIO\Read</method_url>
    <m_description>
		Reads a block of bytes from the specific offset of file object, and copies the data to the given buffer.
    </m_description>
    </method>
    <method>
        <method_name>Write</method_name>
        <method_url>IAbsoluteIO\Write</method_url>
    <m_description>
		Writes the data in buffer into the file object, and returns the data size which are actually written.
    </m_description>
    </method>
    <method>
        <method_name>GetSize</method_name>
        <method_url>IAbsoluteIO\GetSize</method_url>
    <m_description>
		Gets the size of the file object.
    </m_description>
    </method>
    <method>
        <method_name>SetSize</method_name>
        <method_url>IAbsoluteIO\SetSize</method_url>
    <m_description>
        Specifies the size of file object.
    </m_description>
    </method>
    <method>
        <method_name>GetOptimalBlockSize</method_name>
        <method_url>IAbsoluteIO\GetOptimalBlockSize</method_url>
    <m_description>
        Gets Optimal Block Size.
    </m_description>
    </method>
</self>
