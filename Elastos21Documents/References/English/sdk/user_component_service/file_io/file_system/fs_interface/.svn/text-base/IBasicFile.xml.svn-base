    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_e/interfaceman.xsl"?>    <manpage>
    <volume>File System Interface</volume>
    <interface> File Object Interface£ºIBasicFile </interface>
    <description>
        <para>

        </para>
    </description>
    <methods>
<self>
    <method>
        <method_name>Sync</method_name>
        <method_url>IBasicFile\Sync</method_url>
    <m_description>
		Writes the all data of current objects to permanent storage. 
    </m_description>
    </method>
    <method>
        <method_name>GetType</method_name>
        <method_url>IBasicFile\GetType</method_url>
    <m_description>
		Gets the file type of the current object. 
    </m_description>
    </method>
    <method>
        <method_name>GetAccessTime</method_name>
        <method_url>IBasicFile\GetAccessTime</method_url>
    <m_description>
		Gets the last accessed time. Returns time in UTC since January 1, 1970, 00:00:00 to the time of last 
		access, the unit is in seconds. 
    </m_description>
    </method>
    <method>
        <method_name>SetAccessTime</method_name>
        <method_url>IBasicFile\SetAccessTime</method_url>
    <m_description>
        Sets the last access time. Returns time in UTC since January 1, 1970, 00:00:00 to the last access 
        time; the unit is in seconds. 
    </m_description>
    </method>
    <method>
        <method_name>GetModificationTime</method_name>
        <method_url>IBasicFile\GetModificationTime</method_url>
    <m_description>
        Gets the last modified time of the current object. Returns time in UTC since January 1, 1970, 00:00:00 
        to the last modified time, the unit is in seconds. 
    </m_description>
    </method>
    <method>
        <method_name>SetModificationTime</method_name>
        <method_url>IBasicFile\SetModificationTime</method_url>
    <m_description>
         Sets the last modified time of the current object. Returns time in UTC since January 1, 1970, 00:00:00 
         to the last modified time; the unit is in seconds. 
    </m_description>
    </method>
    <method>
        <method_name>GetCreateTime</method_name>
        <method_url>IBasicFile\GetCreateTime</method_url>
    <m_description>
         Gets the created time of the current object. Returns time in UTC since January 1, 1970, 00:00:00 to 
         the created time, the unit is in seconds.
    </m_description>
    </method>
    <method>
        <method_name>CheckAccessMode</method_name>
        <method_url>IBasicFile\CheckAccessMode</method_url>
    <m_description>
        Checks if the current object has specified access mode. 
    </m_description>
    </method>
    <method>
        <method_name>GetAccessMode</method_name>
        <method_url>IBasicFile\GetAccessMode</method_url>
    <m_description>
        Gets the access mode of the current object. 
    </m_description>
    </method>
    <method>
        <method_name>SetAccessMode</method_name>
        <method_url>IBasicFile\SetAccessMode</method_url>
    <m_description>
         Sets the access mode of the current object. 
    </m_description>
    </method>
</self>
    </methods>
    <requirements>
        <system>
            <system_name>
                           Windows 2000 or later£»Elastos 2.0 or later.
            			</system_name>
        </system>
    </requirements>
</manpage>