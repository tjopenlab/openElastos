    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_e/methodmanual.xsl"?><manpage>
<volume>File System Object Interface</volume>
<interface>IBasicFileSystem</interface><method> Sync </method>
    <description>
        <para>
        	Writes all the data in file system memory into pernament storage.
        </para>
    </description>
    <syntax>
        <return_val> ECODE </return_val>
    </syntax>

	<return>
	<table>
	    <th>
		<td>Return Value </td>
		<td>Hex Value</td>
		<td>Description</td>
	    </th>
	    <tr>
		<td>S_OK</td>
		<td>0x00000000</td>
		<td>Function succeeds.</td>
	    </tr>
	</table>
    </return>

	<remarks>
		<para>
		    Users can write new data into permanent storage using Sync after doing one or more
			File write operations.
		</para>
	</remarks>

    <requirements>
        <system>
            <system_name>
				Windows 2000 or later; Elastos 2.0 or later.
			</system_name>
        </system>
    </requirements>
</manpage>