    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_e/methodmanual.xsl"?><manpage>
<volume>Socket Interface </volume>
<interface>Isocket</interface><method> GetPeerName </method>
    <description>
        <para>
         获取对方的套节口地址（IP和PORT）
        </para>
    </description>
    <syntax>
        <return_val> ECODE </return_val>
        <parameters>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> EzByteBuf </param_type>
                <param_name> Name </param_name>
                <param_note>
                    <para>
						对方的套节口地址，其capacity至少大于sizeof(SOCKADDR)，返回的数据结构为struct sockaddr。
                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

	<return>
	<table>
	    <th>
		<td>Returns </td>
		<td>Value</td>
		<td>Description</td>
	    </th>
	    <tr>
		<td>S_OK</td>
		<td>0x00000000</td>
		<td>方法调用成功。 </td>
	    </tr>
	    <tr>
		<td>STATUS_WIN32_ERROR(error)</td>
		<td>见附录</td>
		<td>见附录</td>
	    </tr>
	</table>
    </return>

	<remarks>
		<para>
		</para>
	</remarks>

    <requirements>
        <system>
            <system_name>
				Windows 2000 或更高版本; 《和欣2.0》或更高版本
			</system_name>
        </system>
    </requirements>
</manpage>