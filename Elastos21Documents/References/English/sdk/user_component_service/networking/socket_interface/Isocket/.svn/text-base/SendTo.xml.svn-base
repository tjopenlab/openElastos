    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_e/methodmanual.xsl"?><manpage>
<volume>Socket Interface </volume>
<interface>Isocket</interface><method> SendTo </method>
    <description>
        <para>
         向指定目的地发送数据
        </para>
    </description>
    <syntax>
        <return_val> ECODE </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> UINT32  </param_type>
                <param_name> flags </param_name>
                <param_note>
                    <para>
						调用方式标志位，普通方式为0。
                    </para>
                </param_note>
            </api_param>
        </parameters>
       <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> EzByteBuf   </param_type>
                <param_name> ebbBuffer </param_name>
                <param_note>
                    <para>
						发送缓冲区，其used为期望发送的数据字节数。
                    </para>
                </param_note>
            </api_param>
        </parameters>
        <parameters>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> UINT32 * </param_type>
                <param_name> Len </param_name>
                <param_note>
                    <para>
						返回的实际发送的字节数。
                    </para>
                </param_note>
            </api_param>
        </parameters>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> EzByteBuf  </param_type>
                <param_name> To </param_name>
                <param_note>
                    <para>
						指向目的套节口的地址，其数据结构类型为struct sockaddr。
                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

	<return>
	<table>
	    <th>
		<td>Returns </td>
		<td>Value</td>
		<td>Description</td>
	    </th>
	    <tr>
		<td>S_OK</td>
		<td>0x00000000</td>
		<td>方法调用成功。 </td>
	    </tr>
	    <tr>
		<td>STATUS_WIN32_ERROR(error)</td>
		<td>见附录</td>
		<td>见附录</td>
	    </tr>
	</table>
    </return>

	<remarks>
		<para>
		</para>
	</remarks>

    <requirements>
        <system>
            <system_name>
				Windows 2000 或更高版本; 《和欣2.0》或更高版本
			</system_name>
        </system>
    </requirements>
</manpage>