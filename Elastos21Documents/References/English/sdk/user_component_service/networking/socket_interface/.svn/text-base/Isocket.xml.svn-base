    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="../../../../xsl/xsl_e/interfaceman.xsl"?>    <manpage>
    <volume>Socket Interface</volume>
    <interface> Isocket </interface>
    <description>
        <para>

        </para>
    </description>
    <methods>
<self>
    <method>
        <method_name>Accept</method_name>
        <method_url>Isocket\Accept</method_url>
    <m_description>
		接受一次连接。
    </m_description>
    </method>
    <method>
        <method_name>Bind</method_name>
        <method_url>Isocket\Bind</method_url>
    <m_description>
		将本地地址与一套接口捆绑。
    </m_description>
    </method>
    <method>
        <method_name>Connect</method_name>
        <method_url>Isocket\Connect</method_url>
    <m_description>
		与网络另一端的套接口建立连接。
    </m_description>
    </method>
    <method>
        <method_name>GetPeerName</method_name>
        <method_url>Isocket\GetPeerName</method_url>
    <m_description>
        获取连接对方的套接口地址。
    </m_description>
    </method>
    <method>
        <method_name>GetSockName</method_name>
        <method_url>Isocket\GetSockName</method_url>
    <m_description>
        获取套接口的本地地址。
    </m_description>
    </method>
    <method>
        <method_name>Listen</method_name>
        <method_url>Isocket\Listen</method_url>
    <m_description>
        将套接口设为监听状态，准备被连接。
    </m_description>
    </method>
    <method>
        <method_name>Recv</method_name>
        <method_url>Isocket\Recv</method_url>
    <m_description>
        从套接口接收数据。
    </m_description>
    </method>
    <method>
        <method_name>RecvFrom</method_name>
        <method_url>Isocket\RecvFrom</method_url>
    <m_description>
        接收一个数据报并保存源地址。
    </m_description>
    </method>
    <method>
        <method_name>Send</method_name>
        <method_url>Isocket\Send</method_url>
    <m_description>
        向已连接套接口发送数据。
    </m_description>
    </method>
    <method>
        <method_name>SendTo</method_name>
        <method_url>Isocket\SendTo</method_url>
    <m_description>
        向指定目的地发送数据。
    </m_description>
    </method>
    <method>
        <method_name>Shutdown</method_name>
        <method_url>Isocket\Shutdown</method_url>
    <m_description>
        禁止在套接口上进行数据的接收或发送，但并不释放套接口占用的系统资源。
    </m_description>
    </method>

</self>
    </methods>
    <requirements>
        <system>
            <system_name>
	Windows 2000 或更高版本; 《和欣2.0》或更高版本
			</system_name>
        </system>
    </requirements>
</manpage>