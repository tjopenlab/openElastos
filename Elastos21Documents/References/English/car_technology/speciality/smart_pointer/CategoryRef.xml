<manpage>
    <volume> Category Smart Pointer </volume>
    <interface> CategoryRef </interface>

    <description>
        <para>Category smart pointer class shared method. Category represents any category name.</para>
    </description>

    <methods>
        <self>
        <method>
        <method_name>Instantiate</method_name>
        <method_url>CategoryRef/Instantiate</method_url>
        <m_description>Creates a Category category object and makes corresponding category smart pointer points to this object</m_description>
        </method>
        <method>
        <method_name>IsValid</method_name>
        <method_url>CategoryRef/IsValid</method_url>
        <m_description> Validate Category category smart pointer</m_description>
        </method>
    </self>
    </methods>

    <operators>
        <operator>
            <operator_name>operator ==</operator_name>
            <operator_url>CategoryRef/operator</operator_url>
            <operator_description>Checks if the current category smart pointer points to the same object as specified category smart pointer</operator_description>
        </operator>
    </operators>

</manpage>