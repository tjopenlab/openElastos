<?xml:stylesheet type="text/xsl" href="/xsl/xsl_e/methodmanual.xsl"?>

<manpage>
    <volume> Class Smart Pointer Class Shared Method </volume>
    <interface> ClassNameRef </interface>
    <method>operator ==</method>

    <description>
        <para>Checks if the current class smart pointer points to the same object as specified class smart pointer.</para>
    </description>

    <syntax>
        <return_val>BOOL</return_val>
        <parameters>
        <api_param>
            <param_type>const ClassNameRef&amp;</param_type>
            <param_name> cRef </param_name>
            <param_note>
                <para> Specified class smart pointer </para>
            </param_note>
        </api_param>

        </parameters>
    </syntax>

    <return>
        <para>If two class smart pointers point to the same object, returns TRUE; otherwise, returns FALSE.</para>
    </return>

    <example><pre><![CDATA[
//g_ClassName is class smart pointer that points to ClassName class object.
ClassNameRef g_ClassName;

void Foo(ClassNameRef& senderRef)
{
    if (!senderRef.IsValid()) { //validate class smart pointer senderRef by invoking <b>IsValid</b> method.
        assert(0 && "senderRef is invalid.");
        return;
    }

    if (senderRef == g_ClassName) { //checks whether senderRef and g_ClassName points to the same object.
        бнбн
        return;
    }
    бнбн
}
]]></pre></example>

</manpage>