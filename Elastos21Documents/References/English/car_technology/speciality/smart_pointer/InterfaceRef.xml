<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_e/interfaceman.xsl"?>

<manpage>
    <volume> Interface Smart Pointer </volume>
    <interface> InterfaceRef </interface>

    <description>
        <para>Method owned by all interface smart pointer classes. Interface represents any interface name.</para>
    </description>

    <methods>
        <self>
            <method>
                <method_name>Instantiate</method_name>
                <method_url>InterfaceRef/Instantiate</method_url>
                <m_description>Creates specified class object, current interface smart pointer points to Interface interface in this object.</m_description>
            </method>
            <method>
                <method_name>IsValid</method_name>
                <method_url>InterfaceRef/IsValid</method_url>
                <m_description>Validate interface smart pointer.</m_description>
            </method>
            <method>
                <method_name>IsSameObject</method_name>
                <method_url>InterfaceRef/IsSameObject</method_url>
                <m_description>Checks if the current interface smart pointer points to the same object as the specified smart pointer.</m_description>
            </method>
            <method>
                <method_name>IsSameInterface</method_name>
                <method_url>InterfaceRef/IsSameInterface</method_url>
                <m_description> Checks if the current interface smart pointer points to the same interface in the same object as the specified smart pointer.</m_description>
            </method>
        </self>
    </methods>

</manpage>