<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_e/interfaceman.xsl"?>

<manpage>
    <volume> Class Smart Pointer </volume>
    <interface> ClassNameRef </interface>

    <description>
        <para>Class smart pointer class shared method. ClassName represents any class name.</para>
    </description>

    <methods>
        <self>
        <method>
        <method_name>Instantiate</method_name>
        <method_url>ClassNameRef/Instantiate</method_url>
        <m_description>Creates ClassName class object, makes corresponding class smart pointer points to this object</m_description>
        </method>
        <method>
        <method_name>IsValid</method_name>
        <method_url>ClassNameRef/IsValid</method_url>
        <m_description> Validate class smart pointer of ClassName</m_description>
        </method>
    </self>
    </methods>

    <operators>
        <operator>
            <operator_name>operator ==</operator_name>
            <operator_url>ClassNameRef/operator</operator_url>
            <operator_description>Checks if the current class smart pointer points to the same object as specified class smart pointer.</operator_description>
        </operator>
    </operators>

</manpage>