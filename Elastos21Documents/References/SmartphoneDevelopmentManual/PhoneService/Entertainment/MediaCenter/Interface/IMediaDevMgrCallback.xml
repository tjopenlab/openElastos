    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/interfaceman.xsl"?>    <manpage>
    <volume>媒体中心引擎</volume>
    <interface> IMediaDevMgrCallback </interface>
    <description>
        <para>
         多媒体设备管理控制器事件接口。
        </para>
    </description>
    <methods>
<self>
    <method>
        <method_name>HangUp</method_name>
        <method_url>..\interface\IMediaDevMgrCallback\HangUp</method_url>
    <m_description>
     阻塞播放事件，并且做现场保护。
    </m_description>
    </method>
    <method>
        <method_name>Resume</method_name>
        <method_url>..\interface\IMediaDevMgrCallback\Resume</method_url>
    <m_description>
     使被阻塞的模块可以继续使用设备的事件。
    </m_description>
    </method>
    <method>
        <method_name>HeadsetAttach</method_name>
        <method_url>..\interface\IMediaDevMgrCallback\HeadsetAttach</method_url>
    <m_description>
     耳机插入事件。
    </m_description>
    </method>
    <method>
        <method_name>HeadsetDetach</method_name>
        <method_url>..\interface\IMediaDevMgrCallback\HeadsetDetach</method_url>
    <m_description>
     耳机拔下事件。
    </m_description>
    </method>
</self>
    </methods>
    <requirements>
        <system>
            <system_name>
《和欣 2.0》或更高版本            </system_name>
        </system>
    </requirements>
</manpage>