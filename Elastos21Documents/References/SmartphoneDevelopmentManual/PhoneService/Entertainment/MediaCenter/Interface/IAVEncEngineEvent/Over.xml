    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume>媒体中心引擎</volume>
<interface>IAVEncEngineEvent</interface><method> Over </method>
    <description>
        <para>
          编码结束事件。由于目前在Stop接口方法中采用了同步等待的方式，该接口方法暂时保留。
        </para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> UINT64 * </param_type>
                <param_name> u64Size </param_name>
                <param_note>
                    <para>
                    编码结束后生成的文件的总时长
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> UINT * </param_type>
                <param_name> uDuration </param_name>
                <param_note>
                    <para>
                    编码结束后生成的文件的字节数
                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>
     <return>
	    <table>
	        <th>
	    	    <td>返回值 </td>
	    	    <td>描述</td>
	        </th>
	        <tr>
	    	    <td>NOERROR</td>
	    	    <td>程序执行成功</td>
	        </tr>

	    </table>
	    <para></para>
    </return>
    <requirements>
        <system>
            <system_name>
《和欣 2.0》或更高版本            </system_name>
        </system>

    </requirements>
</manpage>