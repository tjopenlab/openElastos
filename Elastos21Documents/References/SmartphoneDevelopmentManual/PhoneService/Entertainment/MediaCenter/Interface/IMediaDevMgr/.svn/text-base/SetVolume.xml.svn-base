    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume>媒体中心引擎</volume>
<interface>IMediaDevMgr</interface><method> SetVolume </method>
    <description>
        <para>
          设置音量，包括主音量，左声道音量和右声道音量。
        </para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> void * </param_type>
                <param_name> pThis </param_name>
                <param_note>
                    <para>
                      调用模块的对象指针，不能为空。通过判断本指针与设备管理器中当前允许使用设备的模块指针进行比较来决定是否允许该调用请求
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> UINT </param_type>
                <param_name> uMainVol </param_name>
                <param_note>
                    <para>
                       以百分制表示的音量值，范围为0~100
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> UINT </param_type>
                <param_name> uLeftVol </param_name>
                <param_note>
                    <para>
                      以百分制表示的音量值，范围为0~100
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> UINT </param_type>
                <param_name> uRightVol </param_name>
                <param_note>
                    <para>
                       以百分制表示的音量值，范围为0~100
                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>
     <return>
	    <table>
	        <th>
	    	    <td>返回值 </td>
	    	    <td>描述</td>
	        </th>
	        <tr>
	    	    <td>NOERROR</td>
	    	    <td>程序执行成功</td>
	        </tr>
	        <tr>
	    	    <td>E_INVALID_ARGUMENT </td>
	    	    <td>参数错误</td>
	        </tr>
	         <tr>
	    	    <td>E_MEDIA_DEVICE_OUT_OF_YOUR_HAND </td>
	    	    <td> 调用本函数的模块不具备使用当前设备的使用权限  </td>
	        </tr>
	         <tr>
	    	    <td>驱动错误码 </td>
	    	    <td>调用声卡驱动失败</td>
	        </tr>

	    </table>
	   <para>参见<link url="../../../../../QuickReference/ECode.htm">ECode</link>和<link url="../../MediaCenter_Error.htm">MediaCenter_Error</link>相关帮助。</para>
    </return>
    <requirements>
        <system>
            <system_name>
《和欣 2.0》或更高版本            </system_name>
        </system>

    </requirements>
</manpage>