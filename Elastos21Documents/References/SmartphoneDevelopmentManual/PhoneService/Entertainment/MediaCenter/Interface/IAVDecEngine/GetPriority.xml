    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume>媒体中心引擎</volume>
<interface>IAVDecEngine</interface><method> GetPriority </method>
    <description>
        <para>
           设置应用的优先级和生命周期。
        </para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> enum AppPriority * </param_type>
                <param_name> peAppPriority </param_name>
                <param_note>
                    <para>
                      指向<link url="../../DataType/AppPriority.htm">AppPriority</link>枚举型指针，用于表示应用的优先级
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> enum AppLifeSpan * </param_type>
                <param_name> peAppLifeSpan </param_name>
                <param_note>
                    <para>
                    指向<link url="../../DataType/AppLifeSpan.htm">AppLifeSpan</link>枚举型指针，用于表示应用的生命周期　
                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>
     <return>
	    <table>
	        <th>
	    	    <td>返回值 </td>
	    	    <td>描述</td>
	        </th>
	        <tr>
	    	    <td>NOERROR</td>
	    	    <td>程序执行成功</td>
	        </tr>

	    </table>
	   <para>参见<link url="../../../../../QuickReference/ECode.htm">ECode</link>和<link url="../../MediaCenter_Error.htm">MediaCenter_Error</link>相关帮助。</para>
    </return>
    <requirements>
        <system>
            <system_name>
《和欣 2.0》或更高版本            </system_name>
        </system>

    </requirements>
      <references>
        <reference>
            <ref_name>SetPriority</ref_name>
            <ref_url>SetPriority</ref_url>
        </reference>
    </references>
</manpage>