    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume>媒体中心引擎</volume>
<interface>IAVDecEngine</interface><method> SetDisplayRect </method>
    <description>
        <para>
         设置视频的显示区域和diffClip区域。
        </para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> INT </param_type>
                <param_name> nDisplayLeft </param_name>
                <param_note>
                    <para>
                     显示区域的左边界
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> INT </param_type>
                <param_name> nDisplayTop </param_name>
                <param_note>
                    <para>
                      显示区域的上边界
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> INT </param_type>
                <param_name> nDisplayRight </param_name>
                <param_note>
                    <para>
                     显示区域的右边界
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> INT </param_type>
                <param_name> nDisplayBottom </param_name>
                <param_note>
                    <para>
                      显示区域的下边界
                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>
     <return>
	    <table>
	        <th>
	    	    <td>返回值 </td>
	    	    <td>描述</td>
	        </th>
	        <tr>
	    	    <td>NOERROR</td>
	    	    <td>程序执行成功或当该媒体文件不存在视频流或者视频流不被支持时</td>
	        </tr>

	    </table>
	  <para>参见<link url="../../../../../QuickReference/ECode.htm">ECode</link>和<link url="../../MediaCenter_Error.htm">MediaCenter_Error</link>相关帮助。</para>
    </return>
    <requirements>
        <system>
            <system_name>
《和欣 2.0》或更高版本            </system_name>
        </system>

    </requirements>
</manpage>