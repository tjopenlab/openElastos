    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume>媒体中心引擎</volume>
<interface>IAVEncEngine</interface><method> SetOutputFile </method>
    <description>
        <para>
          设置所要编码生成的多媒体文件名。编码引擎会根据文件后缀选择匹配的文件合成器。
        </para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> WString </param_type>
                <param_name> wsFilename </param_name>
                <param_note>
                    <para>
                      保存编码后数据的文件名
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> UINT64 </param_type>
                <param_name> u64MaxSize </param_name>
                <param_note>
                    <para>
                      需要录制的最大文件大小
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> UINT </param_type>
                <param_name> uMaxDuration </param_name>
                <param_note>
                    <para>
                      需要录制的最大的文件时长
                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>
     <return>
	    <table>
	        <th>
	    	    <td>返回值 </td>
	    	    <td>描述</td>
	        </th>
	        <tr>
	    	    <td>NOERROR</td>
	    	    <td>程序执行成功</td>
	        </tr>
	        <tr>
	    	    <td>E_INVALID_ARGUMENT </td>
	    	    <td>参数错误</td>
	        </tr>
	        <tr>
	    	    <td>根据文件后缀匹配文件合成器方法错误返回码 </td>
	    	    <td>根据文件的后缀找不到匹配的文件合成器  </td>
	        </tr>
	        <tr>
	    	    <td>创建文件协议构件方法错误返回码 </td>
	    	    <td>创建文件协议构件失败  </td>
	        </tr>
	        <tr>
	    	    <td>设置文件协议指针到文件合成器方法错误返回码 </td>
	    	    <td>设置文件协议指针到文件合成器失败</td>
	        </tr>
	    </table>
	   <para>参见<link url="../../../../../QuickReference/ECode.htm">ECode</link>和<link url="../../MediaCenter_Error.htm">MediaCenter_Error</link>相关帮助。</para>
    </return>
    <requirements>
        <system>
            <system_name>
《和欣 2.0》或更高版本            </system_name>
        </system>

    </requirements>
</manpage>