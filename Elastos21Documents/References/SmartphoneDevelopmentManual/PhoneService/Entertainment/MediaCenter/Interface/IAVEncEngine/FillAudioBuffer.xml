    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume>媒体中心引擎</volume>
<interface>IAVEncEngine</interface><method> FillAudioBuffer </method>
    <description>
        <para>
          填充原始音频数据到编码引擎的缓冲区中。
        </para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> EzByteBuf </param_type>
                <param_name> ebbAudioFrame </param_name>
                <param_note>
                    <para>
                      用户填充的音频数据
                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>
     <return>
	    <table>
	        <th>
	    	    <td>返回值 </td>
	    	    <td>描述</td>
	        </th>
	        <tr>
	    	    <td>NOERROR</td>
	    	    <td>程序执行成功</td>
	        </tr>
	        <tr>
	    	    <td>E_INVALID_OPERATION  </td>
	    	    <td>当前状态不是EncodeState_Start时  </td>
	        </tr>
	         <tr>
	    	    <td>E_INVALID_ARGUMENT  </td>
	    	    <td>参数错误</td>
	        </tr>

	    </table>
	   <para>参见<link url="../../../../../QuickReference/ECode.htm">ECode</link>和<link url="../../MediaCenter_Error.htm">MediaCenter_Error</link>相关帮助。</para>
    </return>
    <requirements>
        <system>
            <system_name>
《和欣 2.0》或更高版本            </system_name>
        </system>

    </requirements>
</manpage>