    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume>媒体中心引擎</volume>
<interface>IAVEncEngine</interface><method> EndFilling </method>
    <description>
        <para>
        通知引擎音频流或者视频流已经结束。
        </para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> BOOL </param_type>
                <param_name> bAudioEnd </param_name>
                <param_note>
                    <para>
                      音频流是否结束的标志。为TRUE代表音频流结束；为FALSE代表音频流未结束
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> BOOL </param_type>
                <param_name> bVideoEnd </param_name>
                <param_note>
                    <para>
                       视频流是否结束的标志。为TRUE代表视频流结束；为FALSE代表视频流未结束
                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>
     <return>
	    <table>
	        <th>
	    	    <td>返回值 </td>
	    	    <td>描述</td>
	        </th>
	        <tr>
	    	    <td>NOERROR</td>
	    	    <td>程序执行成功</td>
	        </tr>

	    </table>
	  <para>参见<link url="../../../../../QuickReference/ECode.htm">ECode</link>和<link url="../../MediaCenter_Error.htm">MediaCenter_Error</link>相关帮助。</para>
    </return>
    <requirements>
        <system>
            <system_name>
《和欣 2.0》或更高版本            </system_name>
        </system>

    </requirements>
</manpage>