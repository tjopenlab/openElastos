    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume>媒体中心引擎</volume>
<interface>IAVEncEngine</interface><method> FillVideoBuffer </method>
    <description>
        <para>
         传递原始视频图象数据的指针到到编码引擎的视频队列中。
        </para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> BYTE * </param_type>
                <param_name> pVideoFrame </param_name>
                <param_note>
                    <para>
                      指向BYTE型指针，用于表示传递到编码引擎的视频图象数据，不能为空
                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>
     <return>
	    <table>
	        <th>
	    	    <td>返回值 </td>
	    	    <td>描述</td>
	        </th>
	        <tr>
	    	    <td>NOERROR</td>
	    	    <td>程序执行成功</td>
	        </tr>
	        <tr>
	    	    <td>E_INVALID_ARGUMENT </td>
	    	    <td>参数错误 </td>
	        </tr>
	         <tr>
	    	    <td>E_INVALID_OPERATION </td>
	    	    <td>当前状态不是EncodeState_Start时  </td>
	        </tr>

	    </table>
	  <para>参见<link url="../../../../../QuickReference/ECode.htm">ECode</link>和<link url="../../MediaCenter_Error.htm">MediaCenter_Error</link>相关帮助。</para>
    </return>
    <requirements>
        <system>
            <system_name>
《和欣 2.0》或更高版本            </system_name>
        </system>

    </requirements>
</manpage>