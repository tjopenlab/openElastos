    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume>照相摄像</volume>
<interface>ICamera</interface><method> Start </method>
    <description>
        <para>
          启动相机进入照相或摄像模式
        </para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> enum CamMode </param_type>
                <param_name> eCamMode </param_name>
                <param_note>
                    <para>
                        数码相机启动模式（照相或摄像）
                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>
     <return>
	    <table>
	        <th>
	    	    <td>返回值 </td>
	    	    <td>描述</td>
	        </th>
	        <tr>
	    	    <td>NOERROR</td>
	    	    <td>程序执行成功</td>
	        </tr>
	        <tr>
	    	    <td>E_INVALID_ARGUMENT</td>
	    	    <td>无效参数</td>
	        </tr>
	        <tr>
	    	    <td>E_FAIL</td>
	    	    <td>由于驱动未给出完整的错误返回值定义，故暂时用此代表当前所有可能的的驱动错误返回值</td>
	        </tr>
	    </table>
	    <para></para>
    </return>
    <requirements>
        <system>
            <system_name>
《和欣 2.0》或更高版本            </system_name>
        </system>
    </requirements>
</manpage>