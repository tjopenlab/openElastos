    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/interfaceman.xsl"?>    <manpage>
    <volume>照相摄像</volume>
    <interface> ICamCallBack </interface>
    <description>
        <para>
            回调接口
        </para>
    </description>
    <methods>
<self>
    <method>
        <method_name>SnapshotNotify</method_name>
        <method_url>..\interface\ICamCallBack\SnapshotNotify</method_url>
    <m_description>
        通知上层用户去照相帧数据
    </m_description>
    </method>
    <method>
        <method_name>RecordNotify</method_name>
        <method_url>..\interface\ICamCallBack\RecordNotify</method_url>
    <m_description>
        通知上层用户取帧或摄像摄像已停止
    </m_description>
    </method>
    <method>
        <method_name>ErrorNotify</method_name>
        <method_url>..\interface\ICamCallBack\ErrorNotify</method_url>
    <m_description>
        通知上层用户照相/摄像中发生的错误。
    </m_description>
    </method>
</self>
    </methods>
    <requirements>
        <system>
            <system_name>
《和欣 2.0》或更高版本            </system_name>
        </system>
    </requirements>
</manpage>