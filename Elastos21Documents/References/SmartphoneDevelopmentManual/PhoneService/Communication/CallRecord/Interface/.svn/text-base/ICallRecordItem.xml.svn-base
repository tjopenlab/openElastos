    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/interfaceman.xsl"?>    <manpage>
    <volume>通话记录</volume>
    <interface> ICallRecordItem </interface>
    <description>
        <para>
        具体手机通话记录访问对象
        </para>
    </description>
    <methods>
<self>
    <method>
        <method_name>GetId</method_name>
        <method_url>..\interface\ICallRecordItem\GetId</method_url>
    <m_description>
        获取本通话记录的Id号
    </m_description>
    </method>
    <method>
        <method_name>GetCallInfo</method_name>
        <method_url>..\interface\ICallRecordItem\GetCallInfo</method_url>
    <m_description>
        取得本通话记录的详细信息
    </m_description>
    </method>
    <method>
        <method_name>SetCallInfo</method_name>
        <method_url>..\interface\ICallRecordItem\SetCallInfo</method_url>
    <m_description>
       设置某通话记录的详细信息
    </m_description>
    </method>
    <method>
        <method_name>GetReadState</method_name>
        <method_url>..\interface\ICallRecordItem\GetReadState</method_url>
    <m_description>
        获取记录的读取状态
    </m_description>
    </method>
    <method>
        <method_name>SetReadState</method_name>
        <method_url>..\interface\ICallRecordItem\SetReadState</method_url>
    <m_description>
        获取记录的读取状态
    </m_description>
    </method>
    <method>
        <method_name>SetContactName</method_name>
        <method_url>..\interface\ICallRecordItem\SetContactName</method_url>
    <m_description>
        设置通话记录联系人名字
    </m_description>
    </method>
    <method>
        <method_name>GetContactName</method_name>
        <method_url>..\interface\ICallRecordItem\GetContactName</method_url>
    <m_description>
        获取通话记录联系人名字
    </m_description>
    </method>
</self>
    </methods>
    <requirements>
        <system>
            <system_name>
《和欣 2.0》或更高版本            </system_name>
        </system>
    </requirements>
</manpage>