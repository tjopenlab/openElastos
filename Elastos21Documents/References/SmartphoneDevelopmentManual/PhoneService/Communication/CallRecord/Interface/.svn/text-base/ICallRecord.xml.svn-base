    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/interfaceman.xsl"?>    <manpage>
    <volume>通话记录</volume>
    <interface> ICallRecord </interface>
    <description>
        <para>
        管理手机通话记录
        </para>
    </description>
    <methods>
<self>
    <method>
        <method_name>AddRecord</method_name>
        <method_url>..\interface\ICallRecord\AddRecord</method_url>
    <m_description>
        添加一条新的空记录，并更新到数据库
    </m_description>
    </method>
    <method>
        <method_name>GetRecordById</method_name>
        <method_url>..\interface\ICallRecord\GetRecordById</method_url>
    <m_description>
        取得指定序号的电话记录信息，通过接口返回
    </m_description>
    </method>
    <method>
        <method_name>SearchItem</method_name>
        <method_url>..\interface\ICallRecord\SearchItem</method_url>
    <m_description>
        枚举指定类型呼叫的所有通话记录
    </m_description>
    </method>
    <method>
        <method_name>Remove</method_name>
        <method_url>..\interface\ICallRecord\Remove</method_url>
    <m_description>
       从数据库中删除指定记录
    </m_description>
    </method>
    <method>
        <method_name>RemoveById</method_name>
        <method_url>..\interface\ICallRecord\RemoveById</method_url>
    <m_description>
      从数据库中删除指定Id的记录
    </m_description>
    </method>
    <method>
        <method_name>RemoveByType</method_name>
        <method_url>..\interface\ICallRecord\RemoveByType</method_url>
    <m_description>
       删除所有的对应类型的记录。
    </m_description>
    </method>
    <method>
        <method_name>Update</method_name>
        <method_url>..\interface\ICallRecord\Update</method_url>
    <m_description>
       将ICallRecordItem中的数据更新到数据库中
    </m_description>
    </method>
    <method>
        <method_name>GetTotalTime</method_name>
        <method_url>..\interface\ICallRecord\GetTotalTime</method_url>
    <m_description>
        取得电话记录中总的呼入时间和总的呼出时间
    </m_description>
    </method>
    <method>
        <method_name>GetLastCallInfo</method_name>
        <method_url>..\interface\ICallRecord\GetLastCallInfo</method_url>
    <m_description>
        取得最后一次通话记录信息。
    </m_description>
    </method>
    <method>
        <method_name>ResetCallLog</method_name>
        <method_url>..\interface\ICallRecord\ResetCallLog</method_url>
    <m_description>
        清除call_statistics中电话记录相关的数据
    </m_description>
    </method>
    <method>
        <method_name>CumulateFlux</method_name>
        <method_url>..\interface\ICallRecord\CumulateFlux</method_url>
    <m_description>
        在记录中增加网络数据流量
    </m_description>
    </method>
    <method>
        <method_name>GetFlux</method_name>
        <method_url>..\interface\ICallRecord\GetFlux</method_url>
    <m_description>
        在记录中增加网络数据流量
    </m_description>
    </method>
    <method>
        <method_name>ResetFlowCounter</method_name>
        <method_url>..\interface\ICallRecord\ResetFlowCounter</method_url>
    <m_description>
       清除call_statistics中网络数据流量记录相关的数据
    </m_description>
    </method>
</self>
    </methods>
    <requirements>
        <system>
            <system_name>
《和欣 2.0》或更高版本            </system_name>
        </system>
    </requirements>
</manpage>