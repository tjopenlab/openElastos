    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume>通话记录</volume>
<interface>ICallRecordItem</interface><method> SetCallInfo </method>
    <description>
        <para>
         设置某通话记录的详细信息
        </para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> INT </param_type>
                <param_name> nStartTime </param_name>
                <param_note>
                    <para>
                    通话开始时间，实际上是time_t类型
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> WString </param_type>
                <param_name> wsNumber </param_name>
                <param_note>
                    <para>
                    电话号码
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> enum CallType </param_type>
                <param_name> eType </param_name>
                <param_note>
                    <para>
                    电话信息类型
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> INT </param_type>
                <param_name> nDuration </param_name>
                <param_note>
                    <para>
                    通话时间
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> INT </param_type>
                <param_name> nLine </param_name>
                <param_note>
                    <para>
                    通话线路
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> float </param_type>
                <param_name> fUint </param_name>
                <param_note>
                    <para>
                    通话计费单价，￥、$
                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

	<return>
	    <table>
	        <th>
	    	    <td>返回值 </td>
	    	    <td>描述</td>
	        </th>
	        <tr>
	    	    <td>NOERROR</td>
	    	    <td>方法调用成功</td>
	        </tr>
	        <tr>
	    	    <td>E_INVALID_ARGUMENT </td>
	    	    <td>参数错误 </td>
	        </tr>
	        <tr>
	    	    <td>E_DB_INTERNAL_ERROR </td>
	    	    <td>数据库错误 </td>
	        </tr>
	    </table>
	    <para>参见<link url="../../../../../QuickReference/ECode.htm">ECode</link>、<link url="../../../../../QuickReference/DBError.htm">E_DB_INTERNAL_ERROR</link>相关帮助。</para>
    </return>

	<remarks>
	    <para> </para>
	    <para> </para>
	</remarks>

    <requirements>
        <system>
            <system_name>
《和欣 2.0》或更高版本            </system_name>
        </system>
    </requirements>
</manpage>