    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume>通话记录</volume>
<interface>ICallRecordEvent</interface><method> CallRecordOperated </method>
    <description>
        <para>
         当通话记录更新的时候,回调用户注册的函数。
        </para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> enum CallRecordEvent </param_type>
                <param_name> eEventType </param_name>
                <param_note>
                    <para>
                    回调的事件类型
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> UINT </param_type>
                <param_name> uId </param_name>
                <param_note>
                    <para>
                    发生更新的记录的ID，当事件类型为CallRecordEvent_DeletedAll或者CallRecordEvent_Reset时，uId无效
                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

	<return>
	    <table>
	        <th>
	    	    <td>返回值 </td>
	    	    <td>描述</td>
	        </th>
	        <tr>
	    	    <td>NOERROR</td>
	    	    <td>方法调用成功</td>
	        </tr>
	        <tr>
	    	    <td>E_INVALID_ARGUMENT </td>
	    	    <td>参数错误 </td>
        </tr>
	    </table>
	    <para>参见<link url="../../../../../QuickReference/ECode.htm">ECode</link>、<link url="../../../../../QuickReference/DBError.htm">E_DB_INTERNAL_ERROR</link>相关帮助。</para>
    </return>

	<remarks>
	    <para> </para>
	    <para> </para>
	</remarks>

    <requirements>
        <system>
            <system_name>
《和欣 2.0》或更高版本            </system_name>
        </system>
    </requirements>
</manpage>