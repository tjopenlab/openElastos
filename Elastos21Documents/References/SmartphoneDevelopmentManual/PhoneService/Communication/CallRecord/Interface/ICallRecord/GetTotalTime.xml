    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume>通话记录</volume>
<interface>ICallRecord</interface><method> GetTotalTime </method>
    <description>
        <para>
        取得电话记录中总的呼入时间和总的呼出时间
        </para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> INT * </param_type>
                <param_name> pnTotalIn </param_name>
                <param_note>
                    <para>
                    指向INT类型的指针，用于总的呼入时间
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> INT * </param_type>
                <param_name> pnTotalOut </param_name>
                <param_note>
                    <para>
                    指向INT类型的指针，用于总的呼出时间
                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

    	<return>
	    <table>
	        <th>
	    	    <td>返回值 </td>
	    	    <td>描述</td>
	        </th>
	        <tr>
	    	    <td>NOERROR</td>
	    	    <td>方法调用成功</td>
	        </tr>
	        <tr>
	    	    <td>E_INVALID_ARGUMENT </td>
	    	    <td>参数错误，pnTotal或pnTotalOut为空 </td>
	        </tr>
	        <tr>
	    	    <td>E_DB_INTERNAL_ERROR </td>
	    	    <td>数据库错误 </td>
	        </tr>
	    </table>
	    <para>参见<link url="../../../../../QuickReference/ECode.htm">ECode</link>、<link url="../../../../../QuickReference/DBError.htm">E_DB_INTERNAL_ERROR</link>相关帮助。</para>
    </return>

	<remarks>
	    <para>查询Call_static中的相应字段  </para>
	</remarks>

    <requirements>
        <system>
            <system_name>
《和欣 2.0》或更高版本            </system_name>
        </system>
    </requirements>
</manpage>