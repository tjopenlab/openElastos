    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume>通话记录</volume>
<interface>ICallRecord</interface><method> CumulateFlux </method>
    <description>
        <para>
        在记录中增加网络数据流量
        </para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> INT </param_type>
                <param_name> nInflux </param_name>
                <param_note>
                    <para>
                    增加的数据流入量，单位字节byte
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> INT </param_type>
                <param_name> nEfflux </param_name>
                <param_note>
                    <para>
                    增加的数据流出量，单位字节byte
                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

	<return>
	    <table>
	        <th>
	    	    <td>返回值 </td>
	    	    <td>描述</td>
	        </th>
	        <tr>
	    	    <td>NOERROR</td>
	    	    <td>方法调用成功</td>
	        </tr>
	        <tr>
	    	    <td>E_INVALID_ARGUMENT </td>
	    	    <td>参数错误 </td>
	        </tr>
	        <tr>
	    	    <td>E_DB_INTERNAL_ERROR </td>
	    	    <td>数据库错误 </td>
	        </tr>
	    </table>
	    <para>参见<link url="../../../../../QuickReference/ECode.htm">ECode</link>、<link url="../../../../../QuickReference/DBError.htm">E_DB_INTERNAL_ERROR</link>相关帮助。</para>
    </return>

	<remarks>
	    <para>  </para>
	</remarks>

    <requirements>
        <system>
            <system_name>
《和欣 2.0》或更高版本            </system_name>
        </system>
    </requirements>
</manpage>