    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume>通话记录</volume>
<interface>ICallRecord</interface><method> GetRecordById </method>
    <description>
        <para>
         取得指定序号的电话记录信息，通过接口返回。
        </para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> INT </param_type>
                <param_name> nId </param_name>
                <param_note>
                    <para>
                    需要返回某条记录的索引号
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> ICallRecordItemRef * </param_type>
                <param_name> ppItem </param_name>
                <param_note>
                    <para>
                    指向<link url="../ICallRecordItem.htm">ICallRecordItem</link>指针的指针，用于返回对应记录对象
                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

	<return>
	    <table>
	        <th>
	    	    <td>返回值 </td>
	    	    <td>描述</td>
	        </th>
	        <tr>
	    	    <td>NOERROR</td>
	    	    <td>方法调用成功</td>
	        </tr>
	        <tr>
	    	    <td>E_INVALID_ARGUMENT </td>
	    	    <td>nIndex为0或者ppItem为空 </td>
	        </tr>
	        <tr>
	    	    <td>S_FALSE </td>
	    	    <td>没有找到指定记录</td>
	        </tr>
	        <tr>
	    	    <td>E_DB_INTERNAL_ERROR</td>
	    	    <td>数据库错误</td>
	        </tr>
	    </table>
	    <para>参见<link url="../../../../../QuickReference/ECode.htm">ECode</link>、<link url="../../../../../QuickReference/DBError.htm">E_DB_INTERNAL_ERROR</link>相关帮助。</para>
    </return>

	<remarks>
	    <para>  </para>
	</remarks>

    <requirements>
        <system>
            <system_name>
《和欣 2.0》或更高版本            </system_name>
        </system>
    </requirements>
</manpage>