<manpage>
    <volume>通话记录</volume>
    <types> CallRecordEvent </types>
    <description>
        <para>
         通话记录回调事件
        </para>
    </description>
<enum>
     <enum_member>
       <mem_name> CallRecordEvent_Added </mem_name>
       <mem_value> 0x0 </mem_value>
       <mem_description>
         增加一条通话记录
       </mem_description>
     </enum_member>

     <enum_member>
       <mem_name> CallRecordEvent_Deleted </mem_name>
       <mem_value> 0x1 </mem_value>
       <mem_description>
         删除一条通话记录
       </mem_description>
     </enum_member>

     <enum_member>
       <mem_name> CallRecordEvent_AllDeleted </mem_name>
       <mem_value> 0x2 </mem_value>
       <mem_description>
       删除所有通话记录
       </mem_description>
     </enum_member>

     <enum_member>
       <mem_name> CallRecordEvent_Updated </mem_name>
       <mem_value> 0x3 </mem_value>
       <mem_description>
       更新一条通话记录
       </mem_description>
     </enum_member>

     <enum_member>
       <mem_name> CallRecordEvent_Reset </mem_name>
       <mem_value> 0x4 </mem_value>
       <mem_description>
         通话统计信息重置
       </mem_description>
     </enum_member>
   </enum>

 </manpage>