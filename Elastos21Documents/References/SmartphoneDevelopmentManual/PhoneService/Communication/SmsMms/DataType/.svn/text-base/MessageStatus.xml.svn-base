    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/custom_type.xsl"?>    <manpage>
    <volume>短信彩信引擎</volume>
    <types> MessageStatus </types>
    <description>
        <para>
         信息状态
        </para>
    </description>
<enum>
<enum_member>
<mem_name> MessageStatus_Unread </mem_name>
<mem_value> 0x0 </mem_value>
<mem_description>接收的新（未读）消息</mem_description>
</enum_member>

<enum_member>
<mem_name> MessageStatus_Read </mem_name>
<mem_value> 0x1 </mem_value>
<mem_description>已阅读的消息</mem_description>
</enum_member>

<enum_member>
<mem_name> MessageStatus_Unsent </mem_name>
<mem_value> 0x2 </mem_value>
<mem_description>存储的未发送消息</mem_description>
</enum_member>

<enum_member>
<mem_name> MessageStatus_Sent </mem_name>
<mem_value> 0x3 </mem_value>
<mem_description>存储的已发送消息</mem_description>
</enum_member>

<enum_member>
<mem_name> MessageStatus_Draft </mem_name>
<mem_value> 0x4 </mem_value>
<mem_description>存储的消息草稿</mem_description>
</enum_member>

<enum_member>
<mem_name> MessageStatus_Trash_Unread </mem_name>
<mem_value> 0x5 </mem_value>
<mem_description>未读的垃圾消息</mem_description>
</enum_member>

<enum_member>
<mem_name> MessageStatus_Trash_Read </mem_name>
<mem_value> 0x6 </mem_value>
<mem_description>已读垃的圾消息</mem_description>
</enum_member>

<enum_member>
<mem_name> MessageStatus_SentFailed </mem_name>
<mem_value> 0x7 </mem_value>
<mem_description>发送失败消息</mem_description>
</enum_member>

<enum_member>
<mem_name> MessageStatus_Report_Unread </mem_name>
<mem_value> 0x8 </mem_value>
<mem_description>未读的报告消息</mem_description>
</enum_member>

<enum_member>
<mem_name> MessageStatus_Report_Read </mem_name>
<mem_value> 0x9 </mem_value>
<mem_description>已读的报告消息</mem_description>
</enum_member>

</enum>
</manpage>
