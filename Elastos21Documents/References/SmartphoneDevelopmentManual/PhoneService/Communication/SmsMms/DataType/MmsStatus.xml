    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/custom_type.xsl"?>    <manpage>
    <volume>短信彩信引擎</volume>
    <types> MmsStatus </types>
    <description>
        <para>
          彩信状态
        </para>
    </description>
    <enum>
        <enum_member>
            <mem_name> MmsStatus_Expired </mem_name>
            <mem_value> 0x80 </mem_value>
            <mem_description>过时的彩信</mem_description>
        </enum_member>

        <enum_member>
            <mem_name> MmsStatus_Retrieved </mem_name>
            <mem_value> 0x81 </mem_value>
            <mem_description>已接收的彩信</mem_description>
        </enum_member>

        <enum_member>
            <mem_name> MmsStatus_Rejected </mem_name>
            <mem_value> 0x82 </mem_value>
            <mem_description>已拒绝的彩信</mem_description>
        </enum_member>

        <enum_member>
            <mem_name> MmsStatus_Deferred </mem_name>
            <mem_value> 0x83 </mem_value>
            <mem_description>延迟的彩信</mem_description>
        </enum_member>

        <enum_member>
            <mem_name> MmsStatus_Unrecognised </mem_name>
            <mem_value> 0x84 </mem_value>
            <mem_description>未识别的彩信</mem_description>
        </enum_member>


        </enum></manpage>