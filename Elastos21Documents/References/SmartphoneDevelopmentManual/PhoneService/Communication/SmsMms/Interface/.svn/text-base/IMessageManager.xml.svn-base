    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/interfaceman.xsl"?>    <manpage>
    <volume>短信彩信引擎</volume>
    <interface> IMessageManager </interface>
    <description>
        <para>
         信息存储管理接口，包括了对数据库的增、删、改、查基本操作。
        </para>
    </description>
    <methods>
<self>
    <method>
        <method_name>Enum</method_name>
        <method_url>..\interface\IMessageManager\Enum</method_url>
    <m_description>
     枚举特定类型与指定信息箱的所有概要信息
    </m_description>
    </method>
    <method>
        <method_name>EnumUnread</method_name>
        <method_url>..\interface\IMessageManager\EnumUnread</method_url>
    <m_description>
       枚举未读信息
    </m_description>
    </method>
    <method>
        <method_name>EnumRawMsg</method_name>
        <method_url>..\interface\IMessageManager\EnumRawMsg</method_url>
    <m_description>

    </m_description>
    </method>
    <method>
        <method_name>Create</method_name>
        <method_url>..\interface\IMessageManager\Create</method_url>
    <m_description>
     创建特定类型的信息实体
    </m_description>
    </method>
    <method>
        <method_name>Add</method_name>
        <method_url>..\interface\IMessageManager\Add</method_url>
    <m_description>
       向数据库中添加相应的信息实体数据
    </m_description>
    </method>
    <method>
        <method_name>DelById</method_name>
        <method_url>..\interface\IMessageManager\DelById</method_url>
    <m_description>
         按照记录id删出信息数据
    </m_description>
    </method>
    <method>
        <method_name>DelAllByBoxId</method_name>
        <method_url>..\interface\IMessageManager\DelAllByBoxId</method_url>
    <m_description>
       删除指定邮箱内的所有信息
    </m_description>
    </method>
    <method>
        <method_name>Update</method_name>
        <method_url>..\interface\IMessageManager\Update</method_url>
    <m_description>
       更改数据库已经存在的信息实体数据
    </m_description>
    </method>
    <method>
        <method_name>GetById</method_name>
        <method_url>..\interface\IMessageManager\GetById</method_url>
    <m_description>
       根据信息ID(primary key)得到相应的信息实体数据
    </m_description>
    </method>
    <method>
        <method_name>GetInfoByBoxId</method_name>
        <method_url>..\interface\IMessageManager\GetInfoByBoxId</method_url>
    <m_description>
      根据信息箱ID得到相应信箱的全部信息数量和未读信息数量
    </m_description>
    </method>
    <method>
        <method_name>GetPhoneSmsNum</method_name>
        <method_url>..\interface\IMessageManager\GetPhoneSmsNum</method_url>
    <m_description>
       获取手机上全部信息数量和未读信息数量
    </m_description>
    </method>
    <method>
        <method_name>SetBoxID</method_name>
        <method_url>..\interface\IMessageManager\SetBoxID</method_url>
    <m_description>
       设置信息的信息箱属性（移动信息用）
    </m_description>
    </method>
    <method>
        <method_name>GetCBUpdateInfo</method_name>
        <method_url>..\interface\IMessageManager\GetCBUpdateInfo</method_url>
    <m_description>

    </m_description>
    </method>
    <method>
        <method_name>StartTransaction</method_name>
        <method_url>..\interface\IMessageManager\StartTransaction</method_url>
    <m_description>
        数据库事务处理：开始事务处理
    </m_description>
    </method>
    <method>
        <method_name>Commit</method_name>
        <method_url>..\interface\IMessageManager\Commit</method_url>
    <m_description>
        数据库事务处理：提交事务处理
    </m_description>
    </method>
    <method>
        <method_name>Rollback</method_name>
        <method_url>..\interface\IMessageManager\Rollback</method_url>
    <m_description>
        数据库事务处理：事务回滚
    </m_description>
    </method>
</self>
    </methods>
    <requirements>
        <system>
            <system_name>
《和欣 2.0》或更高版本            </system_name>
        </system>
    </requirements>
</manpage>