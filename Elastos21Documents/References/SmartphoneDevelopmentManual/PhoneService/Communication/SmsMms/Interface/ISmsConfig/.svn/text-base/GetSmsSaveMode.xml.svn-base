    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume>短信彩信引擎</volume>
<interface>ISmsConfig</interface><method> GetSmsSaveMode </method>
    <description>
        <para>
         获取短信的存储模式。
        </para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> UINT * </param_type>
                <param_name> puSaveMode </param_name>
                <param_note>
                    <para>
                    指向无符号整型的指针，用于获取短信的存储模式
                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

	<return>
	    <table>
	        <th>
	    	    <td>返回值 </td>
	    	    <td>描述</td>
	        </th>
	        <tr>
	    	    <td>NOERROR</td>
	    	    <td>方法调用成功</td>
	        </tr>
	        <tr>
	    	    <td>E_INVALID_ARGUMENT </td>
	    	    <td>参数错误 </td>
	        </tr>
	        <tr>
	    	    <td>短信彩信错误码 </td>
	    	    <td>短信彩信相关错误</td>
	        </tr>
	        <tr>
	    	    <td>E_DB_INTERNAL_ERROR </td>
	    	    <td>数据库错误 </td>
	        </tr>
	    </table>
	    <para>更多返回值，参见<link url="../../../../../QuickReference/ECode.htm">ECode</link>、<link url="../../SmsMmsError.htm">短信彩信错误码</link>、<link url="../../../../../QuickReference/DBError.htm">E_DB_INTERNAL_ERROR</link>相关帮助。</para>
    </return>

	<remarks>
	    <para>短信存储模式的取值指定为<link url="../../DataType/SmsSavemode.htm">SmsSavemode</link>数据类型。</para>
	</remarks>

    <requirements>
        <system>
            <system_name>
            《Elastos 2.0》或更高版本
            </system_name>
        </system>
        <headerfile>
            <file_name>elatype.h、stdlib.h、mmsdef.h、mmstype.h</file_name>
        </headerfile>
        <library>
            <lib_name>database.lib、elastos.lib、elacrt.lib、elautil.lib</lib_name>
        </library>
    </requirements>
</manpage>