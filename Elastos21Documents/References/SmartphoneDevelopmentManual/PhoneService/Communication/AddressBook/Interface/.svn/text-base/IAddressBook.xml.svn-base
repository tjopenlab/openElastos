    <?xml:stylesheet type="text/xsl" href="D:\Elastos21\Documents\References\Chinese/xsl/xsl_c/interfaceman.xsl"?>    <manpage>
    <volume>地址薄引擎</volume>
    <interface> IAddressBook </interface>
    <description>
        <para>
          用于对地址簿执行基本操作的接口。
        </para>
    </description>
    <methods>
<self>
    <method>
        <method_name>GetMaxId</method_name>
        <method_url>..\interface\IAddressBook\GetMaxId</method_url>
    <m_description>
       获取地址簿中联系人记录或组记录的最大记录ID。
    </m_description>
    </method>
    <method>
        <method_name>GetEntityById</method_name>
        <method_url>..\interface\IAddressBook\GetEntityById</method_url>
    <m_description>
       通过指定的记录ID，查找相应的记录。
    </m_description>
    </method>
    <method>
        <method_name>GetEntityByName</method_name>
        <method_url>..\interface\IAddressBook\GetEntityByName</method_url>
    <m_description>
       通过指定记录的姓名字段值，查找相应的记录集。
    </m_description>
    </method>
    <method>
        <method_name>CreateEntity</method_name>
        <method_url>..\interface\IAddressBook\CreateEntity</method_url>
    <m_description>
       创建联系人或组记录。
    </m_description>
    </method>
    <method>
        <method_name>Save</method_name>
        <method_url>..\interface\IAddressBook\Save</method_url>
    <m_description>
      存储一条记录。
    </m_description>
    </method>
    <method>
        <method_name>Delete</method_name>
        <method_url>..\interface\IAddressBook\Delete</method_url>
    <m_description>
       删除一条记录。
    </m_description>
    </method>
    <method>
        <method_name>Update</method_name>
        <method_url>..\interface\IAddressBook\Update</method_url>
    <m_description>
      更新一条记录。
    </m_description>
    </method>
    <method>
        <method_name>SearchEntities</method_name>
        <method_url>..\interface\IAddressBook\SearchEntities</method_url>
    <m_description>
      通过组合条件，查找相应记录。
    </m_description>
    </method>
    <method>
        <method_name>SearchAllEntities</method_name>
        <method_url>..\interface\IAddressBook\SearchAllEntities</method_url>
    <m_description>
      检索所有记录。
    </m_description>
    </method>
    <method>
        <method_name>RemoveAllEntities</method_name>
        <method_url>..\interface\IAddressBook\RemoveAllEntities</method_url>
    <m_description>
       删除所有的联系人记录或组记录。
    </m_description>
    </method>
    <method>
        <method_name>GetDataStore</method_name>
        <method_url>..\interface\IAddressBook\GetDataStore</method_url>
    <m_description>
        查找与记录相关的数据存储对象。
    </m_description>
    </method>
    <method>
        <method_name>EnumRelatedEntities</method_name>
        <method_url>..\interface\IAddressBook\EnumRelatedEntities</method_url>
    <m_description>
        通过指定的组记录或联系人记录，遍历该组中的联系人成员或联系人的所属组。
    </m_description>
    </method>
    <method>
        <method_name>RemoveContactFromGroup</method_name>
        <method_url>..\interface\IAddressBook\RemoveContactFromGroup</method_url>
    <m_description>
      从组中删除联系人成员。
    </m_description>
    </method>
    <method>
        <method_name>ClearAllRelatedEntities</method_name>
        <method_url>..\interface\IAddressBook\ClearAllRelatedEntities</method_url>
    <m_description>
        删除组中的所有联系人成员或删除联系人与所属的所有组的关系。
    </m_description>
    </method>
    <method>
        <method_name>AddContacttoGroup</method_name>
        <method_url>..\interface\IAddressBook\AddContacttoGroup</method_url>
    <m_description>
        在组中添加联系人成员。
    </m_description>
    </method>
    <method>
        <method_name>GetEntitiesCount</method_name>
        <method_url>..\interface\IAddressBook\GetEntitiesCount</method_url>
    <m_description>
        获取地址簿中联系人记录或组记录的总数。
    </m_description>
    </method>
    <method>
        <method_name>GetInitedState</method_name>
        <method_url>..\interface\IAddressBook\GetInitedState</method_url>
    <m_description>
        获取当前SIM卡的初始状态。
    </m_description>
    </method>
    <method>
        <method_name>SetSortKey</method_name>
        <method_url>..\interface\IAddressBook\SetSortKey</method_url>
    <m_description>
      设置排序关键字。
    </m_description>
    </method>
    <method>
        <method_name>GetContactsByTel</method_name>
        <method_url>..\interface\IAddressBook\GetContactsByTel</method_url>
    <m_description>
        通过指定的电话号码，获取联系人记录集。
    </m_description>
    </method>
    <method>
        <method_name>GetContactByEmail</method_name>
        <method_url>..\interface\IAddressBook\GetContactByEmail</method_url>
    <m_description>
        根据联系人的电子邮箱地址获取联系人记录。
    </m_description>
    </method>
    <method>
        <method_name>LoadSimEntities</method_name>
        <method_url>..\interface\IAddressBook\LoadSimEntities</method_url>
    <m_description>
      加载Sim卡中的所有联系人或组记录。
    </m_description>
    </method>
    <method>
        <method_name>GetAddrStatus</method_name>
        <method_url>..\interface\IAddressBook\GetAddrStatus</method_url>
    <m_description>
       获取信息地址的状态。
    </m_description>
    </method>
    <method>
        <method_name>IsEntityNameRepeated</method_name>
        <method_url>..\interface\IAddressBook\IsEntityNameRepeated</method_url>
    <m_description>
        判断组或联系人是否重名。
    </m_description>
    </method>
    <method>
        <method_name>GetContactByTel</method_name>
        <method_url>..\interface\IAddressBook\GetContactByTel</method_url>
    <m_description>
       通过指定的电话号码，获取联系人记录。
    </m_description>
    </method>
    <method>
        <method_name>GetMaxLengthofSimContactInfo</method_name>
        <method_url>..\interface\IAddressBook\GetMaxLengthofSimContactInfo</method_url>
    <m_description>
       获取SIM卡允许存储的联系人姓名和电话的最大字节数。
    </m_description>
    </method>
    <method>
        <method_name>DeleteEntityById</method_name>
        <method_url>..\interface\IAddressBook\DeleteEntityById</method_url>
    <m_description>
       删除指定记录ID的组或联系人记录。
    </m_description>
    </method>
    <method>
        <method_name>StartTransaction</method_name>
        <method_url>..\interface\IAddressBook\StartTransaction</method_url>
    <m_description>
      开始数据库事务。
    </m_description>
    </method>
    <method>
        <method_name>Commit</method_name>
        <method_url>..\interface\IAddressBook\Commit</method_url>
    <m_description>
       将上一次Commit/ Rollback 语句所做的改变进行提交并释放任何现有Database对象的数据锁。
    </m_description>
    </method>
    <method>
        <method_name>Rollback</method_name>
        <method_url>..\interface\IAddressBook\Rollback</method_url>
    <m_description>
       撤销当前事务对数据库所做的改变和释放数据库对象所持有的任何数据库锁。
    </m_description>
    </method>
    <method>
        <method_name>ClearAllRelatedEntitiesbyId</method_name>
        <method_url>..\interface\IAddressBook\ClearAllRelatedEntitiesbyId</method_url>
    <m_description>
       根据索引号删除组中的所有联系人成员或删除联系人与所属的所有组的关系。
    </m_description>
    </method>
    <method>
        <method_name>AddContacttoGroupbyId</method_name>
        <method_url>..\interface\IAddressBook\AddContacttoGroupbyId</method_url>
    <m_description>
       通过索引号在组中添加联系人成员。
    </m_description>
    </method>
    <method>
        <method_name>RemoveContactFromGroupbyId</method_name>
        <method_url>..\interface\IAddressBook\RemoveContactFromGroupbyId</method_url>
    <m_description>
       从组中清除联系人成员。
    </m_description>
    </method>
    <method>
        <method_name>SetHostInfo</method_name>
        <method_url>..\interface\IAddressBook\SetHostInfo</method_url>
    <m_description>
      设置主机信息。
    </m_description>
    </method>
    <method>
        <method_name>GetHostInfo</method_name>
        <method_url>..\interface\IAddressBook\GetHostInfo</method_url>
    <m_description>
       获取主机信息。
    </m_description>
    </method>

    <method>
        <method_name>GetFirstContactIdbySpell</method_name>
        <method_url>..\interface\IAddressBook\GetFirstContactIdbySpell</method_url>
    <m_description>
        通过指定记录的姓名拼音，查找第一条符合的联系人索引号。
    </m_description>
    </method>
</self>
    </methods>
    <requirements>
        <system>
            <system_name>
《和欣 2.0》或更高版本            </system_name>
        </system>
          <dll>
            <dll_name>database.dll</dll_name>
        </dll>
        <headerfile>
            <file_name>CAddressBook.h, CDBStore.h, CSimStore.h, CContact.h, CContactGroup.h,  Abdata.h, spcomm.h</file_name>
        </headerfile>
        <library>
            <lib_name>elacrt.lib, elastos.lib, utility.lib</lib_name>
        </library>
    </requirements>
</manpage>