    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume>地址薄引擎</volume>
<interface>IDataStore</interface><method> GetContactbySpell </method>
    <description>
        <para>
          根据拼音获取联系人记录。
        </para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> long </param_type>
                <param_name> nGroupId </param_name>
                <param_note>
                    <para>
                      组Id
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> WString </param_type>
                <param_name> wsSpell </param_name>
                <param_note>
                    <para>
                     联系人姓名拼音
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> IObjectEnumeratorRef * </param_type>
                <param_name> ppiEntitiesEnum </param_name>
                <param_note>
                    <para>
                      记录的对象枚举器接口指针，不能为NULL
                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>
     <return>
	    <table>
	        <th>
	    	    <td>返回值 </td>
	    	    <td>描述</td>
	        </th>
	        <tr>
	    	    <td>NOERROR</td>
	    	    <td>程序执行成功</td>
	        </tr>
	        <tr>
	    	    <td>E_INVALID_ARGUMENT</td>
	    	    <td>参数错误</td>
	        </tr>
	        <tr>
	    	    <td>数据库错误码 </td>
	    	    <td>数据库操作错误 </td>
	        </tr>
	    </table>
	    <para>参见<link url="../../../../../QuickReference/ECode.htm">ECode</link>和<link url="../../../../../QuickReference/DBError.htm">数据库错误</link>相关帮助。</para>
    </return>

    <requirements>
        <system>
            <system_name>
《和欣 2.0》或更高版本            </system_name>
        </system>

        <dll>
            <dll_name>database.dll</dll_name>
        </dll>
        <headerfile>
            <file_name>CDBStore.h, CContact.h, CContactGroup.h, abdata.h, CEntityEnumeration.h, spcomm.h</file_name>
        </headerfile>
        <library>
            <lib_name>elacrt.lib, elastos.lib, utility.lib</lib_name>
        </library>
    </requirements>
</manpage>