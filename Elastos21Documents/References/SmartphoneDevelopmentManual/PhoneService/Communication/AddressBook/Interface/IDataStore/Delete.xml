    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume>地址薄引擎</volume>
<interface>IDataStore</interface><method> Delete </method>
    <description>
        <para>
          从SIM卡或数据库中删除一条记录。
        </para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> IEntityRef </param_type>
                <param_name> piEntity </param_name>
                <param_note>
                    <para>
                      指向欲删除记录的IEntity接口指针
                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>
     <return>
	    <table>
	        <th>
	    	    <td>返回值 </td>
	    	    <td>描述</td>
	        </th>
	        <tr>
	    	    <td>NOERROR</td>
	    	    <td>程序执行成功</td>
	        </tr>
	        <tr>
	    	    <td>E_INVALID_ARGUMENT</td>
	    	    <td>参数错误</td>
	        </tr>
	        <tr>
	    	    <td>地址簿错误码 </td>
	    	    <td>地址簿操作错误 </td>
	        </tr>
	    </table>
	    <para>参见<link url="../../../../../QuickReference/ECode.htm">ECode</link>和<link url="../../AddressBook_error.htm">地址簿错误码</link>相关帮助。</para>
    </return>

    <requirements>
        <system>
            <system_name>
《和欣 2.0》或更高版本            </system_name>
        </system>

        <dll>
            <dll_name>database.dll</dll_name>
        </dll>
        <headerfile>
            <file_name>CDBStore.h, CContact.h, CContactGroup.h, abdata.h, CEntityEnumeration.h, spcomm.h</file_name>
        </headerfile>
        <library>
            <lib_name>elacrt.lib, elastos.lib, utility.lib</lib_name>
        </library>
    </requirements>
    <references>
        <reference>
            <ref_name>Save</ref_name>
            <ref_url>Save</ref_url>
        </reference>
        <reference>
            <ref_name>Update</ref_name>
            <ref_url>Update</ref_url>
        </reference>
    </references>
</manpage>