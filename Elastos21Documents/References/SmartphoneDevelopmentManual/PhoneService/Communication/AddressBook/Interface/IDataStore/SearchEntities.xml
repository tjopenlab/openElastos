    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume>地址薄引擎</volume>
<interface>IDataStore</interface><method> SearchEntities </method>
    <description>
        <para>
          通过组合条件，查找相应的记录。
        </para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> enum EntityClass </param_type>
                <param_name> ecEntity </param_name>
                <param_note>
                    <para>
                      为<link url="../../DataType/EntityClass.htm">EntityClass</link>枚举类型，表示组或联系人的记录类型
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> EzArray&lt;UINT&gt; </param_type>
                <param_name> eaFields </param_name>
                <param_note>
                    <para>
                     记录字段ID
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> EzArray&lt;WString&gt; </param_type>
                <param_name> wsTerms </param_name>
                <param_note>
                    <para>
                      参数ecEntity对应的字段值，与参数eaFields构成组合查询条件
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> BOOL </param_type>
                <param_name> bOnlyPrefer </param_name>
                <param_note>
                    <para>
                      是否为唯一选项
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> IObjectEnumeratorRef * </param_type>
                <param_name> ppiEntitiesEnum </param_name>
                <param_note>
                    <para>
                       记录的对象枚举器接口指针，不能为NULL
                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>
     <return>
	    <table>
	        <th>
	    	    <td>返回值 </td>
	    	    <td>描述</td>
	        </th>
	        <tr>
	    	    <td>NOERROR</td>
	    	    <td>程序执行成功</td>
	        </tr>

	        <tr>
	    	    <td>地址簿错误码 </td>
	    	    <td>地址簿操作错误 </td>
	        </tr>
	    </table>
	    <para>参见<link url="../../../../../QuickReference/ECode.htm">ECode</link>、<link url="../../AddressBook_error.htm">地址簿错误码</link>、<link url="../../../../../QuickReference/DBError.htm">数据库错误码</link>相关帮助。</para>
    </return>

    <requirements>
        <system>
            <system_name>
《和欣 2.0》或更高版本            </system_name>
        </system>

        <dll>
            <dll_name>database.dll</dll_name>
        </dll>
        <headerfile>
            <file_name>CDBStore.h, CContact.h, CContactGroup.h, abdata.h, CEntityEnumeration.h, spcomm.h</file_name>
        </headerfile>
        <library>
            <lib_name>elacrt.lib, elastos.lib, utility.lib</lib_name>
        </library>
    </requirements>
</manpage>