<volume>地址薄引擎</volume>
<interface>IAddressBook</interface><method> StartTransaction </method>
    <description>
        <para>
          开始数据库事务。
        </para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <NoArguments/>
    </syntax>
     <return>
     <table>
	        <th>
	    	    <td>返回值 </td>
	    	    <td>描述</td>
	        </th>
	        <tr>
	    	    <td>NOERROR</td>
	    	    <td>程序执行成功</td>
	        </tr>
	        <tr>
	    	    <td>数据库错误码 </td>
	    	    <td>数据操作操作错误 </td>
	        </tr>
	    </table>
	    <para>参见<link url="../../../../../QuickReference/ECode.htm">ECode</link>和<link url="../../../../../QuickReference/DBError.htm">数据库错误码</link>相关帮助。</para>
    </return>

    <requirements>
        <system>
            <system_name>
《和欣 2.0》或更高版本            </system_name>
        </system>
         <dll>
            <dll_name>database.dll</dll_name>
        </dll>
        <headerfile>
            <file_name>CAddressBook.h, CDBStore.h, CSimStore.h, CContact.h, CContactGroup.h,  Abdata.h, spcomm.h</file_name>
        </headerfile>
        <library>
            <lib_name>elacrt.lib, elastos.lib, utility.lib</lib_name>
        </library>
    </requirements>
</manpage>