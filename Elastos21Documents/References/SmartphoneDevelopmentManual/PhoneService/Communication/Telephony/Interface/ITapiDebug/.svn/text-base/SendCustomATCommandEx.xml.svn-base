    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume>通讯引擎</volume>
<interface>ITapiDebug</interface><method> SendCustomATCommandEx </method>
    <description>
        <para>
           发送自定义AT指令并接收响应数据。
        </para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> EzByteBuf </param_type>
                <param_name> ebAtCommand </param_name>
                <param_note>
                    <para>
                       需要发送的自定义AT指令
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> EzByteBuf </param_type>
                <param_name> ebRspBuf </param_name>
                <param_note>
                    <para>
                      存放发送AT指令响应的缓冲区
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> DWORD </param_type>
                <param_name> dwSizeToRead </param_name>
                <param_note>
                    <para>
                       读取AT响应的缓冲区最大容量
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> INT </param_type>
                <param_name> nTimeout </param_name>
                <param_note>
                    <para>
                      命令执行返回的最大允许时间（单位：秒）
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> BOOL </param_type>
                <param_name> bRetry </param_name>
                <param_note>
                    <para>
                      是否失败后重试
                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>
     <return>
	    <table>
	        <th>
	    	    <td>返回值 </td>
	    	    <td>描述</td>
	        </th>
	        <tr>
	    	    <td>NOERROR</td>
	    	    <td>程序执行成功</td>
	        </tr>
	        <tr>
	    	    <td>E_GEN_NOTREADY</td>
	    	    <td>通讯模块没有准备好，原因可能是没有被初始化或者连接线接触不良</td>
	        </tr>
	         <tr>
	    	    <td>E_GEN_SENDLINE</td>
	    	    <td>发送中遇到错误，原因可能是通讯模块故障或者连接线接触不良</td>
	        </tr>
	         <tr>
	    	    <td>E_GEN_TIMEOUT</td>
	    	    <td>命令发送后，通讯模块没有及时响应而超时</td>
	        </tr>
	    </table>
	    <para>参见<link url="../../../../../QuickReference/ECode.htm">ECode</link>和<link url="../../Telephony_Error.htm">Telephony_Error</link>相关帮助。</para>
	   </return>
	   <remarks>
	     <para>
	       本命令主要用于调试，也可以用于发送自定义AT指令到通讯模块。
	     </para>
	   </remarks>

    <requirements>
        <system>
            <system_name>
《和欣 2.0》或更高版本            </system_name>
        </system>
         <headerfile>
            <file_name>elastos.h, stdio.h, stdlib.h, CTelephony.h </file_name>
        </headerfile>
        <library>
            <lib_name>elastos.lib, elacrt.lib, elautil.lib, _atspec.lib, _atcommon.lib</lib_name>
        </library>
    </requirements>
</manpage>