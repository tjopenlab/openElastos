    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume>通讯引擎</volume>
<interface>ITapiEventHandler</interface><method> OnSimStatus </method>
    <description>
        <para>
          SIM卡状态相关改变提示事件。
        </para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> LPTAPISIMSTATUS </param_type>
                <param_name> ptsStatus </param_name>
                <param_note>
                    <para>
                       指向TAPI_SIMSTATUS结构类型的指针，用于表示SIM卡状态
                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

    <requirements>
        <system>
            <system_name>
《和欣 2.0》或更高版本            </system_name>
        </system>
        <headerfile>
            <file_name>elastos.h, stdio.h, stdlib.h, CTelephony.h </file_name>
        </headerfile>
        <library>
            <lib_name>elastos.lib, elacrt.lib, elautil.lib, _atspec.lib, _atcommon.lib</lib_name>
        </library>
    </requirements>
</manpage>