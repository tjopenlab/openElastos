    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/interfaceman.xsl"?>    <manpage>
    <volume>通讯引擎</volume>
    <interface> ITapiDebug </interface>
    <description>
        <para>
          通讯控制控件的除错接口。
        </para>
    </description>
    <methods>
<self>
    <method>
        <method_name>Transmit</method_name>
        <method_url>..\interface\ITapiDebug\Transmit</method_url>
    <m_description>
        向通讯模块发送数据。
    </m_description>
    </method>
    <method>
        <method_name>SendCustomATCommand</method_name>
        <method_url>..\interface\ITapiDebug\SendCustomATCommand</method_url>
    <m_description>
        发送自定义AT指令。
    </m_description>
    </method>
    <method>
        <method_name>SendCustomATCommandEx</method_name>
        <method_url>..\interface\ITapiDebug\SendCustomATCommandEx</method_url>
    <m_description>
        发送自定义AT指令并接收响应数据。
    </m_description>
    </method>
</self>
    </methods>
    <requirements>
        <system>
            <system_name>
《和欣 2.0》或更高版本            </system_name>
        </system>
        <headerfile>
            <file_name>elastos.h, stdio.h, stdlib.h, CTelephony.h </file_name>
        </headerfile>
        <library>
            <lib_name>elastos.lib, elacrt.lib, elautil.lib, _atspec.lib, _atcommon.lib</lib_name>
        </library>
    </requirements>
</manpage>