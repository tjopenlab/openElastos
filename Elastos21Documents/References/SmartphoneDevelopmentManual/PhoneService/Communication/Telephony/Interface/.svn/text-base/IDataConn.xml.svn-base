    <volume>通讯引擎</volume>
    <interface> IDataConn </interface>
    <description>
        <para>
           数据连接设置接口。
        </para>
    </description>
    <methods>
<self>
    <method>
        <method_name>RequestGPRSService</method_name>
        <method_url>..\interface\IDataConn\RequestGPRSService</method_url>
    <m_description>
        请求GPRS服务。
    </m_description>
    </method>
    <method>
        <method_name>CloseGPRSService</method_name>
        <method_url>..\interface\IDataConn\CloseGPRSService</method_url>
    <m_description>
        断开GPRS服务。
    </m_description>
    </method>
    <method>
        <method_name>RequestCSDService</method_name>
        <method_url>..\interface\IDataConn\RequestCSDService</method_url>
    <m_description>
        请求CSD数据服务。
    </m_description>
    </method>
    <method>
        <method_name>CloseCSDService</method_name>
        <method_url>..\interface\IDataConn\CloseCSDService</method_url>
    <m_description>
        断开CSD数据业务。
    </m_description>
    </method>
    <method>
        <method_name>SetHSCSDState</method_name>
        <method_url>..\interface\IDataConn\SetHSCSDState</method_url>
    <m_description>
        设定CSD通道格式。
    </m_description>
    </method>
    <method>
        <method_name>ReadData</method_name>
        <method_url>..\interface\IDataConn\ReadData</method_url>
    <m_description>
        读取数据。
    </m_description>
    </method>
    <method>
        <method_name>WriteData</method_name>
        <method_url>..\interface\IDataConn\WriteData</method_url>
    <m_description>
        写入数据。
    </m_description>
    </method>
</self>
    </methods>
