    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/interfaceman.xsl"?>    <manpage>
    <volume>通讯引擎</volume>
    <interface> ISimMgr </interface>
    <description>
        <para>
           SIM卡管理接口。
        </para>
    </description>
    <methods>
<self>
    <method>
        <method_name>GetDevCaps</method_name>
        <method_url>..\interface\ISimMgr\GetDevCaps</method_url>
    <m_description>
        查询SIM卡的性能参数。
    </m_description>
    </method>
    <method>
        <method_name>ReadPhonebookEntry</method_name>
        <method_url>..\interface\ISimMgr\ReadPhonebookEntry</method_url>
    <m_description>
        读电话本记录。
    </m_description>
    </method>
    <method>
        <method_name>GetPhonebookStatus</method_name>
        <method_url>..\interface\ISimMgr\GetPhonebookStatus</method_url>
    <m_description>
        查询电话本存储项的存储状态。
    </m_description>
    </method>
    <method>
        <method_name>WritePhonebookEntry</method_name>
        <method_url>..\interface\ISimMgr\WritePhonebookEntry</method_url>
    <m_description>
        添加电话本记录。
    </m_description>
    </method>
    <method>
        <method_name>DeletePhonebookEntry</method_name>
        <method_url>..\interface\ISimMgr\DeletePhonebookEntry</method_url>
    <m_description>
         删除电话本记录。
    </m_description>
    </method>
    <method>
        <method_name>GetPhoneLockedState</method_name>
        <method_url>..\interface\ISimMgr\GetPhoneLockedState</method_url>
    <m_description>
         查询SIM卡的锁定状态。
    </m_description>
    </method>
    <method>
        <method_name>UnlockPhone</method_name>
        <method_url>..\interface\ISimMgr\UnlockPhone</method_url>
    <m_description>
        输入密码解除SIM卡的锁定。
    </m_description>
    </method>
    <method>
        <method_name>GetChvInfo</method_name>
        <method_url>..\interface\ISimMgr\GetChvInfo</method_url>
    <m_description>
        查询SIM卡密码的尝试信息。
    </m_description>
    </method>
    <method>
        <method_name>GetLockingStatus</method_name>
        <method_url>..\interface\ISimMgr\GetLockingStatus</method_url>
    <m_description>
        查询手机或网络功能的锁定状态。
    </m_description>
    </method>
    <method>
        <method_name>SetLockingStatus</method_name>
        <method_url>..\interface\ISimMgr\SetLockingStatus</method_url>
    <m_description>
        设置手机或网络功能的锁定状态。
    </m_description>
    </method>
    <method>
        <method_name>ChangeLockingPassword</method_name>
        <method_url>..\interface\ISimMgr\ChangeLockingPassword</method_url>
    <m_description>
        修改电话或网络功能的锁定密码。
    </m_description>
    </method>
    <method>
        <method_name>GetSmsStorageStatus</method_name>
        <method_url>..\interface\ISimMgr\GetSmsStorageStatus</method_url>
    <m_description>
        查询SIM卡短信的存储状态。
    </m_description>
    </method>
    <method>
        <method_name>ReadOriginalMessage</method_name>
        <method_url>..\interface\ISimMgr\ReadOriginalMessage</method_url>
    <m_description>
         读取原始短信息数据。
    </m_description>
    </method>
    <method>
        <method_name>ReadMessage</method_name>
        <method_url>..\interface\ISimMgr\ReadMessage</method_url>
    <m_description>
        读SIM卡上的短信。
    </m_description>
    </method>
    <method>
        <method_name>WriteMessage</method_name>
        <method_url>..\interface\ISimMgr\WriteMessage</method_url>
    <m_description>
        向SIM卡存入短信。
    </m_description>
    </method>
    <method>
        <method_name>SetMessageState</method_name>
        <method_url>..\interface\ISimMgr\SetMessageState</method_url>
    <m_description>
        设置SIM卡中短信状态。
    </m_description>
    </method>
    <method>
        <method_name>DeleteMessage</method_name>
        <method_url>..\interface\ISimMgr\DeleteMessage</method_url>
    <m_description>
        删除SIM卡上的短信。
    </m_description>
    </method>
    <method>
        <method_name>ReadRecord</method_name>
        <method_url>..\interface\ISimMgr\ReadRecord</method_url>
    <m_description>
        从SIM卡中读文件。
    </m_description>
    </method>
    <method>
        <method_name>WriteRecord</method_name>
        <method_url>..\interface\ISimMgr\WriteRecord</method_url>
    <m_description>
        向SIM卡中写文件。
    </m_description>
    </method>
    <method>
        <method_name>GetRecordInfo</method_name>
        <method_url>..\interface\ISimMgr\GetRecordInfo</method_url>
    <m_description>
        获取SIM卡中的文件信息（目前未实现）。
    </m_description>
    </method>
</self>
    </methods>
    <requirements>
        <system>
            <system_name>
《和欣 2.0》或更高版本            </system_name>
        </system>
        <headerfile>
            <file_name>elastos.h, stdio.h, stdlib.h, CTelephony.h </file_name>
        </headerfile>
        <library>
            <lib_name>elastos.lib, elacrt.lib, elautil.lib, _atspec.lib, _atcommon.lib</lib_name>
        </library>
    </requirements>
</manpage>