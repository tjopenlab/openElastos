    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume>通讯引擎</volume>
<interface>ISms</interface><method> DecodeBroadcastMessage </method>
    <description>
        <para>
           解码信息广播。
        </para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> EzByteBuf </param_type>
                <param_name> szPdu </param_name>
                <param_note>
                    <para>
                       信息广播的原始PDU信息
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> BROADCAST_PROVIDER_SPECIFIC_DATA * </param_type>
                <param_name> pProviderData </param_name>
                <param_note>
                    <para>
                      指向<link url="../../DataType\Sim\BROADCAST_PROVIDER_SPECIFIC_DATA.htm">BROADCAST_PROVIDER_SPECIFIC_DATA</link>结构的指针，用于表示信息广播的提供者信息
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> LPSMS_MULTIPART </param_type>
                <param_name> psmiMultiPartInfo </param_name>
                <param_note>
                    <para>
                       多段信息广播的分页信息
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> WCHAR * </param_type>
                <param_name> pwszMessage </param_name>
                <param_note>
                    <para>
                       存放解码后的信息广播内容的缓冲区
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> DWORD </param_type>
                <param_name> dwMessageBufferSize </param_name>
                <param_note>
                    <para>
                      pwszMessage的容量，单位：Byte
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> DWORD * </param_type>
                <param_name> pdwBytesRead </param_name>
                <param_note>
                    <para>
                      实际用到的pwszMessage 缓冲区的大小，单位：Byte
                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>
     <return>
	    <table>
	        <th>
	    	    <td>返回值 </td>
	    	    <td>描述</td>
	        </th>
	        <tr>
	    	    <td>NOERROR</td>
	    	    <td>程序执行成功</td>
	        </tr>
	        <tr>
	    	    <td>FAMILY_GEN 类或FAMILY_CME类错误值</td>
	    	    <td>表示指令发送到通讯模块过程中遇到错误</td>
	        </tr>
	    </table>
	    <para>参见<link url="../../../../../QuickReference/ECode.htm">ECode</link>和<link url="../../Telephony_Error.htm">Telephony_Error</link>相关帮助。</para>
	   </return>

    <requirements>
        <system>
            <system_name>
《和欣 2.0》或更高版本            </system_name>
        </system>
         <headerfile>
            <file_name>elastos.h, stdio.h, stdlib.h, CTelephony.h </file_name>
        </headerfile>
        <library>
            <lib_name>elastos.lib, elacrt.lib, elautil.lib, _atspec.lib, _atcommon.lib</lib_name>
        </library>
    </requirements>
</manpage>