    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume>通讯引擎</volume>
<interface>ISms</interface><method> SendMessage </method>
    <description>
        <para>
          发送短信息。
        </para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> SMS_MSGTYPE </param_type>
                <param_name> smsmtMsgType </param_name>
                <param_note>
                    <para>
                      为<link url="../../DataType\Sim\SMS_MSGTYPE.htm">SMS_MSGTYPE</link>结构类型，表示短消息类型
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> SMS_ADDRESS * </param_type>
                <param_name> psmsaSMSCAddress </param_name>
                <param_note>
                    <para>
                      指向<link url="../../DataType\Sim\SMS_ADDRESS.htm">SMS_ADDRESS</link>结构类型的指针，用于表示短信中心地址。该项通常设为NULL,采用默认的短信中心地址。
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> SMS_ADDRESS * </param_type>
                <param_name> psmsaDestinationAddress </param_name>
                <param_note>
                    <para>
                      指向<link url="../../DataType\Sim\SMS_ADDRESS.htm">SMS_ADDRESS</link>结构类型的指针，用于表示短信的目的地址
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> LPTAPITIME </param_type>
                <param_name> pstValidityPeriod </param_name>
                <param_note>
                    <para>
                      指向<link url="../../DataType\System\TAPITIME.htm">TAPITIME</link>结构类型，表示短信的有效时间
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> VOID * </param_type>
                <param_name> pvData </param_name>
                <param_note>
                    <para>
                       要发送的短信内容
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> DWORD </param_type>
                <param_name> dwDataSize </param_name>
                <param_note>
                    <para>
                      pbData的大小
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> BYTE * </param_type>
                <param_name> pbProviderSpecificData </param_name>
                <param_note>
                    <para>
                      与短信类型相关的信息
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> DWORD </param_type>
                <param_name> dwProviderSpecificDataSize </param_name>
                <param_note>
                    <para>
                      pbProviderSpecificData的大小
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> SMS_DATA_ENCODING </param_type>
                <param_name> smsdeDataEncoding </param_name>
                <param_note>
                    <para>
                      为<link url="../../DataType\Sim\SMS_ADDRESS.htm">SMS_ADDRESS</link>枚举类型，表示短信内容的编码方式
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> DWORD </param_type>
                <param_name> dwOptions </param_name>
                <param_note>
                    <para>
                       可选项
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> SMS_MESSAGE_ID * </param_type>
                <param_name> psmsmidMessageID </param_name>
                <param_note>
                    <para>
                      短信的参考ID
                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>
       <return>
	    <table>
	        <th>
	    	    <td>返回值 </td>
	    	    <td>描述</td>
	        </th>
	        <tr>
	    	    <td>NOERROR</td>
	    	    <td>程序执行成功</td>
	        </tr>
	        <tr>
	    	    <td>FAMILY_GEN 类或FAMILY_CME类错误值</td>
	    	    <td>表示指令发送到通讯模块过程中遇到错误</td>
	        </tr>
	    </table>
	    <para>参见<link url="../../../../../QuickReference/ECode.htm">ECode</link>和<link url="../../Telephony_Error.htm">Telephony_Error</link>相关帮助。</para>
	   </return>

    <requirements>
        <system>
            <system_name>
《和欣 2.0》或更高版本            </system_name>
        </system>
         <headerfile>
            <file_name>elastos.h, stdio.h, stdlib.h, CTelephony.h </file_name>
        </headerfile>
        <library>
            <lib_name>elastos.lib, elacrt.lib, elautil.lib, _atspec.lib, _atcommon.lib</lib_name>
        </library>
    </requirements>
</manpage>