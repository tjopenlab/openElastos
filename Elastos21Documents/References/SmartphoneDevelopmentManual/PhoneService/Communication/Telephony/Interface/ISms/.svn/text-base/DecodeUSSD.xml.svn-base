    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume>通讯引擎</volume>
<interface>ISms</interface><method> DecodeUSSD </method>
    <description>
        <para>
          解码USSD信息。
        </para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> EzByteBuf </param_type>
                <param_name> szUSSD </param_name>
                <param_note>
                    <para>
                      USSD原数据（通过事件管理器获得）
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> WCHAR * </param_type>
                <param_name> pwszUSSD </param_name>
                <param_note>
                    <para>
                      存放解码后的USSD数据的buffer
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> DWORD </param_type>
                <param_name> dwUSSDBufferSize </param_name>
                <param_note>
                    <para>
                      pwszUSSD的容量，单位：Byte
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> PDWORD </param_type>
                <param_name> pdwBytesRead </param_name>
                <param_note>
                    <para>
                      实际用到的pwszUSSD的字节数，单位：Byte
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> PDWORD </param_type>
                <param_name> pdwFlags </param_name>
                <param_note>
                    <para>
                      由网络发出的USSD数据的指定标记
                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>
    <return>
	    <table>
	        <th>
	    	    <td>返回值 </td>
	    	    <td>描述</td>
	        </th>
	        <tr>
	    	    <td>NOERROR</td>
	    	    <td>程序执行成功</td>
	        </tr>
	        <tr>
	    	    <td>FAMILY_GEN 类或FAMILY_CME类错误值</td>
	    	    <td>表示指令发送到通讯模块过程中遇到错误</td>
	        </tr>
	    </table>
	    <para>参见<link url="../../../../../QuickReference/ECode.htm">ECode</link>和<link url="../../Telephony_Error.htm">Telephony_Error</link>相关帮助。</para>
	   </return>

    <remarks>
      <para>
        pdwFlags可能的取值定义如下:
      </para>
      <table>
		<th>
       <td>取值</td><td>描述</td>
    </th>
    <tr>
       <td>TAPI_USSDFLAG_ACTIONREQUIRED</td><td>网络需要USSD返回信息</td>
    </tr>
    <tr>
       <td>TAPI_USSDFLAG_ACTIONNOTNEEDED</td><td>网络不需要更多USSD信息</td>
    </tr>
    <tr>
       <td>TAPI_USSDFLAG_TERMINATED</td><td>USSD会话被网络中止</td>
    </tr>
    <tr>
       <td>TAPI_USSDFLAG_OTHERCLIENTRESPONDED</td><td>由于其他客户的响应，网络中止了USSD会话</td>
    </tr>
    <tr>
       <td>TAPI_USSDFLAG_UNSUPPORTED</td><td>网络不支持USSD</td>
    </tr>
    <tr>
       <td>TAPI_USSDFLAG_TIMEOUT</td><td>	USSD会话超时</td>
    </tr>
    </table>
    </remarks>


    <requirements>
        <system>
            <system_name>
《和欣 2.0》或更高版本            </system_name>
        </system>
         <headerfile>
            <file_name>elastos.h, stdio.h, stdlib.h, CTelephony.h </file_name>
        </headerfile>
        <library>
            <lib_name>elastos.lib, elacrt.lib, elautil.lib, _atspec.lib, _atcommon.lib</lib_name>
        </library>
    </requirements>
</manpage>