    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume>通讯引擎</volume>
<interface>ISms</interface><method> DecodeMessage </method>
    <description>
        <para>
           解码短信息。
        </para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> EzByteBuf </param_type>
                <param_name> szPdu </param_name>
                <param_note>
                    <para>
                      短信Pdu串
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> SMS_STATE </param_type>
                <param_name> smsstState </param_name>
                <param_note>
                    <para>
                      指向<link url="../../DataType\Sim\SMS_STATE.htm">SMS_STATE</link>枚举类型的指针，用于表示短信的状态
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> SMS_MSGTYPE * </param_type>
                <param_name> psmsmtMsgType </param_name>
                <param_note>
                    <para>
                      短信类型
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> SMS_ADDRESS * </param_type>
                <param_name> psmsaSMSCAddress </param_name>
                <param_note>
                    <para>
                      短信中心号码
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> SMS_ADDRESS * </param_type>
                <param_name> psmsaAddress </param_name>
                <param_note>
                    <para>
                      发送短信方号码
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> LPTAPITIME </param_type>
                <param_name> pstReceiveTime </param_name>
                <param_note>
                    <para>
                        收到短信的时间,短信中心收到并转发短信时打上的时间戳
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> VOID * </param_type>
                <param_name> pcBuffer </param_name>
                <param_note>
                    <para>
                       存放短信内容的缓冲区
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> DWORD </param_type>
                <param_name> dwBufferSize </param_name>
                <param_note>
                    <para>
                      pbBuffer的大小
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> LPSMS_MULTIPART </param_type>
                <param_name> pMultiPart </param_name>
                <param_note>
                    <para>
                      分段型短信的信息
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> BYTE * </param_type>
                <param_name> pbProviderSpecificBuffer </param_name>
                <param_note>
                    <para>
                      与短信类型相关的信息
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> DWORD </param_type>
                <param_name> dwProviderSpecificDataBuffer </param_name>
                <param_note>
                    <para>
                       pbProviderSpecificBuffer的大小
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> DWORD * </param_type>
                <param_name> pdwBytesRead </param_name>
                <param_note>
                    <para>
                      实际读入pbBuffer中的字符数
                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>
      <return>
	    <table>
	        <th>
	    	    <td>返回值 </td>
	    	    <td>描述</td>
	        </th>
	        <tr>
	    	    <td>NOERROR</td>
	    	    <td>程序执行成功</td>
	        </tr>
	        <tr>
	    	    <td>FAMILY_GEN 类或FAMILY_CME类错误值</td>
	    	    <td>表示指令发送到通讯模块过程中遇到错误</td>
	        </tr>
	    </table>
	    <para>参见<link url="../../../../../QuickReference/ECode.htm">ECode</link>和<link url="../../Telephony_Error.htm">Telephony_Error</link>相关帮助。</para>
	   </return>
	  <remarks>
	   	<para>分析收到的原始短信Pdu串，解码得到短信的各项具体内容。</para>
	  </remarks>

    <requirements>
        <system>
            <system_name>
《和欣 2.0》或更高版本            </system_name>
        </system>
         <headerfile>
            <file_name>elastos.h, stdio.h, stdlib.h, CTelephony.h </file_name>
        </headerfile>
        <library>
            <lib_name>elastos.lib, elacrt.lib, elautil.lib, _atspec.lib, _atcommon.lib</lib_name>
        </library>
    </requirements>
</manpage>