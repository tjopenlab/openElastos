    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume>通讯引擎</volume>
<interface>IHwMgr</interface><method> GetLanguageId </method>
    <description>
        <para>
          获取使用语言资源ID。
        </para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> PWORD </param_type>
                <param_name> langId </param_name>
                <param_note>
                    <para>
                       语言id
                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>
    <return>
	    <table>
	        <th>
	    	    <td>返回值 </td>
	    	    <td>描述</td>
	        </th>
	        <tr>
	    	    <td>NOERROR</td>
	    	    <td>程序执行成功</td>
	        </tr>
	        <tr>
	    	    <td>_INVALID_ARGUMENT</td>
	    	    <td>参数错误</td>
	        </tr>
	    </table>
	    <para>参见<link url="../../../../../QuickReference/ECode.htm">ECode</link>和<link url="../../telephony_error.htm">telephony_error</link>相关帮助。</para>
    </return>
    <remarks>
		<para>此API用于获取运营商名称等语言资源。</para>
		<para>语言Id定下如下：</para>
		<table>
		<th>
       <td>取值</td><td>描述</td>
    </th>
    <tr>
       <td>TAPI_LANGID_ENG</td><td>英文</td>
    </tr>
    <tr>
       <td>TAPI_LANGID_CHT</td><td>中文正体字</td>
    </tr>
    <tr>
       <td>TAPI_LANGID_CHS</td><td>中文简化字</td>
    </tr>
    </table>
    </remarks>
    <requirements>
        <system>
            <system_name>
《和欣 2.0》或更高版本
</system_name>
        </system>
        <headerfile>
            <file_name>elastos.h, stdio.h, stdlib.h, CTelephony.h </file_name>
        </headerfile>
        <library>
            <lib_name>elastos.lib, elacrt.lib, elautil.lib, _atspec.lib, _atcommon.lib</lib_name>
        </library>
    </requirements>
</manpage>