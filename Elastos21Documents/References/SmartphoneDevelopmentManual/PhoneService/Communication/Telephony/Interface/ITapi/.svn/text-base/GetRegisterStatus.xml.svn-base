    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume>通讯引擎</volume>
<interface>ITapi</interface><method> GetRegisterStatus </method>
    <description>
        <para>
          获取当前的网络注册状态信息。
        </para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> PDWORD </param_type>
                <param_name> pdwRegisterStatus </param_name>
                <param_note>
                    <para>
                      指向DWORD类型的指针，用于保存返回的网络注册状态信息
                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>
     <return>
	    <table>
	        <th>
	    	    <td>返回值 </td>
	    	    <td>描述</td>
	        </th>
	        <tr>
	    	    <td>NOERROR</td>
	    	    <td>程序执行成功</td>
	        </tr>
	         <tr>
	    	    <td>FAMILY_GEN 类或FAMILY_CME类错误值</td>
	    	    <td>表示指令发送到通讯模块过程中遇到错误</td>
	        </tr>
	    </table>
	    <para>参见<link url="../../../../../QuickReference/ECode.htm">ECode</link>和<link url="../../Telephony_Error.htm">Telephony_Error</link>相关帮助。</para>
	   </return>

	   <remarks>
	     <para>
	         网络注册状态信息定义如下：
	     </para>
	     <table>
	        <th>
	    	    <td>取值 </td>
	    	    <td>描述</td>
	        </th>

	        <tr>
	    	    <td>TAPI_REGSTATUS_UNREGISTERED</td><td>	未注册，且并不在搜索中</td>
	        </tr>
          <tr>
	    	    <td>TAPI_REGSTATUS_HOME</td><td>已注册，本地</td>
	        </tr>
          <tr>
	    	    <td>TAPI_REGSTATUS_ATTEMPTING	</td><td>未注册，在搜索中</td>
	        </tr>
          <tr>
	    	    <td>TAPI_REGSTATUS_DENIED</td><td>注册被拒绝</td>
	        </tr>
          <tr>
	    	    <td>TAPI_REGSTATUS_UNKNOWN</td><td>未知</td>
	        </tr>
          <tr>
	    	    <td>TAPI_REGSTATUS_ROAM</td><td>已注册，漫游</td>
	        </tr>
	       </table>
	   </remarks>

    <requirements>
        <system>
            <system_name>
《和欣 2.0》或更高版本            </system_name>
        </system>
         <headerfile>
            <file_name>elastos.h, stdio.h, stdlib.h, CTelephony.h </file_name>
        </headerfile>
        <library>
            <lib_name>elastos.lib, elacrt.lib, elautil.lib, _atspec.lib, _atcommon.lib</lib_name>
        </library>
    </requirements>
</manpage>