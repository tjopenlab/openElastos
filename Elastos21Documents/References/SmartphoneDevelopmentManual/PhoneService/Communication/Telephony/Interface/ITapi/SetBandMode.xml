    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume>通讯引擎</volume>
<interface>ITapi</interface><method> SetBandMode </method>
    <description>
        <para>
           设定GSM模块当前工作波段。
        </para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> DWORD </param_type>
                <param_name> band </param_name>
                <param_note>
                    <para>
                      波段值
                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>
      <return>
	    <table>
	        <th>
	    	    <td>返回值 </td>
	    	    <td>描述</td>
	        </th>
	        <tr>
	    	    <td>NOERROR</td>
	    	    <td>程序执行成功</td>
	        </tr>
	        <tr>
	    	    <td>FAMILY_GEN 类或FAMILY_CME类错误值</td>
	    	    <td>表示指令发送到通讯模块过程中遇到错误</td>
	        </tr>
	    </table>
	    <para>参见<link url="../../../../../QuickReference/ECode.htm">ECode</link>和<link url="../../Telephony_Error.htm">Telephony_Error</link>相关帮助。</para>
	   </return>
	   <remarks>
		<para>band类型定义如下：</para>

		<table>
		<th>
       <td>取值</td><td>描述</td>
    </th>
    <tr>
       <td>SIM_PBSTORAGE_EMERGENCY</td><td>紧急电话列表</td>
    </tr>
    <tr>
       <td>TAPI_BANDMODE_850</td><td>	850</td>
    </tr>
    <tr>
       <td>TAPI_BANDMODE_900E</td><td> 900E</td>
    </tr>
    <tr>
       <td>TAPI_BANDMODE_1800</td><td>1800</td>
    </tr>
    <tr>
       <td>TAPI_BANDMODE_1900</td><td>1900</td>
    </tr>
    <tr>
       <td>TAPI_BANDMODE_850_1900</td><td>	850 1900</td>
    </tr>
    <tr>
       <td>TAPI_BANDMODE_900E_1800</td><td>	900E 1800</td>
    </tr>
    <tr>
       <td>TAPI_BANDMODE_900E_1900</td><td>	900E 1900</td>
    </tr>
    <tr>
       <td>TAPI_BANDMODE_850_1900_900E</td><td>	850 1900 900E</td>
    </tr>
    <tr>
       <td>TAPI_BANDMODE_850_1900_900E_1800</td><td>	850 1900 900E 1800</td>
    </tr>
    <tr>
       <td>TAPI_BANDMODE_AUTO</td><td>自动</td>
    </tr>
    </table>
    </remarks>

    <requirements>
        <system>
            <system_name>
《和欣 2.0》或更高版本            </system_name>
        </system>
         <headerfile>
            <file_name>elastos.h, stdio.h, stdlib.h, CTelephony.h </file_name>
        </headerfile>
        <library>
            <lib_name>elastos.lib, elacrt.lib, elautil.lib, _atspec.lib, _atcommon.lib</lib_name>
        </library>
    </requirements>
</manpage>