    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume>通讯引擎</volume>
<interface>ITapi</interface><method> GetCallBarringCaps </method>
    <description>
        <para>
          获取允许设置的通话控制模式。
        </para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> PDWORD </param_type>
                <param_name> pdwModes </param_name>
                <param_note>
                    <para>
                      指向DWORD类型的指针，用于保存返回的模式信息
                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>
     <return>
	    <table>
	        <th>
	    	    <td>返回值 </td>
	    	    <td>描述</td>
	        </th>
	        <tr>
	    	    <td>NOERROR</td>
	    	    <td>程序执行成功</td>
	        </tr>
	        <tr>
	    	    <td>FAMILY_GEN 类或FAMILY_CME类错误值</td>
	    	    <td>表示指令发送到通讯模块过程中遇到错误</td>
	        </tr>
	    </table>
	    <para>参见<link url="../../../../../QuickReference/ECode.htm">ECode</link>和<link url="../../Telephony_Error.htm">Telephony_Error</link>相关帮助。</para>
	   </return>
    <remarks>
      <para>
         *pdwModes保存返回的模式信息，以下的每个取值对应DWORD的一个bit:
      </para>
		<table>
		<th>
       <td>取值</td><td>描述</td>
    </th>
    <tr>
       <td>TAPI_BARRMODE_OUT</td><td>	禁止拨打电话</td>
    </tr>
    <tr>
       <td>TAPI_BARRMODE_OUT_INT</td><td>	禁止拨打国际电话</td>
    </tr>
    <tr>
       <td>TAPI_BARRMODE_OUT_INTEXTOHOME</td><td>	禁止拨打国际电话，除了拨打到本国的</td>
    </tr>
    <tr>
       <td>TAPI_BARRMODE_IN</td><td>	禁止接听电话</td>
    </tr>
    <tr>
       <td>TAPI_BARRMODE_IN_ROAM</td><td>	在外漫游时禁止接听电话</td>
    </tr>
    <tr>
       <td>TAPI_BARRMODE_IN_NOTINSIM</td><td>	禁止接收号码不在本机SIM卡中的电话</td>
    </tr>
    <tr>
       <td>TAPI_BARRMODE_ALL</td><td>	所有的禁止服务</td>
    </tr>
    <tr>
       <td>TAPI_BARRMODE_ALL_OUT</td><td>	所有的禁止拨出服务</td>
    </tr>
    <tr>
       <td>TAPI_BARRMODE_ALL_IN	</td><td>所有的禁止接听服务</td>
    </tr>
    </table>
    </remarks>

    <requirements>
        <system>
            <system_name>
《和欣 2.0》或更高版本            </system_name>
        </system>
         <headerfile>
            <file_name>elastos.h, stdio.h, stdlib.h, CTelephony.h </file_name>
        </headerfile>
        <library>
            <lib_name>elastos.lib, elacrt.lib, elautil.lib, _atspec.lib, _atcommon.lib</lib_name>
        </library>
    </requirements>
</manpage>