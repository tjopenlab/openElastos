    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/custom_type.xsl"?>    <manpage>
    <volume>通讯引擎</volume>
    <types> SMS_MSGTYPE </types>
    <description>
        <para>
            短消息类型。
        </para>
    </description>
    <enum>
       <enum_member>
          <mem_name> SMS_MSGTYPE_TEXT </mem_name>
          <mem_value> 0x0 </mem_value>
          <mem_description> 文本型 </mem_description>
       </enum_member>
       <enum_member>
          <mem_name> SMS_MSGTYPE_BROADCAST </mem_name>
          <mem_value> 0x1 </mem_value>
          <mem_description> 广播消息 </mem_description>
       </enum_member>
       <enum_member>
          <mem_name> SMS_MSGTYPE_NOTIFICATION </mem_name>
          <mem_value> 0x2 </mem_value>
          <mem_description>  通知 </mem_description>
       </enum_member>
       <enum_member>
          <mem_name> SMS_MSGTYPE_RAW </mem_name>
          <mem_value> 0x3 </mem_value>
          <mem_description> 未处理短信</mem_description>
       </enum_member>
       <enum_member>
          <mem_name> SMS_MSGTYPE_STATUS </mem_name>
          <mem_value> 0x4 </mem_value>
          <mem_description> </mem_description>
       </enum_member>
        <enum_member>
          <mem_name> SMS_MSGTYPE_WCMP </mem_name>
          <mem_value> 0x5 </mem_value>
          <mem_description> WCMP</mem_description>
       </enum_member>
       <enum_member>
          <mem_name> SMS_MSGTYPE_WDP </mem_name>
          <mem_value> 0x6 </mem_value>
          <mem_description> WDP</mem_description>
       </enum_member>
       <enum_member>
          <mem_name> SMS_MSGTYPE_UNSUPPORTED </mem_name>
          <mem_value> 0xffffffff </mem_value>
          <mem_description> 不支持类型</mem_description>
       </enum_member>

    </enum>

</manpage>

