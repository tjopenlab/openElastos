       </enum_member>
       <enum_member>
          <mem_name> SMSST_STO_SENT </mem_name>
          <mem_value> 0x3 </mem_value>
          <mem_description> 保存的已发送短信</mem_description>
       </enum_member>
       <enum_member>
          <mem_name> SMSST_ALL </mem_name>
          <mem_value> 0x4 </mem_value>
          <mem_description> 所有短信</mem_description>
       </enum_member>

    </enum>

