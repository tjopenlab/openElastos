    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/custom_type.xsl"?>    <manpage>
    <volume>通讯引擎</volume>
    <types> SMS_STATE </types>
    <description>
        <para>
            短信状态。
        </para>
    </description>
    <enum>
       <enum_member>
          <mem_name> SMSST_REC_UNREAD </mem_name>
          <mem_value> 0x0 </mem_value>
          <mem_description> 收到的未读短信 </mem_description>
       </enum_member>
       <enum_member>
          <mem_name> SMSST_REC_READ </mem_name>
