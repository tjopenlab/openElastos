    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/interfaceman.xsl"?>    <manpage>
    <volume></volume>
    <interface> IConnMgr </interface>
    <description>
        <para>
           对数据连接的建立、断开等进行管理
        </para>
    </description>
    <methods>
<self>
    <method>
        <method_name>GetAvailConnection</method_name>
        <method_url>..\interface\IConnMgr\GetAvailConnection</method_url>
    <m_description>
        获取可用连接信息
    </m_description>
    </method>
    <method>
        <method_name>Connect</method_name>
        <method_url>..\interface\IConnMgr\Connect</method_url>
    <m_description>
        建立数据连接
    </m_description>
    </method>
    <method>
        <method_name>Disconnect</method_name>
        <method_url>..\interface\IConnMgr\Disconnect</method_url>
    <m_description>
        断开数据连接
    </m_description>
    </method>
    <method>
        <method_name>GetConnectionStatistics</method_name>
        <method_url>..\interface\IConnMgr\GetConnectionStatistics</method_url>
    <m_description>
        统计数据流量
    </m_description>
    </method>
    <method>
        <method_name>SetConnectionStrategy</method_name>
        <method_url>..\interface\IConnMgr\SetConnectionStrategy</method_url>
    <m_description>
        数据连接配置
    </m_description>
    </method>
    <method>
        <method_name>Terminate</method_name>
        <method_url>..\interface\IConnMgr\Terminate</method_url>
    <m_description>
        终止数据连接
    </m_description>
    </method>
</self>
    </methods>
    <requirements>
        <system>
            <system_name>
《和欣 2.1》或更高版本            </system_name>
        </system>
    </requirements>
</manpage>