    <interface> IConnMgrCallback </interface>
    <description>
        <para>
           对数据连接的建立、断开等事件的管理
        </para>
    </description>
    <methods>
<self>
    <method>
        <method_name>OnConnectStatus</method_name>
        <method_url>..\interface\IConnMgrCallback\OnConnectStatus</method_url>
    <m_description>
        数据连接状态事件
    </m_description>
    </method>
    <method>
        <method_name>OnRequestConnect</method_name>
        <method_url>..\interface\IConnMgrCallback\OnRequestConnect</method_url>
    <m_description>
        数据连接请求响应事件
    </m_description>
    </method>
    <method>
        <method_name>OnDetectConflicting</method_name>
        <method_url>..\interface\IConnMgrCallback\OnDetectConflicting</method_url>
    <m_description>
        数据连接冲突检测事件
    </m_description>
    </method>
</self>
    </methods>
    <requirements>
        <system>
            <system_name>
《和欣 2.1》或更高版本            </system_name>
        </system>
    </requirements>
