<interface>IConnMgr</interface><method> GetAvailConnection </method>
    <description>
        <para>
           获取可用连接信息
        </para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> CONNHANDLEINFO * </param_type>
                <param_name> avail </param_name>
                <param_note>
                    <para>
                       指向CONNHANDLEINFO结构型的指针，用于返回可用连接信息
                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

<return>
	    <table>
	        <th>
	    	    <td>返回值 </td>
	    	    <td>描述 </td>
	        </th>
	        <tr>
	    	    <td>NOERROR</td>
	    	    <td>方法调用成功 </td>
	        </tr>
	        <tr>
	    	    <td>E_INVALID_ARGUMENT </td>
	    	    <td>参数错误 </td>
	        </tr>
	    </table>
</return>

    <requirements>
        <system>
            <system_name>
《和欣 2.1》或更高版本            </system_name>
        </system>
    </requirements>
