    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume></volume>
<interface>IConnMgr</interface><method> Connect </method>
    <description>
        <para>
           建立数据连接
        </para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> IConnectionSetting * </param_type>
                <param_name> connSetting </param_name>
                <param_note>
                    <para>
                       指向IConnectionSetting接口的指针，用于表示网络属性设置
                    </para>
                </param_note>
            </api_param>
        </parameters>
        <parameters>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> HANDLE * </param_type>
                <param_name> connHandle </param_name>
                <param_note>
                    <para>
                       连接句柄
                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

<return>
	    <table>
	        <th>
	    	    <td>返回值 </td>
	    	    <td>描述</td>
	        </th>
	        <tr>
	    	    <td>NOERROR</td>
	    	    <td>方法调用成功</td>
	        </tr>
	        <tr>
	    	    <td>E_INVALID_ARGUMENT</td>
	    	    <td>参数错误</td>
	        </tr>
	        <tr>
	    	    <td>E_CONNMGR_OPERTEMPNOTALLOWED</td>
	    	    <td>连接暂时未被允许 </td>
	        </tr>
	        <tr>
	    	    <td>E_CONNMGR_CONNECTFAILED</td>
	    	    <td>连接失败 </td>
	        </tr>
	        <tr>
	    	    <td>E_CONNMGR_KILLCONFLICTEDFAILED</td>
	    	    <td>结束冲突的连接失败 </td>
	        </tr>
	    </table>
</return>

    <requirements>
        <system>
            <system_name>
《和欣 2.1》或更高版本            </system_name>
        </system>
    </requirements>
</manpage>