    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/interfaceman.xsl"?>    <manpage>
    <volume></volume>
    <interface> IBlacklist </interface>
    <description>
        <para>
              黑名单引擎接口。
        </para>
    </description>
    <methods>
<self>
    <method>
        <method_name>CreateEntry</method_name>
        <method_url>..\interface\IBlacklist\CreateEntry</method_url>
    <m_description>
                创建黑名单记录接口指针，通过调用该接口中的方法获取相关数据项，在调用后面所述的接口完成在数据库中添加记录的操作。
    </m_description>
    </method>
    <method>
        <method_name>SearchEntry</method_name>
        <method_url>..\interface\IBlacklist\SearchEntry</method_url>
    <m_description>
                根据给定的黑名单内容，返回查找到的记录。
    </m_description>
    </method>
    <method>
        <method_name>GetEntryByNumberFuzzyMatch</method_name>
        <method_url>..\interface\IBlacklist\GetEntryByNumberFuzzyMatch</method_url>
    <m_description>
                根据给定的黑名单号码，返回查找到的记录。
    </m_description>
    </method>
    <method>
        <method_name>GetEntryById</method_name>
        <method_url>..\interface\IBlacklist\GetEntryById</method_url>
    <m_description>
                根据给定的黑名单记录ID，返回查找到的记录。
    </m_description>
    </method>
    <method>
        <method_name>Update</method_name>
        <method_url>..\interface\IBlacklist\Update</method_url>
    <m_description>
                在数据库中更新一条记录。
    </m_description>
    </method>
    <method>
        <method_name>RemoveAll</method_name>
        <method_url>..\interface\IBlacklist\RemoveAll</method_url>
    <m_description>
                删除所有的黑名单记录。
    </m_description>
    </method>
    <method>
        <method_name>GetCount</method_name>
        <method_url>..\interface\IBlacklist\GetCount</method_url>
    <m_description>
                返回总的黑名单条目数。
    </m_description>
    </method>
    <method>
        <method_name>DeleteEntryById</method_name>
        <method_url>..\interface\IBlacklist\DeleteEntryById</method_url>
    <m_description>
                根据给定的黑名单记录ID，删除查找到的记录。
    </m_description>
    </method>
    <method>
        <method_name>DeleteEntriesByIds</method_name>
        <method_url>..\interface\IBlacklist\DeleteEntriesByIds</method_url>
    <m_description>
                根据给定的一组黑名单记录ID，删除查找到的记录。
    </m_description>
    </method>
</self>
    </methods>
    <requirements>
        <system>
            <system_name>
《和欣 2.0》或更高版本            </system_name>
        </system>
    </requirements>
</manpage>