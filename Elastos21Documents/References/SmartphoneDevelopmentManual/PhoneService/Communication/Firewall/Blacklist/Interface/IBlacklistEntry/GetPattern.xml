<volume></volume>
<interface>IBlacklistEntry</interface><method> GetPattern </method>
    <description>
        <para>
              获取黑名单记录中的黑名单内容字段。
        </para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> EzWStrBuf </param_type>
                <param_name> ewsbPattern </param_name>
                <param_note>
                    <para>
                          返回相应的屏蔽内容
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> INT * </param_type>
                <param_name> pnType </param_name>
                <param_note>
                    <para>
                          黑名单类型，电话号码或者email
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> INT * </param_type>
                <param_name> pnBlockType </param_name>
                <param_note>
                    <para>
                          屏蔽类型
                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>
    	<return>
	    <table>
	        <th>
	    	    <td>返回值 </td>
	    	    <td>描述</td>
	        </th>
	        <tr>
	    	    <td>NOERROR</td>
	    	    <td>方法调用成功</td>
	        </tr>
	        <tr>
	    	    <td>E_INVALID_ARGUMENT </td>
	    	    <td>参数错误 </td>
	        </tr>
	        <tr>
	    	    <td>数据库错误码 </td>
	    	    <td>数据库错误 </td>
	        </tr>
	    </table>
	    <para>参见<link url="../../../../../../QuickReference/ECode.htm">ECode</link>和<link url="../../../../../../QuickReference/DBError.htm">数据库错误码</link>相关帮助。</para>
    </return>

    <requirements>
        <system>
            <system_name>
《和欣 2.0》或更高版本            </system_name>
        </system>
    </requirements>
</manpage>