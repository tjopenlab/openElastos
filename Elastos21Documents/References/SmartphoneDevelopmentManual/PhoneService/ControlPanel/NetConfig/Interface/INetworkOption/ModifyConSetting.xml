<interface>INetworkOption</interface><method> ModifyConSetting </method>
    <description>
        <para>
           更新某个网络配置
        </para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> int </param_type>
                <param_name> nNetworkID </param_name>
                <param_note>
                    <para>
                       网络配置的ID
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> IConnectionSetting * </param_type>
                <param_name> pIConSetting </param_name>
                <param_note>
                    <para>
                       指向IConnectionSetting 接口指针，用于表示要更新的网络配置
                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

<return>
	    <table>
	        <th>
	    	    <td>返回值 </td>
	    	    <td>描述</td>
	        </th>
	        <tr>
	    	    <td>NOERROR</td>
	    	    <td>方法调用成功</td>
	        </tr>
	        <tr>
	    	    <td>数据库错误码</td>
	    	    <td>数据库操作错误</td>
	        </tr>
	    </table>
</return>
    <requirements>
        <system>
            <system_name>
《和欣 2.1》或更高版本            </system_name>
        </system>
    </requirements>
</manpage>