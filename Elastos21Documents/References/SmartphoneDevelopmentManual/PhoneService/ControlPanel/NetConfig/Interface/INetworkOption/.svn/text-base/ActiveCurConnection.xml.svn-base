<interface>INetworkOption</interface><method> ActiveCurConnection </method>
    <description>
        <para>
           设置某个网络配置为默认配置
        </para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> int </param_type>
                <param_name> nNetworkID </param_name>
                <param_note>
                    <para>
                       网络配置的ID
                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

