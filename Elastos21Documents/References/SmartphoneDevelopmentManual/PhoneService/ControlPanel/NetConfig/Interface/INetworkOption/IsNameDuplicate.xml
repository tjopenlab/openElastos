    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume></volume>
<interface>INetworkOption</interface><method> IsNameDuplicate </method>
    <description>
        <para>
           判断某个配置连接是否存在
        </para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> WString </param_type>
                <param_name> wsConName </param_name>
                <param_note>
                    <para>
                       连接名称
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> BOOL * </param_type>
                <param_name> pbDuplicate </param_name>
                <param_note>
                    <para>
                       指向BOOL型的指针，用于表示某个连接是否存在，为TRUE表示该连接存在，为FALSE表示该连接不存在
                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

<return>
	    <table>
	        <th>
	    	    <td>返回值 </td>
	    	    <td>描述 </td>
	        </th>
	        <tr>
	    	    <td>NOERROR</td>
	    	    <td>方法调用成功 </td>
	        </tr>
	        <tr>
	    	    <td>E_INVALID_ARGUMENT</td>
	    	    <td>参数错误 </td>
	        </tr>
	        <tr>
	    	    <td>数据库错误码 </td>
	    	    <td>数据库操作错误 </td>
	        </tr>
	    </table>
</return>

    <requirements>
        <system>
            <system_name>
《和欣 2.1》或更高版本            </system_name>
        </system>
    </requirements>
</manpage>