    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume></volume>
<interface>INetworkOption</interface><method> ListConnections </method>
    <description>
        <para>
           列表显示网络配置
        </para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> enum WapType </param_type>
                <param_name> uWapType </param_name>
                <param_note>
                    <para>
                       为<link url="../../DataType/WapType.htm">WapType</link>枚举类型，包括GPRS和CSD，GPRS表示GPRS，CSD表示CSD
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> IObjectEnumerator ** </param_type>
                <param_name> ppEnum </param_name>
                <param_note>
                    <para>
                       指向IObjectEnumerator接口指针的指针，用于存放所有网络配置的名称
                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

<return>
	    <table>
	        <th>
	    	    <td>返回值 </td>
	    	    <td>描述 </td>
	        </th>
	        <tr>
	    	    <td>NOERROR</td>
	    	    <td>方法调用成功 </td>
	        </tr>
	        <tr>
	    	    <td>E_INVALID_ARGUMENT</td>
	    	    <td>参数错误 </td>
	        </tr>
	        <tr>
	    	    <td>数据库错误码 </td>
	    	    <td>数据库操作错误 </td>
	        </tr>
	    </table>
</return>
    
    <requirements>
        <system>
            <system_name>
《和欣 2.1》或更高版本            </system_name>
        </system>
    </requirements>
</manpage>