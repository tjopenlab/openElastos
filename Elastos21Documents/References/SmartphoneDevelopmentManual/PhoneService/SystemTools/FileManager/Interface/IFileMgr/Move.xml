    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume>文件管理器</volume>
<interface>IFileMgr</interface><method> Move </method>
    <description>
        <para>
        移动文件
        </para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> WString </param_type>
                <param_name> wsSrc </param_name>
                <param_note>
                    <para>包含全路径的源文件名
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> WString </param_type>
                <param_name> wsDst </param_name>
                <param_note>
                    <para>目的路径，包含全路径
                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>
     <return>
	    <table>
	        <th>
	    	    <td>返回值 </td>
	    	    <td>描述</td>
	        </th>
	        <tr>
	    	    <td>NOERROR</td>
	    	    <td>程序执行成功</td>
	        </tr>
	        <tr>
	    	    <td>E_INVALID_ARGUMENT</td>
	    	    <td>无效的参数</td>
	        </tr>
	        <tr>
	    	    <td>E_ACCESS_DENIED</td>
	    	    <td>源文件名或文件夹不可移动</td>
	        </tr>
	        <tr>
	    	    <td>E_PATH_INVALID_OR_NOT_FOUND</td>
	    	    <td>路径不正确</td>
	        </tr>
	        <tr>
	    	    <td>E_PATH_LEN_INVALID</td>
	    	    <td>路径的长度超过最大长度</td>
	        </tr>
	    </table>
	    <para>参见<link url="../../../../../QuickReference/ECode.htm">ECode</link>相关帮助。</para>
    </return>
    <requirements>
        <system>
            <system_name>
《和欣 2.0》或更高版本            </system_name>
        </system>
    </requirements>
</manpage>