    <?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/interfaceman.xsl"?>    <manpage>
    <volume>文件管理器</volume>
    <interface> IFileMgr </interface>
    <description>
        <para>
            文件管理器接口
        </para>
    </description>
    <methods>
<self>
    <method>
        <method_name>IsDir</method_name>
        <method_url>..\interface\IFileMgr\IsDir</method_url>
    <m_description>
        判断是否为文件夹
    </m_description>
    </method>
    <method>
        <method_name>MkDir</method_name>
        <method_url>..\interface\IFileMgr\MkDir</method_url>
    <m_description>
        新建文件夹
    </m_description>
    </method>
    <method>
        <method_name>RmDir</method_name>
        <method_url>..\interface\IFileMgr\RmDir</method_url>
    <m_description>
        删除文件夹
    </m_description>
    </method>
    <method>
        <method_name>Rename</method_name>
        <method_url>..\interface\IFileMgr\Rename</method_url>
    <m_description>
        修改文件夹名或文件名
    </m_description>
    </method>
    <method>
        <method_name>Remove</method_name>
        <method_url>..\interface\IFileMgr\Remove</method_url>
    <m_description>
        删除文件
    </m_description>
    </method>
    <method>
        <method_name>Move</method_name>
        <method_url>..\interface\IFileMgr\Move</method_url>
    <m_description>
        移动文件
    </m_description>
    </method>
    <method>
        <method_name>GetFileUpdateTime</method_name>
        <method_url>..\interface\IFileMgr\GetFileUpdateTime</method_url>
    <m_description>
        取得文件修改时间
    </m_description>
    </method>
    <method>
        <method_name>GetSize</method_name>
        <method_url>..\interface\IFileMgr\GetSize</method_url>
    <m_description>
        取得文件大小
    </m_description>
    </method>
    <method>
        <method_name>GetItemCountInDir</method_name>
        <method_url>..\interface\IFileMgr\GetItemCountInDir</method_url>
    <m_description>
        取得子文件夹和文件的总数和总文件大小
    </m_description>
    </method>
    <method>
        <method_name>GetSizeAndCount</method_name>
        <method_url>..\interface\IFileMgr\GetSizeAndCount</method_url>
    <m_description>
        取得路径下的子文件夹、子文件的总数和文件总大小
    </m_description>
    </method>
    <method>
        <method_name>Cancel</method_name>
        <method_url>..\interface\IFileMgr\Cancel</method_url>
    <m_description>
        取消当前的操作
    </m_description>
    </method>
    <method>
        <method_name>Copy</method_name>
        <method_url>..\interface\IFileMgr\Copy</method_url>
    <m_description>
        文件或文件夹复制
    </m_description>
    </method>
    <method>
        <method_name>EnumInit</method_name>
        <method_url>..\interface\IFileMgr\EnumInit</method_url>
    <m_description>
        初始化文件枚举
    </m_description>
    </method>
    <method>
        <method_name>EnumNext</method_name>
        <method_url>..\interface\IFileMgr\EnumNext</method_url>
    <m_description>
        枚举下一个文件
    </m_description>
    </method>
    <method>
        <method_name>GetFreeSpace</method_name>
        <method_url>..\interface\IFileMgr\GetFreeSpace</method_url>
    <m_description>
        文件系统的总大小和剩余空间大小
    </m_description>
    </method>
    <method>
        <method_name>SetProgressEventState</method_name>
        <method_url>..\interface\IFileMgr\SetProgressEventState</method_url>
    <m_description>
        设置监视进度事件的状态
    </m_description>
    </method>
    <method>
        <method_name>GetFileOperatedInfo</method_name>
        <method_url>..\interface\IFileMgr\GetFileOperatedInfo</method_url>
    <m_description>
        取得当前的操作信息
    </m_description>
    </method>
    <method>
        <method_name>GetSizeAndCountInfo</method_name>
        <method_url>..\interface\IFileMgr\GetSizeAndCountInfo</method_url>
    <m_description>
        取得当前的操作信息
    </m_description>
    </method>
</self>
    </methods>
    <requirements>
        <system>
            <system_name>
《和欣 2.0》或更高版本            </system_name>
        </system>
    </requirements>
</manpage>