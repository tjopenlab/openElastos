<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/shellmanual.xsl"?>

<manpage>
    <title> kill </title>
    <volume>微软的工具</volume>

    <description>
        <para>杀死一个进程。详细内容请参见微软文档。</para>
    </description>

    <syntax>
        <cmd_name> KILL </cmd_name>
        <cmd_param> [-f] pID|cmd </cmd_param>
    </syntax>
    <parameters>
    <param>
        <param_name>  -f </param_name>
        <param_note> 强制杀死一个进程</param_note>
    </param>
    <param>
        <param_name>  pID </param_name>
        <param_note> 进程号</param_note>
    </param>
    <param>
        <param_name>  cmd </param_name>
        <param_note> 进程名</param_note>
    </param>
    </parameters>

    <examples>
    <example>
        <eg_line>kill 1544 </eg_line>
        <eg_note> 终止进程号为1544的进程。输出结果（与实际使用环境有关）：</eg_note>
        <eg_result>
        <![CDATA[
process winhlp32.exe (1544) - 'Dependency Walker 2.0 Beta 5 Help' killed
]]></eg_result>

    </example>
    </examples>

</manpage>