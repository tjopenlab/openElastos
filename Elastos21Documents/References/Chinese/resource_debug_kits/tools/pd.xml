<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/shellmanual.xsl"?>

<manpage>
    <title> pd 工具</title>
    <volume>XDK常用工具</volume>

    <description>
        <para> 在目录之间的切换。</para>
    </description>

    <syntax>
        <cmd_name> PD  </cmd_name>
        <cmd_param> -?|dir|~|@|inc|lib|cls </cmd_param>
    </syntax>
    <parameters>
        <param>
            <param_name> -? </param_name>
            <param_note> 显示帮助信息 </param_note>
        </param>
        <param>
            <param_name> dir </param_name>
            <param_note> 目录名</param_note>
        </param>
        <param>
            <param_name>  ~ </param_name>
            <param_note> 切换到（压栈）镜像目录</param_note>
        </param>
        <param>
            <param_name>  无参数 </param_name>
            <param_note> 退出栈到压栈前目录</param_note>
        </param>
        <param>
            <param_name>  @ </param_name>
            <param_note> 切换到（压栈）XDK_TARGET目录</param_note>
        </param>
        <param>
            <param_name>  inc </param_name>
            <param_note> 切换到（压栈）inc目录</param_note>
        </param>
        <param>
            <param_name>  lib </param_name>
            <param_note> 切换到（压栈）lib目录</param_note>
        </param>
        <param>
            <param_name>  cls </param_name>
            <param_note> 切换到（压栈）cls目录</param_note>
        </param>
    </parameters>

    <examples>
        <example>
            <eg_line>pd </eg_line>
            <eg_note>退出到压栈前的目录。</eg_note>
        </example>
        <example>
            <eg_line>pd ~</eg_line>
            <eg_note>切换到（压栈）镜像目录。</eg_note>
        </example>
    </examples>

</manpage>