<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/shellmanual.xsl"?>

<manpage>
    <title> mkfloppy </title>
    <volume>Elastos常用工具</volume>

    <description>
        <para>制作GRUB安装盘。</para>
    </description>

    <syntax>
        <cmd_name> MKFLOPPY </cmd_name>
        <cmd_param> [-? | -s | -m | -g | -i | -e] </cmd_param>
    </syntax>
    <parameters>
    <param>
        <param_name>  -? </param_name>
        <param_note> 显示帮助信息 </param_note>
    </param>
    <param>
        <param_name> -s </param_name>
        <param_note> 制作GRUB安装盘</param_note>
    </param>
    <param>
        <param_name> -m </param_name>
        <param_note> 制作一张空盘</param_note>
    </param>
    <param>
        <param_name>-g  </param_name>
        <param_note> 显示磁盘信息</param_note>
    </param>
    <param>
        <param_name> -i filename </param_name>
        <param_note> 从一个镜像文件建立一镜像盘</param_note>
    </param>
    <param>
        <param_name> -e filename [size] </param_name>
        <param_note> 给一镜像文件创建一镜像盘</param_note>
    </param>
    </parameters>

    <examples>
    <example>
        <eg_line>mkfloppy -s  </eg_line>
        <eg_note> 制作GRUB安装盘。</eg_note>
    </example>

    </examples>

</manpage>