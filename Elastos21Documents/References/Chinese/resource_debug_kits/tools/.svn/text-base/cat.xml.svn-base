<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/shellmanual.xsl"?>

<manpage>
    <title> cat </title>
    <volume>Elastos常用工具</volume>

    <description>
        <para>显示文件内容。</para>
    </description>

    <syntax>
        <cmd_name> CAT </cmd_name>
        <cmd_param> source1[source2...] </cmd_param>
    </syntax>
    <parameters>
    <param>
        <param_name>  source1 </param_name>
        <param_note> 源文件名</param_note>
    </param>
    <param>
        <param_name>  source2 </param_name>
        <param_note> 源文件名</param_note>
    </param>
    </parameters>

    <examples>
    <example>
        <eg_line>cat file.txt</eg_line>
        <eg_note> 显示文件file.txt的内容。</eg_note>
    </example>
    </examples>

</manpage>