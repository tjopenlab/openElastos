<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/shellmanual.xsl"?>

<manpage>
    <title> cp </title>
    <volume>Elastos常用工具</volume>

    <description>
        <para>拷贝文件。</para>
    </description>

    <syntax>
        <cmd_name> CP </cmd_name>
        <cmd_param> [--help] [-p] [-a] source1 [ source2 ...] target </cmd_param>
    </syntax>
    <parameters>
    <param>
        <param_name>  --help </param_name>
        <param_note> 显示帮助信息 </param_note>
    </param>
    <param>
        <param_name>  -p </param_name>
        <param_note> 在覆盖文件之前提示用户，由用户确认</param_note>
    </param>

    <param>
        <param_name>  -a </param_name>
        <param_note> 将源文件内容添加到目标文件</param_note>
    </param>

    <param>
        <param_name>  source1,source2 ... </param_name>
        <param_note> 原文件名</param_note>
    </param>
    <param>
        <param_name>  target </param_name>
        <param_note> 目标名</param_note>
    </param>
    </parameters>

    <examples>
    <example>
        <eg_line>cp file.txt  newfile.txt  </eg_line>
        <eg_note> 将当前目录下的文件file.txt拷贝到当前目录下，并重新命名为newfile.txt文件。</eg_note>
    </example>
    </examples>

    <references>
    <reference>
        <ref_name> mv</ref_name>
        <ref_url> mv</ref_url>
    </reference>
    </references>

</manpage>