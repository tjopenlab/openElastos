<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/shellmanual.xsl"?>

<manpage>
    <title> sleep </title>
    <volume>Elastos常用工具</volume>

    <description>
        <para> 使系统等待数秒。</para>
    </description>

    <syntax>
        <cmd_name> SLEEP </cmd_name>
        <cmd_param> [-?][time-to-sleep-in-seconds] </cmd_param>
    </syntax>
    <parameters>
        <param>
            <param_name>  -?  </param_name>
            <param_note> 显示帮助信息</param_note>
        </param>
        <param>
            <param_name> time-to-sleep-in-seconds </param_name>
            <param_note> 系统等待的相应秒数</param_note>
        </param>
    </parameters>
    <examples>
        <example>
            <eg_line>sleep 10  </eg_line>
            <eg_note> 系统等待10秒钟，在这段时间内系统不进行任何操作。</eg_note>
        </example>
    </examples>

</manpage>