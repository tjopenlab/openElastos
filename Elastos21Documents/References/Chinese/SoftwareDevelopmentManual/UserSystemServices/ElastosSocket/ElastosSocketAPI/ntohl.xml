<?xml version="1.0" encoding="GB2312"?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/apimanual.xsl"?>

<manpage>
    <sample url="elasock\UDP"></sample>
    <volume>elasock API</volume>
    <title>ntohl</title>
    <routine_idx>ElastosSocketIndex.htm</routine_idx>

    <description>
        <para>将无符号长整型数从网络字节顺序转换成主机字节顺序。</para>
    </description>

    <syntax>
        <return_val>u_long</return_val>
        <parameters>
            <api_param>
                <param_type>u_long</param_type>
                <param_name>netlong</param_name>
                <param_note>
                    <para>[in]网络字节顺序的32位无符号长整型数据。</para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

    <return>
        <para>成功时返回32位本地格式的字节顺序。</para>
    </return>

    <remarks>
        <para>函数将32位的数据从TCP/IP网络字节顺序转换成本地字节顺序。</para>
    </remarks>

    <requirements>
        <system>
            <system_name>
                Elastos 操作系统
            </system_name>
        </system>
        <headerfile>
            <file_name> winsock.h</file_name>
        </headerfile>
        <library>
            <lib_name> ElSocket.lib  </lib_name>
        </library>
    </requirements>

    <references>
        <reference>
            <ref_name>htonl</ref_name>
            <ref_url>htonl</ref_url>
        </reference>
        <reference>
            <ref_name>htons</ref_name>
            <ref_url>htons</ref_url>
        </reference>
        <reference>
            <ref_name>ntohs</ref_name>
            <ref_url>ntohs</ref_url>
        </reference>
    </references>
    <example>
    </example>
</manpage>
