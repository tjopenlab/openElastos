<?xml version="1.0" encoding="GB2312"?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/interfaceman.xsl"?>    <manpage>
    <volume></volume>
    <interface> ISocket </interface>
    <description>
        <para>
            网络接口。
        </para>
    </description>
    <methods>
<self>
    <method>
        <method_name>Accept</method_name>
        <method_url>..\SocketInterface\ISocket\Accept</method_url>
    <m_description>
            接受一次连接。
    </m_description>
    </method>
    <method>
        <method_name>Bind</method_name>
        <method_url>..\SocketInterface\ISocket\Bind</method_url>
    <m_description>
            将本地地址与一套接口捆绑。
    </m_description>
    </method>
    <method>
        <method_name>Connect</method_name>
        <method_url>..\SocketInterface\ISocket\Connect</method_url>
    <m_description>
            与网络另一端的套接口建立连接。
    </m_description>
    </method>
    <method>
        <method_name>GetPeerName</method_name>
        <method_url>..\SocketInterface\ISocket\GetPeerName</method_url>
    <m_description>
            获取对方的套节口地址（IP和PORT）。
    </m_description>
    </method>
    <method>
        <method_name>GetSockName</method_name>
        <method_url>..\SocketInterface\ISocket\GetSockName</method_url>
    <m_description>
            获取套接口的本地地址。
    </m_description>
    </method>
    <method>
        <method_name>Listen</method_name>
        <method_url>..\SocketInterface\ISocket\Listen</method_url>
    <m_description>
            将套接口设为监听状态，准备被连接。
    </m_description>
    </method>
    <method>
        <method_name>Recv</method_name>
        <method_url>..\SocketInterface\ISocket\Recv</method_url>
    <m_description>
            从套接口接收数据。
    </m_description>
    </method>
    <method>
        <method_name>RecvFrom</method_name>
        <method_url>..\SocketInterface\ISocket\RecvFrom</method_url>
    <m_description>
            接收一个数据报并保存其源地址。
    </m_description>
    </method>
    <method>
        <method_name>Send</method_name>
        <method_url>..\SocketInterface\ISocket\Send</method_url>
    <m_description>
            向已连接套接口发送数据。
    </m_description>
    </method>
    <method>
        <method_name>SendTo</method_name>
        <method_url>..\SocketInterface\ISocket\SendTo</method_url>
    <m_description>
            向指定目的地发送数据。
    </m_description>
    </method>
    <method>
        <method_name>Shutdown</method_name>
        <method_url>..\SocketInterface\ISocket\Shutdown</method_url>
    <m_description>
            禁止在一个套接口上进行数据的接收或发送，但并不释放套接口占用的系统资源。
    </m_description>
    </method>
</self>
    </methods>
    <requirements>
        <system>
            <system_name>
Elastos 操作系统            </system_name>
        </system>
    </requirements>
</manpage>