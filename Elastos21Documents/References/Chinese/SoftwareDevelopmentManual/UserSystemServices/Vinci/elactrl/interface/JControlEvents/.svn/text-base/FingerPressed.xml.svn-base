<?xml version="1.0" encoding="GB2312"?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume>图形系统</volume>
<interface>JControlEvents</interface><method> FingerPressed </method>
    <description>
        <para>
            手指按下时触发。
        </para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> Int32 </param_type>
                <param_name> fingers </param_name>
                <param_note>
                    <para>
同时移动的手指数</para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> Int32 </param_type>
                <param_name> index </param_name>
                <param_note>
                    <para>
按下手指的索引值，即手指与屏幕接触点的索引值</para>
                </param_note>
            </api_param>        
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> Int32 </param_type>
                <param_name> x </param_name>
                <param_note>
                    <para>
触点的横坐标</para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> Int32 </param_type>
                <param_name> y </param_name>
                <param_note>
                    <para>
触点的纵坐标</para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> Int64 </param_type>
                <param_name> timestamp </param_name>
                <param_note>
                    <para>
时间戳，时间戳</para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

<remarks>
    <para>
        更多回调事件信息，参见 Callback 机制介绍。
    </para>
</remarks>
<references>
    <reference>
        <ref_name>Control 回调事件列表</ref_name>
        <ref_url>../JControlEvents</ref_url>
    </reference>
</references>
</manpage>