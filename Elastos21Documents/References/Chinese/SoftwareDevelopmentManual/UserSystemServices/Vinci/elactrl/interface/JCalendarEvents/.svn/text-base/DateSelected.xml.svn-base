<?xml version="1.0" encoding="GB2312"?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume>图形系统</volume>
<interface>JCalendarEvents</interface><method> DateSelected </method>
    <description>
        <para>
            选择特定日期时触发的事件。
        </para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> Int32 </param_type>
                <param_name> year </param_name>
                <param_note>
                    <para>
被选择的日期的年份，取值在 1970～2037 之间                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> Int32 </param_type>
                <param_name> month </param_name>
                <param_note>
                    <para>
被选择的日期的月份                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> Int32 </param_type>
                <param_name> day </param_name>
                <param_note>
                    <para>
被选择日期的日                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

<remarks>
    <para>
        更多回调事件信息，参见 Callback 机制介绍。
    </para>
</remarks>
<references>
    <reference>
        <ref_name>CCalendar 类介绍</ref_name>
        <ref_url>../../class/Calendar</ref_url>
    </reference>
    <reference>
        <ref_name>Calendar 回调事件列表</ref_name>
        <ref_url>../JCalendarEvents</ref_url>
    </reference>
</references>  
</manpage>
