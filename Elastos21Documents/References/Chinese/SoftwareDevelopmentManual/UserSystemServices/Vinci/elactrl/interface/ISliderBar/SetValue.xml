<?xml version="1.0" encoding="GB2312"?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume>图形系统</volume>
<interface>ISliderBar</interface><method> SetValue </method>
    <description>
        <para>
            设置进度条控件滑块的当前值。
        </para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> Int32 </param_type>
                <param_name> value </param_name>
                <param_note>
                    <para>
进度条控件滑块的当前值，在最大值和最小值之间                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

<return>
    <para>
            如果方法调用成功，则返回NOERROR。    </para>
</return>
    <requirements>
        <system>
            <system_name>
Elastos操作系统             </system_name>
        </system>
        <headerfile>
            <file_name>ElControls.h</file_name>
        </headerfile>
        <library>
            <lib_name>elastos.lib</lib_name>
        </library>
    </requirements>
</manpage>