<?xml version="1.0" encoding="GB2312"?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume>图形系统</volume>
<interface>IFontConfig</interface><method> GetFontSize </method>
    <description>
        <para>
            获取皮肤字体大小。
        </para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> ControlID  </param_type>
                <param_name> controlID </param_name>
                <param_note>
                    <para>
 <link url="../../datatype/ControlID.htm">          控件ID             </link>                   </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> Int32*  </param_type>
                <param_name> fontSize </param_name>
                <param_note>
                    <para>
一个指向Int32型的指针，用来返回字体大小。                 </para>
                </param_note>
            </api_param>            
        </parameters>
    </syntax>

<return>
    <para>
            如果方法调用成功，则返回NOERROR。    </para>
</return>
    <requirements>
        <system>
            <system_name>
Elastos操作系统             </system_name>
        </system>
        <headerfile>
            <file_name>ElControls.h</file_name>
        </headerfile>
        <library>
            <lib_name>elastos.lib</lib_name>
        </library>
    </requirements>
</manpage>