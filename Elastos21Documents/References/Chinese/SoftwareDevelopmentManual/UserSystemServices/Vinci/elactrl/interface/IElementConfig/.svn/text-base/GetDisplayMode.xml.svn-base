<?xml version="1.0" encoding="GB2312"?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume>图形系统</volume>
<interface>IElementConfig</interface><method> GetDisplayMode </method>
    <description>
        <para>
            获取显示模式。
        </para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> ElementID  </param_type>
                <param_name> elementID </param_name>
                <param_note>
                    <para>
 <link url="../../datatype/ElementID.htm">           元素ID             </link>                   </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> DisplayMode*  </param_type>
                <param_name> displayMode </param_name>
                <param_note>
                    <para>
 <link url="../../datatype/DisplayMode.htm">           显示模式             </link>                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

<return>
    <para>
            如果方法调用成功，则返回NOERROR。    </para>
</return>
    <requirements>
        <system>
            <system_name>
Elastos操作系统              </system_name>
        </system>
        <headerfile>
            <file_name>ElControls.h</file_name>
        </headerfile>
        <library>
            <lib_name>elastos.lib</lib_name>
        </library>
    </requirements>
</manpage>