<?xml version="1.0" encoding="GB2312"?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume>图形系统</volume>
<method>ImagePanelPane</method>

    <syntax>
        <return_val> struct </return_val>
        <parameters>
            <api_param>
                <param_type> Int32  </param_type>
                <param_name> x </param_name>
                <param_note>
                    <para>
相对于父亲控件左上角的横坐标                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_type> Int32  </param_type>
                <param_name> y </param_name>
                <param_note>
                    <para>
相对于父亲控件左上角的纵坐标                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_type> Int32  </param_type>
                <param_name> width </param_name>
                <param_note>
                    <para>
图片宽度                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_type> Int32  </param_type>
                <param_name> height </param_name>
                <param_note>
                    <para>
图片高度                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_type> Int32  </param_type>
                <param_name> carveX </param_name>
                <param_note>
                    <para>
所给图片中的X坐标                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_type> Int32  </param_type>
                <param_name> carveY </param_name>
                <param_note>
                    <para>
所给图片中的Y坐标                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_type> Int32  </param_type>
                <param_name> index </param_name>
                <param_note>
                    <para>
窗格索引                    </para>
                </param_note>
            </api_param>                                                
            <api_param>
                <param_type> ImagePanelPaneStyle </param_type>
                <param_name> style </param_name>
                <param_note>
                    <para>
<link url="../../datatype/ImagePanelPaneStyle.htm">           ImagePanelPane风格             </link>                  </para>
                </param_note>
            </api_param>
            <api_param>
                <param_type> Boolean  </param_type>
                <param_name> checked </param_name>
                <param_note>
                    <para>
可选状态                   </para>
                </param_note>
            </api_param>
            <api_param>
                <param_type> Boolean  </param_type>
                <param_name> enabled </param_name>
                <param_note>
                    <para>
可用状态                   </para>
                </param_note>
            </api_param>
            <api_param>
                <param_type> Boolean  </param_type>
                <param_name> visible </param_name>
                <param_note>
                    <para>
可见状态                   </para>
                </param_note>
            </api_param>
            <api_param>
                <param_type> Int32  </param_type>
                <param_name> acceleratorKey </param_name>
                <param_note>
                    <para>
快捷键                    </para>
                </param_note>
            </api_param>                         
            <api_param>
                <param_type> WChar  </param_type>
                <param_name> text[MaxPaneTextLength] </param_name>
                <param_note>
                    <para>
文本信息                </para>
                </param_note>
            </api_param>                                                             
        </parameters>
    </syntax>

<remarks>
    <para>
                MaxPaneTextLength=256,即text数组的大小为256个(实际能放255，最后要置0)。
	</para>    
</remarks>
</manpage>