<?xml version="1.0" encoding="GB2312"?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/custom_type.xsl"?>    <manpage>
    <volume>图形系统</volume>
    <types> ListViewStyle </types>
    <description>
        <para>
            列表视图风格。
        </para>
    </description>
<enum>
    <enum_member>    <mem_name> ListViewStyle_Details </mem_name>    <mem_value> 0x00000000 </mem_value>    <mem_description>        详细资料    </mem_description>    </enum_member>
    <enum_member>    <mem_name> ListViewStyle_LargeIcon </mem_name>    <mem_value> 0x00000001 </mem_value>    <mem_description>        大图标    </mem_description>    </enum_member>
    <enum_member>    <mem_name> ListViewStyle_List </mem_name>    <mem_value> 0x00000002 </mem_value>    <mem_description>        列表    </mem_description>    </enum_member>
    <enum_member>    <mem_name> ListViewStyle_SmallIcon </mem_name>    <mem_value> 0x00000003 </mem_value>    <mem_description>        小图标    </mem_description>    </enum_member>
    <enum_member>    <mem_name> ListViewStyle_AutoWrap </mem_name>    <mem_value> 0x00000004 </mem_value>    <mem_description>        自动换行    </mem_description>    </enum_member>
    <enum_member>    <mem_name> ListViewStyle_MultiLine </mem_name>    <mem_value> 0x00000005 </mem_value>    <mem_description>        多行显示    </mem_description>    </enum_member>    
    <enum_member>    <mem_name> ListViewStyle_Grid </mem_name>    <mem_value> 0x00000006 </mem_value>    <mem_description>        九宫图    </mem_description>    </enum_member>
    <enum_member>    <mem_name> ListViewStyle_Count </mem_name>    <mem_value> 0x00000007 </mem_value>    <mem_description>        列表视图风格个数    </mem_description>    </enum_member>
    <enum_member>    <mem_name> ListViewStyle_NoSelection </mem_name>    <mem_value> 0x00000010 </mem_value>    <mem_description>        没有选择    </mem_description>    </enum_member>
    <enum_member>    <mem_name> ListViewStyle_Sort </mem_name>    <mem_value> 0x00000100 </mem_value>    <mem_description>        自动排序    </mem_description>    </enum_member>
    <enum_member>    <mem_name> ListViewStyle_HideSelection </mem_name>    <mem_value> 0x00000020 </mem_value>    <mem_description>        隐藏选项    </mem_description>    </enum_member>
    <enum_member>    <mem_name> ListViewStyle_GridLines  </mem_name>    <mem_value> 0x00000200 </mem_value>    <mem_description>        分隔线    </mem_description>    </enum_member>
    <enum_member>    <mem_name> ListViewStyle_Loop </mem_name>    <mem_value> 0x00000080 </mem_value>    <mem_description>        上下键循环，不切焦点    </mem_description>    </enum_member>
    <enum_member>    <mem_name> ListViewStyle_AcceleratorKey </mem_name>    <mem_value> 0x00000040 </mem_value>    <mem_description>        快捷键    </mem_description>    </enum_member>
    <enum_member>    <mem_name> ListViewStyle_AutoSize </mem_name>    <mem_value> 0x00000400 </mem_value>    <mem_description>        自动调整大小    </mem_description>    </enum_member>
</enum>
    <remarks>
        <para>
            如果需要ListView的分隔线，则还需要加上风格ListViewStyle_GridLines。
        </para>
    </remarks>
</manpage>