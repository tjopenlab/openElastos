<?xml version="1.0" encoding="GB2312"?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/custom_type.xsl"?>    <manpage>
    <volume>图形系统</volume>
    <types> TransitionEffect </types>
    <description>
        <para>
            动画效果。
        </para>
    </description>
<enum>    <enum_member>    <mem_name> TransitionEffect_None </mem_name>    <mem_value> 0x00000000 </mem_value>    <mem_description>  无动画       </mem_description>    </enum_member>    
          <enum_member>    <mem_name> TransitionEffect_FadeIn </mem_name>    <mem_value> 0x00000001 </mem_value>    <mem_description> 淡入效果        </mem_description>    </enum_member>    
          <enum_member>    <mem_name> TransitionEffect_FadeOut </mem_name>    <mem_value> 0x00000100 </mem_value>    <mem_description>   淡出效果      </mem_description>    </enum_member>    
          <enum_member>    <mem_name> TransitionEffect_ZoomIn </mem_name>    <mem_value> 0x00000002 </mem_value>    <mem_description>  渐变式放大       </mem_description>    </enum_member>    
          <enum_member>    <mem_name> TransitionEffect_ZoomOut </mem_name>    <mem_value> 0x00000200 </mem_value>    <mem_description>   渐变式缩小      </mem_description>    </enum_member>    
          <enum_member>    <mem_name> TransitionEffect_FlyIn </mem_name>    <mem_value> 0x00000004 </mem_value>    <mem_description>   飞进来      </mem_description>    </enum_member>    
          <enum_member>    <mem_name> TransitionEffect_FlyOut </mem_name>    <mem_value> 0x00000400 </mem_value>    <mem_description>  飞出去       </mem_description>    </enum_member>
          <enum_member>    <mem_name> TransitionEffect_PushIn </mem_name>    <mem_value> 0x00000008 </mem_value>    <mem_description>   推进来（挤走原来的）      </mem_description>    </enum_member>
          <enum_member>    <mem_name> TransitionEffect_PushOut </mem_name>    <mem_value> 0x00000800 </mem_value>    <mem_description>   推出去（被新来的挤走）      </mem_description>    </enum_member>
          <enum_member>    <mem_name> TransitionEffect_DampingIn </mem_name>    <mem_value> 0x00000010 </mem_value>    <mem_description>  阻尼式进入       </mem_description>    </enum_member>
          <enum_member>    <mem_name> TransitionEffect_DampingOut </mem_name>    <mem_value> 0x00001000 </mem_value>    <mem_description>  阻尼式离开       </mem_description>    </enum_member>
          <enum_member>    <mem_name> TransitionEffect_Random </mem_name>    <mem_value> 0xFFFFFFFF </mem_value>    <mem_description>   随机动画效果      </mem_description>    </enum_member>
</enum></manpage>