<?xml version="1.0" encoding="GB2312"?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/custom_type.xsl"?>    <manpage>
    <volume>图形系统</volume>
    <types> DateTimeFormat </types>
    <description>
        <para>
            时间格式。
        </para>
    </description>
<enum>    <enum_member>    <mem_name> DateTimeFormat_24Hour </mem_name>    <mem_value> 0x00000001 </mem_value>    <mem_description>     24小时制，默认    </mem_description>    </enum_member> 
          <enum_member>    <mem_name> DateTimeFormat_12Hour </mem_name>    <mem_value> 0x00000002 </mem_value>    <mem_description>    12小时制    </mem_description>    </enum_member>  
          <enum_member>    <mem_name> DateTimeFormat_HourMinuteSecond </mem_name>    <mem_value> 0x00000004 </mem_value>    <mem_description>    小时分钟秒    </mem_description>    </enum_member> 
          <enum_member>    <mem_name> DateTimeFormat_HourMinute </mem_name>    <mem_value> 0x00000008 </mem_value>    <mem_description>    小时分钟    </mem_description>    </enum_member>   
          <enum_member>    <mem_name> DateTimeFormat_YearMonthDay </mem_name>    <mem_value> 0x00000010 </mem_value>    <mem_description>    年月日    </mem_description>    </enum_member>   
          <enum_member>    <mem_name> DateTimeFormat_DayMonthYear </mem_name>    <mem_value> 0x00000020 </mem_value>    <mem_description>    日月年    </mem_description>    </enum_member>
          <enum_member>    <mem_name> DateTimeFormat_UserDefinedRange </mem_name>    <mem_value> 0x00000040 </mem_value>    <mem_description>    用户定义范围    </mem_description>    </enum_member>
          </enum>
<remarks>
			<para>
						当用户使用倒计时等非日常时间时，会用到DateTimeFormat_UserDefinedRange风格。
			</para>
</remarks>          
</manpage>