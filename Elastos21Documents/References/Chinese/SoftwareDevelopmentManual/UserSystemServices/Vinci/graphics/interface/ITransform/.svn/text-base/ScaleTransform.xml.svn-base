<?xml version="1.0" encoding="GB2312"?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume></volume>
<interface>ITransform</interface><method> ScaleTransform </method>
    <description>
        <para>
            缩放变换，将每一点的横坐标放大（缩小）sx倍；纵坐标放大（缩小）sy倍。
        </para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> Double </param_type>
                <param_name> sx </param_name>
                <param_note>
                    <para>
横坐标放大（缩小）倍数                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> Double </param_type>
                <param_name> sy </param_name>
                <param_note>
                    <para>
纵坐标放大（缩小）倍数                    </para>
                </param_note>
            </api_param>            
        </parameters>
    </syntax>

<return>
    <para>
            如果方法调用成功，则返回NOERROR。    </para>
</return>
    <requirements>
        <system>
            <system_name>
Elastos操作系统            </system_name>
        </system>
         <headerfile>
            <file_name>ElGrafix.h</file_name>
        </headerfile>
        <library>
            <lib_name>elastos.lib</lib_name>
        </library>
    </requirements>
</manpage>