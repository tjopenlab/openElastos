<?xml version="1.0" encoding="GB2312"?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/interfaceman.xsl"?>    <manpage>
    <volume></volume>
    <interface> IGraphics </interface>
    <description>
        <para>
            绘图接口。
        </para>
    </description>
    <methods>
<self>
    <method>
        <method_name>GetBufferInformation</method_name>
        <method_url>..\interface\IGraphics\GetBufferInformation</method_url>
    <m_description>
            获取缓冲区信息。
    </m_description>
    </method>
    <method>
        <method_name>ResetClip</method_name>
        <method_url>..\interface\IGraphics\ResetClip</method_url>
    <m_description>
            将此 Graphics 对象的剪辑区域重置为无限区域。
    </m_description>
    </method>    
    <method>
        <method_name>GetClip</method_name>
        <method_url>..\interface\IGraphics\GetClip</method_url>
    <m_description>
            获取Graphics 对象的剪裁矩形。
    </m_description>
    </method>    
    <method>
        <method_name>SetClip</method_name>
        <method_url>..\interface\IGraphics\SetClip</method_url>
    <m_description>
            设置Graphics对象的绘图区域。
    </m_description>
    </method>    
    <method>
        <method_name>SetLineStyle</method_name>
        <method_url>..\interface\IGraphics\SetLineStyle</method_url>
    <m_description>
            设置线风格。
    </m_description>
    </method>    
    <method>
        <method_name>GetLineStyle</method_name>
        <method_url>..\interface\IGraphics\GetLineStyle</method_url>
    <m_description>
            获取线风格。
    </m_description>
    </method>     
    <method>
        <method_name>SetLineCap</method_name>
        <method_url>..\interface\IGraphics\SetLineCap</method_url>
    <m_description>
            设置线头风格。
    </m_description>
    </method>
    <method>
        <method_name>GetLineCap</method_name>
        <method_url>..\interface\IGraphics\GetLineCap</method_url>
    <m_description>
            获取线头风格。
    </m_description>
    </method>
    <method>
        <method_name>SetLineJoin</method_name>
        <method_url>..\interface\IGraphics\SetLineJoin</method_url>
    <m_description>
            设置线连接点。
    </m_description>
    </method>        
    <method>
        <method_name>GetLineJoin</method_name>
        <method_url>..\interface\IGraphics\GetLineJoin</method_url>
    <m_description>
            获取线连接点。
    </m_description>
    </method>    
    <method>
        <method_name>SetLineWidth</method_name>
        <method_url>..\interface\IGraphics\SetLineWidth</method_url>
    <m_description>
            设置线宽度。
    </m_description>
    </method>     
    <method>
        <method_name>GetLineWidth</method_name>
        <method_url>..\interface\IGraphics\GetLineWidth</method_url>
    <m_description>
            获取线宽度。
    </m_description>
    </method>
    <method>
        <method_name>SetLineDash</method_name>
        <method_url>..\interface\IGraphics\SetLineDash</method_url>
    <m_description>
            设置用户自定义划线。
    </m_description>
    </method>        
    <method>
        <method_name>GetLineDash</method_name>
        <method_url>..\interface\IGraphics\GetLineDash</method_url>
    <m_description>
            获取用户自定义划线。
    </m_description>
    </method>     
    <method>
        <method_name>SetPattern</method_name>
        <method_url>..\interface\IGraphics\SetPattern</method_url>
    <m_description>
            设置绘图模式。
    </m_description>
    </method>    
    <method>
        <method_name>GetPattern</method_name>
        <method_url>..\interface\IGraphics\GetPattern</method_url>
    <m_description>
            获取绘图模式。
    </m_description>
    </method>    
    <method>
        <method_name>UpdateRectangle</method_name>
        <method_url>..\interface\IGraphics\UpdateRectangle</method_url>
    <m_description>
            将绘制在内存中图像绘制到屏幕上（图像在指定矩形内）。
    </m_description>
    </method>     
    <method>
        <method_name>Update</method_name>
        <method_url>..\interface\IGraphics\Update</method_url>
    <m_description>
            将绘制在内存中的全部图像绘制到屏幕上。
    </m_description>
    </method>    
    <method>
        <method_name>Blit</method_name>
        <method_url>..\interface\IGraphics\Blit</method_url>
    <m_description>
            将源Graphics的数据拷贝至当前Graphics中。
    </m_description>
    </method>    
    <method>
        <method_name>BltStretched</method_name>
        <method_url>..\interface\IGraphics\BltStretched</method_url>
    <m_description>
            将源Graphics的数据拷贝并拉伸至当前Graphics中。
    </m_description>
    </method>     
    <method>
        <method_name>Clear</method_name>
        <method_url>..\interface\IGraphics\Clear</method_url>
    <m_description>
            清除整个绘图面并以指定背景色填充。
    </m_description>
    </method>
    <method>
        <method_name>GetPixel</method_name>
        <method_url>..\interface\IGraphics\GetPixel</method_url>
    <m_description>
            在指定的坐标获取一个颜色。
    </m_description>
    </method>    
    <method>
        <method_name>SetPixel</method_name>
        <method_url>..\interface\IGraphics\SetPixel</method_url>
    <m_description>
            在指定的坐标处设置颜色。
    </m_description>
    </method>    
    <method>
        <method_name>DrawLine</method_name>
        <method_url>..\interface\IGraphics\DrawLine</method_url>
    <m_description>
            绘制由两个坐标指定的一条直线。
    </m_description>
    </method>
    <method>
        <method_name>DrawPolygon</method_name>
        <method_url>..\interface\IGraphics\DrawPolygon</method_url>
    <m_description>
            绘制由一组 EzPoint 结构定义的多边形。
    </m_description>
    </method>
    <method>
        <method_name>DrawRectangle</method_name>
        <method_url>..\interface\IGraphics\DrawRectangle</method_url>
    <m_description>
            绘制由坐标对、宽度和高度指定的矩形。
    </m_description>
    </method>              
    <method>
        <method_name>DrawArc</method_name>
        <method_url>..\interface\IGraphics\DrawArc</method_url>
    <m_description>
            绘制拱形。
    </m_description>
    </method>    
    <method>
        <method_name>DrawEllipse</method_name>
        <method_url>..\interface\IGraphics\DrawEllipse</method_url>
    <m_description>
            绘制一个由边框（该边框由一对坐标、高度和宽度指定）定义的椭圆。
    </m_description>
    </method>    
    <method>
        <method_name>DrawBezier</method_name>
        <method_url>..\interface\IGraphics\DrawBezier</method_url>
    <m_description>
            绘制贝塞尔曲线。
    </m_description>
    </method>     
    <method>
        <method_name>FillPolygon</method_name>
        <method_url>..\interface\IGraphics\FillPolygon</method_url>
    <m_description>
            填充多边形的内部（边框由EzPoint结构指定的点数组所定义）。
    </m_description>
    </method>    
    <method>
        <method_name>FillRectangle</method_name>
        <method_url>..\interface\IGraphics\FillRectangle</method_url>
    <m_description>
            填充矩形的内部（边框由一对坐标、一个宽度和一个高度定义）。
    </m_description>
    </method>    
    <method>
        <method_name>FillArc</method_name>
        <method_url>..\interface\IGraphics\FillArc</method_url>
    <m_description>
            填充拱形。
    </m_description>
    </method>    
    <method>
        <method_name>FillEllipse</method_name>
        <method_url>..\interface\IGraphics\FillEllipse</method_url>
    <m_description>
            填充边框所定义的椭圆的内部（边框由一对坐标、一个宽度和一个高度定义）。
    </m_description>
    </method>    
    <method>
        <method_name>DrawImage</method_name>
        <method_url>..\interface\IGraphics\DrawImage</method_url>
    <m_description>
            在指定位置并且按原始大小绘制指定的图像。
    </m_description>
    </method>    
    <method>
        <method_name>DrawImageInRectangle</method_name>
        <method_url>..\interface\IGraphics\DrawImageInRectangle</method_url>
    <m_description>
            在指定位置并且按原始大小绘制指定的 Image 接口表示的图像。
    </m_description>
    </method>    
    <method>
        <method_name>DrawStretchedImage</method_name>
        <method_url>..\interface\IGraphics\DrawStretchedImage</method_url>
    <m_description>
            绘制拉伸后的图像。
    </m_description>
    </method>    
    <method>
        <method_name>DrawString</method_name>
        <method_url>..\interface\IGraphics\DrawString</method_url>
    <m_description>
            在指定位置绘制文本字符串。
    </m_description>
    </method>
</self>
    </methods>
    <requirements>
        <system>
            <system_name>
Elastos操作系统            </system_name>
        </system>
                <headerfile>
            <file_name>ElGrafix.h</file_name>
        </headerfile>
        <library>
            <lib_name>elastos.lib</lib_name>
        </library>
    </requirements>
</manpage>