<?xml version="1.0" encoding="GB2312"?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume></volume>
<interface>ICanvas</interface><method> DrawEdge </method>
    <description>
        <para>
            绘制控件3D外观效果的轮廓线。
        </para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> int </param_type>
                <param_name> left </param_name>
                <param_note>
                    <para>
button的左上角x坐标                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> int </param_type>
                <param_name> top </param_name>
                <param_note>
                    <para>
button的左上角y坐标                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> int </param_type>
                <param_name> right </param_name>
                <param_note>
                    <para>
button的右下角x坐标                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> int </param_type>
                <param_name> bottom </param_name>
                <param_note>
                    <para>
button的右下角y坐标                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> UInt32 </param_type>
                <param_name> edge </param_name>
                <param_note>
                    <para>
说明绘制的是内线还是外线:BDR_OUTER, BDR_INNER                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> UInt32 </param_type>
                <param_name> grfFlags </param_name>
                <param_note>
                    <para>
暂时需要具体写程序查看一下                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

<return>
    <para>
            如果方法调用成功，则返回NOERROR。    </para>
</return>
    <requirements>
        <system>
            <system_name>
Elastos操作系统            </system_name>
        </system>
        <headerfile>
            <file_name>ElGrafix.h</file_name>
        </headerfile>
        <library>
            <lib_name>elastos.lib</lib_name>
        </library>
    </requirements>
</manpage>