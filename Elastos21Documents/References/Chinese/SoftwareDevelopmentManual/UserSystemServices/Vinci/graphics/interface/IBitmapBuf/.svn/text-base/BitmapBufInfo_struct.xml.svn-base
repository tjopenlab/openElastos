<?xml version="1.0" encoding="GB2312"?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume>绘图系统 </volume>
<method>BitmapBufInfo </method>
<description>
	      <para>
	      	  位图信息 
	      </para>
    </description>
    <syntax>
        <return_val> struct </return_val>
        <parameters>
            <api_param>
                <param_type> PixelFormat  </param_type>
                <param_name> pixelFormat </param_name>
                <param_note><para>
像素格式， <link url="../../datatype/PixelFormat.htm"> pixelFormat </link>枚举类型                  
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_type> Int32  </param_type>
                <param_name> width </param_name>
                <param_note>
                    <para>
位图宽度                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_type> Int32  </param_type>
                <param_name> height </param_name>
                <param_note>
                    <para>
位图高度                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_type> Int32  </param_type>
                <param_name> pitch </param_name>
                <param_note>
                    <para>
位图像素值                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_type> Int32  </param_type>
                <param_name> bitsPerPixel </param_name>
                <param_note>
                    <para>
每个像素的位数                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_type> Address  </param_type>
                <param_name> address </param_name>
                <param_note>
                    <para>
buffer的地址                    </para>
                </param_note>
            </api_param>                                                            
        </parameters>
    </syntax>
</manpage>   
