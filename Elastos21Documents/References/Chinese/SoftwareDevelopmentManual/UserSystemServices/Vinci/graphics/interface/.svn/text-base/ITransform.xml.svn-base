    <interface> ITransform </interface>
    <description>
        <para>
            矩阵变换接口。
        </para>
    </description>
    <methods>
<self>
    <method>
        <method_name>SetTransform</method_name>
        <method_url>..\interface\ITransform\SetTransform</method_url>
    <m_description>
            设置变换矩阵。
    </m_description>
    </method>
    <method>
        <method_name>GetTransform</method_name>
        <method_url>..\interface\ITransform\GetTransform</method_url>
    <m_description>
            获取变换矩阵信息。
    </m_description>
    </method>
    <method>
        <method_name>ResetTransform</method_name>
        <method_url>..\interface\ITransform\ResetTransform</method_url>
    <m_description>
            复位变换矩阵。
    </m_description>
    </method>
    <method>
        <method_name>TranslateTransform</method_name>
        <method_url>..\interface\ITransform\TranslateTransform</method_url>
    <m_description>
            平移变换。
    </m_description>
    </method>    
    <method>
        <method_name>ScaleTransform</method_name>
        <method_url>..\interface\ITransform\ScaleTransform</method_url>
    <m_description>
            缩放变换。
    </m_description>
    </method>    
    <method>
        <method_name>RotateTransform</method_name>
        <method_url>..\interface\ITransform\RotateTransform</method_url>
    <m_description>
            旋转变换。
    </m_description>
    </method>    
    <method>
        <method_name>MultiplyTransform</method_name>
        <method_url>..\interface\ITransform\MultiplyTransform</method_url>
    <m_description>
            在现变换矩阵基础上再进行次矩阵变换。
    </m_description>
    </method>     
</self>
