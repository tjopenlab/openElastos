<?xml version="1.0" encoding="GB2312"?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume></volume>
<interface>ICanvas</interface><method> FillRoundRect </method>
    <description>
        <para>
            绘制填充圆角矩形。
        </para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> int </param_type>
                <param_name> left </param_name>
                <param_note>
                    <para>
button的左上角x坐标                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> int </param_type>
                <param_name> top </param_name>
                <param_note>
                    <para>
button的左上角y坐标                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> int </param_type>
                <param_name> width </param_name>
                <param_note>
                    <para>
button的宽度                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> int </param_type>
                <param_name> height </param_name>
                <param_note>
                    <para>
button的高度                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> COLORREF </param_type>
                <param_name> lineColor </param_name>
                <param_note>
                    <para>
矩形边框颜色                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> COLORREF </param_type>
                <param_name> faceColor </param_name>
                <param_note>
                    <para>
矩形表面颜色                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

<return>
    <para>
            如果方法调用成功，返回NOERROR。    </para>
</return>
    <requirements>
        <system>
            <system_name>
Elastos操作系统            </system_name>
        </system>
        <headerfile>
            <file_name>ElGrafix.h</file_name>
        </headerfile>
        <library>
            <lib_name>elastos.lib</lib_name>
        </library>
    </requirements>
    <remarks>
        <para>
            矩形边框和表面颜色的取值指定为            <link url="../../datatype/Color.htm">            Color            </link>            数据类型。 </para><para>         本方法暂未实现。
        </para>
    </remarks>
</manpage>