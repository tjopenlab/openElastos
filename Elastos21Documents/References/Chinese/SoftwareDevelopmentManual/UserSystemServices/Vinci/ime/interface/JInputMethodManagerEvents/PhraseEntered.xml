    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume></volume>
<interface>JInputMethodManagerEvents</interface><method> PhraseEntered </method>
    <description>
        <para>
            键入字符串事件。
        </para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> WString </param_type>
                <param_name> phrase </param_name>
                <param_note>
                    <para>
现有字符串。                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> WString </param_type>
                <param_name> candidatePhrase </param_name>
                <param_note>
                    <para>
候选字符串，准备代替现有字符串。                    </para>
                </param_note>
            </api_param>            
       </parameters>
    </syntax>

<return>
    <para>
            如果方法调用成功，则返回NOERROR。    </para>
</return>
    <requirements>
        <system>
            <system_name>
Elastos操作系统             </system_name>
        </system>
        <headerfile>
            <file_name>ElControls.h</file_name>
        </headerfile>
        <library>
            <lib_name>elastos.lib </lib_name>
        </library>
    </requirements>
</manpage>
