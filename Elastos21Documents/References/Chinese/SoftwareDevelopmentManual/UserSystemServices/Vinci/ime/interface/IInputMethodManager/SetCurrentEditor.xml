<interface>IInputMethodManager</interface><method> SetCurrentEditor </method>
    <description>
        <para>
            设置当前的IME。
        </para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> WString  </param_type>
                <param_name> editorName </param_name>
                <param_note>
                    <para>
输入法名称                   </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> WString  </param_type>
                <param_name> methodName </param_name>
                <param_note>
                    <para>
输入法类型                   </para>
                </param_note>
            </api_param>            
       </parameters>
    </syntax>

<return>
    <para>
            如果方法调用成功，则返回NOERROR。    </para>
</return>
    <requirements>
        <system>
            <system_name>
Elastos操作系统            </system_name>
        </system>
        <headerfile>
            <file_name>ElControls.h</file_name>
        </headerfile>
        <library>
            <lib_name>elastos.lib </lib_name>
        </library>
    </requirements>
</manpage>