<interface>IInputMethodEditor</interface><method> GetCurrentInputMethod </method>
    <description>
        <para>
            获取当前输入法类型。
        </para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> WStringBuf_&lt;MAX_INPUT_METHOD_STRING_LENGTH&gt; </param_type>
                <param_name> methodName </param_name>
                <param_note>
                    <para>
当前输入法类型名。                    </para>
                </param_note>
            </api_param>
       </parameters>
    </syntax>

<return>
    <para>
            如果方法调用成功，则返回NOERROR。    </para>
</return>
    <requirements>
        <system>
            <system_name>
Elastos操作系统              </system_name>
        </system>
        <headerfile>
            <file_name>ElControls.h</file_name>
        </headerfile>
        <library>
            <lib_name>elastos.lib </lib_name>
        </library>
    </requirements>
</manpage>