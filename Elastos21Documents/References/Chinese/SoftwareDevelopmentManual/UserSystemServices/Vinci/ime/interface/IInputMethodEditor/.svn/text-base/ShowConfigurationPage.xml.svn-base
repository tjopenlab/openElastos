<?xml version="1.0" encoding="GB2312"?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume></volume>
<interface>IInputMethodEditor</interface><method> ShowConfigurationPage </method>
    <description>
        <para>
            显示输入法配置页面。输入法提供此页面，用于改变输入法的一些配置内容，如手写时笔的粗细等。
        </para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <NoArguments/>
    </syntax>

<return>
      <para>    有配置页面，返回NOERROR；无配置页面，返回E_FAIL。 </para>
</return>
    <requirements>
        <system>
            <system_name>
Elastos操作系统             </system_name>
        </system>
        <headerfile>
            <file_name>ElControls.h</file_name>
        </headerfile>
        <library>
            <lib_name>elastos.lib </lib_name>
        </library>
    </requirements>
</manpage>