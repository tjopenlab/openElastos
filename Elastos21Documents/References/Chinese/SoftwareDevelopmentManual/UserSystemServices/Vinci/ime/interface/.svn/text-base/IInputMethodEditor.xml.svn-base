<?xml version="1.0" encoding="GB2312"?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/interfaceman.xsl"?>    <manpage>
    <volume></volume>
    <interface> IInputMethodEditor </interface>
    <description>
        <para>
            输入法编辑器。
        </para>
    </description>
    <methods>
<self>
    <method>
        <method_name>NotifyKey</method_name>
        <method_url>..\interface\IInputMethodEditor\NotifyKey</method_url>
    <m_description>
            由调用者通知输入法有一按键事件产生。
    </m_description>
    </method>
    <method>
        <method_name>Reset</method_name>
        <method_url>..\interface\IInputMethodEditor\Reset</method_url>
    <m_description>
            重置输入法。
    </m_description>
    </method>
    <method>
        <method_name>IsVisible</method_name>
        <method_url>..\interface\IInputMethodEditor\IsVisible</method_url>
    <m_description>
            获取输入法可见状态。
    </m_description>
    </method>
    <method>
        <method_name>SetVisible</method_name>
        <method_url>..\interface\IInputMethodEditor\SetVisible</method_url>
    <m_description>
            设置输入法可见状态。
    </m_description>
    </method>
    <method>
        <method_name>NotifyCaretPosition</method_name>
        <method_url>..\interface\IInputMethodEditor\NotifyCaretPosition</method_url>
    <m_description>
            通知输入法光标的位置，输入法面板可以根据这个位置调整其坐标。
    </m_description>
    </method>
    <method>
        <method_name>NotifyCharBeforeCaret</method_name>
        <method_url>..\interface\IInputMethodEditor\NotifyCharBeforeCaret</method_url>
    <m_description>
            通知输入法光标前一字，输入法可以根据此值实现联想等功能。
    </m_description>
    </method>
    <method>
        <method_name>GetCurrentInputMethod</method_name>
        <method_url>..\interface\IInputMethodEditor\GetCurrentInputMethod</method_url>
    <m_description>
            获取当前输入法类型。
    </m_description>
    </method>
    <method>
        <method_name>SetCurrentInputMethod</method_name>
        <method_url>..\interface\IInputMethodEditor\SetCurrentInputMethod</method_url>
    <m_description>
            设置当前输入法类型。
    </m_description>
    </method>
    <method>
        <method_name>GetPosition</method_name>
        <method_url>..\interface\IInputMethodEditor\GetPosition</method_url>
    <m_description>
            获取输入法面板的起始位置。
    </m_description>
    </method>
    <method>
        <method_name>SetPosition</method_name>
        <method_url>..\interface\IInputMethodEditor\SetPosition</method_url>
    <m_description>
            设置输入法面板的位置。
    </m_description>
    </method>
    <method>
        <method_name>GetClientSize</method_name>
        <method_url>..\interface\IInputMethodEditor\GetClientSize</method_url>
    <m_description>
            获取输入法面板的大小。
    </m_description>
    </method>
    <method>
        <method_name>ShowConfigurationPage</method_name>
        <method_url>..\interface\IInputMethodEditor\ShowConfigurationPage</method_url>
    <m_description>
            显示输入法配置页面。输入法提供此页面，用于改变输入法的一些配置内容，如手写时笔的粗细等。
    </m_description>
    </method>
    <method>
        <method_name>GetEditorName</method_name>
        <method_url>..\interface\IInputMethodEditor\GetEditorName</method_url>
    <m_description>
            获取输入法名称。
    </m_description>
    </method>  
    <method>
        <method_name>GetAllInputMethods</method_name>
        <method_url>..\interface\IInputMethodEditor\GetAllInputMethods</method_url>
    <m_description>
            获取输入法支持的类型。
    </m_description>
    </method>  
    <method>
        <method_name>GetLanguageName</method_name>
        <method_url>..\interface\IInputMethodEditor\GetLanguageName</method_url>
    <m_description>
            获取输入法类型所对应的语言。
    </m_description>
    </method>                   
</self>
    </methods>
    <requirements>
        <system>
            <system_name>
Elastos操作系统            </system_name>
        </system>
        <headerfile>
            <file_name>ElControls.h</file_name>
        </headerfile>
        <library>
            <lib_name>elastos.lib </lib_name>
        </library>
    </requirements>
</manpage>