<?xml version="1.0" encoding="GB2312"?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume></volume>
<interface>IInputMethodManager</interface><method> GetAllEditors </method>
    <description>
        <para>
             获取当前IMM中的所有输入法名。
        </para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> BufferOf_&lt;InputMethodString&gt; </param_type>
                <param_name> editorName </param_name>
                <param_note>
                    <para>
输入法名称。                    </para>
                </param_note>
            </api_param>
       </parameters>
    </syntax>

<return>
    <para>
            如果方法调用成功，则返回NOERROR。    </para>
</return>
    <requirements>
        <system>
            <system_name>
Elastos操作系统             </system_name>
        </system>
        <headerfile>
            <file_name>ElControls.h</file_name>
        </headerfile>
        <library>
            <lib_name>elastos.lib </lib_name>
        </library>
    </requirements>
<remarks>
      <para>  
            InputMethodString的定义是WStringBuf_&lt;MAX_INPUT_METHOD_STRING_LENGTH&gt;。
      </para>
</remarks>    
</manpage>