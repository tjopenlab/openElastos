<a name="l00114"></a>00114     <span class="keywordflow">switch</span> (nEvent) {
<a name="l00115"></a>00115         <span class="keywordflow">case</span> 0:
<a name="l00116"></a>00116             printf(<span class="stringliteral">"======= %d   %d =========\n"</span>, nEvent, nParam);
