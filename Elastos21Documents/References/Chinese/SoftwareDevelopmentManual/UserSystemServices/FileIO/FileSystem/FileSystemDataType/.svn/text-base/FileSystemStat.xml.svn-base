<?xml version="1.0" encoding="GB2312"?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/custom_type.xsl"?>

<manpage>
    <volume>文件系统数据类型</volume>
    <types>FileSystemStat</types>
    <routine_idx> FileSystemDataType.htm </routine_idx>

    <description>
        <para>与文件系统相关的统计信息。</para>
    </description>

    <struct>
        <struct_member>
            <mem_type>FatType</mem_type>
            <mem_name>fatType</mem_name>
            <mem_description>磁盘分区的FAT格式类型，   参见数据结构<link url="FatType.htm">FatType</link> </mem_description>
        </struct_member>
        <struct_member>
            <mem_type>Int32</mem_type>
            <mem_name>clusterSize</mem_name>
            <mem_description>磁盘分区中簇的大小（单位是字节）</mem_description>
        </struct_member>
        <struct_member>
            <mem_type>Int32</mem_type>
            <mem_name>totalClusters</mem_name>
            <mem_description>磁盘分区中总簇数</mem_description>
        </struct_member>
        <struct_member>
            <mem_type>Int32</mem_type>
            <mem_name>freeClusters</mem_name>
            <mem_description>磁盘分区中空闲簇数</mem_description>
        </struct_member>
    </struct>
</manpage>
