    <volume>文件系统接口</volume>
    <interface> IBasicDirectory </interface>
    <description>
        <para>
            目录对象接口。
        </para>
    </description>
    <methods>
<parent>
<interface_name>IBasicFile</interface_name><interface_url>IBasicFile</interface_url>    <method>
        <method_name>Sync</method_name>
        <method_url>IBasicFile\Sync</method_url>
    <m_description>
            把当前操作对象的所有数据写入永久存储器。
    </m_description>
    </method>
    <method>
        <method_name>GetType</method_name>
        <method_url>IBasicFile\GetType</method_url>
    <m_description>
            获取操作对象的文件类型。
    </m_description>
    </method>
    <method>
        <method_name>GetAccessTime</method_name>
        <method_url>IBasicFile\GetAccessTime</method_url>
    <m_description>
            获取操作对象的最后一次访问时间。
    </m_description>
    </method>
    <method>
        <method_name>SetAccessTime</method_name>
        <method_url>IBasicFile\SetAccessTime</method_url>
    <m_description>
            设置操作对象的最后一次访问时间。
    </m_description>
    </method>
    <method>
        <method_name>GetModificationTime</method_name>
        <method_url>IBasicFile\GetModificationTime</method_url>
    <m_description>
            获取操作对象的最后一次修改时间。
    </m_description>
    </method>
    <method>
        <method_name>SetModificationTime</method_name>
        <method_url>IBasicFile\SetModificationTime</method_url>
    <m_description>
            设置操作对象的最后一次修改时间。
    </m_description>
    </method>
    <method>
        <method_name>GetCreateTime</method_name>
        <method_url>IBasicFile\GetCreateTime</method_url>
    <m_description>
            获取操作对象的创建时间。
    </m_description>
    </method>
    <method>
        <method_name>CheckAccessMode</method_name>
        <method_url>IBasicFile\CheckAccessMode</method_url>
    <m_description>
            检验操作对象是否具有指定的读写模式。
    </m_description>
    </method>
    <method>
        <method_name>GetAccessMode</method_name>
        <method_url>IBasicFile\GetAccessMode</method_url>
    <m_description>
            获取操作对象的读写模式。
    </m_description>
    </method>
    <method>
        <method_name>SetAccessMode</method_name>
        <method_url>IBasicFile\SetAccessMode</method_url>
    <m_description>
            设置操作对象的读写模式。
    </m_description>
    </method>
    <method>
        <method_name>GetShareMode</method_name>
        <method_url>IBasicFile\GetShareMode</method_url>
    <m_description>
            获取文件的共享方式，本方法暂不支持。
    </m_description>
    </method>
    <method>
        <method_name>SetShareMode</method_name>
        <method_url>IBasicFile\SetShareMode</method_url>
    <m_description>
            设置文件的共享方式，本方法暂不支持。
    </m_description>
    </method>
</parent>
<self>
    <method>
        <method_name>Lookup</method_name>
        <method_url>IBasicDirectory\Lookup</method_url>
    <m_description>
            在当前目录对象中查找一个文件或子目录。
    </m_description>
    </method>
    <method>
        <method_name>CreateFile</method_name>
        <method_url>IBasicDirectory\CreateFile</method_url>
    <m_description>
            在当前目录对象中创建一个文件。
    </m_description>
    </method>
    <method>
        <method_name>DeleteFile</method_name>
        <method_url>IBasicDirectory\DeleteFile</method_url>
    <m_description>
            在当前目录对象中删除一个文件。
    </m_description>
    </method>
    <method>
        <method_name>Rename</method_name>
        <method_url>IBasicDirectory\Rename</method_url>
    <m_description>
            把当前目录对象中的文件或子目录改名，或者移动到另一个目录中。
    </m_description>
    </method>
    <method>
        <method_name>CreateDirectory</method_name>
        <method_url>IBasicDirectory\CreateDirectory</method_url>
    <m_description>
            在当前目录对象中创建一个子目录。
    </m_description>
    </method>
    <method>
        <method_name>DeleteDirectory</method_name>
        <method_url>IBasicDirectory\DeleteDirectory</method_url>
    <m_description>
            在当前目录对象中删除一个子目录。
    </m_description>
    </method>
    <method>
        <method_name>GetDirEntries</method_name>
        <method_url>IBasicDirectory\GetDirEntries</method_url>
    <m_description>
            获取当前目录对象中的所有子目录项和文件项。
    </m_description>
    </method>
</self>
