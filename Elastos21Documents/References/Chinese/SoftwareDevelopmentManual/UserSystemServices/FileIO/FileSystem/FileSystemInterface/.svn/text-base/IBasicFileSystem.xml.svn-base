<?xml version="1.0" encoding="GB2312"?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/interfaceman.xsl"?>    <manpage>
<volume>IBasicFileSystem</volume>
<interface> IBasicFileSystem </interface>
    <description>
        <para>
            文件系统对象接口。
        </para>
    </description>
<methods>
	<self>
    	<method>
        	<method_name>Sync</method_name>
        	<method_url>IBasicFileSystem\Sync</method_url>
    		<m_description>
            	把文件系统在内存中的所有数据都写入到永久存储器之中。
    		</m_description>
    		</method>
    	<method>
        	<method_name>GetRoot</method_name>
        	<method_url>IBasicFileSystem\GetRoot</method_url>
    		<m_description>
            	获取文件系统的根目录。
    		</m_description>
    	</method>
    	<method>
        	<method_name>GetStat</method_name>
        	<method_url>IBasicFileSystem\GetStat</method_url>
    		<m_description>
            	获取文件系统当前的统计信息。
    		</m_description>
    	</method>
    	<method>
        	<method_name>Lookup</method_name>
        	<method_url>IBasicFileSystem\Lookup</method_url>
    		<m_description>
            	根据所给的路径名查找对应的文件对象或者目录对象。路径名是从根目录开始的。
    		</m_description>
    	</method>
	</self>
</methods>

<requirements>
	<system>
		<system_name>Elastos 操作系统</system_name>
	</system>
</requirements>
</manpage>