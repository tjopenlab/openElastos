<?xml version="1.0" encoding="GB2312"?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/interfaceman.xsl"?>    <manpage>
    <volume>文件系统接口</volume>
    <interface> IBasicFile </interface>
    <description>
        <para>
            文件对象接口。
        </para>
    </description>
    <methods>
<self>
    <method>
        <method_name>Sync</method_name>
        <method_url>IBasicFile\Sync</method_url>
    <m_description>
            把当前操作对象的所有数据写入永久存储器。
    </m_description>
    </method>
    <method>
        <method_name>GetType</method_name>
        <method_url>IBasicFile\GetType</method_url>
    <m_description>
            获取操作对象的文件类型。
    </m_description>
    </method>
    <method>
        <method_name>GetAccessTime</method_name>
        <method_url>IBasicFile\GetAccessTime</method_url>
    <m_description>
            获取操作对象的最后一次访问时间。
    </m_description>
    </method>
    <method>
        <method_name>SetAccessTime</method_name>
        <method_url>IBasicFile\SetAccessTime</method_url>
    <m_description>
            设置操作对象的最后一次访问时间。
    </m_description>
    </method>
    <method>
        <method_name>GetModificationTime</method_name>
        <method_url>IBasicFile\GetModificationTime</method_url>
    <m_description>
            获取操作对象的最后一次修改时间。
    </m_description>
    </method>
    <method>
        <method_name>SetModificationTime</method_name>
        <method_url>IBasicFile\SetModificationTime</method_url>
    <m_description>
            设置操作对象的最后一次修改时间。
    </m_description>
    </method>
    <method>
        <method_name>GetCreateTime</method_name>
        <method_url>IBasicFile\GetCreateTime</method_url>
    <m_description>
            获取操作对象的创建时间。
    </m_description>
    </method>
    <method>
        <method_name>CheckAccessMode</method_name>
        <method_url>IBasicFile\CheckAccessMode</method_url>
    <m_description>
            检验操作对象是否具有指定的读写模式。
    </m_description>
    </method>
    <method>
        <method_name>GetAccessMode</method_name>
        <method_url>IBasicFile\GetAccessMode</method_url>
    <m_description>
            获取操作对象的读写模式。
    </m_description>
    </method>
    <method>
        <method_name>SetAccessMode</method_name>
        <method_url>IBasicFile\SetAccessMode</method_url>
    <m_description>
            设置操作对象的读写模式。
    </m_description>
    </method>
    <method>
        <method_name>GetShareMode</method_name>
        <method_url>IBasicFile\GetShareMode</method_url>
    <m_description>
            获取文件的共享方式。
    </m_description>
    </method>
    <method>
        <method_name>SetShareMode</method_name>
        <method_url>IBasicFile\SetShareMode</method_url>
    <m_description>
            设置文件的共享方式。
    </m_description>
    </method>
</self>
    </methods>
    <requirements>
        <system>
            <system_name>
Elastos 操作系统            </system_name>
        </system>
    </requirements>
</manpage>