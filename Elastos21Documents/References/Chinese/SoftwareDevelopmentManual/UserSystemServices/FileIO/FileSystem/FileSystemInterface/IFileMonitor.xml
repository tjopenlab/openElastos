<volume>文件系统接口</volume>
<interface> IFileMonitor </interface>
    <description>
        <para>文件修改通知机制接口。</para>
    </description>

<methods>
	<self>
		<method>
        	<method_name>AddFileMonitor</method_name>
        	<method_url>IFileMonitor\AddFileMonitor</method_url>
    		<m_description>注册文件修改通知。</m_description>
    	</method>

    	<method>
        	<method_name>RemoveFileMonitor</method_name>
        	<method_url>IFileMonitor\RemoveFileMonitor</method_url>
    		<m_description>取消文件修改通知。 </m_description>
   		</method>
	</self>
</methods>

<requirements>
	<system>
		<system_name>Elastos 操作系统</system_name>
	</system>
