<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/interfaceman.xsl"?>
<manpage>
    <volume>控制台服务</volume>
    <interface> CConsole类 </interface>
    <description>
        <para>
            主要针对系统控制台操作。
        </para>
    </description>
    <methods>
<self>
    <method>
        <method_name>Write</method_name>
        <method_url>CConsole\Write</method_url>
    <m_description>
            将字符串写入系统控制台标准输出设备，不回车换行。
    </m_description>
    </method>
    <method>
        <method_name>WriteLine</method_name>
        <method_url>CConsole\WriteLine</method_url>
    <m_description>
            将字符串写入系统控制台标准输出设备。并回车换行。
    </m_description>
    </method>
    <method>
        <method_name>Read</method_name>
        <method_url>CConsole\Read</method_url>
    <m_description>
            从系统控制台读入一个整数。
    </m_description>
    </method>
    <method>
        <method_name>ReadLine</method_name>
        <method_url>CConsole\ReadLine</method_url>
    <m_description>
            从系统控制台读入一个字符串。
    </m_description>
    </method>
    <method>
        <method_name>SetIn</method_name>
        <method_url>CConsole\SetIn</method_url>
    <m_description>
            重定向系统控制台标准输入设备。
    </m_description>
    </method>
    <method>
        <method_name>SetOut</method_name>
        <method_url>CConsole\SetOut</method_url>
    <m_description>
            重定向系统控制台标准输出设备。
    </m_description>
    </method>
    <method>
        <method_name>SetError</method_name>
        <method_url>CConsole\SetError</method_url>
    <m_description>
            重定向系统控制台标准错误处理设备。
    </m_description>
    </method>
</self>
    </methods>
    <requirements>
        <system>
            <system_name>
                Elastos操作系统
            </system_name>
        </system>
    </requirements>
</manpage>