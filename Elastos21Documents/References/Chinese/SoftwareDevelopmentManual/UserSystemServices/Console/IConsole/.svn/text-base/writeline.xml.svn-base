    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>

    <volume>IConsole接口函数</volume>

    <interface>IConsole</interface><method> WriteLine </method>
    <description>
        <para>将字符串写入窗口控制台标准输出设备。本函数还没实现</para>
    </description>

    <syntax>
        <return_val>ECode</return_val>
        <parameters>
            <api_param>
                <param_type> WString </param_type>
                <param_name> wstr </param_name>
                <param_note>
                    <para>要写入输出设备的字符串</para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

    <return>
        <para>返回NOERROR。</para>
    </return>

    <remarks>
        <para>函数WriteLine将str输出到标准输出设备，并换行。</para>
    </remarks>

    <requirements>
        <system>
            <system_name>
                Elastos操作系统  
            </system_name>
        </system>
        <headerfile>
            <file_name> elastos.h </file_name>
        </headerfile>
    </requirements>

