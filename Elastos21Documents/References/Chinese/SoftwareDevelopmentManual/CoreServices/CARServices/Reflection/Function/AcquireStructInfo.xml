<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/apimanual.xsl"?>

<manpage>
    <volume>CAR反射机制</volume>
    <title>CReflector::AcquireStructInfo</title>
    <routine_idx>../../Reflection.htm</routine_idx>

    <description>
        <para>获取结构体信息接口。</para>
    </description>

    <syntax>
        <return_val>ECode</return_val>
        <parameters>
            <api_param>
                <param_type> /* [in] */ AString </param_type>
                <param_name> name </param_name>
                <param_note>
                    <para>指定的结构体名</para>
                </param_note>
            </api_param>
            <api_param>
                <param_type> /* [in] */ const BufferOf&lt;AString&gt;&amp; </param_type>
                <param_name> fieldNames </param_name>
                <param_note>
                    <para>指定了结构体中各成员的成员名的数组</para>
                </param_note>
            </api_param>
            <api_param>
                <param_type> /* [in] */ const BufferOf&lt;IInterface *&gt;&amp; </param_type>
                <param_name> fieldTypeInfos </param_name>
                <param_note>
                    <para>指定了结构体中各成员类型指针的数组</para>
                </param_note>
            </api_param>
            <api_param>
                <param_type> /* [out] */ IStructInfo  ** </param_type>
                <param_name> ppStructInfo </param_name>
                <param_note>
                    <para>返回的指向结构体信息接口(即<link url="../Interface/IStructInfo.htm">IStructInfo</link>接口)指针的指针</para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

    <return>
    	<table>
            <th>
                <td>返回值</td><td>描述</td>
            </th>
            <tr>
                <td>NOERROR</td><td>方法调用成功</td>
            </tr>
            <tr>
                <td>E_INVALID_ARGUMENT</td><td>指定的参数无效</td>
            </tr>
            <tr>
                <td>E_OUT_OF_MEMORY</td><td>内存分配不成功</td>
            </tr>
            <tr>
                <td>E_DATAINFO_EXIST</td><td>已经存在同名，但成员信息不一样的StructInfo</td>
            </tr>
        </table>
        <para>参见<link url="../../../../../car_technology/fundamental/development_environment/car/ECode.htm">ECode</link>相关帮助。</para>
    </return>


    <requirements>
        <headerfile>
            <file_name> elastos.h </file_name>
        </headerfile>
        <library>
            <lib_name> elastos.lib </lib_name>
        </library>
    </requirements>

    <example>
    </example>

</manpage>
