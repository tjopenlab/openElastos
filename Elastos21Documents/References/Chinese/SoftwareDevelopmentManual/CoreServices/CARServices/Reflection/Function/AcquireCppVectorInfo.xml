<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/apimanual.xsl"?>

<manpage>
    <volume>CAR反射机制</volume>
    <title>CReflector::AcquireCppVectorInfo</title>
    <routine_idx>../../Reflection.htm</routine_idx>

    <description>
        <para>获取C++数组信息接口。 </para>
    </description>

    <syntax>
        <return_val>ECode</return_val>
        <parameters>
            <api_param>
                <param_type> /* [in] */ IDataTypeInfo * </param_type>
                <param_name> pElementTypeInfo </param_name>
                <param_note>
                    <para>指定的数组元素的类型信息接口指针</para>
                </param_note>
            </api_param>
            <api_param>
                <param_type> /* [in] */ Int32 </param_type>
                <param_name> length </param_name>
                <param_note>
                    <para>指定的数组长度</para>
                </param_note>
            </api_param>
            <api_param>
                <param_type> /* [out] */ ICppVectorInfo ** </param_type>
                <param_name> ppCppVectorInfo </param_name>
                <param_note>
                    <para>返回的指向C++数组信息接口(即<link url="../Interface/ICppVectorInfo.htm">ICppVectorInfo</link>接口)指针的指针</para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

    <return>
    	<table>
            <th>
                <td>返回值</td><td>描述</td>
            </th>
            <tr>
                <td>NOERROR</td><td>方法调用成功</td>
            </tr>
            <tr>
                <td>E_INVALID_ARGUMENT</td><td>指定的参数无效</td>
            </tr>
            <tr>
                <td>E_OUT_OF_MEMORY</td><td>内存分配不成功</td>
            </tr>
        </table>
        <para>参见<link url="../../../../../car_technology/fundamental/development_environment/car/ECode.htm">ECode</link>相关帮助。</para>
    </return>


    <requirements>
        <headerfile>
            <file_name> elastos.h </file_name>
        </headerfile>
        <library>
            <lib_name> elastos.lib </lib_name>
        </library>
    </requirements>

    <example>
    </example>

</manpage>
