<volume>IModuleInfo接口函数</volume><interface>IModuleInfo</interface><method> GetClassInfo </method>
    <description>
        <para>
通过名字获取类信息。
        </para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> U8String </param_type>
                <param_name> name </param_name>
                <param_note>
                    <para>
                    类名
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> IClassInfo ** </param_type>
                <param_name> ppClassInfo </param_name>
                <param_note>
                    <para>
                    IClassInfo接口指针的指针，用来返回类信息。
                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>
     <return>
	    <table>
	        <th>
	    	    <td>返回值 </td>
	    	    <td>描述</td>
	        </th>
	        <tr>
	    	    <td>NOERROR</td>
	    	    <td>成功返回值</td>
	        </tr>
	        <tr>
	    	    <td>E_INVALID_ARGUMENT</td>
	    	    <td>参数错误</td>
	        </tr>
	        
	    </table>
	    <para>参见<link url="../../../../../../car_technology/fundamental/development_environment/car/ECode.htm">ECode</link>相关帮助。</para>
    </return>
<remarks>
        <para></para>
    </remarks>
    <requirements>
        <system>
            <system_name>
Elastos操作系统            </system_name>
        </system>
    </requirements>
</manpage>