<?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume>IU8StringBufGetter接口函数</volume>
<interface>IU8StringBufGetter</interface>
<method> GetLength </method>
    <description>
        <para>
获取指定U8StringBuf中字符串的长度。
        </para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>

                <param_inout>[out]</param_inout>
                <param_type> Int32 * </param_type>
                <param_name> length </param_name>
                
                <param_note>
                    <para>
                    获取指定U8StringBuf的字符串长度。
                    </para>
                </param_note>         
            </api_param>        
        </parameters>
    </syntax>
     <return>
	    <table>
	        <th>
	    	    <td>返回值 </td>
	    	    <td>描述</td>
	        </th>
	        <tr>
	    	    <td>NOERROR</td>
	    	    <td>成功返回值</td>
	        </tr>
	        <tr>
	    	    <td>E_INVALID_ARGUMENT</td>
	    	    <td>参数错误</td>
	        </tr>
	        
	    </table>
	    <para>参见<link url="../../../../../../car_technology/fundamental/development_environment/car/ECode.htm">ECode</link>相关帮助。</para>
    </return>
</manpage>