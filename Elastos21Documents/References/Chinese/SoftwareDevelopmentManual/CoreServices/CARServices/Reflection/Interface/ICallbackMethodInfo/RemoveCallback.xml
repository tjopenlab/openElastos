<volume>ICallbackMethodInfo接口函数</volume>
<Interface>ICallbackMethodInfo</Interface><method> RemoveCallback </method>
    <description>
        <para>
           注销server的回调代理
        </para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> PInterface </param_type>
                <param_name> server </param_name>
                <param_note>
                    <para>
                    指定的接口指针
                    </para>
                </param_note>
            </api_param>
        </parameters>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> EventHandler </param_type>
                <param_name> handler </param_name>
                <param_note>
                    <para>
                    所注销的回调代理
                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>
    <return>
	    <table>
	        <th>
	    	    <td>返回值 </td>
	    	    <td>描述</td>
	        </th>
	        <tr>
	    	    <td>E_INVALID_ARGUMENT</td>
	    	    <td>参数server错误</td>
	        </tr>

	    </table>
	    <para>参见<link url="../../../../../../car_technology/fundamental/development_environment/car/ECode.htm">ECode</link>相关帮助。</para>
    </return>
<remarks>
        <para>
        注销回调代理。（可参考<link url="../../../../../../car_technology/speciality/callback/RemoveXXXCallback.htm">RemoveXXXCallback</link>）
        </para>
    </remarks>
    <requirements>
        <system>
            <system_name>
Elastos操作系统            </system_name>
        </system>
    </requirements>
</manpage>