    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/interfaceman.xsl"?>    <manpage>
    <volume>Car反射机制</volume>
    <interface> IInterfaceInfo </interface>
    <description>
        <para>
             接口信息接口
        </para>
    </description>
    <methods>
<self>
    <method>
        <method_name>GetId</method_name>
        <method_url>..\Interface\IInterfaceInfo\GetId</method_url>
    <m_description>
获取接口ID号。
    </m_description>
    </method>
    <method>
        <method_name>GetModuleInfo</method_name>
        <method_url>..\Interface\IInterfaceInfo\GetModuleInfo</method_url>
    <m_description>
获取构件信息。
    </m_description>
    </method>
    <method>
        <method_name>IsLocal</method_name>
        <method_url>..\Interface\IInterfaceInfo\IsLocal</method_url>
    <m_description>
判断是否是本地接口。
    </m_description>
    </method>
    <method>
        <method_name>HasBase</method_name>
        <method_url>..\Interface\IInterfaceInfo\HasBase</method_url>
    <m_description>
判断是否有基接口。
    </m_description>
    </method>
    <method>
        <method_name>GetBaseInfo</method_name>
        <method_url>..\Interface\IInterfaceInfo\GetBaseInfo</method_url>
    <m_description>
获取上级基接口。
    </m_description>
    </method>
    <method>
        <method_name>GetMethodCount</method_name>
        <method_url>..\Interface\IInterfaceInfo\GetMethodCount</method_url>
    <m_description>
获取方法个数。
    </m_description>
    </method>
    <method>
        <method_name>GetAllMethodInfos</method_name>
        <method_url>..\Interface\IInterfaceInfo\GetAllMethodInfos</method_url>
    <m_description>
获取所有方法信息。
    </m_description>
    </method>
    <method>
        <method_name>GetMethodInfo</method_name>
        <method_url>..\Interface\IInterfaceInfo\GetMethodInfo</method_url>
    <m_description>
通过名字获取方法信息。
    </m_description>
    </method>
</self>
    </methods>
    <requirements>
        <system>
            <system_name>
Elastos操作系统           </system_name>
        </system>
    </requirements>
<remarks>    
