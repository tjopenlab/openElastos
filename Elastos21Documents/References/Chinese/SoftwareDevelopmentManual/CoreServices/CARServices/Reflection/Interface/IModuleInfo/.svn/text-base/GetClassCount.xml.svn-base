<volume>IModuleInfo接口函数</volume><interface>IModuleInfo</interface><method> GetClassCount </method>
    <description>
        <para>
获取类的个数。
        </para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> Int32 * </param_type>
                <param_name> pCount </param_name>
                <param_note>
                    <para>
                    Int32型指针，用来返回类的个数
                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>
     <return>
	    <table>
	        <th>
	    	    <td>返回值 </td>
	    	    <td>描述</td>
	        </th>
	        <tr>
	    	    <td>NOERROR</td>
	    	    <td>成功返回值</td>
	        </tr>
	        <tr>
	    	    <td>E_INVALID_ARGUMENT</td>
	    	    <td>参数错误</td>
	        </tr>
	        
	    </table>
	    <para>参见<link url="../../../../../../car_technology/fundamental/development_environment/car/ECode.htm">ECode</link>相关帮助。</para>
    </return>
<remarks>
        <para></para>
    </remarks>
    <requirements>
        <system>
            <system_name>
Elastos操作系统            </system_name>
        </system>
    </requirements>
