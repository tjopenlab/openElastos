<?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume>IWStringBufInfo接口函数</volume>
<interface>IWStringBufInfo</interface>
<method> CreateVariable</method>
    <description>
        <para>
创建变量对象，并在堆上分配数据区。
        </para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>

                <param_inout>[in]</param_inout>
                <param_type> Int32 </param_type>
                <param_name> capacity </param_name>                
                <param_note>
                    <para>
                    要创建的WStringBuf对象变量的容量。
                    </para>
                </param_note>         
            </api_param> 
            <api_param>

                <param_inout>[out]</param_inout>
                <param_type> IVariableOfWStringBuf ** </param_type>
                <param_name> varBox </param_name>                
                <param_note>
                    <para>
                    返回指向IVariableOfAStringBuf类型的指针。
                    </para>
                </param_note>         
            </api_param>                  
        </parameters>
    </syntax>
     <return>
	    <table>
	        <th>
	    	    <td>返回值 </td>
	    	    <td>描述</td>
	        </th>
	        <tr>
	    	    <td>NOERROR</td>
	    	    <td>成功返回值</td>
	        </tr>
	        <tr>
	    	    <td>E_INVALID_ARGUMENT</td>
	    	    <td>参数错误</td>
	        </tr>
	        
	    </table>
	    <para>参见<link url="../../../../../../car_technology/fundamental/development_environment/car/ECode.htm">ECode</link>相关帮助。</para>
    </return>
</manpage>

