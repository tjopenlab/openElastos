<?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
    <sample url=""></sample>
<volume>ICallbackArgumentList接口函数</volume>
<Interface>ICallbackArgumentList</Interface><method> GetBooleanArgument </method>
    <description>
        <para>
通过序号获取类型为Boolean的参数。
        </para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> Int32 </param_type>
                <param_name> index </param_name>
                <param_note>
                    <para>
                    参数序号
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> Boolean * </param_type>
                <param_name> value </param_name>
                <param_note>
                    <para>
                    Boolean型指针，用来返回Boolean型参数
                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>
    <return>
	    <table>
	        <th>
	    	    <td>返回值 </td>
	    	    <td>描述</td>
	        </th>
	        <tr>
	    	    <td>NOERROR</td>
	    	    <td>成功返回值</td>
	        </tr>
	        <tr>
	    	    <td>E_INVALID_ARGUMENT</td>
	    	    <td>输入无效参数或与要取值的参数数据类型不匹配</td>
	        </tr>
	    </table>
	    <para>参见<link url="../../../../../../car_technology/fundamental/development_environment/car/ECode.htm">ECode</link>相关帮助。</para>
    </return>
<remarks>
        <para></para>
    </remarks>
    <requirements>
        <system>
            <system_name>
Elastos操作系统            </system_name>
        </system>
    </requirements>
</manpage>