<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?>

<manpage>
    <volume>ICppVectorGetter接口函数</volume>
    <interface>ICppVectorGetter</interface>
<method> GetDoubleElement </method>
    <description>
        <para>
通过序号获取类型为Double的元素的值。
        </para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> Int32 </param_type>
                <param_name> index </param_name>
                <param_note>
                    <para>
                    数组序号
                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> Double * </param_type>
                <param_name> value </param_name>
                <param_note>
                    <para>
                    返回的指向Double的指针
                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>
     <return>
	    <table>
	        <th>
	    	    <td>返回值 </td>
	    	    <td>描述</td>
	        </th>
	        <tr>
	    	    <td>NOERROR</td>
	    	    <td>成功返回值</td>
	        </tr>
	        <tr>
	    	    <td>E_INVALID_ARGUMENT</td>
	    	    <td>参数错误</td>
	        </tr>
	        
	    </table>
	    <para>参见<link url="../../../../../../car_technology/fundamental/development_environment/car/ECode.htm">ECode</link>相关帮助。</para>
    </return>

</manpage>