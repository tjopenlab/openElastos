<volume>IArgumentList接口函数</volume>

<interface>IArgumentList</interface><method> GetFunctionInfo </method>
    <description>
        <para>
            获取函数信息接口
        </para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> IFunctionInfo ** </param_type>
                <param_name> functionInfo </param_name>
                <param_note>
                    <para>
                        <link url="../IFunctionInfo.htm">IFunctionInfo</link>接口指针的指针，用于返回IFunctionInfo接口指针
                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>
    <return>
        <table>
            <th>
                <td>返回值</td><td>描述</td>
            </th>
            <tr>
                <td>NOERROR</td><td>程序正确执行</td>
            </tr>
            <tr>
                <td>E_INVALID_ARGUMENT</td><td>functionInfo为空</td>
            </tr>

        </table>
        <para>参见<link url="../../../../../../car_technology/fundamental/development_environment/car/ECode.htm">ECode</link>相关帮助。</para>
    </return>
    <remarks>
        <para></para>
    </remarks>

    <requirements>
        <system>
            <system_name>
Elastos操作系统            </system_name>
        </system>
    </requirements>
</manpage>