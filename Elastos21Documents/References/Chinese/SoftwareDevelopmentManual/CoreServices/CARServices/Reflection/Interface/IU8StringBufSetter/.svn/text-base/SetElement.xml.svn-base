<?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume>IU8StringBufSetter接口函数</volume>
<interface>IU8StringBufSetter</interface>
<method> SetElement </method>
    <description>
        <para>
设置U8StringBuf中指定索引号对应的字符。
        </para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>

                <param_inout>[in]</param_inout>
                <param_type> Int32 </param_type>
                <param_name> index </param_name>
                
                <param_note>
                    <para>
                    指定的索引号。
                    </para>
                </param_note>         
            </api_param>
            
            <api_param>

                <param_inout>[in]</param_inout>
                <param_type> AChar </param_type>
                <param_name> ach </param_name>
                
                <param_note>
                    <para>
                    设置的index索引号对应的字符。
                    </para>
                </param_note>         
            </api_param>            
            
            
        </parameters>
    </syntax>
     <return>
	    <table>
	        <th>
	    	    <td>返回值 </td>
	    	    <td>描述</td>
	        </th>
	        <tr>
	    	    <td>NOERROR</td>
	    	    <td>成功返回值</td>
	        </tr>
	        <tr>
	    	    <td>E_INVALID_ARGUMENT</td>
	    	    <td>参数错误</td>
	        </tr>
	        
	    </table>
	    <para>参见<link url="../../../../../../car_technology/fundamental/development_environment/car/ECode.htm">ECode</link>相关帮助。</para>
    </return>
</manpage>
