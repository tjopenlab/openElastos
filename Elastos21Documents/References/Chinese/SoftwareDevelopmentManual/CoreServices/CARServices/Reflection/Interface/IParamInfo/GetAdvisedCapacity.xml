<volume>IParamInfo接口函数</volume><Interface>IParamInfo</Interface><method> GetAdvisedCapacity </method>
    <description>
        <para>
        获得指定自描述类型的建议容量大小。
        </para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> Int32 * </param_type>
                <param_name> pAdvisedCapacity </param_name>
                <param_note>
                    <para>
                    指定自描述类型的容量
                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>
     <return>
	    <table>
	        <th>
	    	    <td>返回值 </td>
	    	    <td>描述</td>
	        </th>
	        <tr>
	    	    <td>NOERROR</td>
	    	    <td>成功返回值</td>
	        </tr>
	        <tr>
	    	    <td>E_INVALID_ARGUMENT</td>
	    	    <td>参数错误</td>
	        </tr>
	        <tr>
	    	    <td>E_INVALID_OPERATION</td>
	    	    <td>无效操作</td>
	        </tr>
	        
	    </table>
	    <para>参见<link url="../../../../../../car_technology/fundamental/development_environment/car/ECode.htm">ECode</link>相关帮助。</para>
    </return>
<remarks>
        <para></para>
    </remarks>
    <requirements>
        <system>
            <system_name>
Elastos操作系统             </system_name>
        </system>
    </requirements>
