<?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume>IWStringBufGett接口函数</volume>
<interface>IWStringBufGetter</interface>
<method> GetElement </method>
    <description>
        <para>
获取WStringBuf中指定索引号对应的宽字符。
        </para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> Int32 </param_type>
                <param_name> index </param_name>   
                <param_note>
                    <para>
                     指定的索引号。
                    </para>
                </param_note>         
            </api_param>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> WChar * </param_type>
                <param_name> wch </param_name>   
                <param_note>
                    <para>
                    返回指定宽字符的指针。
                    </para>
                </param_note>         
            </api_param>            
            
        </parameters>
    </syntax>
     <return>
	    <table>
	        <th>
	    	    <td>返回值 </td>
	    	    <td>描述</td>
	        </th>
	        <tr>
	    	    <td>NOERROR</td>
	    	    <td>成功返回值</td>
	        </tr>
	        <tr>
	    	    <td>E_INVALID_ARGUMENT</td>
	    	    <td>参数错误</td>
	        </tr>
	        
	    </table>
	    <para>参见<link url="../../../../../../car_technology/fundamental/development_environment/car/ECode.htm">ECode</link>相关帮助。</para>
    </return>
</manpage>