<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/apimanual.xsl"?>

<manpage>
    <volume>基本CAR函数</volume>
    <title>CObject::EnterContext</title>
    <routine_idx>../../BasicCAR.htm</routine_idx>

    <description>
        <para>对象进入context语境。</para>
    </description>

    <syntax>
        <return_val>ECode</return_val>
         <api_name>EnterContext</api_name>
        <parameters>
            <api_param>
                <param_type>PInterface </param_type>
                <param_name> pObject </param_name>
                <param_note>
                    <para>指向构件对象的接口指针。</para>
                </param_note>
            </api_param>
            <api_param>
                <param_type>PInterface </param_type>
                <param_name>pCtxObj </param_name>
                <param_note>
                    <para>指向语境对象的指针。</para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

   <return>
    	<table>
            <th>
                <td>返回值</td><td>描述</td>
            </th>
            <tr>
                <td>NOERROR</td><td>程序执行成功</td>
            </tr>
            <tr>
                <td>E_INVALID_ARGUMENT</td><td>指定的参数无效</td>
            </tr>
            <tr>
                <td>E_OUT_OF_MEMORY</td><td>内存不足</td>
            </tr>
        </table>
        <para>参见<link url="../../../../../../car_technology/fundamental/development_environment/car/ECode.htm">ECode</link>相关信息。</para>
    </return>
    <requirements>
        <headerfile>
            <file_name> elastos.h </file_name>
        </headerfile>
        <library>
            <lib_name> elastos.lib </lib_name>
        </library>
    </requirements>

   
</manpage>

