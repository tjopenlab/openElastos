<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/custom_type.xsl"?>

<manpage>
    <volume>系统基本服务</volume>
    <types>ThreadLocalSlotType</types>

    <description>
        <para>TLS类型</para>
    </description>

    <enum>
        <enum_member>
            <mem_name>ThreadLocalSlotType_Value</mem_name>
            <mem_value>0x01</mem_value>
            <mem_description>槽内存放的是立即数</mem_description>
        </enum_member>
        <enum_member>
            <mem_name>ThreadLocalSlotType_Mem</mem_name>
            <mem_value>0x02</mem_value>
            <mem_description>表示槽内存放的是堆内存的指针</mem_description>
        </enum_member>
        <enum_member>
            <mem_name>ThreadLocalSlotType_Obj</mem_name>
            <mem_value>0x03</mem_value>
            <mem_description>槽内存放的是构件对象的指针</mem_description>
        </enum_member>    
    </enum>
</manpage>