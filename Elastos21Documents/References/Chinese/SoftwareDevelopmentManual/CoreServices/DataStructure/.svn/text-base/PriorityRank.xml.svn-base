<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/custom_type.xsl"?>

<manpage>
    <volume>系统基本服务</volume>
    <types>ThreadPriorityRank</types>

    <description>
        <para>进程的优先级类别</para>
    </description>

    <enum>
        <enum_member>
            <mem_name>ThreadPriorityRank_Idle</mem_name>
            <mem_value>0x01</mem_value>
            <mem_description>优先级类别为空闲</mem_description>
        </enum_member>
        <enum_member>
            <mem_name>ThreadPriorityRank_Normal</mem_name>
            <mem_value>0x02</mem_value>
            <mem_description>优先级类别为正常</mem_description>
        </enum_member>
        <enum_member>
            <mem_name>ThreadPriorityRank_SoftRealTime</mem_name>
            <mem_value>0x03</mem_value>
            <mem_description>优先级类别为软实时</mem_description>
        </enum_member>
        <enum_member>
            <mem_name>ThreadPriorityRank_HardRealTime</mem_name>
            <mem_value>0x04</mem_value>
            <mem_description>优先级类别为硬实时</mem_description>
        </enum_member>
    </enum>
</manpage>