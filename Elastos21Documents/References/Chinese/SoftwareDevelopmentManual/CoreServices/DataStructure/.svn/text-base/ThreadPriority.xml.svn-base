<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/custom_type.xsl"?>

<manpage>
    <volume>系统基本服务</volume>
    <types>ThreadPriority</types>

    <description>
        <para>线程优先级</para>
    </description>

    <enum>
        <enum_member>
            <mem_name>ThreadPriority_Lowest</mem_name>
            <mem_value>0x000</mem_value>
            <mem_description>最低优先级</mem_description>
        </enum_member>
        <enum_member>
            <mem_name>ThreadPriority_BelowNormal</mem_name>
            <mem_value>0x0ff</mem_value>
            <mem_description>较低优先级</mem_description>
        </enum_member>
        <enum_member>
            <mem_name>ThreadPriority_Normal</mem_name>
            <mem_value>0x1ff</mem_value>
            <mem_description>正常优先级</mem_description>
        </enum_member>
        <enum_member>
            <mem_name>ThreadPriority_AboveNormal</mem_name>
            <mem_value>0x2ff</mem_value>
            <mem_description>较高优先级</mem_description>
        </enum_member>
        <enum_member>
            <mem_name>ThreadPriority_Highest</mem_name>
            <mem_value>0x3ff</mem_value>
            <mem_description>最高优先级</mem_description>
        </enum_member>
    </enum>
</manpage>