<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/custom_type.xsl"?>

<manpage>
    <volume>系统基本服务</volume>
    <types>SystemEvent</types>

    <description>
        <para>系统事件</para>
    </description>

    <enum>
        <enum_member>
            <mem_name>SystemEvent_Break</mem_name>
            <mem_value>0x01</mem_value>
            <mem_description>终止程序运行</mem_description>
        </enum_member>
        <enum_member>
            <mem_name>SystemEvent_SessionEnd</mem_name>
            <mem_value>0x02</mem_value>
            <mem_description>会话结束</mem_description>
        </enum_member>
        <enum_member>
            <mem_name>SystemEvent_TimeChanged</mem_name>
            <mem_value>0x04</mem_value>
            <mem_description>系统时间被改变</mem_description>
        </enum_member>
        <enum_member>
            <mem_name>SystemEvent_LowMemory</mem_name>
            <mem_value>0x08</mem_value>
            <mem_description>系统内存不足</mem_description>
        </enum_member>
        <enum_member>
            <mem_name>SystemEvent_CriticallyLowMemory</mem_name>
            <mem_value>0x10</mem_value>
            <mem_description>系统内存极端不足</mem_description>
        </enum_member>
        <enum_member>
            <mem_name>SystemEvent_Shutdown</mem_name>
            <mem_value>0x20</mem_value>
            <mem_description>系统准备关闭</mem_description>
        </enum_member>
    </enum>
</manpage>
