<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/custom_type.xsl"?>

<manpage>
    <volume>系统基本服务</volume>
    <types>IoRemapFlag</types>

    <description>
        <para>IO缓存</para>
    </description>

    <enum>
        <enum_member>
            <mem_name>IoRemapFlag_Direct</mem_name>
            <mem_value>0x08</mem_value>
            <mem_description>直接读写物理内存，没有缓存（Cache）、没有写缓存（WriteBuffer）</mem_description>
        </enum_member>
        <enum_member>
            <mem_name>IoRemapFlag_BufferedWrite</mem_name>
            <mem_value>0x18</mem_value>
            <mem_description>没有缓存（Cache），有写缓存（WriteBuffer）</mem_description>
        </enum_member>    
    </enum>
</manpage>