<!--<sample url="sdk\operating_system\system_com_interface\IActiveTimer">GetPriority</sample>-->
<volume>定时器</volume>
<interface>IActiveTimer</interface><method> GetPriority </method>
    <description>
        <para>获取定时器的线程的优先级</para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
       <parameters>
            <api_param>
               <param_inout>[out]</param_inout>
                <param_type> ThreadPriority * </param_type>
                <param_name> priority </param_name>
                <param_note>
                    <para>存放获取的优先级。参见数据结构中的<link url="../../../DataStructure/ThreadPriority.htm">ThreadPriority</link>枚举类型说明。</para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

	<return>
	    <table>
	        <th>
	    	    <td>返回值 </td>
	    	    <td>描述</td>
	        </th>
	        <tr>
	    	    <td>NOERROR</td>
	    	    <td>方法调用成功</td>
	        </tr>
	        <tr>
	    	    <td>E_INVALID_ARGUMENT</td>
	    	    <td>参数无效</td>
	        </tr>
	    </table>
	    <para>参见<link url="../../../../../car_technology/fundamental/development_environment/car/ECode.htm">ECode</link>相关帮助。</para>
    </return>

	<remarks>
  </remarks>

    <requirements>
        <system>
            <system_name>
				Windows 2000 或更高版本；Elastos操作系统
			</system_name>
        </system>
    </requirements>
