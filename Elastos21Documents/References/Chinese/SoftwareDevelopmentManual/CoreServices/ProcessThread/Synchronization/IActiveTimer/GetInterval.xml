<volume>定时器</volume>
<interface>IActiveTimer</interface><method> GetInterval </method>
    <description>
        <para>获取时间间隔</para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
       <parameters>
            <api_param>
              <param_inout>[out]</param_inout>
                <param_type> Millisecond32 * </param_type>
                <param_name> interval </param_name>
                <param_note>
                    <para>存放获取的时间间隔</para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

	<return>
	    <table>
	        <th>
	    	    <td>返回值 </td>
	    	    <td>描述</td>
	        </th>
	        <tr>
	    	    <td>NOERROR</td>
	    	    <td>方法调用成功</td>
	        </tr>
	        <tr>
	    	    <td>E_INVALID_ARGUMENT</td>
	    	    <td>参数无效</td>
	        </tr>
	    </table>
	    <para>参见<link url="../../../../../car_technology/fundamental/development_environment/car/ECode.htm">ECode</link>相关帮助。</para>
    </return>

	<remarks>
	</remarks>

    <requirements>
        <system>
            <system_name>
				Windows 2000 或更高版本；Elastos操作系统
			</system_name>
        </system>
    </requirements>
