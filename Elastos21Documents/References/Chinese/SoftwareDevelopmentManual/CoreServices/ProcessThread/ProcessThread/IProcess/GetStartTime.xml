<?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume>进程</volume>
<interface>IProcess</interface><method> GetStartTime </method>
    <description>
        <para>
            获取进程启动时的系统时间。
        </para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> SystemTime *   </param_type>
                <param_name> pTime </param_name>
                <param_note>
                    <para>
						指向<link url="..\..\..\OtherCoreService\DataType\SystemTime.htm">SystemTime</link>类型的指针，用于获取进程的启动时间，不能为空
                    </para>
                </param_note>
            </api_param>

        </parameters>
    </syntax>

	<return>
	    <table>
	        <th>
	    	    <td>返回值 </td>
	    	    <td>描述</td>
	        </th>
	        <tr>
	    	    <td>NOERROR</td>
	    	    <td>成功获取进程的启动时间</td>
	        </tr>
	        <tr>
	    	    <td>E_INVALID_ARGUMENT</td>
	    	    <td>参数错误</td>
	        </tr>
	        <tr>
	    	    <td>E_PROCESS_NOT_STARTED</td>
	    	    <td>进程还没启动</td>
	        </tr>
    </table>
	    <para>参见<link url="../../../../../car_technology/fundamental/development_environment/car/ECode.htm">ECode</link>相关帮助。</para>
    </return>



    <requirements>
        <system>
            <system_name>
				Windows 2000 或更高版本；Elastos操作系统
			</system_name>
        </system>
    </requirements>
</manpage>