<?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<sample url="sdk\operating_system\system_com_interface\IModule\NameAndPath"> GetName </sample>
<volume>可执行文件和模块</volume>
<interface>IModule</interface>
<method> GetName </method>
    <description>
        <para>
            获取模块的名字。
        </para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
			<parameters>
				<api_param>
					<param_inout>[out]</param_inout>
					<param_type> WStringBuf *  </param_type>
					<param_name> esbName </param_name>
					<param_note>
						<para>
						    用于存放模块名字的字符串缓冲区，不能为空
						</para>
					</param_note>
				</api_param>
			</parameters>
    </syntax>

	<return>
	    <table>
	        <th>
	    	    <td>返回值</td>
	    	    <td>描述</td>
	        </th>
	        <tr>
	    	    <td>NOERROR</td>
	    	    <td>成功获取模块名</td>
	        </tr>
	        <tr>
	    	    <td>E_INVALID_ARGUMENT</td>
	    	    <td>参数错误</td>
	        </tr>
	    </table>
	    <para>参见<link url="../../../../../car_technology/fundamental/development_environment/car/ECode.htm">ECode</link>相关帮助。</para>
    </return>

	<remarks>
		<para>
		    如果模块名比字符串缓冲区的长度要长，模块名将被截断。
		</para>
	</remarks>

    <requirements>
        <system>
            <system_name>
				Windows 2000 或更高版本；Elastos操作系统 
			</system_name>
        </system>
    </requirements>

</manpage>