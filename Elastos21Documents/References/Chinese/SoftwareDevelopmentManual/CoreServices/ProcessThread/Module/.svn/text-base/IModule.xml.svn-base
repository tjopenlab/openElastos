<?xml version = "1.0" encoding="GB2312" ?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/interfaceman.xsl"?>
<manpage>
    <volume>可执行文件和模块</volume>
    <interface> IModule </interface>
    <description>
        <para>
			模块对象接口
        </para>
    </description>
    <methods>
<self>
    <method>
        <method_name>FindResource</method_name>
        <method_url>IModule\FindResource</method_url>
    <m_description>
		获取模块中指定的资源的内存地址和大小
    </m_description>
    </method>
    <method>
        <method_name>GetBaseAddress</method_name>
        <method_url>IModule\GetBaseAddress</method_url>
    <m_description>
		获取模块的装载地址
    </m_description>
    </method>
    <method>
        <method_name>GetEntryPointAddress</method_name>
        <method_url>IModule\GetEntryPointAddress</method_url>
    <m_description>
		获取模块入口点地址
    </m_description>
    </method>
    <method>
        <method_name>GetMemorySize</method_name>
        <method_url>IModule\GetMemorySize</method_url>
    <m_description>
		获取模块占用的内存大小
    </m_description>
    </method>
    <method>
        <method_name>GetName</method_name>
        <method_url>IModule\GetName</method_url>
    <m_description>
		获取模块的名字
    </m_description>
    </method>
    <method>
        <method_name>GetFullPath</method_name>
        <method_url>IModule\GetFullPath</method_url>
    <m_description>
		获取模块的全路径名
    </m_description>
    </method>
    <method>
        <method_name>GetProcAddressByName</method_name>
        <method_url>IModule\GetProcAddressByName</method_url>
    <m_description>
         根据函数名字取函数的起始地址
    </m_description>
    </method>
    <method>
        <method_name>GetProcAddressByOrdinal</method_name>
        <method_url>IModule\GetProcAddressByOrdinal</method_url>
    <m_description>
         根据函数序号取函数的起始地址
    </m_description>
    </method>

	<method>
        <method_name>GetResourceSection</method_name>
        <method_url>IModule\GetResourceSection</method_url>
    <m_description>
        获取模块资源段的起始地址
    </m_description>
    </method>

    <method>
        <method_name>GetSectionByName</method_name>
        <method_url>IModule\GetSectionByName</method_url>
    <m_description>
         根据段名字取段的起始地址
    </m_description>
    </method>

</self>
    </methods>
    <requirements>
        <system>
            <system_name>
				Windows 2000 或更高版本；Elastos操作系统 
			</system_name>
        </system>
    </requirements>
</manpage>