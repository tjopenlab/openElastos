<?xml version="1.0" encoding="GB2312"?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/manual.xsl"?>

<manpage>
   
    <title> futime</title>
    <volume>C 运行库函数</volume>
    <routine_idx> utime_idx.htm </routine_idx>

    <description>
        <para>为某个已经打开的文件设置修改时间。 </para>
    </description>
    <syntax>
        <return_val>int</return_val>
        <func_name>futime</func_name>
        <parameters>
            <func_param>
                <param_type>int </param_type>
                <param_name>handle</param_name>
                <param_note>
                    <para>指明文件位置的路径。</para>
                </param_note>
            </func_param>
            <func_param>
                <param_type>const  utimebuf *</param_type>
                <param_name>filetime</param_name>
                <param_note>
                    <para>文件修改的时间</para>
                </param_note>
            </func_param>
        </parameters>
     </syntax>
     <return>
        <para>成功执行时，返回0。失败返回-1。  </para>
        
    </return>
    

    <requirements>
        <routine>
            <routine_name>futime</routine_name>
            <headerfile>utime.h</headerfile>
            <compatibility>Windows 2000, Elastos </compatibility>
        </routine>
       
    </requirements>

</manpage>