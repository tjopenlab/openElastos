<?xml version="1.0" encoding="GB2312"?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/manual.xsl"?>

<manpage>
    <sample url="sdk\operating_system\crt\libc\io\_isatty">_isatty</sample>
    <title> _isatty</title>
    <volume>C 运行库函数</volume>
    <routine_idx> io_idx.htm </routine_idx>

    <description>
        <para>检查句柄是否和字符驱动（终端，串口，控制台，打印机）关联。</para>
    </description>

    <syntax>
        <return_val>int</return_val>
        <func_name>_isatty</func_name>
        <parameters>
            <func_param>
                <param_type>int</param_type>
                <param_name>handle</param_name>
                <param_note>
                    <para>指向驱动的句柄</para>
                </param_note>
            </func_param>
        </parameters>
    </syntax>
    <return>
        <para>运行成功返回非零，否则返回零值。</para>
    </return>

    <remarks>
    </remarks>

    <requirements>
        <routine>
            <routine_name>_isatty</routine_name>
            <headerfile>io.h</headerfile>
            <compatibility>Windows 2000, Elastos </compatibility>
        </routine>
    </requirements>
</manpage>