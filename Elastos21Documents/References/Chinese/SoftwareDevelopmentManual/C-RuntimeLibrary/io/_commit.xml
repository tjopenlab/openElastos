    </description>

    <syntax>
        <return_val>int</return_val>
        <func_name>_commit</func_name>
        <parameters>
            <func_param>
                <param_type>int</param_type>
                <param_name>handle</param_name>
                <param_note>
                    <para>指向文件名的句柄</para>
                </param_note>
            </func_param>
        </parameters>
    </syntax>
    <return>
        <para>运行成功返回0。</para>
    </return>

    <remarks>
    </remarks>

