    <title> _getdcwd _wgetdcwd</title>
    <volume>C 运行库函数</volume>
    <routine_idx> dir_idx.htm </routine_idx>

    <description>
        <para>获取指定磁盘的路径</para>
    </description>

    <syntax>
        <return_val>char*</return_val>
        <func_name>_getdcwd</func_name>
        <parameters>
            <func_param>
                <param_type>int</param_type>
                <param_name>drive</param_name>
                <param_note>
                    <para>磁盘号</para>
                </param_note>
            </func_param>
            <func_param>
                <param_type>char *</param_type>
                <param_name>buffer</param_name>
                <param_note>
                    <para>返回的路径名</para>
                </param_note>
            </func_param>
            <func_param>
                <param_type>int</param_type>
                <param_name>maxlen</param_name>
                <param_note>
                    <para>路径最大长度</para>
                </param_note>
            </func_param>
        </parameters>
    </syntax>
    <return>
    </return>

    <remarks>

    </remarks>

