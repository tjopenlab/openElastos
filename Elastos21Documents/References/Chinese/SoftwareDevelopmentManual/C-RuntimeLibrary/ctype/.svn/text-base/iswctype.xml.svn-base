<?xml version="1.0" encoding="GB2312"?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/manual.xsl"?>

<manpage>
    <sample url="sdk\operating_system\crt\libc\ctype\iswctype">iswctype</sample>
    <title>iswctype</title>
    <volume>C 运行库函数</volume>
    <routine_idx> ctype_idx.htm </routine_idx>

    <description>
        <para>测试字符属性.</para>
    </description>

    <syntax>
        <return_val>int</return_val>
        <func_name>iswctype</func_name>
        <parameters>
            <func_param>
                <param_type>wint_t</param_type>
                <param_name>c</param_name>
                <param_note>
                    <para>需要判断的整型值</para>
                </param_note>
            </func_param>
            <func_param>
                <param_type>wctype_t</param_type>
                <param_name>desc</param_name>
                <param_note>
                    <para>判断所需的属性值</para>
                </param_note>
            </func_param>
        </parameters>
    </syntax>

    <return>
        <para>如果c具有desc的属性，返回非零；否则返回0。</para>
    </return>

    <requirements>
        <routine>
            <routine_name>iswctype</routine_name>
            <headerfile>ctype.h</headerfile>
            <compatibility>ANSI, Windows 2000, Elastos </compatibility>
        </routine>
    </requirements>
</manpage>