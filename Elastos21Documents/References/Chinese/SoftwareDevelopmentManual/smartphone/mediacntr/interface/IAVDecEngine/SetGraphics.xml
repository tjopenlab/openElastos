<?xml version="1.0" encoding="GB2312"?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume>媒体中心</volume>
<interface>IAVDecEngine</interface><method> SetGraphics </method>
    <description>
        <para>
            设置视频显示需要使用的绘图功能。
        </para>
    </description>
    <syntax>
        <return_val> ECODE </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> IGraphicsRef </param_type>
                <param_name> pIGraphics </param_name>
                <param_note>
                    <para>
<link url="IGraphics.htm">IGraphics</link>接口指针，指定视频渲染过程中需要用到的参数                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

<return>
    <para>
            方法调用成功时，返回 S_OK 。    </para>
</return>
    <requirements>
        <system>
            <system_name>
《Elastos 2.0》或更高版本            </system_name>
        </system>
    </requirements>
</manpage>