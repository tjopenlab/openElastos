    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume>媒体中心</volume>
<interface>IMediaDevMgrCallback</interface><method> Resume </method>
    <description>
        <para>
            使被阻塞的模块可以继续使用设备的事件。
        </para>
    </description>
    <syntax>
        <return_val> ECODE </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> void * </param_type>
                <param_name> pThis </param_name>
                <param_note>
                    <para>
当前调用模块的对象指针                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

<return>
    <para>
            方法调用成功时，返回 S_OK。    </para>
</return>
    <requirements>
        <system>
            <system_name>
《Elastos 2.0》或更高版本            </system_name>
        </system>
    </requirements>
    <references>
        <reference>
            <ref_name>HangUp</ref_name>
            <ref_url>HangUp</ref_url>
        </reference>
    </references>
</manpage>