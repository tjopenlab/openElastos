<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?>
<manpage>
<volume>电话控制</volume>
<interface>ISettingActs</interface><method>OnSetOnwerNumbers</method>
    <description>
        <para>
            设置线路一和线路二的本机号码到SIM卡。
        </para>
    </description>
    <syntax>
        <return_val> ECODE </return_val>
        <parameters>
        	<api_param>
                <param_inout>[in]</param_inout>
                <param_type> EzStr </param_type>
                <param_name> strNumber1 </param_name>
                <param_note>
                    <para>线路一的本机号码</para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> EzStr </param_type>
                <param_name> strNumber2 </param_name>
                <param_note>
                    <para>线路二的本机号码</para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

<return>
    <para>方法调用成功时，返回S_OK。</para>
</return>
    <requirements>
        <system>
            <system_name>《Elastos 2.0》或更高版本</system_name>
        </system>
    </requirements>

</manpage>