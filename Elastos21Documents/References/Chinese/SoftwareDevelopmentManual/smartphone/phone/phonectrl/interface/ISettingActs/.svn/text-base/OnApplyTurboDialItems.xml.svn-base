<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?>
<manpage>
<volume>电话控制</volume>
<interface>ISettingActs</interface><method>OnApplyTurboDialItems</method>
    <description>
        <para>
            响应用户UI请求，设置快捷拨号项。
        </para>
    </description>
    <syntax>
        <return_val> ECODE </return_val>
        <parameters>
        <api_param>
                <param_inout>[in]</param_inout>
                <param_type> EzArray&lt;EzStr&gt; </param_type>
                <param_name> arDialStrings </param_name>
                <param_note>
                    <para>快捷按键代表的拨号内容</para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

<return>
    <para>方法调用成功时，返回S_OK。</para>
</return>
    <requirements>
        <system>
            <system_name>《Elastos 2.0》或更高版本</system_name>
        </system>
    </requirements>

</manpage>