<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?>
<manpage>
<volume>电话控制</volume>
<interface>ISettingActs</interface><method>OnOnwerNumber</method>
    <description>
        <para>
            响应用户UI请求进行本机号码设置，显示本机号码设置页面。
        </para>
    </description>
    <syntax>
        <return_val> ECODE </return_val>
        <NoArguments/>
    </syntax>

<return>
    <para>方法调用成功时，返回S_OK。</para>
</return>
<requirements>
    <system>
        <system_name>《Elastos 2.0》或更高版本</system_name>
    </system>
</requirements>

</manpage>