<?xml version="1.0" encoding="GB2312"?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume>电话控制</volume>
<interface>IPhoneActs</interface><method> OnSwitchLines </method>
    <description>
        <para>
            在线路1和线路2之间切换。
        </para>
    </description>
    <syntax>
        <return_val> ECODE </return_val>
        <parameters>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> INT * </param_type>
                <param_name> pnSwitchedLine </param_name>
                <param_note>
                    <para>指向INT型的指针，用于获取线路号</para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

<return>
    <para>
            方法调用成功，返回S_OK。    </para>
</return>
    <requirements>
        <system>
            <system_name>
《Elastos 2.0》或更高版本            </system_name>
        </system>
    </requirements>
</manpage>