<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?>
<manpage>
<volume>电话控制</volume>
<interface>ISettingActs</interface><method>OnSelectNetworkSearchType</method>
    <description>
        <para>
            响应用户UI请求，选择网络查找的方式。
        </para>
    </description>
    <syntax>
        <return_val> ECODE </return_val>
        <parameters>
        <api_param>
                <param_inout>[in]</param_inout>
                <param_type> UInt32 </param_type>
                <param_name> dwSearchType </param_name>
                <param_note>
                    <para>网络查找的方式，dwSearchType的取值可以为：</para>
                    <para_table>
                        <th>
	                    	<td>  参数值 </td>
	                    	<td>  描述 </td>
	                	</th>
	                	<tr>
	                    	<td>TAPI_REGMODE_AUTOMATIC</td>
	                    	<td>自动方式</td>
	                	</tr>
	                	<tr>
	                    	<td>TAPI_REGMODE_MANUAL</td>
	                    	<td>手动方式</td>
	                	</tr>
	         		</para_table>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

<return>
    <para>方法调用成功时，返回S_OK。</para>
</return>
    <requirements>
        <system>
            <system_name>《Elastos 2.0》或更高版本</system_name>
        </system>
    </requirements>

</manpage>