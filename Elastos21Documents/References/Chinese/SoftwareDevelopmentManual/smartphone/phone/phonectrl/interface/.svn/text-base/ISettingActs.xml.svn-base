<?xml version="1.0" encoding="GB2312"?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/interfaceman.xsl"?>    <manpage>
    <volume>电话控制</volume>
    <interface> ISettingActs </interface>
    <description>
        <para>
            网络设置接口。
        </para>
    </description>
    <methods>
		<self>
			<method>
        		<method_name>OnApplyAutoAnswerIncomingCallState</method_name>
        		<method_url>..\interface\ISettingActs\OnApplyAutoAnswerIncomingCallState</method_url>
    		<m_description>设置自动接听的状态。</m_description>
    		</method>
    		<method>
        		<method_name>OnApplyCallRestrictState</method_name>
        		<method_url>..\interface\ISettingActs\OnApplyCallRestrictState</method_url>
    		<m_description>响应用户UI请求，设置呼叫限制状态。</m_description>
    		</method>
    		<method>
        		<method_name>OnApplyCallTimelyRemindState</method_name>
        		<method_url>..\interface\ISettingActs\OnApplyCallTimelyRemindState</method_url>
    		<m_description>设置用户通话提醒的状态。</m_description>
    		</method>
    		<method>
        		<method_name>OnApplyForwardSetting</method_name>
        		<method_url>..\interface\ISettingActs\OnApplyForwardSetting</method_url>
    		<m_description>应用呼叫转移功能设置。</m_description>
    		</method>
    		<method>
        		<method_name>OnApplyTurboDialItems</method_name>
        		<method_url>..\interface\ISettingActs\OnApplyTurboDialItems</method_url>
    		<m_description>响应用户UI请求，设置快捷拨号项。</m_description>
    		</method>
    		<method>
        		<method_name>OnCallForwardSetting</method_name>
        		<method_url>..\interface\ISettingActs\OnCallForwardSetting</method_url>
    		<m_description>响应用户UI请求进行呼叫转移设置，显示呼叫转移设置页面</m_description>
    		</method>
    		<method>
        		<method_name>OnCallRestrictSetting</method_name>
        		<method_url>..\interface\ISettingActs\OnCallRestrictSetting</method_url>
    		<m_description>响应用户UI请求进行呼叫限制设置，显示呼叫限制设置页面。</m_description>
    		</method>
    		<method>
        		<method_name>OnCallWaitingSetting</method_name>
        		<method_url>..\interface\ISettingActs\OnCallWaitingSetting</method_url>
    		<m_description>响应用户UI请求进行呼叫等待，显示呼叫等待设置页面。</m_description>
    		</method>
    		<method>
        		<method_name>OnCleanTurboDialItems</method_name>
        		<method_url>..\interface\ISettingActs\OnCleanTurboDialItems</method_url>
    		<m_description>响应用户UI请求取消所有快捷拨号项。</m_description>
    		</method>
    		<method>
        		<method_name>OnClirSetting</method_name>
        		<method_url>..\interface\ISettingActs\OnClirSetting</method_url>
    		<m_description>响应用户UI请求，进行CLIR设置。</m_description>
    		</method>
    		<method>
        		<method_name>OnGeneralSetting</method_name>
        		<method_url>..\interface\ISettingActs\OnGeneralSetting</method_url>
    		<m_description>进行电话常规设置，显示电话常规设置页面。</m_description>
    		</method>
    		<method>
        		<method_name>OnGetAutoAnswerIncomingCallState</method_name>
        		<method_url>..\interface\ISettingActs\OnGetAutoAnswerIncomingCallState</method_url>
    		<m_description>获取自动接听的状态。</m_description>
    		</method>
    		<method>
        		<method_name>OnGetCallRestrictState</method_name>
        		<method_url>..\interface\ISettingActs\OnGetCallRestrictState</method_url>
    		<m_description>响应用户UI请求，获取呼叫限制状态，根据状态显示UI控件。</m_description>
    		</method>
    		<method>
        		<method_name>OnGetCallTimelyRemindState</method_name>
        		<method_url>..\interface\ISettingActs\OnGetCallTimelyRemindState</method_url>
    		<m_description>获取用户通话提醒的状态。</m_description>
    		</method>
    		<method>
        		<method_name>OnNetworkSetting</method_name>
        		<method_url>..\interface\ISettingActs\OnNetworkSetting</method_url>
    		<m_description>响应用户UI请求进行网络管理设置，显示网络管理设置页面。</m_description>
    		</method>
    		<method>
        		<method_name>OnOnwerNumber</method_name>
        		<method_url>..\interface\ISettingActs\OnOnwerNumber</method_url>
    		<m_description>响应用户UI请求进行本机号码设置，显示本机号码设置页面。</m_description>
    		</method>
    		<method>
        		<method_name>OnSelectCallWaiting</method_name>
        		<method_url>..\interface\ISettingActs\OnSelectCallWaiting</method_url>
    		<m_description>响应用户UI请求，选择呼叫等待设置。</m_description>
    		</method>
    		<method>
        		<method_name>OnSelectNetworkSearchType</method_name>
        		<method_url>..\interface\ISettingActs\OnSelectNetworkSearchType</method_url>
    		<m_description>响应用户UI请求，选择网络查找的方式。</m_description>
    		</method>
    		<method>
        		<method_name>OnSetOnwerNumbers</method_name>
        		<method_url>..\interface\ISettingActs\OnSetOnwerNumbers</method_url>
    		<m_description>设置线路一和线路二的本机号码到SIM卡。</m_description>
    		</method>
    		<method>
        		<method_name>OnTurboDialSetting</method_name>
        		<method_url>..\interface\ISettingActs\OnTurboDialSetting</method_url>
    		<m_description>响应用户UI请求进行快捷拨号，显示快捷拨号设置页面。</m_description>
    		</method>
		</self>
    </methods>
    <requirements>
        <system>
            <system_name>
《Elastos 2.0》或更高版本            </system_name>
        </system>
    </requirements>
</manpage>