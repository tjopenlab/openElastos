<?xml version="1.0" encoding="GB2312"?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume>电话控制</volume>
<interface>IPhoneActs</interface><method> OnSwitchToHoldLayer </method>
    <description>
        <para>
            有多个通话时，将界面切换到保持的通话上。
        </para>
    </description>
    <syntax>
        <return_val> ECODE </return_val>
        <NoArguments/>
    </syntax>

<return>
    <para>
            方法调用成功时，返回S_OK。    </para>
</return>
    <requirements>
        <system>
            <system_name>
《Elastos 2.0》或更高版本            </system_name>
        </system>
    </requirements>
</manpage>