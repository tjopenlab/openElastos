<?xml version="1.0" encoding="GB2312"?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/interfaceman.xsl"?>    <manpage>
    <volume>电话控制</volume>
    <interface> IPhoneActs </interface>
    <description>
        <para>
            通话管理接口。
        </para>
    </description>
    <methods>
<self>
    <method>
        <method_name>OnInit</method_name>
        <method_url>..\interface\IPhoneActs\OnInit</method_url>
    <m_description>
            用于其它应用模块通过指定的拨号字符串，初始化电话模块。
    </m_description>
    </method>
    <method>
        <method_name>OnExit</method_name>
        <method_url>..\interface\IPhoneActs\OnExit</method_url>
    <m_description>
            退出电话应用。
    </m_description>
    </method>
    <method>
        <method_name>OnDial</method_name>
        <method_url>..\interface\IPhoneActs\OnDial</method_url>
    <m_description>
            打电话或完成一些特殊功能。
    </m_description>
    </method>
    <method>
        <method_name>OnCancelCalling</method_name>
        <method_url>..\interface\IPhoneActs\OnCancelCalling</method_url>
    <m_description>
            结束正在拨出的呼叫。
    </m_description>
    </method>
    <method>
        <method_name>OnHandFree</method_name>
        <method_url>..\interface\IPhoneActs\OnHandFree</method_url>
    <m_description>
            免提。
    </m_description>
    </method>
    <method>
        <method_name>OnHoldActive</method_name>
        <method_url>..\interface\IPhoneActs\OnHoldActive</method_url>
    <m_description>
            保持一个活动的通话。
    </m_description>
    </method>
    <method>
        <method_name>OnMute</method_name>
        <method_url>..\interface\IPhoneActs\OnMute</method_url>
    <m_description>
            设置静音。
    </m_description>
    </method>
    <method>
        <method_name>OnDrop</method_name>
        <method_url>..\interface\IPhoneActs\OnDrop</method_url>
    <m_description>
            挂断一通电话。
    </m_description>
    </method>
    <method>
        <method_name>OnSelectDialerKeyboard</method_name>
        <method_url>..\interface\IPhoneActs\OnSelectDialerKeyboard</method_url>
    <m_description>
            切换界面到拨号盘。
    </m_description>
    </method>
    <method>
        <method_name>OnSelectDialRecord</method_name>
        <method_url>..\interface\IPhoneActs\OnSelectDialRecord</method_url>
    <m_description>
            切换界面到通话记录界面。
    </m_description>
    </method>
    <method>
        <method_name>OnSelectDtmfKeyboard</method_name>
        <method_url>..\interface\IPhoneActs\OnSelectDtmfKeyboard</method_url>
    <m_description>
            把拨号盘切换到dtmf状态，在此界面下可以发送dtmf。
    </m_description>
    </method>
    <method>
        <method_name>OnStoreNumberDialog</method_name>
        <method_url>..\interface\IPhoneActs\OnStoreNumberDialog</method_url>
    <m_description>
            调用地址簿中保存号码的界面。
    </m_description>
    </method>
    <method>
        <method_name>OnStoreNumber</method_name>
        <method_url>..\interface\IPhoneActs\OnStoreNumber</method_url>
    <m_description>
            调用地址簿中带选项的保存号码的界面。
    </m_description>
    </method>
    <method>
        <method_name>OnOwnerNumber</method_name>
        <method_url>..\interface\IPhoneActs\OnOwnerNumber</method_url>
    <m_description>
            调用用于显示本机号码的弹出窗口。
    </m_description>
    </method>
    <method>
        <method_name>OnTransferCall</method_name>
        <method_url>..\interface\IPhoneActs\OnTransferCall</method_url>
    <m_description>
            来电转移。
    </m_description>
    </method>
    <method>
        <method_name>OnNewDial</method_name>
        <method_url>..\interface\IPhoneActs\OnNewDial</method_url>
    <m_description>
            调用电话界面，执行一个新呼叫。
    </m_description>
    </method>
    <method>
        <method_name>OnDropAll</method_name>
        <method_url>..\interface\IPhoneActs\OnDropAll</method_url>
    <m_description>
            挂断所有通话。
    </m_description>
    </method>
    <method>
        <method_name>OnSwitchToHoldLayer</method_name>
        <method_url>..\interface\IPhoneActs\OnSwitchToHoldLayer</method_url>
    <m_description>
            有多个通话时，将界面切换到保持的通话上。
    </m_description>
    </method>
    <method>
        <method_name>OnSwitchToHoldAndActive</method_name>
        <method_url>..\interface\IPhoneActs\OnSwitchToHoldAndActive</method_url>
    <m_description>
            有多个通话时，将页面切换到保持的通话上，并激活它。
    </m_description>
    </method>
    <method>
        <method_name>OnConnectCalls</method_name>
        <method_url>..\interface\IPhoneActs\OnConnectCalls</method_url>
    <m_description>
            将当前通话加入电话会议。
    </m_description>
    </method>
    <method>
        <method_name>OnDropCurrentCall</method_name>
        <method_url>..\interface\IPhoneActs\OnDropCurrentCall</method_url>
    <m_description>
            挂断当前位于界面前端的通话。
    </m_description>
    </method>
    <method>
        <method_name>OnDropHoldCalls</method_name>
        <method_url>..\interface\IPhoneActs\OnDropHoldCalls</method_url>
    <m_description>
            挂断保持的通话。
    </m_description>
    </method>
    <method>
        <method_name>OnDropActiveCalls</method_name>
        <method_url>..\interface\IPhoneActs\OnDropActiveCalls</method_url>
    <m_description>
            挂断活动的通话。
    </m_description>
    </method>
    <method>
        <method_name>OnSeparateCall</method_name>
        <method_url>..\interface\IPhoneActs\OnSeparateCall</method_url>
    <m_description>
            把一通电话从电话会议中分离出来。
    </m_description>
    </method>
    <method>
        <method_name>OnHoldActiveButOne</method_name>
        <method_url>..\interface\IPhoneActs\OnHoldActiveButOne</method_url>
    <m_description>
            除指定ID的通话外，保持所有激活的通话。
    </m_description>
    </method>
    <method>
        <method_name>OnDropInComing</method_name>
        <method_url>..\interface\IPhoneActs\OnDropInComing</method_url>
    <m_description>
            挂断正在呼入的电话。
    </m_description>
    </method>
    <method>
        <method_name>OnBusyInComing</method_name>
        <method_url>..\interface\IPhoneActs\OnBusyInComing</method_url>
    <m_description>
            给正在呼入的电话置忙音。
    </m_description>
    </method>
    <method>
        <method_name>OnAcceptInComing</method_name>
        <method_url>..\interface\IPhoneActs\OnAcceptInComing</method_url>
    <m_description>
            接听正在呼入的电话。
    </m_description>
    </method>
    <method>
        <method_name>OnAcceptAndDropOne</method_name>
        <method_url>..\interface\IPhoneActs\OnAcceptAndDropOne</method_url>
    <m_description>
            挂断一路通话，接听新来电。
    </m_description>
    </method>
    <method>
        <method_name>OnSelectSettings</method_name>
        <method_url>..\interface\IPhoneActs\OnSelectSettings</method_url>
    <m_description>
            用于呼叫通话设定窗口？
    </m_description>
    </method>
    <method>
        <method_name>OnSwitchLines</method_name>
        <method_url>..\interface\IPhoneActs\OnSwitchLines</method_url>
    <m_description>
            在线路1和线路2之间切换。
    </m_description>
    </method>
</self>
    </methods>
    <requirements>
        <system>
            <system_name>
《Elastos 2.0》或更高版本            </system_name>
        </system>
    </requirements>
    <remarks>
        <para>
            通话的控制部分，主要提供一些控制逻辑。该接口通过利用通话引擎和其它应用引擎来完成电话的应用框架。
        </para>
    </remarks>
</manpage>