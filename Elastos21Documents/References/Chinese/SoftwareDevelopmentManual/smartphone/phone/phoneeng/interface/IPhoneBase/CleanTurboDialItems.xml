<volume>电话引擎</volume>
<interface>IPhoneBase</interface><method> CleanTurboDialItems </method>
    <description>
        <para>
            清空所有快捷拨号项。
        </para>
    </description>
    <syntax>
        <return_val> ECODE </return_val>
        <NoArguments/>
    </syntax>

<return>
    <para>
            如果方法调用成功，返回S_OK。    </para>
</return>
    <requirements>
        <system>
            <system_name>
《Elastos 2.0》或更高版本            </system_name>
        </system>
        <dll>
            <dll_name>telephony.dll,database.dll</dll_name>
        </dll>
        <headerfile>
            <file_name>phonedef.h,database.h</file_name>
        </headerfile>
        <library>
            <lib_name>elastos.lib,ElCRuntime.lib</lib_name>
        </library>
    </requirements>
    <references>
        <reference>
            <ref_name>SetTurboDialItem</ref_name>
            <ref_url>SetTurboDialItem</ref_url>
        </reference>
        <reference>
            <ref_name>GetTurboDialItem</ref_name>
            <ref_url>GetTurboDialItem</ref_url>
        </reference>
    </references>
</manpage>