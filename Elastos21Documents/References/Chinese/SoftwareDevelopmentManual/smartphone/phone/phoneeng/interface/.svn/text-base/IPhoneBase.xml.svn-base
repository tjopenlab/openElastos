<?xml version="1.0" encoding="GB2312"?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/interfaceman.xsl"?>    <manpage>
    <volume>电话引擎</volume>
    <interface> IPhoneBase </interface>
    <description>
        <para>
            电话引擎接口，提供基本的通话API。
        </para>
    </description>
    <methods>
<self>
    <method>
        <method_name>GetCurrentCallInfo</method_name>
        <method_url>..\interface\IPhoneBase\GetCurrentCallInfo</method_url>
    <m_description>
            获取当前所有通话的信息。
    </m_description>
    </method>
    <method>
        <method_name>GetIncomingCallInfo</method_name>
        <method_url>..\interface\IPhoneBase\GetIncomingCallInfo</method_url>
    <m_description>
            获取当前来电的通话信息。
    </m_description>
    </method>
    <method>
        <method_name>GetActiveCallInfo</method_name>
        <method_url>..\interface\IPhoneBase\GetActiveCallInfo</method_url>
    <m_description>
            获取当前活动通话的信息。
    </m_description>
    </method>
    <method>
        <method_name>GetHeldCallInfo</method_name>
        <method_url>..\interface\IPhoneBase\GetHeldCallInfo</method_url>
    <m_description>
            获取当前保持通话的信息。
    </m_description>
    </method>
    <method>
        <method_name>GetDialingCallInfo</method_name>
        <method_url>..\interface\IPhoneBase\GetDialingCallInfo</method_url>
    <m_description>
            获取当前拨号中的通话信息。
    </m_description>
    </method>
    <method>
        <method_name>GetConversationCallInfo</method_name>
        <method_url>..\interface\IPhoneBase\GetConversationCallInfo</method_url>
    <m_description>
            获取当前通话中的通话信息。
    </m_description>
    </method>
    <method>
        <method_name>IsMultiparty</method_name>
        <method_url>..\interface\IPhoneBase\IsMultiparty</method_url>
    <m_description>
            判断一个号码是否在电话会议中。
    </m_description>
    </method>
    <method>
        <method_name>GetDialStringType</method_name>
        <method_url>..\interface\IPhoneBase\GetDialStringType</method_url>
    <m_description>
            获取指定号码的拨号类型。
    </m_description>
    </method>
    <method>
        <method_name>ParseDialString</method_name>
        <method_url>..\interface\IPhoneBase\ParseDialString</method_url>
    <m_description>
            分离指定号码的号码信息和dtmf控制信息。
    </m_description>
    </method>
    <method>
        <method_name>SetTurboDialItem</method_name>
        <method_url>..\interface\IPhoneBase\SetTurboDialItem</method_url>
    <m_description>
            设置快捷拨号项。
    </m_description>
    </method>
    <method>
        <method_name>GetTurboDialItem</method_name>
        <method_url>..\interface\IPhoneBase\GetTurboDialItem</method_url>
    <m_description>
            获取快捷拨号项所对应的号码。
    </m_description>
    </method>
    <method>
        <method_name>CleanTurboDialItems</method_name>
        <method_url>..\interface\IPhoneBase\CleanTurboDialItems</method_url>
    <m_description>
            清空所有快捷拨号项。
    </m_description>
    </method>
    <method>
        <method_name>GetCurrentDialContent</method_name>
        <method_url>..\interface\IPhoneBase\GetCurrentDialContent</method_url>
    <m_description>
            获取当前拨号的信息。
    </m_description>
    </method>
</self>
    </methods>
    <requirements>
        <system>
            <system_name>
《Elastos 2.0》或更高版本            </system_name>
        </system>
        <dll>
            <dll_name>telephony.dll,database.dll</dll_name>
        </dll>
        <headerfile>
            <file_name>phonedef.h,database.h</file_name>
        </headerfile>
        <library>
            <lib_name>elastos.lib,ElCRuntime.lib</lib_name>
        </library>
    </requirements>
</manpage>