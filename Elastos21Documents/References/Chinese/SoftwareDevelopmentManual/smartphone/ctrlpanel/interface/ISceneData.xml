    <interface> ISceneData </interface>
    <description>
        <para>
            在对系统定义的情景模式和自定义情景模式操作时，数据存取在该模块中，实现操作和数据的分离
        </para>
    </description>
    <methods>
<self>
    <method>
        <method_name>GetValue</method_name>
        <method_url>..\interface\ISceneData\GetValue</method_url>
    <m_description>
            根据某一key值，获取其value值。
    </m_description>
    </method>
    <method>
        <method_name>SetValue</method_name>
        <method_url>..\interface\ISceneData\SetValue</method_url>
    <m_description>
            根据某一key值，设置其value值。
    </m_description>
    </method>
</self>
    </methods>
