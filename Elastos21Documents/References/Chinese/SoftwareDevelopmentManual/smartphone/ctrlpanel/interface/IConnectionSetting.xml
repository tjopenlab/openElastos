    <volume></volume>
    <interface> IConnectionSetting </interface>
    <description>
        <para>
            提供GPRS和CSD的属性的设置和获取。
        </para>
    </description>
    <methods>
<self>
    <method>
        <method_name>GetValue</method_name>
        <method_url>..\interface\IConnectionSetting\GetValue</method_url>
    <m_description>
            获取网络配置某项的值。
    </m_description>
    </method>
    <method>
        <method_name>SetValue</method_name>
        <method_url>..\interface\IConnectionSetting\SetValue</method_url>
    <m_description>
            设置网络配置某项的值。
    </m_description>
    </method>
</self>
    </methods>
    <requirements>
        <system>
            <system_name>
《Elastos 2.0》或更高版本            </system_name>
        </system>
    </requirements>
</manpage>