<?xml version="1.0" encoding="GB2312"?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume></volume>
<interface>IDateTimeOption</interface><method> GetCurDate </method>
    <description>
        <para>
            获取当前日期。
        </para>
    </description>
    <syntax>
        <return_val> ECODE </return_val>
        <parameters>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> EzStrBuf </param_type>
                <param_name> esbDate </param_name>
                <param_note>
                    <para>
返回的系统当前日期                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

<return>
    <para>
            方法调用成功，返回S_OK；esbDate为空，返回E_INVALIDARG。    </para>
</return>
    <requirements>
        <system>
            <system_name>
《Elastos 2.0》或更高版本            </system_name>
        </system>
    </requirements>
    <remarks>
        <para>
            日期格式为YYYY/MM/DD。
        </para>
    </remarks>
</manpage>