<?xml version="1.0" encoding="GB2312"?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/interfaceman.xsl"?>    <manpage>
    <volume></volume>
    <interface> IConfig </interface>
    <description>
        <para>
            设置、获取配置信息
        </para>
    </description>
    <methods>
<self>
    <method>
        <method_name>GetValue</method_name>
        <method_url>..\interface\IConfig\GetValue</method_url>
    <m_description>
            获取某个配置项的值。
    </m_description>
    </method>
    <method>
        <method_name>SetValue</method_name>
        <method_url>..\interface\IConfig\SetValue</method_url>
    <m_description>
            设置某个配置项的值。
    </m_description>
    </method>
    <method>
        <method_name>InsertItem</method_name>
        <method_url>..\interface\IConfig\InsertItem</method_url>
    <m_description>
            添加新的配置项。
    </m_description>
    </method>
    <method>
        <method_name>DelItem</method_name>
        <method_url>..\interface\IConfig\DelItem</method_url>
    <m_description>
            删除某个非系统配置项。
    </m_description>
    </method>
</self>
    </methods>
    <requirements>
        <system>
            <system_name>
《Elastos 2.0》或更高版本            </system_name>
        </system>
    </requirements>
    <remarks>
        <para>
            该接口是针对配置信息存储的文件进行查询和更新。
        </para>
    </remarks>
</manpage>