<?xml version="1.0" encoding="GB2312"?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/interfaceman.xsl"?>    <manpage>
    <volume></volume>
    <interface> INetworkOption </interface>
    <description>
        <para>
            网络设置：对网络连接的各项参数进行设置。
        </para>
    </description>
    <methods>
<self>
    <method>
        <method_name>GetCurConSetting</method_name>
        <method_url>..\interface\INetworkOption\GetCurConSetting</method_url>
    <m_description>
            获得默认的网络配置。
    </m_description>
    </method>
    <method>
        <method_name>SetCurConSetting</method_name>
        <method_url>..\interface\INetworkOption\SetCurConSetting</method_url>
    <m_description>
            设置默认的网络配置。
    </m_description>
    </method>
    <method>
        <method_name>GetConSettingByName</method_name>
        <method_url>..\interface\INetworkOption\GetConSettingByName</method_url>
    <m_description>
            根据设备名称获取网络配置。
    </m_description>
    </method>
    <method>
        <method_name>CreateConSetting</method_name>
        <method_url>..\interface\INetworkOption\CreateConSetting</method_url>
    <m_description>
            创建空的网络配置。
    </m_description>
    </method>
    <method>
        <method_name>AddConSetting</method_name>
        <method_url>..\interface\INetworkOption\AddConSetting</method_url>
    <m_description>
            往数据库添加一条网络配置记录。
    </m_description>
    </method>
    <method>
        <method_name>DelConSettingByName</method_name>
        <method_url>..\interface\INetworkOption\DelConSettingByName</method_url>
    <m_description>
            根据设备名称删除某个网络配置。
    </m_description>
    </method>
    <method>
        <method_name>DelAllConSetting</method_name>
        <method_url>..\interface\INetworkOption\DelAllConSetting</method_url>
    <m_description>
            删除数据库中所有的网络配置。
    </m_description>
    </method>
    <method>
        <method_name>ModifyConSetting</method_name>
        <method_url>..\interface\INetworkOption\ModifyConSetting</method_url>
    <m_description>
            更新某个网络配置。
    </m_description>
    </method>
    <method>
        <method_name>ListConnections</method_name>
        <method_url>..\interface\INetworkOption\ListConnections</method_url>
    <m_description>
            列表显示网络配置的设备名称。
    </m_description>
    </method>
    <method>
        <method_name>ActiveCurConnection</method_name>
        <method_url>..\interface\INetworkOption\ActiveCurConnection</method_url>
    <m_description>
            设置某个网络配置为默认配置。
    </m_description>
    </method>
    <method>
        <method_name>GetConnectionCount</method_name>
        <method_url>..\interface\INetworkOption\GetConnectionCount</method_url>
    <m_description>
            获得当前数据库中的网络配置的总数。
    </m_description>
    </method>
    <method>
        <method_name>IsNameDuplicate</method_name>
        <method_url>..\interface\INetworkOption\IsNameDuplicate</method_url>
    <m_description>
            根据设备名称判断某个网络配置是否存在。
    </m_description>
    </method>
</self>
    </methods>
    <requirements>
        <system>
            <system_name>
《Elastos 2.0》或更高版本            </system_name>
        </system>
    </requirements>
</manpage>