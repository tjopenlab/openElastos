<?xml version="1.0" encoding="GB2312"?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/custom_type.xsl"?>    <manpage>
    <volume>照相摄像引擎模块</volume>
    <types> CamParam </types>
    <description>
        <para>
            相机参数枚举类型
        </para>
    </description>
<enum>    <enum_member>    <mem_name> CamParam_PhotoResolution </mem_name>    <mem_value> 0x0 </mem_value>    <mem_description>        照片分辨率等级    </mem_description>    </enum_member>    <enum_member>    <mem_name> CamParam_PhotoDimensions </mem_name>    <mem_value> 0x1 </mem_value>    <mem_description>        拍照具体分辨率    </mem_description>    </enum_member>    <enum_member>    <mem_name> CamParam_VideoResolution </mem_name>    <mem_value> 0x2 </mem_value>    <mem_description>        摄像分辨率等级    </mem_description>    </enum_member>    <enum_member>    <mem_name> CamParam_VideoDimensions </mem_name>    <mem_value> 0x3 </mem_value>    <mem_description>        摄像具体分辨率    </mem_description>    </enum_member>    <enum_member>    <mem_name> CamParam_FrameRate </mem_name>    <mem_value> 0x4 </mem_value>    <mem_description>        帧刷新率    </mem_description>    </enum_member>    <enum_member>    <mem_name> CamParam_ZoomRatio </mem_name>    <mem_value> 0x5 </mem_value>    <mem_description>        变焦比例    </mem_description>    </enum_member>    <enum_member>    <mem_name> CamParam_Brightness </mem_name>    <mem_value> 0x6 </mem_value>    <mem_description>        亮度    </mem_description>    </enum_member>    <enum_member>    <mem_name> CamParam_Contrast </mem_name>    <mem_value> 0x7 </mem_value>    <mem_description>        对比度    </mem_description>    </enum_member>    <enum_member>    <mem_name> CamParam_Sharpness </mem_name>    <mem_value> 0x8 </mem_value>    <mem_description>        锐利度    </mem_description>    </enum_member>    <enum_member>    <mem_name> CamParam_WhiteBalance </mem_name>    <mem_value> 0x9 </mem_value>    <mem_description>        白平衡    </mem_description>    </enum_member>    <enum_member>    <mem_name> CamParam_ColorMode </mem_name>    <mem_value> 0xa </mem_value>    <mem_description>        色彩模式    </mem_description>    </enum_member>    <enum_member>    <mem_name> CamParam_SceneMode </mem_name>    <mem_value> 0xb </mem_value>    <mem_description>        情景模式    </mem_description>    </enum_member>    <enum_member>    <mem_name> CamParam_Direction </mem_name>    <mem_value> 0xc </mem_value>    <mem_description>        拍摄方向    </mem_description>    </enum_member>    <enum_member>    <mem_name> CamParam_Flashlight </mem_name>    <mem_value> 0xd </mem_value>    <mem_description>        闪光灯控制    </mem_description>    </enum_member>    <enum_member>    <mem_name> CamParam_ISO </mem_name>    <mem_value> 0xe </mem_value>    <mem_description>        感光值    </mem_description>    </enum_member>    <enum_member>    <mem_name> CamParam_ExpCompansation </mem_name>    <mem_value> 0xf </mem_value>    <mem_description>        曝光补偿    </mem_description>    </enum_member>    <enum_member>    <mem_name> CamParam_Snapshots </mem_name>    <mem_value> 0x10 </mem_value>    <mem_description>        快照张数    </mem_description>    </enum_member>    <enum_member>    <mem_name> CamParam_SnapshotSize </mem_name>    <mem_value> 0x11 </mem_value>    <mem_description>        每张照片所占缓冲区的大小    </mem_description>    </enum_member></enum>    <remarks>
        <para>
            CamParam_PhotoResolution、CamParam_VideoResolution只能作为Set 参数类型；CamParam_PhotoDimensions、CamParam_VideoDimensions、CamParam_SnapshotSize只能作为Get 参数类型。其他的参数可同时作为Get和Set参数类型。
        </para>
    </remarks>
</manpage>