<?xml version="1.0" encoding="GB2312"?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/interfaceman.xsl"?>    <manpage>
    <volume>短信彩信引擎</volume>
    <interface> IMmsParser </interface>
    <description>
        <para>
            提供彩信PDU数据的解析。
        </para>
    </description>
    <methods>
<self>
    <method>
        <method_name>ParseRetrieveConf</method_name>
        <method_url>..\interface\IMmsParser\ParseRetrieveConf</method_url>
    <m_description>
            解析接收到的彩信PDU数据。
    </m_description>
    </method>
    <method>
        <method_name>SmilParserEntry</method_name>
        <method_url>..\interface\IMmsParser\SmilParserEntry</method_url>
    <m_description>
            申请空间，通过Smil（同步多媒体集成语言）解析彩信。
    </m_description>
    </method>
    <method>
        <method_name>SmilFree</method_name>
        <method_url>..\interface\IMmsParser\SmilFree</method_url>
    <m_description>
            释放空间解析使用的空间。
    </m_description>
    </method>
    <method>
        <method_name>InitCurPar</method_name>
        <method_url>..\interface\IMmsParser\InitCurPar</method_url>
    <m_description>
            初始化解析得到的页面结构，使得播放页面指向彩信数据的首页。
    </m_description>
    </method>
    <method>
        <method_name>GetDuration</method_name>
        <method_url>..\interface\IMmsParser\GetDuration</method_url>
    <m_description>
            获取当前页面的播放延迟时间。
    </m_description>
    </method>
    <method>
        <method_name>NextSlide</method_name>
        <method_url>..\interface\IMmsParser\NextSlide</method_url>
    <m_description>
            移动彩信当前的播放页面指针，指向下一页。
    </m_description>
    </method>
    <method>
        <method_name>PrevSlide</method_name>
        <method_url>..\interface\IMmsParser\PrevSlide</method_url>
    <m_description>
            移动彩信当前的播放页面指针，指向上一页。
    </m_description>
    </method>
    <method>
        <method_name>GetDeliveryIndSize</method_name>
        <method_url>..\interface\IMmsParser\GetDeliveryIndSize</method_url>
    <m_description>
            获取传送报告的大小。
    </m_description>
    </method>
    <method>
        <method_name>ParseDeliveryInd</method_name>
        <method_url>..\interface\IMmsParser\ParseDeliveryInd</method_url>
    <m_description>
            解析接收到的传送报告数据。
    </m_description>
    </method>
    <method>
        <method_name>ParseNotificationInd</method_name>
        <method_url>..\interface\IMmsParser\ParseNotificationInd</method_url>
    <m_description>
            获取彩信通知消息的索引。
    </m_description>
    </method>
    <method>
        <method_name>GetCurText</method_name>
        <method_url>..\interface\IMmsParser\GetCurText</method_url>
    <m_description>
            获取当前页的文本资源。
    </m_description>
    </method>
    <method>
        <method_name>GetCurImage</method_name>
        <method_url>..\interface\IMmsParser\GetCurImage</method_url>
    <m_description>
            获取当前页的图像资源。
    </m_description>
    </method>
    <method>
        <method_name>GetCurAudio</method_name>
        <method_url>..\interface\IMmsParser\GetCurAudio</method_url>
    <m_description>
            获取当前页的音频资源。
    </m_description>
    </method>
    <method>
        <method_name>GetCurVideo</method_name>
        <method_url>..\interface\IMmsParser\GetCurVideo</method_url>
    <m_description>
            获取当前页的视频资源。
    </m_description>
    </method>
</self>
    </methods>
    <requirements>
        <system>
            <system_name>
《Elastos 2.0》或更高版本            </system_name>
        </system>
        <headerfile>
            <file_name>elatype.h、stdlib.h、mmsdef.h、mmstype.h、CMmsParser.h、_CMmsParser.h</file_name>
        </headerfile>
        <library>
            <lib_name>ElDatabase.lib、elastos.lib、ElCRuntime.lib、ElUtilities.lib</lib_name>
        </library>
    </requirements>
</manpage>