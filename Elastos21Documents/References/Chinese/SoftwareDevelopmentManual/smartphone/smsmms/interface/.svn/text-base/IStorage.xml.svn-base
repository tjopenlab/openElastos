<?xml version="1.0" encoding="GB2312"?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/interfaceman.xsl"?>    <manpage>
    <volume>短信彩信引擎</volume>
    <interface> IStorage </interface>
    <description>
        <para>
            提供与信息存储相关的方法。
        </para>
    </description>
    <methods>
<self>
    <method>
        <method_name>AddMessageBox</method_name>
        <method_url>..\interface\IStorage\AddMessageBox</method_url>
    <m_description>
            添加信息箱。
    </m_description>
    </method>
    <method>
        <method_name>GetMessageBoxID</method_name>
        <method_url>..\interface\IStorage\GetMessageBoxID</method_url>
    <m_description>
            根据信息箱名称获取所对应的信息箱的索引。
    </m_description>
    </method>
    <method>
        <method_name>DelMessageBox</method_name>
        <method_url>..\interface\IStorage\DelMessageBox</method_url>
    <m_description>
            删除指定信息箱的信息记录。
    </m_description>
    </method>
    <method>
        <method_name>UpdateMessageBox</method_name>
        <method_url>..\interface\IStorage\UpdateMessageBox</method_url>
    <m_description>
            更新信息箱。
    </m_description>
    </method>
    <method>
        <method_name>GetMessageBox</method_name>
        <method_url>..\interface\IStorage\GetMessageBox</method_url>
    <m_description>
            获取信息箱的完整信息项。
    </m_description>
    </method>
    <method>
        <method_name>AccessMessageBox</method_name>
        <method_url>..\interface\IStorage\AccessMessageBox</method_url>
    <m_description>
            遍历信息箱。
    </m_description>
    </method>
    <method>
        <method_name>FormatMsgTime</method_name>
        <method_url>..\interface\IStorage\FormatMsgTime</method_url>
    <m_description>
            解析信息时间成相应格式。
    </m_description>
    </method>
    <method>
        <method_name>PutMmsData</method_name>
        <method_url>..\interface\IStorage\PutMmsData</method_url>
    <m_description>
            添加或更新彩信的发送、接收信息项。
    </m_description>
    </method>
    <method>
        <method_name>PutSmsData</method_name>
        <method_url>..\interface\IStorage\PutSmsData</method_url>
    <m_description>
            添加或更新短信的发送、接收信息项。
    </m_description>
    </method>
    <method>
        <method_name>PutWapData</method_name>
        <method_url>..\interface\IStorage\PutWapData</method_url>
    <m_description>
            添加或更新WAP信息的接收信息项。
    </m_description>
    </method>
    <method>
        <method_name>GetMmsData</method_name>
        <method_url>..\interface\IStorage\GetMmsData</method_url>
    <m_description>
            获取彩信的数据元素封装。
    </m_description>
    </method>
    <method>
        <method_name>GetSmsData</method_name>
        <method_url>..\interface\IStorage\GetSmsData</method_url>
    <m_description>
            获取短信的数据元素封装。
    </m_description>
    </method>
    <method>
        <method_name>GetWapData</method_name>
        <method_url>..\interface\IStorage\GetWapData</method_url>
    <m_description>
            获取WAP信息的数据元素封装。
    </m_description>
    </method>
    <method>
        <method_name>AccessMessage</method_name>
        <method_url>..\interface\IStorage\AccessMessage</method_url>
    <m_description>
            遍历信息。
    </m_description>
    </method>
    <method>
        <method_name>DelMessage</method_name>
        <method_url>..\interface\IStorage\DelMessage</method_url>
    <m_description>
            删出指定的信息数据。
    </m_description>
    </method>
    <method>
        <method_name>DelAllMessage</method_name>
        <method_url>..\interface\IStorage\DelAllMessage</method_url>
    <m_description>
            删除指定信息箱内的所有信息。
    </m_description>
    </method>
    <method>
        <method_name>GetUnReadMessageInfo</method_name>
        <method_url>..\interface\IStorage\GetUnReadMessageInfo</method_url>
    <m_description>
            获取某一类型未读信息的总数、号码和时间，供ASR模块使用。
    </m_description>
    </method>
    <method>
        <method_name>AddResource</method_name>
        <method_url>..\interface\IStorage\AddResource</method_url>
    <m_description>

    </m_description>
    </method>
    <method>
        <method_name>DelResource</method_name>
        <method_url>..\interface\IStorage\DelResource</method_url>
    <m_description>
            删除指定的资源。
    </m_description>
    </method>
    <method>
        <method_name>UpdateResource</method_name>
        <method_url>..\interface\IStorage\UpdateResource</method_url>
    <m_description>
            更新指定的资源。
    </m_description>
    </method>
    <method>
        <method_name>GetResource</method_name>
        <method_url>..\interface\IStorage\GetResource</method_url>
    <m_description>
            获取指定的资源。
    </m_description>
    </method>
    <method>
        <method_name>GetResourceSize</method_name>
        <method_url>..\interface\IStorage\GetResourceSize</method_url>
    <m_description>
            获取指定资源的大小。
    </m_description>
    </method>
    <method>
        <method_name>AccessResource</method_name>
        <method_url>..\interface\IStorage\AccessResource</method_url>
    <m_description>
            遍历数据库中的资源。
    </m_description>
    </method>
    <method>
        <method_name>ClearResources</method_name>
        <method_url>..\interface\IStorage\ClearResources</method_url>
    <m_description>
            根据类型清空数据库中的资源。
    </m_description>
    </method>
    <method>
        <method_name>GetResourceNum</method_name>
        <method_url>..\interface\IStorage\GetResourceNum</method_url>
    <m_description>
            获取指定的资源类型在数据库中的数目。
    </m_description>
    </method>
</self>
    </methods>
    <requirements>
        <system>
            <system_name>
《Elastos 2.0》或更高版本            </system_name>
        </system>
        <headerfile>
            <file_name>elatype.h、stdlib.h、mmsdef.h、mmstype.h、CStorage.h、_CStorage.h</file_name>
        </headerfile>
        <library>
            <lib_name>ElDatabase.lib、elastos.lib、ElCRuntime.lib、ElUtilities.lib</lib_name>
        </library>
    </requirements>
</manpage>