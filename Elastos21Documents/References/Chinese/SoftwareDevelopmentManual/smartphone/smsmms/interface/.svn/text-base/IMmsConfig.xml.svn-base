<?xml version="1.0" encoding="GB2312"?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/interfaceman.xsl"?>    <manpage>
    <volume>短信彩信引擎</volume>
    <interface> IMmsConfig </interface>
    <description>
        <para>
            提供彩信的配置，包括发送报告、过期时间、网络设置等。
        </para>
    </description>
    <methods>
<self>
    <method>
        <method_name>GetMmsExpiry</method_name>
        <method_url>..\interface\IMmsConfig\GetMmsExpiry</method_url>
    <m_description>
            获取彩信的过期时间（过期时间是指彩信在服务中心存储的时间期限）。
    </m_description>
    </method>
    <method>
        <method_name>SetMmsExpiry</method_name>
        <method_url>..\interface\IMmsConfig\SetMmsExpiry</method_url>
    <m_description>
            设置彩信的过期时间（过期时间是指彩信在服务中心存储的时间期限）。
    </m_description>
    </method>
    <method>
        <method_name>GetMaxServerSptPdu</method_name>
        <method_url>..\interface\IMmsConfig\GetMaxServerSptPdu</method_url>
    <m_description>
            获取彩信服务端所支持的最大数据量。
    </m_description>
    </method>
    <method>
        <method_name>SetMaxServerSptPdu</method_name>
        <method_url>..\interface\IMmsConfig\SetMaxServerSptPdu</method_url>
    <m_description>
            设置彩信服务端所支持的最大数据量。
    </m_description>
    </method>
    <method>
        <method_name>GetMaxClientSptPdu</method_name>
        <method_url>..\interface\IMmsConfig\GetMaxClientSptPdu</method_url>
    <m_description>
            获取彩信客户端所支持的最大数据量。
    </m_description>
    </method>
    <method>
        <method_name>SetMaxClientSptPdu</method_name>
        <method_url>..\interface\IMmsConfig\SetMaxClientSptPdu</method_url>
    <m_description>
            设置彩信客户端所支持的最大数据量。
    </m_description>
    </method>
    <method>
        <method_name>GetSenderVisibility</method_name>
        <method_url>..\interface\IMmsConfig\GetSenderVisibility</method_url>
    <m_description>
            获取彩信的发送是否匿名。
    </m_description>
    </method>
    <method>
        <method_name>SetSenderVisibility</method_name>
        <method_url>..\interface\IMmsConfig\SetSenderVisibility</method_url>
    <m_description>
            设置彩信的发送是否匿名。
    </m_description>
    </method>
    <method>
        <method_name>GetAllowDeliveryReportStatus</method_name>
        <method_url>..\interface\IMmsConfig\GetAllowDeliveryReportStatus</method_url>
    <m_description>
            获取彩信的发送是否允许状态报告。
    </m_description>
    </method>
    <method>
        <method_name>SetAllowDeliveryReportStatus</method_name>
        <method_url>..\interface\IMmsConfig\SetAllowDeliveryReportStatus</method_url>
    <m_description>
            设置彩信的发送是否允许状态报告。
    </m_description>
    </method>
    <method>
        <method_name>GetNeedDeliveryReportStatus</method_name>
        <method_url>..\interface\IMmsConfig\GetNeedDeliveryReportStatus</method_url>
    <m_description>
            获取彩信的发送是否需要状态报告。
    </m_description>
    </method>
    <method>
        <method_name>SetNeedDeliveryReportStatus</method_name>
        <method_url>..\interface\IMmsConfig\SetNeedDeliveryReportStatus</method_url>
    <m_description>
            设置彩信的发送是否需要状态报告。
    </m_description>
    </method>
    <method>
        <method_name>GetNeedReadReportStatus</method_name>
        <method_url>..\interface\IMmsConfig\GetNeedReadReportStatus</method_url>
    <m_description>
            获取是否需要彩信的阅读状态报告。
    </m_description>
    </method>
    <method>
        <method_name>SetNeedReadReportStatus</method_name>
        <method_url>..\interface\IMmsConfig\SetNeedReadReportStatus</method_url>
    <m_description>
            设置是否需要彩信的阅读状态报告。
    </m_description>
    </method>
    <method>
        <method_name>GetMessagePriority</method_name>
        <method_url>..\interface\IMmsConfig\GetMessagePriority</method_url>
    <m_description>
            获取彩信的重要性。
    </m_description>
    </method>
    <method>
        <method_name>SetMessagePriority</method_name>
        <method_url>..\interface\IMmsConfig\SetMessagePriority</method_url>
    <m_description>
            设置彩信的重要性。
    </m_description>
    </method>
    <method>
        <method_name>GetMmsDefaultSetup</method_name>
        <method_url>..\interface\IMmsConfig\GetMmsDefaultSetup</method_url>
    <m_description>
            获取彩信的默认设置。
    </m_description>
    </method>
    <method>
        <method_name>SetMmsDefaultSetup</method_name>
        <method_url>..\interface\IMmsConfig\SetMmsDefaultSetup</method_url>
    <m_description>
            设置彩信的默认配置。
    </m_description>
    </method>
    <method>
        <method_name>GetMmsSetupContent</method_name>
        <method_url>..\interface\IMmsConfig\GetMmsSetupContent</method_url>
    <m_description>
            根据指定的配置名称获取彩信的配置。
    </m_description>
    </method>
    <method>
        <method_name>AccessMmsSetupContent</method_name>
        <method_url>..\interface\IMmsConfig\AccessMmsSetupContent</method_url>
    <m_description>
            根据指定模式遍历彩信的配置。
    </m_description>
    </method>
    <method>
        <method_name>AddMmsSetupContent</method_name>
        <method_url>..\interface\IMmsConfig\AddMmsSetupContent</method_url>
    <m_description>
            添加彩信的配置。
    </m_description>
    </method>
    <method>
        <method_name>DelMmsSetupContent</method_name>
        <method_url>..\interface\IMmsConfig\DelMmsSetupContent</method_url>
    <m_description>
            根据指定的配置名称删除彩信的配置。
    </m_description>
    </method>
    <method>
        <method_name>UpdateMmsSetupContent</method_name>
        <method_url>..\interface\IMmsConfig\UpdateMmsSetupContent</method_url>
    <m_description>
            更新彩信的配置。
    </m_description>
    </method>
</self>
    </methods>
    <requirements>
        <system>
            <system_name>
《Elastos 2.0》或更高版本            </system_name>
        </system>
        <headerfile>
            <file_name>elatype.h、stdlib.h、mmsdef.h、mmstype.h、</file_name>
        </headerfile>
        <library>
            <lib_name>ElDatabase.lib、elastos.lib、ElCRuntime.lib、ElUtilities.lib</lib_name>
        </library>
    </requirements>
</manpage>