<?xml version="1.0" encoding="GB2312"?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/interfaceman.xsl"?>    <manpage>
    <volume>短信彩信引擎</volume>
    <interface> ISmsEngine </interface>
    <description>
        <para>
            提供短信发送的操作。
        </para>
    </description>
    <methods>
<self>
    <method>
        <method_name>SendSmsData</method_name>
        <method_url>..\interface\ISmsEngine\SendSmsData</method_url>
    <m_description>
            异步方式发送短信数据。
    </m_description>
    </method>
    <method>
        <method_name>SetServiceConfig</method_name>
        <method_url>..\interface\ISmsEngine\SetServiceConfig</method_url>
    <m_description>
            设置短信服务配置。
    </m_description>
    </method>
    <method>
        <method_name>SetBroadcastMsgRanges</method_name>
        <method_url>..\interface\ISmsEngine\SetBroadcastMsgRanges</method_url>
    <m_description>
            设置短信广播的频段域值。
    </m_description>
    </method>
    <method>
        <method_name>SetMessageNotification</method_name>
        <method_url>..\interface\ISmsEngine\SetMessageNotification</method_url>
    <m_description>
            设置新到信息的提示方式。
    </m_description>
    </method>
    <method>
        <method_name>GetSMSC</method_name>
        <method_url>..\interface\ISmsEngine\GetSMSC</method_url>
    <m_description>
            获取短信中心号码。
    </m_description>
    </method>
    <method>
        <method_name>AddEventHandler</method_name>
        <method_url>..\interface\ISmsEngine\AddEventHandler</method_url>
    <m_description>
            为引擎注册事件处理函数。
    </m_description>
    </method>
    <method>
        <method_name>RemoveEventHandler</method_name>
        <method_url>..\interface\ISmsEngine\RemoveEventHandler</method_url>
    <m_description>
            注销引擎内注册事件处理函数。
    </m_description>
    </method>
</self>
    </methods>
    <requirements>
        <system>
            <system_name>
《Elastos 2.0》或更高版本            </system_name>
        </system>
        <headerfile>
            <file_name>elatype.h、stdlib.h、mmsdef.h、mmstype.h</file_name>
        </headerfile>
        <library>
            <lib_name>ElDatabase.lib、elastos.lib、ElCRuntime.lib、ElUtilities.lib</lib_name>
        </library>
    </requirements>
</manpage>