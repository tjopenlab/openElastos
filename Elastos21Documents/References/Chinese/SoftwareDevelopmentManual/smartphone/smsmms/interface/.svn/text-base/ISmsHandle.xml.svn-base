<?xml version="1.0" encoding="GB2312"?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/interfaceman.xsl"?>    <manpage>
    <volume>短信彩信引擎</volume>
    <interface> ISmsHandle </interface>
    <description>
        <para>
            提供短信的接收方法的注册。
        </para>
    </description>
    <methods>
<self>
    <method>
        <method_name>OnSmsMessage</method_name>
        <method_url>..\interface\ISmsHandle\OnSmsMessage</method_url>
    <m_description>
            提供短信接收时的UI状态流程交互回调处理。
    </m_description>
    </method>
    <method>
        <method_name>OnCbmMessage</method_name>
        <method_url>..\interface\ISmsHandle\OnCbmMessage</method_url>
    <m_description>
            提供广播接收时的UI状态流程交互回调处理。
    </m_description>
    </method>
    <method>
        <method_name>OnSmsStatusReport</method_name>
        <method_url>..\interface\ISmsHandle\OnSmsStatusReport</method_url>
    <m_description>
            提供短信状态报告接收时的UI状态流程交互回调处理。
    </m_description>
    </method>
</self>
    </methods>
    <requirements>
        <system>
            <system_name>
《Elastos 2.0》或更高版本            </system_name>
        </system>
        <headerfile>
            <file_name>elatype.h、stdlib.h、mmsdef.h、mmstype.h</file_name>
        </headerfile>
        <library>
            <lib_name>ElDatabase.lib、elastos.lib、ElCRuntime.lib、ElUtilities.lib</lib_name>
        </library>
    </requirements>
</manpage>