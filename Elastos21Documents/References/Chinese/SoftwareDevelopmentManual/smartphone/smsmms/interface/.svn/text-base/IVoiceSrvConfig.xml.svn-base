<?xml version="1.0" encoding="GB2312"?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/interfaceman.xsl"?>    <manpage>
    <volume>短信彩信引擎</volume>
    <interface> IVoiceSrvConfig </interface>
    <description>
        <para>
            提供语音信箱的配置。
        </para>
    </description>
    <methods>
<self>
    <method>
        <method_name>GetVoiceMailbox</method_name>
        <method_url>..\interface\IVoiceSrvConfig\GetVoiceMailbox</method_url>
    <m_description>
            获取指定语音信箱的配置。
    </m_description>
    </method>
    <method>
        <method_name>SetVoiceMailbox</method_name>
        <method_url>..\interface\IVoiceSrvConfig\SetVoiceMailbox</method_url>
    <m_description>
            设置指定语音信箱的配置。
    </m_description>
    </method>
</self>
    </methods>
    <requirements>
        <system>
            <system_name>
《Elastos 2.0》或更高版本            </system_name>
        </system>
        <headerfile>
            <file_name>elatype.h、stdlib.h、mmsdef.h、mmstype.h、</file_name>
        </headerfile>
        <library>
            <lib_name>ElDatabase.lib、elastos.lib、ElCRuntime.lib、ElUtilities.lib</lib_name>
        </library>
    </requirements>
</manpage>