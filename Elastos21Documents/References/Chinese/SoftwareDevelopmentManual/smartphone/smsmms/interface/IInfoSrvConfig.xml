<?xml version="1.0" encoding="GB2312"?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/interfaceman.xsl"?>    <manpage>
    <volume>短信彩信引擎</volume>
    <interface> IInfoSrvConfig </interface>
    <description>
        <para>
            提供信息频道服务相关选项的配置。
        </para>
    </description>
    <methods>
<self>
    <method>
        <method_name>GetServiceStatus</method_name>
        <method_url>..\interface\IInfoSrvConfig\GetServiceStatus</method_url>
    <m_description>
            获取信息的服务状态。
    </m_description>
    </method>
    <method>
        <method_name>SetServiceStatus</method_name>
        <method_url>..\interface\IInfoSrvConfig\SetServiceStatus</method_url>
    <m_description>
            设置信息的服务状态。
    </m_description>
    </method>
    <method>
        <method_name>AddChanel</method_name>
        <method_url>..\interface\IInfoSrvConfig\AddChanel</method_url>
    <m_description>
            添加频道。
    </m_description>
    </method>
    <method>
        <method_name>DelChanel</method_name>
        <method_url>..\interface\IInfoSrvConfig\DelChanel</method_url>
    <m_description>
            删除频道。
    </m_description>
    </method>
    <method>
        <method_name>AccessChanel</method_name>
        <method_url>..\interface\IInfoSrvConfig\AccessChanel</method_url>
    <m_description>
            遍历所有的频道。
    </m_description>
    </method>
    <method>
        <method_name>GetChanelLanguage</method_name>
        <method_url>..\interface\IInfoSrvConfig\GetChanelLanguage</method_url>
    <m_description>
            根据指定的语言索引号获取频道语言。
    </m_description>
    </method>
    <method>
        <method_name>SetChanelLanguage</method_name>
        <method_url>..\interface\IInfoSrvConfig\SetChanelLanguage</method_url>
    <m_description>
            根据指定的语言索引号设置频道语言。
    </m_description>
    </method>
</self>
    </methods>
    <requirements>
        <system>
            <system_name>
《Elastos 2.0》或更高版本            </system_name>
        </system>
        <headerfile>
            <file_name>elatype.h、stdlib.h、mmsdef.h、mmstype.h、</file_name>
        </headerfile>
        <library>
            <lib_name>ElDatabase.lib、elastos.lib、ElCRuntime.lib、ElUtilities.lib</lib_name>
        </library>
    </requirements>
</manpage>