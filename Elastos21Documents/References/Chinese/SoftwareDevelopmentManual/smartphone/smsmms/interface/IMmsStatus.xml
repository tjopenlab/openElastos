<?xml version="1.0" encoding="GB2312"?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/interfaceman.xsl"?>    <manpage>
    <volume>短信彩信引擎</volume>
    <interface> IMmsStatus </interface>
    <description>
        <para>
            提供彩信状态的通讯控制。
        </para>
    </description>
    <methods>
<self>
    <method>
        <method_name>OnWapConnect</method_name>
        <method_url>..\interface\IMmsStatus\OnWapConnect</method_url>
    <m_description>
            彩信发送及接收过程中的UI状态流程交互，获取WAP信息接到网关的状态。
    </m_description>
    </method>
    <method>
        <method_name>OnMmsStream</method_name>
        <method_url>..\interface\IMmsStatus\OnMmsStream</method_url>
    <m_description>
            彩信发送及接收过程中的UI状态流程交互，获取或送出数据的进度。
    </m_description>
    </method>
    <method>
        <method_name>OnMmsReceiving</method_name>
        <method_url>..\interface\IMmsStatus\OnMmsReceiving</method_url>
    <m_description>
            彩信接收过程中的UI状态流程交互，获取接收状态值。
    </m_description>
    </method>
    <method>
        <method_name>OnMmsSending</method_name>
        <method_url>..\interface\IMmsStatus\OnMmsSending</method_url>
    <m_description>
            彩信接收过程中的UI状态流程交互，获取发送状态。
    </m_description>
    </method>
    <method>
        <method_name>OnMmsReceived</method_name>
        <method_url>..\interface\IMmsStatus\OnMmsReceived</method_url>
    <m_description>
            彩信接收过程中的UI状态流程交互，获取接收到的彩信数据。
    </m_description>
    </method>
</self>
    </methods>
    <requirements>
        <system>
            <system_name>
《Elastos 2.0》或更高版本            </system_name>
        </system>
        <headerfile>
            <file_name>elatype.h、stdlib.h、mmsdef.h、mmstype.h</file_name>
        </headerfile>
        <library>
            <lib_name>ElDatabase.lib、elastos.lib、ElCRuntime.lib、ElUtilities.lib</lib_name>
        </library>
    </requirements>
</manpage>