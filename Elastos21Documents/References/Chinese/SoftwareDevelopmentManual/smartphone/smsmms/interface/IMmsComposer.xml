<?xml version="1.0" encoding="GB2312"?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/interfaceman.xsl"?>    <manpage>
    <volume>短信彩信引擎</volume>
    <interface> IMmsComposer </interface>
    <description>
        <para>
            提供彩信PDU数据的操作。
        </para>
    </description>
    <methods>
<self>
    <method>
        <method_name>NewMms</method_name>
        <method_url>..\interface\IMmsComposer\NewMms</method_url>
    <m_description>
            初始化Composer模块新建彩信，不支持重入。
    </m_description>
    </method>
    <method>
        <method_name>EditMms</method_name>
        <method_url>..\interface\IMmsComposer\EditMms</method_url>
    <m_description>
            编辑已经存在的彩信数据。
    </m_description>
    </method>
    <method>
        <method_name>Quit</method_name>
        <method_url>..\interface\IMmsComposer\Quit</method_url>
    <m_description>
            释放和退出Composer模块的资源。
    </m_description>
    </method>
    <method>
        <method_name>AddSlide</method_name>
        <method_url>..\interface\IMmsComposer\AddSlide</method_url>
    <m_description>
            在当前页面的后面添加一个新的页面。
    </m_description>
    </method>
    <method>
        <method_name>DelCurSlide</method_name>
        <method_url>..\interface\IMmsComposer\DelCurSlide</method_url>
    <m_description>
            删除当前页面及其包含的媒体资源信息。
    </m_description>
    </method>
    <method>
        <method_name>SetCurSlideDelay</method_name>
        <method_url>..\interface\IMmsComposer\SetCurSlideDelay</method_url>
    <m_description>
            设置当前页面的延时时间。
    </m_description>
    </method>
    <method>
        <method_name>NextSlide</method_name>
        <method_url>..\interface\IMmsComposer\NextSlide</method_url>
    <m_description>
            移动到下一个页面。
    </m_description>
    </method>
    <method>
        <method_name>PrevSlide</method_name>
        <method_url>..\interface\IMmsComposer\PrevSlide</method_url>
    <m_description>
            移动到上一个页面。
    </m_description>
    </method>
    <method>
        <method_name>GetCurSlideIndex</method_name>
        <method_url>..\interface\IMmsComposer\GetCurSlideIndex</method_url>
    <m_description>
            获得当前页面的索引值。
    </m_description>
    </method>
    <method>
        <method_name>GetTotalSize</method_name>
        <method_url>..\interface\IMmsComposer\GetTotalSize</method_url>
    <m_description>
            获取当前编辑彩信资源的大小（只包括资源本身，不包括编码产生的额外数据）。
    </m_description>
    </method>
    <method>
        <method_name>GetMmsContentPdu</method_name>
        <method_url>..\interface\IMmsComposer\GetMmsContentPdu</method_url>
    <m_description>
            获取当前编辑彩信PDU数据的具体内容及其长度（不包括header）。
    </m_description>
    </method>
    <method>
        <method_name>MakeSendMessagePdu</method_name>
        <method_url>..\interface\IMmsComposer\MakeSendMessagePdu</method_url>
    <m_description>
            把用户添加的多媒体消息编码成PDU数据。
    </m_description>
    </method>
    <method>
        <method_name>CurImageExist</method_name>
        <method_url>..\interface\IMmsComposer\CurImageExist</method_url>
    <m_description>
            判断当前页面是否包含图像资源。
    </m_description>
    </method>
    <method>
        <method_name>CurTextExist</method_name>
        <method_url>..\interface\IMmsComposer\CurTextExist</method_url>
    <m_description>
            判断当前页面是否包含文本资源。
    </m_description>
    </method>
    <method>
        <method_name>CurAudioExist</method_name>
        <method_url>..\interface\IMmsComposer\CurAudioExist</method_url>
    <m_description>
            判断当前页面是否包含音频资源。
    </m_description>
    </method>
    <method>
        <method_name>CurVideoExist</method_name>
        <method_url>..\interface\IMmsComposer\CurVideoExist</method_url>
    <m_description>
            判断当前页面是否包含视频资源。
    </m_description>
    </method>
    <method>
        <method_name>DelCurImage</method_name>
        <method_url>..\interface\IMmsComposer\DelCurImage</method_url>
    <m_description>
            删除当前页面的图像资源。
    </m_description>
    </method>
    <method>
        <method_name>DelCurText</method_name>
        <method_url>..\interface\IMmsComposer\DelCurText</method_url>
    <m_description>
            删除当前页面的文本资源。
    </m_description>
    </method>
    <method>
        <method_name>DelCurAudio</method_name>
        <method_url>..\interface\IMmsComposer\DelCurAudio</method_url>
    <m_description>
            删除当前页面的音频资源。
    </m_description>
    </method>
    <method>
        <method_name>DelCurVideo</method_name>
        <method_url>..\interface\IMmsComposer\DelCurVideo</method_url>
    <m_description>
            删除当前页面的视频资源。
    </m_description>
    </method>
    <method>
        <method_name>AddText</method_name>
        <method_url>..\interface\IMmsComposer\AddText</method_url>
    <m_description>
            在当前页添加文本资源。
    </m_description>
    </method>
    <method>
        <method_name>AddImage</method_name>
        <method_url>..\interface\IMmsComposer\AddImage</method_url>
    <m_description>
            在当前页添加图像资源。
    </m_description>
    </method>
    <method>
        <method_name>AddAudio</method_name>
        <method_url>..\interface\IMmsComposer\AddAudio</method_url>
    <m_description>
            在当前页添加音频资源。
    </m_description>
    </method>
    <method>
        <method_name>AddVideo</method_name>
        <method_url>..\interface\IMmsComposer\AddVideo</method_url>
    <m_description>
            在当前页添加视频资源。
    </m_description>
    </method>
    <method>
        <method_name>GetCurText</method_name>
        <method_url>..\interface\IMmsComposer\GetCurText</method_url>
    <m_description>
            获取当前页文本资源信息。
    </m_description>
    </method>
    <method>
        <method_name>GetCurImage</method_name>
        <method_url>..\interface\IMmsComposer\GetCurImage</method_url>
    <m_description>
            获取当前页图像信息。
    </m_description>
    </method>
    <method>
        <method_name>GetCurAudio</method_name>
        <method_url>..\interface\IMmsComposer\GetCurAudio</method_url>
    <m_description>
            获取当前页音频信息。
    </m_description>
    </method>
    <method>
        <method_name>GetCurVideo</method_name>
        <method_url>..\interface\IMmsComposer\GetCurVideo</method_url>
    <m_description>
            获取当前页视频信息。
    </m_description>
    </method>
    <method>
        <method_name>ComposeCurAsTemplate</method_name>
        <method_url>..\interface\IMmsComposer\ComposeCurAsTemplate</method_url>
    <m_description>
            将当前页面编码作为模版。
    </m_description>
    </method>
    <method>
        <method_name>ParseTemplateIntoCurFrame</method_name>
        <method_url>..\interface\IMmsComposer\ParseTemplateIntoCurFrame</method_url>
    <m_description>
            模版解码到当前页面。
    </m_description>
    </method>
</self>
    </methods>
    <requirements>
        <system>
            <system_name>
《Elastos 2.0》或更高版本            </system_name>
        </system>
        <dll>
            <dll_name>elatype.h、stdlib.h、mmsdef.h、mmstype.h、CMmsComposer.h、_CMmsComposer.h</dll_name>
        </dll>
        <library>
            <lib_name>ElDatabase.lib、elastos.lib、ElCRuntime.lib、ElUtilities.lib</lib_name>
        </library>
    </requirements>
</manpage>