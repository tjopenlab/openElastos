<?xml version="1.0" encoding="GB2312"?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume>短信彩信引擎</volume>
<interface>IMmsSendReq</interface><method> SetDeliveryTime </method>
    <description>
        <para>
            设置彩信的发送时间。
        </para>
    </description>
    <syntax>
        <return_val> ECODE </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> LONG </param_type>
                <param_name> lTime </param_name>
                <param_note>
                    <para>
彩信的发送时间                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

<return>
    <para>
            如果方法调用成功，则返回S_OK。</para><para>            更多返回值，参见            <link url="../../sms_error.htm">            短信彩信返回值            </link>            相关帮助。    </para>
</return>
    <requirements>
        <system>
            <system_name>
《Elastos 2.0》或更高版本            </system_name>
        </system>
        <headerfile>
            <file_name>elatype.h、stdlib.h、mmsdef.h、MessageData.h、IMmsData、CMmsSendReq.h、_CMmsSendReq.h</file_name>
        </headerfile>
        <library>
            <lib_name>ElDatabase.lib、elastos.lib、ElCRuntime.lib、ElUtilities.lib</lib_name>
        </library>
    </requirements>
</manpage>