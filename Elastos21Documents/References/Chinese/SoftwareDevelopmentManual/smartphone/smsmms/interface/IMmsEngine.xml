<?xml version="1.0" encoding="GB2312"?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/interfaceman.xsl"?>    <manpage>
    <volume>短信彩信引擎</volume>
    <interface> IMmsEngine </interface>
    <description>
        <para>
            提供彩信发送控制。
        </para>
    </description>
    <methods>
<self>
    <method>
        <method_name>SetServiceConfig</method_name>
        <method_url>..\interface\IMmsEngine\SetServiceConfig</method_url>
    <m_description>
            配置彩信服务参数。
    </m_description>
    </method>
    <method>
        <method_name>SendMmsData</method_name>
        <method_url>..\interface\IMmsEngine\SendMmsData</method_url>
    <m_description>
            发送一条多媒体消息到多媒体消息中心。
    </m_description>
    </method>
    <method>
        <method_name>ReceiveMmsData</method_name>
        <method_url>..\interface\IMmsEngine\ReceiveMmsData</method_url>
    <m_description>
            根据彩信通知消息接收彩信。
    </m_description>
    </method>
    <method>
        <method_name>Stop</method_name>
        <method_url>..\interface\IMmsEngine\Stop</method_url>
    <m_description>
            停止发送或接收。
    </m_description>
    </method>
    <method>
        <method_name>AddEventHandler</method_name>
        <method_url>..\interface\IMmsEngine\AddEventHandler</method_url>
    <m_description>
            为引擎注册事件处理函数。
    </m_description>
    </method>
    <method>
        <method_name>RemoveEventHandler</method_name>
        <method_url>..\interface\IMmsEngine\RemoveEventHandler</method_url>
    <m_description>
            注销引擎内注册事件处理函数。
    </m_description>
    </method>
</self>
    </methods>
    <requirements>
        <system>
            <system_name>
《Elastos 2.0》或更高版本            </system_name>
        </system>
        <headerfile>
            <file_name>elatype.h、stdlib.h、mmsdef.h、mmstype.h、_CMmsEngine.h、CMmsEngine.h</file_name>
        </headerfile>
        <library>
            <lib_name>ElDatabase.lib、elastos.lib、ElCRuntime.lib、ElUtilities.lib</lib_name>
        </library>
    </requirements>
</manpage>