<?xml version="1.0" encoding="GB2312"?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/interfaceman.xsl"?>    <manpage>
    <volume>短信彩信引擎</volume>
    <interface> IElement </interface>
    <description>
        <para>
            提供彩信元素数据（包括文本、图像、音频、视频等）的操作。
        </para>
    </description>
    <methods>
<self>
    <method>
        <method_name>SetData</method_name>
        <method_url>..\interface\IElement\SetData</method_url>
    <m_description>
            设置彩信元素数据：文本(ASCII字符)，图像，音频，视频等。
    </m_description>
    </method>
    <method>
        <method_name>GetData</method_name>
        <method_url>..\interface\IElement\GetData</method_url>
    <m_description>
            获取彩信元素数据：文本(ASCII字符)，图像，音频，视频等。
    </m_description>
    </method>
    <method>
        <method_name>GetDataSize</method_name>
        <method_url>..\interface\IElement\GetDataSize</method_url>
    <m_description>
            获取彩信元素数据的大小。
    </m_description>
    </method>
    <method>
        <method_name>SetDataType</method_name>
        <method_url>..\interface\IElement\SetDataType</method_url>
    <m_description>
            设置彩信元素数据的类型。
    </m_description>
    </method>
    <method>
        <method_name>GetDataType</method_name>
        <method_url>..\interface\IElement\GetDataType</method_url>
    <m_description>
            获取彩信元素数据的类型。
    </m_description>
    </method>
    <method>
        <method_name>SetTimeRange</method_name>
        <method_url>..\interface\IElement\SetTimeRange</method_url>
    <m_description>
            设置彩信元素播放的时间域。
    </m_description>
    </method>
    <method>
        <method_name>GetTimeRange</method_name>
        <method_url>..\interface\IElement\GetTimeRange</method_url>
    <m_description>
            获取彩信元素播放的时间域。
    </m_description>
    </method>
    <method>
        <method_name>SetPosition</method_name>
        <method_url>..\interface\IElement\SetPosition</method_url>
    <m_description>
            设置彩信元素播放的位置域。
    </m_description>
    </method>
    <method>
        <method_name>GetPosition</method_name>
        <method_url>..\interface\IElement\GetPosition</method_url>
    <m_description>
            获取彩信元素播放的位置域。
    </m_description>
    </method>
</self>
    </methods>
    <requirements>
        <system>
            <system_name>
《Elastos 2.0》或更高版本            </system_name>
        </system>
        <headerfile>
            <file_name>elatype.h、stdlib.h、mmsdef.h、MessageData.h、CElement.h、_CElement.h</file_name>
        </headerfile>
        <library>
            <lib_name>ElDatabase.lib、elastos.lib、ElCRuntime.lib、ElUtilities.lib</lib_name>
        </library>
    </requirements>
</manpage>