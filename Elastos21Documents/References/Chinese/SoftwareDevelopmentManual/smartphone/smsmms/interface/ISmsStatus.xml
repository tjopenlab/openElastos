    <volume>短信彩信引擎</volume>
    <interface> ISmsStatus </interface>
    <description>
        <para>
            提供短信状态的通讯控制。
        </para>
    </description>
    <methods>
<self>
    <method>
        <method_name>OnSmsReceiving</method_name>
        <method_url>..\interface\ISmsStatus\OnSmsReceiving</method_url>
    <m_description>
            短信接收过程中的UI状态流程交互。
    </m_description>
    </method>
    <method>
        <method_name>OnSmsSending</method_name>
        <method_url>..\interface\ISmsStatus\OnSmsSending</method_url>
    <m_description>
            短信发送过程中的UI状态流程交互。
    </m_description>
    </method>
</self>
    </methods>
