    <interface> IClipBoard </interface>
    <description>
        <para>

        </para>
    </description>
    <methods>
<self>
    <method>
        <method_name>Empty</method_name>
        <method_url>..\interface\IClipBoard\Empty</method_url>
    <m_description>
            清空clipboard，释放剪贴板中现有数据相关的资源。
    </m_description>
    </method>
    <method>
        <method_name>GetData</method_name>
        <method_url>..\interface\IClipBoard\GetData</method_url>
    <m_description>
            获取format指定格式对应的数据。
    </m_description>
    </method>
    <method>
        <method_name>EnumFormats</method_name>
        <method_url>..\interface\IClipBoard\EnumFormats</method_url>
    <m_description>
            枚举当前剪贴板上的数据类型。
    </m_description>
    </method>
    <method>
        <method_name>IsAvailable</method_name>
        <method_url>..\interface\IClipBoard\IsAvailable</method_url>
    <m_description>
            判断剪贴板是否有指定的格式数据存在。
    </m_description>
    </method>
    <method>
        <method_name>SetData</method_name>
        <method_url>..\interface\IClipBoard\SetData</method_url>
    <m_description>
            把指定的格式的数据放到剪贴板上。
    </m_description>
    </method>
</self>
