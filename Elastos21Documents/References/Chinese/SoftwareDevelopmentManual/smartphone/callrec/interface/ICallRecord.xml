<?xml version="1.0" encoding="GB2312"?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/interfaceman.xsl"?>    <manpage>
    <volume></volume>
    <interface> ICallRecord </interface>
    <description>
        <para>
            管理手机通话记录
        </para>
    </description>
    <methods>
<self>
    <method>
        <method_name>AddRecord</method_name>
        <method_url>..\interface\ICallRecord\AddRecord</method_url>
    <m_description>
            添加一条新的空记录，需要通过返回的指针填写数据内容，并更新到数据库。
    </m_description>
    </method>
    <method>
        <method_name>GetRecordByIndex</method_name>
        <method_url>..\interface\ICallRecord\GetRecordByIndex</method_url>
    <m_description>
            获取指定类型、序号的电话记录信息，通过接口返回。
    </m_description>
    </method>
    <method>
        <method_name>BeginEnum</method_name>
        <method_url>..\interface\ICallRecord\BeginEnum</method_url>
    <m_description>
            枚举指定类型呼叫的所有通话记录。
    </m_description>
    </method>
    <method>
        <method_name>NextRecord</method_name>
        <method_url>..\interface\ICallRecord\NextRecord</method_url>
    <m_description>
            返回结果集中当前记录的下一条记录所对应的对象。
    </m_description>
    </method>
    <method>
        <method_name>EndEnum</method_name>
        <method_url>..\interface\ICallRecord\EndEnum</method_url>
    <m_description>
            完成一次查询遍历处理，释放相关资源。
    </m_description>
    </method>
    <method>
        <method_name>GetTotalTime</method_name>
        <method_url>..\interface\ICallRecord\GetTotalTime</method_url>
    <m_description>
            获取电话记录中总的通话时间和总的呼出时间。
    </m_description>
    </method>
    <method>
        <method_name>GetLastCallInfo</method_name>
        <method_url>..\interface\ICallRecord\GetLastCallInfo</method_url>
    <m_description>
            获取最后一次通话记录信息。
    </m_description>
    </method>
    <method>
        <method_name>RemoveRecords</method_name>
        <method_url>..\interface\ICallRecord\RemoveRecords</method_url>
    <m_description>
            删除所有的对应类型的记录。
    </m_description>
    </method>
    <method>
        <method_name>Remove</method_name>
        <method_url>..\interface\ICallRecord\Remove</method_url>
    <m_description>
            从数据库中删除该开始时间对应的记录。
    </m_description>
    </method>
    <method>
        <method_name>Update</method_name>
        <method_url>..\interface\ICallRecord\Update</method_url>
    <m_description>
            将ICallRecordItem中的数据更新到数据库中。
    </m_description>
    </method>
    <method>
        <method_name>Reset</method_name>
        <method_url>..\interface\ICallRecord\Reset</method_url>
    <m_description>
            清除Call_static中的数据。
    </m_description>
    </method>
</self>
    </methods>
    <requirements>
        <system>
            <system_name>
《Elastos 2.0》或更高版本            </system_name>
        </system>
    </requirements>
</manpage>