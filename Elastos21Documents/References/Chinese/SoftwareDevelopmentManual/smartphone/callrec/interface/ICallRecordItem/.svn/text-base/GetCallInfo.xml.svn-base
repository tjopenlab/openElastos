<?xml version="1.0" encoding="GB2312"?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume></volume>
<interface>ICallRecordItem</interface><method> GetCallInfo </method>
    <description>
        <para>
            具体手机通话记录访问对象。
        </para>
    </description>
    <syntax>
        <return_val> ECODE </return_val>
        <parameters>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> INT * </param_type>
                <param_name> pnIndex </param_name>
                <param_note>
                    <para>
指向INT类型的指针，用于获取通话纪录的序号                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> INT * </param_type>
                <param_name> pnStartTime </param_name>
                <param_note>
                    <para>
指向INT类型的指针，用于获取通话开始时间                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> EzStrBuf </param_type>
                <param_name> esbNumber </param_name>
                <param_note>
                    <para>
电话号码                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> INT * </param_type>
                <param_name> pnType </param_name>
                <param_note>
                    <para>
指向INT类型的指针，用于获取电话信息类型                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> INT * </param_type>
                <param_name> pnDuration </param_name>
                <param_note>
                    <para>
指向INT类型的指针，用于获取通话时间                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> INT * </param_type>
                <param_name> pnLine </param_name>
                <param_note>
                    <para>
指向INT类型的指针，用于获取通话线路                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> float * </param_type>
                <param_name> pfUint </param_name>
                <param_note>
                    <para>
指向float类型的指针，用于获取通话计费单价                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

<return>
    <para>
            方法调用成功时，返回S_OK。    </para>
</return>
    <requirements>
        <system>
            <system_name>
《Elastos 2.0》或更高版本            </system_name>
        </system>
    </requirements>
</manpage>