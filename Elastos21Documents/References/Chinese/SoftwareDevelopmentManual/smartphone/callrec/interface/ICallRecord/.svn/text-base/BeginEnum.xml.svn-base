<?xml version="1.0" encoding="GB2312"?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume></volume>
<interface>ICallRecord</interface><method> BeginEnum </method>
    <description>
        <para>
            枚举指定类型呼叫的所有通话记录。
        </para>
    </description>
    <syntax>
        <return_val> ECODE </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> INT </param_type>
                <param_name> nType </param_name>
                <param_note>
                    <para>
通话记录类型                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

<return>
    <para>
            方法调用成功时，返回S_OK。    </para>
</return>
    <requirements>
        <system>
            <system_name>
《Elastos 2.0》或更高版本            </system_name>
        </system>
    </requirements>
    <remarks>
        <para>
            nType支持            <link url="../../datatype/_CallType.htm">            CT_ALL            </link>            ，表示取得所有记录，并按照时间排序。
        </para>
    </remarks>
</manpage>