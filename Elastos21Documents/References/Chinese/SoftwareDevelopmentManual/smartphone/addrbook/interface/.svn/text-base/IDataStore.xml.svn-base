<?xml version="1.0" encoding="GB2312"?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/interfaceman.xsl"?>    <manpage>
    <volume>地址簿引擎</volume>
    <interface> IDataStore </interface>
    <description>
        <para>
            为数据库或SIM提供数据存取操作的接口。
        </para>
    </description>
    <methods>
<self>
    <method>
        <method_name>GetMaxId</method_name>
        <method_url>..\interface\IDataStore\GetMaxId</method_url>
    <m_description>
            获取当前地址簿中联系人或组记录的最大记录ID。
    </m_description>
    </method>
    <method>
        <method_name>GetStoreClass</method_name>
        <method_url>..\interface\IDataStore\GetStoreClass</method_url>
    <m_description>
            获取当前的存储类型。
    </m_description>
    </method>
    <method>
        <method_name>GetStoreCapacity</method_name>
        <method_url>..\interface\IDataStore\GetStoreCapacity</method_url>
    <m_description>
            获取当前数据库或SIM卡的容量。
    </m_description>
    </method>
    <method>
        <method_name>GetEntitiesCount</method_name>
        <method_url>..\interface\IDataStore\GetEntitiesCount</method_url>
    <m_description>
            获取当前数据库或SIM卡中的记录总数。
    </m_description>
    </method>
    <method>
        <method_name>SearchEntityById</method_name>
        <method_url>..\interface\IDataStore\SearchEntityById</method_url>
    <m_description>
            通过指定的ID，查找组或联系人。
    </m_description>
    </method>
    <method>
        <method_name>SearchEntities</method_name>
        <method_url>..\interface\IDataStore\SearchEntities</method_url>
    <m_description>
            通过组合条件，查找相应的记录
    </m_description>
    </method>
    <method>
        <method_name>SearchRelatedEntities</method_name>
        <method_url>..\interface\IDataStore\SearchRelatedEntities</method_url>
    <m_description>
            通过指定的ID，查找联系人的所属组或组中的联系人成员。
    </m_description>
    </method>
    <method>
        <method_name>AddRelation</method_name>
        <method_url>..\interface\IDataStore\AddRelation</method_url>
    <m_description>
            添加组和联系人的关系。
    </m_description>
    </method>
    <method>
        <method_name>RemoveRelation</method_name>
        <method_url>..\interface\IDataStore\RemoveRelation</method_url>
    <m_description>
            删除组和联系人的关系。
    </m_description>
    </method>
    <method>
        <method_name>RemoveAllRelation</method_name>
        <method_url>..\interface\IDataStore\RemoveAllRelation</method_url>
    <m_description>
            通过指定的ID，删除联系人与所属组的关系或删除组中的所有联系人成员。
    </m_description>
    </method>
    <method>
        <method_name>SearchAll</method_name>
        <method_url>..\interface\IDataStore\SearchAll</method_url>
    <m_description>
            查询地址簿中所有的组或联系人。
    </m_description>
    </method>
    <method>
        <method_name>RemoveAll</method_name>
        <method_url>..\interface\IDataStore\RemoveAll</method_url>
    <m_description>
            删除数据库或SIM卡中指定类型的所有记录。
    </m_description>
    </method>
    <method>
        <method_name>Save</method_name>
        <method_url>..\interface\IDataStore\Save</method_url>
    <m_description>
            保存一条记录到SIM卡或数据库。
    </m_description>
    </method>
    <method>
        <method_name>Delete</method_name>
        <method_url>..\interface\IDataStore\Delete</method_url>
    <m_description>
            从SIM卡或数据库中删除一条记录。
    </m_description>
    </method>
    <method>
        <method_name>Update</method_name>
        <method_url>..\interface\IDataStore\Update</method_url>
    <m_description>
            更新数据库或SIM卡中的一条记录。
    </m_description>
    </method>
    <method>
        <method_name>SetSortKey</method_name>
        <method_url>..\interface\IDataStore\SetSortKey</method_url>
    <m_description>
            设置排序的关键字。
    </m_description>
    </method>
    <method>
        <method_name>GetContactbyTel</method_name>
        <method_url>..\interface\IDataStore\GetContactbyTel</method_url>
    <m_description>
            通过指定的电话号码，查询联系人记录。
    </m_description>
    </method>
</self>
    </methods>
    <requirements>
        <system>
            <system_name>
《Elastos 2.0》或更高版本            </system_name>
        </system>
        <dll>
            <dll_name>database.dll</dll_name>
        </dll>
        <headerfile>
            <file_name>CDBStore.h, CContact.h, CContactGroup.h, abdata.h, CEntityEnumeration.h, spcomm.h</file_name>
        </headerfile>
        <library>
            <lib_name>ElCRuntime.lib, elastos.lib, utility.lib</lib_name>
        </library>
    </requirements>
</manpage>