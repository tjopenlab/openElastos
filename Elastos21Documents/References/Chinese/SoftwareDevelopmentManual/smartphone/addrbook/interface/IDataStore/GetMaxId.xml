<?xml version="1.0" encoding="GB2312"?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume>地址簿引擎</volume>
<interface>IDataStore</interface><method> GetMaxId </method>
    <description>
        <para>
            获取当前地址簿中联系人或组记录的最大记录ID。
        </para>
    </description>
    <syntax>
        <return_val> ECODE </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> ENTITY_CLASS </param_type>
                <param_name> ecEntity </param_name>
                <param_note>
                    <para>
指定记录的类型，参见数据结构<link url="../../datatype/_ENTITY_CLASS.htm">ENTITY_CLASS</link>                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> long * </param_type>
                <param_name> nEntityId </param_name>
                <param_note>
                    <para>
指向long类型的指针，用于获取返回的记录ID                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

<return>
    <para>
            如果方法调用成功，返回S_OK。</para><para>            其他参见            <link url="../../ab_error.htm">            地址簿错误返回值列表            </link>            。    </para>
</return>
    <requirements>
        <system>
            <system_name>
《Elastos 2.0》或更高版本            </system_name>
        </system>
        <dll>
            <dll_name>database.dll</dll_name>
        </dll>
        <headerfile>
            <file_name>CDBStore.h, CContact.h, CContactGroup.h, abdata.h, CEntityEnumeration.h, spcomm.h</file_name>
        </headerfile>
        <library>
            <lib_name>ElCRuntime.lib, elastos.lib, utility.lib</lib_name>
        </library>
    </requirements>
</manpage>