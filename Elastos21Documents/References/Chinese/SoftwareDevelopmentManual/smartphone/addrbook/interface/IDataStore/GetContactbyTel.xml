<?xml version="1.0" encoding="GB2312"?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume>地址簿引擎</volume>
<interface>IDataStore</interface><method> GetContactbyTel </method>
    <description>
        <para>
            通过指定的电话号码，查询联系人记录。
        </para>
    </description>
    <syntax>
        <return_val> ECODE </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> EzStr </param_type>
                <param_name> esTel </param_name>
                <param_note>
                    <para>
指定的电话号码                    </para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> IObjectEnumeratorRef * </param_type>
                <param_name> ppiContactsEnum </param_name>
                <param_note>
                    <para>
记录的对象枚举器指针                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

<return>
    <para>
            如果方法调用成功，返回S_OK。</para><para>            其他参见            <link url="../../ab_error.htm">            地址簿错误返回值列表            </link>            。    </para>
</return>
    <requirements>
        <system>
            <system_name>
《Elastos 2.0》或更高版本            </system_name>
        </system>
        <dll>
            <dll_name>database.dll</dll_name>
        </dll>
        <headerfile>
            <file_name>CDBStore.h, CContact.h, CContactGroup.h, abdata.h, CEntityEnumeration.h, spcomm.h</file_name>
        </headerfile>
        <library>
            <lib_name>ElCRuntime.lib, elastos.lib, utility.lib</lib_name>
        </library>
    </requirements>
</manpage>