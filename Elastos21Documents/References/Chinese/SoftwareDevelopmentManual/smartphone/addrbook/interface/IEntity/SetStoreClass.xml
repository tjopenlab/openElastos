<?xml version="1.0" encoding="GB2312"?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume>地址簿引擎</volume>
<interface>IEntity</interface><method> SetStoreClass </method>
    <description>
        <para>
            设置记录的存储类型。
        </para>
    </description>
    <syntax>
        <return_val> ECODE </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> STORE_CLASS </param_type>
                <param_name> scStore </param_name>
                <param_note>
                    <para>
记录的存储类型，参见数据结构<link url="../../datatype/_STORE_CLASS.htm">STORE_CLASS</link>                    </para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

<return>
    <para>
            如果方法调用成功，返回S_OK。</para><para>            其他参见            <link url="../../ab_error.htm">            地址簿错误返回值列表            </link>            。    </para>
</return>
    <requirements>
        <system>
            <system_name>
《Elastos 2.0》或更高版本            </system_name>
        </system>
    </requirements>
    <references>
        <reference>
            <ref_name>GetStoreClass</ref_name>
            <ref_url>GetStoreClass</ref_url>
        </reference>
    </references>
</manpage>