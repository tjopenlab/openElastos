<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/shellmanual.xsl"?>

<manpage>
    <title> regcom 工具</title>
    <volume>XDK常用工具</volume>

    <description>
        <para>注册构件。 </para>
    </description>

    <syntax>
        <cmd_name> REGCOM </cmd_name>
        <cmd_param> [-?] [/i][/l][/rs][lr][/rc remote_server_name][/u] dllname</cmd_param>
    </syntax>
    <parameters>
    <param>
        <param_name> -? </param_name>
        <param_note> 显示帮助信息 </param_note>
    </param>
    <param>
        <param_name> /i </param_name>
        <param_note> 把构件注册为服务器内部进程</param_note>
    </param>
    <param>
        <param_name> /l
        </param_name>
        <param_note>把构件注册为服务器本地进程</param_note>
    </param>
    <param>
        <param_name>/rs  </param_name>
        <param_note> 把远程构件信息注册到服务器</param_note>
    </param>
    <param>
        <param_name> /lr </param_name>
        <param_note> 把构件注册到本地或远程服务器</param_note>
    </param>
    <param>
        <param_name> /rc remote_server_name</param_name>
        <param_note> 把远程构件信息注册到客户机</param_note>
    </param>
    <param>
        <param_name> /u </param_name>
        <param_note> 取消构件注册</param_note>
    </param>
    <param>
        <param_name> dllname </param_name>
        <param_note> 构件名字</param_note>
    </param>
    </parameters>

    <examples>
    <example>
        <eg_line>regcom helloworld.dll </eg_line>
        <eg_note> 注册构件helloworld.dll到本机。</eg_note>
    </example>
    </examples>

</manpage>