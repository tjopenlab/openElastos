<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/shellmanual.xsl"?>

<manpage>
    <title> dropfiles 工具</title>
    <volume>XDK常用工具</volume>

    <description>
        <para>在XDK_SOURCE_PATH内搜索droplist中列出的用户要drop出来的文件列表，并把drop出来的文件拷贝到drop目录下对应的子目录中</para>
    </description>

     <syntax>
        <cmd_name> dropfiles </cmd_name>
        <cmd_param> [-?] </cmd_param>
    </syntax>
    <parameters>
    <param>
        <param_name> -?  </param_name>
        <param_note> 显示帮助信息 </param_note>
    </param>
    <param>
        <param_name> 无参数  </param_name>
        <param_note>  在XDK_SOURCE_PATH内搜索droplist中列出的用户要drop出来的文件列表，并把drop出来的文件拷贝到drop目录下对应的子目录中</param_note>
    </param>
     </parameters>

</manpage>