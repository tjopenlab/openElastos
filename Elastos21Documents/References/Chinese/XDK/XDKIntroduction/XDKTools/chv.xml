<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/shellmanual.xsl"?>

<manpage>
    <title> chv 工具</title>
    <volume>XDK常用工具</volume>

    <description>
        <para>chv工具用于版本切换、产品切换和开发包切换，以及板卡切换，命令格式： </para>
    </description>

    <syntax>
        <cmd_name> chv </cmd_name>
        <cmd_param> [-?] [dbg] [opt] [pre] [rls] [ddk] [pdk] [sdk] [board]</cmd_param>
    </syntax>
    <parameters>
    <param>
        <param_name> -? </param_name>
        <param_note> 显示帮助信息 </param_note>
    </param>
    <param>
        <param_name> dbg  </param_name>
        <param_note> 将开发环境切换到DEBUG版的环境 </param_note>
    </param>
    <param>
        <param_name> opt  </param_name>
        <param_note> 将开发环境切换到OPTIMIZE版的环境 </param_note>
    </param>
    <param>
        <param_name> pre  </param_name>
        <param_note> 将开发环境切换到PRERELEASE版的环境 </param_note>
    </param>
    <param>
        <param_name> rls  </param_name>
        <param_note> 将开发环境切换到RELEASE版的环境 </param_note>
    </param>
    <param>
        <param_name> ddk  </param_name>
        <param_note> 将开发环境切换到ddk的环境 </param_note>
    </param>
    <param>
        <param_name> pdk </param_name>
        <param_note> 将开发环境切换到pdk的环境 </param_note>
    </param>
    <param>
        <param_name> sdk  </param_name>
        <param_note> 将开发环境切换到sdk的环境 </param_note>
    </param>
    <param>
        <param_name> board  </param_name>
        <param_note> 将开发环境切换到指定的板卡 </param_note>
    </param>
    </parameters>

    
</manpage>