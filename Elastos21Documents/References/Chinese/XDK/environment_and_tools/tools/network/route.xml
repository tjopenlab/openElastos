<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/shellmanual.xsl"?>

<manpage>
    <title> route </title>
    <volume>网络命令</volume>

    <description>
        <para> 配置默认网关或路由器。</para>
    </description>

    <syntax>
        <cmd_name> route.exe</cmd_name>
        <cmd_param>[add|delete|change][address]</cmd_param>
    </syntax>

    <parameters>
        <param>
            <param_name>[add|delete|change]</param_name>
            <param_note>添加，删除或改变一个网关或路由</param_note>
        </param>
        <param>
            <param_name>[address]</param_name>
            <param_note>网关</param_note>
        </param>
    </parameters>

    <examples>
        <example>
            <eg_line>route.exe add default 192.168.0.1</eg_line>
            <eg_note>添加默认网关。</eg_note>
        </example>
    </examples>

</manpage>