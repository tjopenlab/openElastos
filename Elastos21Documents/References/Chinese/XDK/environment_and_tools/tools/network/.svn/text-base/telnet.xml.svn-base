<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/shellmanual.xsl"?>

<manpage>
    <title> telnet </title>
    <volume>网络命令</volume>

    <description>
        <para> 远程登陆工具。</para>
    </description>

    <syntax>
        <cmd_name> telnet </cmd_name>
        <cmd_param>[host_ip|host_domain_name][port]</cmd_param>
    </syntax>

    <parameters>
        <param>
            <param_name> [host-ip|host_domain_name]</param_name>
            <param_note> 主机的IP地址或主机名 </param_note>
        </param>
        <param>
            <param_name>[port]</param_name>
            <param_note> 端口号，即各种Internet应用程序地址</param_note>
        </param>
    </parameters>

    <examples>
        <example>
            <eg_line>telnet 166.111.8.238</eg_line>
        </example>
        <example>
            <eg_line>telnet bbs.tsinghua.edu.cn</eg_line>
        </example>
    </examples>

</manpage>