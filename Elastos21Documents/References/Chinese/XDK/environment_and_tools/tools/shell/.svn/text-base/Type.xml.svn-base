<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/shellmanual.xsl"?>

<manpage>
    <title> type </title>
    <volume>Shell支持的命令</volume>

    <description>
        <para> 显示文件的内容。</para>
    </description>

    <syntax>
        <cmd_name> type </cmd_name>
        <cmd_param>[path]filename</cmd_param>
    </syntax>

    <parameters>
        <param>
            <param_name>[path]</param_name>
            <param_note>指定的目录路径</param_note>
        </param>
        <param>
            <param_name>filename</param_name>
            <param_note>指定的文件名</param_note>
        </param>
    </parameters>
</manpage>