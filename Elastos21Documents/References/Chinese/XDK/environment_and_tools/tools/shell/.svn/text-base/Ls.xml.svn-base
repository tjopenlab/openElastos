<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/shellmanual.xsl"?>

<manpage>
    <title> ls </title>
    <volume>Shell支持的命令</volume>

    <description>
        <para> 显示指定文件或路径列表。</para>
    </description>

    <syntax>
        <cmd_name> ls </cmd_name>
        <cmd_param>[files]</cmd_param>
    </syntax>

    <parameters>
        <param>
            <param_name>[files]</param_name>
            <param_note>指定的文件</param_note>
        </param>
    </parameters>

</manpage>