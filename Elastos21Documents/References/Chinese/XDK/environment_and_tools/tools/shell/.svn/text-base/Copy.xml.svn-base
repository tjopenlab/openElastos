<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/shellmanual.xsl"?>

<manpage>
    <title> copy </title>
    <volume>Shell支持的命令</volume>

    <description>
        <para> 复制一个或多个文件到本地的其它地方。</para>
    </description>

    <syntax>
        <cmd_name> copy </cmd_name>
        <cmd_param>[-y] source [ source[ ...]] [destination]</cmd_param>
    </syntax>
    <parameters>
        <param>
            <param_name>  [-y] </param_name>
            <param_note> 不提示用户是否覆盖已经存在的文件 </param_note>
        </param>
        <param>
            <param_name>  source [ source[ ...]] </param_name>
            <param_note> 指定被拷贝的文件 </param_note>
        </param>
        <param>
            <param_name>[destination]</param_name>
            <param_note>指定新文件的路径和/或文件名</param_note>
        </param>
    </parameters>

</manpage>