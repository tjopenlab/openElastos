<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/shellmanual.xsl"?>

<manpage>
    <title> memtrace </title>
    <volume>Shell支持的命令</volume>

    <description>
        <para> 在运行应用程序的过程中监测是否存在内存泄漏。</para>
    </description>

    <syntax>
        <cmd_name> memtrace </cmd_name>
        <cmd_param>[-d] [index=] address= [ProgramName] ProgramArgument</cmd_param>
    </syntax>
    <syntax>
        <cmd_name> memtrace </cmd_name>
        <cmd_param>[-v] [ProgramName] ProgramArgument</cmd_param>
    </syntax>
    <parameters>
        <param>
            <param_name>  [-d] </param_name>
            <param_note> 监测到内存泄漏时调试程序 </param_note>
        </param>
        <param>
            <param_name> [-v] </param_name>
            <param_note> 详细报告内存泄漏情况，增加了调用栈列表ClassStack信息 </param_note>
        </param>
        <param>
            <param_name>[ProgramName]</param_name>
            <param_note>应用程序名</param_note>
        </param>
        <param>
            <param_name>[index=]</param_name>
            <param_note>索引号</param_note>
        </param>
        <param>
            <param_name>address=</param_name>
            <param_note>内存地址</param_note>
        </param>
    </parameters>

</manpage>