<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/shellmanual.xsl"?>

<manpage>
    <title> mkdir </title>
    <volume>Shell支持的命令</volume>

    <description>
        <para> 创建指定目录。</para>
    </description>

    <syntax>
        <cmd_name> mkdir </cmd_name>
        <cmd_param>path</cmd_param>
    </syntax>

    <parameters>
        <param>
            <param_name>path</param_name>
            <param_note>指定的目录路径</param_note>
        </param>
    </parameters>

</manpage>