<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/shellmanual.xsl"?>

<manpage>
    <title> debug </title>
    <volume>Shell支持的命令</volume>

    <description>
        <para> 使程序一运行便进入调试状态。</para>
    </description>

    <syntax>
        <cmd_name> debug </cmd_name>
        <cmd_param> program [progargs]</cmd_param>
    </syntax>

    <parameters>
        <param>
            <param_name>program</param_name>
            <param_note>指定的程序名</param_note>
        </param>
        <param>
            <param_name>[progargs]</param_name>
            <param_note>程序参数</param_note>
        </param>
    </parameters>
</manpage>