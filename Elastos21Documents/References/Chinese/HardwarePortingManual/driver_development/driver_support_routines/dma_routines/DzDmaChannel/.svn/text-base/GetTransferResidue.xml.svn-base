<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume>DMA例程</volume>
<interface>DzDmaChannel</interface><method> GetTransferResidue </method>
    <description>
        <para>
            获取当前传输事务剩下的传输量。
        </para>
    </description>
    <syntax_nopara>
        <return_val> uint32_t </return_val>
        <NoArguments/>
    </syntax_nopara>

    <return>
        <para>返回值是剩余的传输量。</para>
    </return>

    <remarks>
        <para>
            该方法返回当前DMA通道的传输事务的剩余传输量。
        </para>
    </remarks>

    <requirements>
        <system>
            <system_name>
                Elastos操作系统
            </system_name>
        </system>
    </requirements>
</manpage>