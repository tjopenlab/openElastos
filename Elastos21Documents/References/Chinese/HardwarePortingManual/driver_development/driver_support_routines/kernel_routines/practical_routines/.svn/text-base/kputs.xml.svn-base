<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/apimanual.xsl"?>

<manpage>
    <volume>实用例程</volume>
    <title>kputs</title>
    <routine_idx> ../practical_routines.htm </routine_idx>

    <description>
        <para>将一个字符串输出到内核控制台。</para>
    </description>

    <syntax>
        <return_val>int</return_val>
        <parameters>
            <api_param>
                <param_type> const char *</param_type>
                <param_name> szMessage </param_name>
                <param_note>
                    <para>指向需要输出的字符串</para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

    <return>
        <table>
            <th>
                <td>返回值</td><td>描述</td>
            </th>
            <tr>
                <td>非负值</td><td>函数调用成功</td>
            </tr>
            <tr>
                <td>EOF</td><td>函数调用失败</td>
            </tr>
        </table>
    </return>

    <remarks>
        <para>
            本函数可以输出的字符串的最大长度为512个字节。
        </para>
        <para>
            本函数专为调试提供。
        </para>
    </remarks>

    <requirements>
        <system>
            <system_name>
                Elastos操作系统
            </system_name>
        </system>
        <headerfile>
            <file_name> ddk.h </file_name>
        </headerfile>
    </requirements>

</manpage>
