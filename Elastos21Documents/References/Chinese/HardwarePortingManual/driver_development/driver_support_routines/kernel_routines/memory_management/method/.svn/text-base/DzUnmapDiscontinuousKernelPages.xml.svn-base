<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/apimanual.xsl"?>

<manpage>
    
    <volume>内存管理函数</volume>
    <title>DzUnmapDiscontinuousKernelPages</title>
    <routine_idx> ../method.htm </routine_idx>

    <description>
        <para>取消指定内存块的映射。 </para>
    </description>

    <syntax>
        <return_val>void</return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> virtaddr_t</param_type>
                <param_name> va </param_name>
                <param_note>
                    <para>需取消映射的内存块的起始地址</para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> uint_t</param_type>
                <param_name> uPages </param_name>
                <param_note>
                    <para>需要取消映射的内存块的页数</para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

    <return>
 <para>无返回值。  </para>
    </return>

    <remarks>
        <para>用于取消由 DzRemapDiscontinuousKernelPages 建立的映射。</para>
    </remarks>

    <requirements>
        <system>
            <system_name>
                Elastos操作系统
            </system_name>
        </system>
        <headerfile>
            <file_name>ddk.h</file_name>
        </headerfile>

    </requirements>
</manpage>
