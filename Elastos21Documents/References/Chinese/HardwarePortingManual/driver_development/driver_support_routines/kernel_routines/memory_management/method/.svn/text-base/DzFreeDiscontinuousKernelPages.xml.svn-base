<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/apimanual.xsl"?>

<manpage>
    
    <volume>内存管理函数</volume>
    <title>DzFreeDiscontinuousKernelPages</title>
    <routine_idx> ../method.htm </routine_idx>

    <description>
        <para>释放指定地址的内核页 </para>
    </description>

    <syntax>
        <return_val>void</return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> virtaddr_t</param_type>
                <param_name> va </param_name>
                <param_note>
                    <para>指定的内核页地址</para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

    <return>
 <para>无返回值。</para>
    </return>


    <remarks>
        <para>用于释放由 DzAllocDiscontinuousKernelPages 分配出来的内存。</para>
    </remarks>
    
    <requirements>
        <system>
            <system_name>
                Elastos操作系统
            </system_name>
        </system>
        <headerfile>
            <file_name>ddk.h</file_name>
        </headerfile>

    </requirements>
</manpage>
