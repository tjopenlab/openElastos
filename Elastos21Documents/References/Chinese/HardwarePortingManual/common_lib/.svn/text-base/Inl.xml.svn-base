<manpage>
    <volume>公用库</volume>
    <title>Inl</title>
    <routine_idx> ../common_library.htm </routine_idx>

    <description>
        <para>从指定的32位I/O端口输入四个字节的数据。</para>
    </description>

    <syntax>
        <return_val>uint32_t</return_val>
        <parameters>
            <api_param>
                <param_type> ioport_t </param_type>
                <param_name> port </param_name>
                <param_note>
                    <para>指定的32位I/O端口的地址</para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

    <return>
        <para>此函数返回所输入的四个字节数据。</para>
    </return>

    <requirements>
        <system>
            <system_name>
                Elastos操作系统
            </system_name>
        </system>
        <headerfile>
            <file_name> ddk.h </file_name>
        </headerfile>
    </requirements>

