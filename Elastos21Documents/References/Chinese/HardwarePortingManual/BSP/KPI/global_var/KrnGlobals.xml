<manpage>
    <volume>内核提供接口</volume>
    <types>KrnGlobals</types>

    <description>
        <para>内核定义的，提供给板卡支持包使用的全局变量  </para>
    </description>

    <struct>

        <struct_member>
            <mem_type>virtaddr_t</mem_type>
            <mem_name>vaKernelVirtBase</mem_name>
            <mem_description>内核所使用的虚拟内存基地址</mem_description>
        </struct_member>
          
       <struct_member>
            <mem_type>uint_t</mem_type>
            <mem_name>uTickRate</mem_name>
            <mem_description>定时器中断发生的频率，单位为赫兹（HZ）</mem_description>
        </struct_member> 
                                                
        
    </struct>
</manpage>