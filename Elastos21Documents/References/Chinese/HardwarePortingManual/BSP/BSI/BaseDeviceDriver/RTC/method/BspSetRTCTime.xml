<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/apimanual.xsl"?>

<manpage>
    <volume>实时钟函数</volume>
    <title>BspSetRTCTime</title>
    <routine_idx> ../method.htm </routine_idx>

    <description>
        <para>设置实时钟时间。</para>
    </description>

    <syntax>
        <return_val>void</return_val>
        <parameters>
            <api_param>
                <param_type> const RTCTime *</param_type>
                <param_name> pRTCTime </param_name>
                <param_note>
                    <para>指向表示需要设置的实时钟时间的RTCTime类型数据，参见<link url="../data_structure/RTCTime.htm">RTCTime</link>结构。此参数不可为NULL。</para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

	<return>
	    <NoReturnValue/>
    </return>

    <remarks>
        <para>本函数用来设置实时钟时间。</para>
    </remarks>

    <requirements>
        <system>
            <system_name>
                《Elastos2.0》或更高版本
            </system_name>
        </system>
        <headerfile>
            <file_name> ddk.h </file_name>
        </headerfile>
    </requirements>
    <references>
        <reference>
            <ref_name>BspGetRTCTime</ref_name>
            <ref_url>BspGetRTCTime</ref_url>
        </reference>
    </references>

</manpage>
 