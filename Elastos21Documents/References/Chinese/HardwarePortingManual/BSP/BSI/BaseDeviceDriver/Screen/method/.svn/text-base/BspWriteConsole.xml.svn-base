<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/apimanual.xsl"?>

<manpage>
    <volume>内核控制台函数</volume>
    <title>BspWriteConsole</title>
    <routine_idx> ../method.htm </routine_idx>

    <description>
        <para>向内核控制台输出信息</para>
    </description>

    <syntax>
        <return_val>void</return_val>
        <parameters>
            <api_param>
                <param_type> const char *</param_type>
                <param_name> szMessage </param_name>
                <param_note>
                    <para>指定字符串信息，用来向内核控制台输出</para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

	<return>
	    <NoReturnValue/>
    </return>

    <remarks>
        <para>此函数用来向内核控制台输出指定信息。</para>
    </remarks>

    <requirements>
        <system>
            <system_name>
                《Elastos2.0》或更高版本
            </system_name>
        </system>
        <headerfile>
            <file_name> ddk.h </file_name>
        </headerfile>
    </requirements>

</manpage>
 