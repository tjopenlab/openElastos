<manpage>
    <volume>数据枚举值 </volume>
    <types>ComPortParity</types>

    <description>
        <para> 串口通讯的数据奇偶校验方式</para>
    </description>

    <enum>
        <enum_member>
            <mem_name>ComPortParity_No</mem_name>
            <mem_value>0</mem_value>
            <mem_description>无数据校验</mem_description>
        </enum_member>
        <enum_member>
            <mem_name>ComPortParity_Odd</mem_name>
            <mem_value>1</mem_value>
            <mem_description>奇校验</mem_description>
        </enum_member>
        <enum_member>
            <mem_name>ComPortParity_Even </mem_name>
            <mem_value>2</mem_value>
            <mem_description>偶校验</mem_description>
        </enum_member>
        <enum_member>
            <mem_name>ComPortParity_Mark </mem_name>
            <mem_value>3</mem_value>
            <mem_description>Mark校验</mem_description>
        </enum_member>
        <enum_member>
            <mem_name>ComPortParity_Space</mem_name>
            <mem_value>4</mem_value>
            <mem_description>Space校验</mem_description>
        </enum_member>
        <enum_member>
            <mem_name>ComPortParity_Default</mem_name>
            <mem_value>ComPortParity_No</mem_value>
            <mem_description>缺省情况下，无数据校验</mem_description>
        </enum_member>
    </enum>
</manpage>

