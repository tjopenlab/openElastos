<manpage>
    <volume>串口相关数据结构 </volume>
    <types>ComPortSettings</types>

    <description>
        <para>串口设置结构</para>
    </description>

    <struct>
        <struct_member>
            <mem_type>uint_t </mem_type>
            <mem_name>uBaudRate </mem_name>
            <mem_description>通讯的波特率 </mem_description>
        </struct_member>
        <struct_member>
            <mem_type>ComPortByteSize </mem_type>
            <mem_name> byteSize </mem_name>
            <mem_description>发送和接收字节的位数，参见<link url="../enum/ComPortByteSize.htm">ComPortByteSize</link>枚举类型。 </mem_description>
        </struct_member>
        <struct_member>
            <mem_type>ComPortParity</mem_type>
            <mem_name>parity </mem_name>
            <mem_description>数据奇偶校验方式，参见<link url="../enum/ComPortParity.htm">ComPortParity</link>枚举类型。 </mem_description>
        </struct_member>
        <struct_member>
            <mem_type>ComPortStopBits</mem_type>
            <mem_name>stopBits </mem_name>
            <mem_description>停止位的位数，参见<link url="../enum/ComPortStopBits.htm">ComPortStopBits</link>枚举类型。 </mem_description>
        </struct_member>
    </struct>
