<interface>ICallConnection</interface><method> GetLineIndex </method>
    <description>
        <para>获取当前电话线路的index值。</para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> Int32* </param_type>
                <param_name> index </param_name>
                <param_note>
                    <para>线路的index值。</para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

<return>
    <table>
	        <th>
	    	    <td>常见返回值 </td>
	    	    <td>描述</td>
	        </th>
	        <tr>
