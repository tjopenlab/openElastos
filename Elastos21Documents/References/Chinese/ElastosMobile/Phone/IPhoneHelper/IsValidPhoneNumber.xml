<volume>CPhoneHelper Functions</volume>
<interface>IPhoneHelper</interface><method> IsValidPhoneNumber </method>
    <description>
        <para>判断电话号码是否有效。</para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> WString </param_type>
                <param_name> number </param_name>
                <param_note>
                    <para>通话连接ID。</para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> Boolean* </param_type>
                <param_name> flag </param_name>
                <param_note>
                    <para>TRUE为号码有效，FALSE为号码无效。</para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

<return>
    <table>
	        <th>
	    	    <td>常见返回值 </td>
	    	    <td>描述</td>
	        </th>
	        <tr>
	    	    <td>NOERROR</td>
	    	    <td>成功调用该方法</td>
	        </tr>
	    </table>
    <para>其它参见<link url="../../../car_technology/fundamental/development_environment/car/ECode.htm">ECode</link>相关帮助。</para>
</return>
<remarks>    
</remarks>
