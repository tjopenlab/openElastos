<volume>CPhone Functions</volume>
<interface>IPhone</interface><method> GetCallByID </method>
    <description>
        <para>根据通话ID获取通话(Call)对象接口。</para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> Int32 </param_type>
                <param_name> callId  </param_name>
                <param_note>
                    <para>通话ID。</para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> <link url="../ICall.htm">ICall</link>** </param_type>
                <param_name> call  </param_name>
                <param_note>
                    <para>ICall接口指针。</para>
                </param_note>
            </api_param>

        </parameters>
    </syntax>

<return>
    <table>
	        <th>
	    	    <td>常见返回值 </td>
	    	    <td>描述</td>
	        </th>
	        <tr>
	    	    <td>NOERROR</td>
	    	    <td>成功调用该方法</td>
	        </tr>
	    </table>
    <para>其它参见<link url="../../../car_technology/fundamental/development_environment/car/ECode.htm">ECode</link>相关帮助。</para>
</return>
<remarks>    
