<?xml version="1.0" encoding="GB2312"?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/custom_type.xsl"?>    <manpage>
    <volume>Phone Enumerations </volume>
    <types> CLIRState </types>
    <description>
        <para>
            主叫线路识别限制枚举类型，即是否隐藏本机号码。
        </para>
    </description>
<enum> 
          <enum_member>    <mem_name> CLIRState_Default </mem_name>    <mem_value>0 </mem_value>    <mem_description>运营商网络默认值   </mem_description>    </enum_member>  
          <enum_member>    <mem_name> CLIRState_Enabled </mem_name>    <mem_value>1 </mem_value>    <mem_description>开启显示本机号码功能   </mem_description>    </enum_member>  
          <enum_member>    <mem_name> CLIRState_Disabled </mem_name>    <mem_value>2 </mem_value>    <mem_description>关闭显示本机号码功能    </mem_description>    </enum_member>  
</enum>
</manpage>