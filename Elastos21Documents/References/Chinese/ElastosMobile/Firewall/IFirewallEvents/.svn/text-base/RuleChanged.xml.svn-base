<volume>CFirewall Functions</volume>
<interface>IFirewallEvents</interface><method> RuleChanged </method>
    <description>
        <para>防火墙设置信息改变时(即执行<link url="../IFirewallRule/save.htm">save</link>操作)触发该事件。</para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> <link url="../IFirewallRule.htm">IFirewallRule</link>* </param_type>
                <param_name> rule </param_name>
                <param_note>
                    <para>改变的防火墙规则。</para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

<return>
    <table>
	        <th>
	    	    <td>常见返回值 </td>
	    	    <td>描述</td>
	        </th>
	        <tr>
	    	    <td>NOERROR</td>
	    	    <td>成功调用该方法</td>
	        </tr>
	    </table>
    <para>其它参见<link url="../../../car_technology/fundamental/development_environment/car/ECode.htm">ECode</link>相关帮助。</para>
</return>
