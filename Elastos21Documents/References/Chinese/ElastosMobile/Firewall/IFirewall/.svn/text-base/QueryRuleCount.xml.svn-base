<volume>CFirewall Functions</volume>
<interface>IFirewall</interface><method> QueryRuleCount </method>
    <description>
        <para>查询指定类型的防火墙设置项总数。</para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> <link url="../Enum/FirewallActivity.htm">FirewallActivity</link> </param_type>
                <param_name> activity </param_name>
                <param_note>
                    <para>防火墙防御目标。</para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> WString </param_type>
                <param_name> endpoint </param_name>
                <param_note>
                    <para>防御对象。</para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> <link url="../Enum/FirewallPermission.htm">FirewallPermission</link>  </param_type>
                <param_name> permission </param_name>
                <param_note>
                    <para>防火墙启用状态。</para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> Int32* </param_type>
                <param_name> count </param_name>
                <param_note>
                    <para>防火墙总数。</para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

<return>
    <table>
	        <th>
	    	    <td>常见返回值 </td>
	    	    <td>描述</td>
	        </th>
	        <tr>
	    	    <td>NOERROR</td>
	    	    <td>成功调用该方法</td>
	        </tr>
	        <tr>
	    	    <td>E_DB_INTERNAL_ERROR</td>
	    	    <td>数据库打开失败</td>
	        </tr>
	        <tr>
	    	    <td>E_INVALID_ARGUMENT</td>
	    	    <td>参数无效</td>
	        </tr>
	    </table>
    <para>其它参见<link url="../../../car_technology/fundamental/development_environment/car/ECode.htm">ECode</link>相关帮助。</para>
</return>
