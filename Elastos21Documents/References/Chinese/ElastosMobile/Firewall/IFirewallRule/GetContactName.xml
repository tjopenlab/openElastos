<?xml version="1.0" encoding="GB2312"?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume>CFirewallRule Functions</volume>
<interface>IFirewallRule</interface><method> GetContactName </method>
    <description>
        <para>获取联系人防御对象的姓名。</para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> WStringBuf_&lt;<link url="../Datatype.htm">MAX_NAME</link>&gt; </param_type>
                <param_name> name </param_name>
                <param_note>
                    <para>联系人姓名。</para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> WStringBuf_&lt;<link url="../Datatype.htm">MAX_ASCII_NAME</link>&gt; </param_type>
                <param_name> asciiName </param_name>
                <param_note>
                    <para>姓名拼写。</para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

<return>
    <table>
	        <th>
	    	    <td>常见返回值 </td>
	    	    <td>描述</td>
	        </th>
	        <tr>
	    	    <td>NOERROR</td>
	    	    <td>成功调用该方法</td>
	        </tr>
	        <tr>
	    	    <td>E_INVALID_ARGUMENT</td>
	    	    <td>参数无效</td>
	        </tr>
	        <tr>
	    	    <td>E_BUFFER_TOO_SMALL</td>
	    	    <td>参数asciiName的长度大于常量MAX_ASCII_NAME所指定的长度</td>
	        </tr>

	    </table>
    <para>其它参见<link url="../../../car_technology/fundamental/development_environment/car/ECode.htm">ECode</link>相关帮助。</para>
</return>
<remarks>    
</remarks>
