    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume>CFirewallRule Functions</volume>
<interface>IFirewallRule</interface><method> GetNote </method>
    <description>
        <para>获取防火墙备注信息。</para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> WStringBuf_&lt;<link url="../Datatype.htm">MAX_NOTE</link>&gt; </param_type>
                <param_name> note </param_name>
                <param_note>
                    <para>防火墙备注信息。</para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

<return>
    <table>
	        <th>
	    	    <td>常见返回值 </td>
	    	    <td>描述</td>
	        </th>
	        <tr>
	    	    <td>NOERROR</td>
	    	    <td>成功调用该方法</td>
	        </tr>
	        <tr>
	    	    <td>E_INVALID_ARGUMENT</td>
	    	    <td>参数无效</td>
	        </tr>
	        <tr>
	    	    <td>E_BUFFER_TOO_SMALL</td>
	    	    <td>参数note的长度大于常量MAX_NOTE所指定的长度</td>
	        </tr>
	    </table>
    <para>其它参见<link url="../../../car_technology/fundamental/development_environment/car/ECode.htm">ECode</link>相关帮助。</para>
</return>
<remarks>    
</remarks>
