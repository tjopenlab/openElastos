<?xml version="1.0" encoding="GB2312"?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/custom_type.xsl"?>    <manpage>
    <volume>TimePiece Enumerations </volume>
    <types> Weekday </types>
    <description>
        <para>
            闹钟设置的可选时间枚举类型。
        </para>
    </description>
<enum>
	        <enum_member>    <mem_name> Weekday_Sunday </mem_name>    <mem_value>0x1 </mem_value>    <mem_description>星期天    </mem_description>    </enum_member>  
          <enum_member>    <mem_name> Weekday_Monday </mem_name>    <mem_value>0x2  </mem_value>    <mem_description>星期一    </mem_description>    </enum_member>    
          <enum_member>    <mem_name> Weekday_Tuesday </mem_name>    <mem_value>0x4  </mem_value>    <mem_description>星期二    </mem_description>    </enum_member>    
	        <enum_member>    <mem_name> Weekday_Wednesday </mem_name>    <mem_value>0x8 </mem_value>    <mem_description>星期三    </mem_description>    </enum_member>  
          <enum_member>    <mem_name> Weekday_Thursday	 </mem_name>    <mem_value>0x10  </mem_value>    <mem_description>星期四    </mem_description>    </enum_member>    
          <enum_member>    <mem_name> Weekday_Friday </mem_name>    <mem_value>0x20  </mem_value>    <mem_description>星期五    </mem_description>    </enum_member>    
	        <enum_member>    <mem_name> Weekday_Saturday </mem_name>    <mem_value>0x40 </mem_value>    <mem_description>星期六    </mem_description>    </enum_member>  
          <enum_member>    <mem_name> Weekday_Weekdays </mem_name>    <mem_value>0x3E  </mem_value>    <mem_description>工作日(周一到周五)    </mem_description>    </enum_member>    
          <enum_member>    <mem_name> Weekday_Weekends </mem_name>    <mem_value>0x41  </mem_value>    <mem_description>周末    </mem_description>    </enum_member>    
          <enum_member>    <mem_name> Weekday_Everyday </mem_name>    <mem_value>0x7F  </mem_value>    <mem_description>每天    </mem_description>    </enum_member>    
</enum>
</manpage>