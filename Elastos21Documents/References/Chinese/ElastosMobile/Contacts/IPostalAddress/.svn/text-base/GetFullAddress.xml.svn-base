    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume>CPostalAddress  Functions</volume>
<interface>IPostalAddress</interface><method> GetFullAddress </method>
    <description>
        <para>获取全地址，会把所有地址字段拼成一个全地址。</para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> <link url="../Enum/PostalAddressStyle.htm">PostalAddressStyle</link> </param_type>
                <param_name> addrStyle</param_name>
                <param_note>
                    <para>需要获取的地址类型。</para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> WStringBuf_&lt;<link url="../Datatype.htm">MAX_ADDRESS</link>&gt;  </param_type>
                <param_name> fullAddress</param_name>
                <param_note>
                    <para>存放获取的全地址，不允许为空。</para>
                </param_note>
            </api_param>

        </parameters>
    </syntax>

<return>
    <table>
	        <th>
	    	    <td>常见返回值 </td>
	    	    <td>描述</td>
	        </th>
	        <tr>
	    	    <td>NOERROR</td>
	    	    <td>成功调用该方法</td>
	        </tr>
	        <tr>
	    	    <td>E_INVALID_ARGUMENT</td>
	    	    <td>参数无效</td>
	        </tr>
	    </table>
    <para>其它参见<link url="../../../car_technology/fundamental/development_environment/car/ECode.htm">ECode</link>相关帮助。</para>
</return>
<remarks>    
</remarks>
