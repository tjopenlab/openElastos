    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume>CPostalAddress  Functions</volume>
<interface>IPostalAddress</interface><method> GetCountryOrRegion </method>
    <description>
        <para>获取国家或地区。</para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> WStringBuf_&lt;<link url="../Datatype.htm">MAX_COUNTRYORREGION</link>&gt; </param_type>
                <param_name> countryOrRegion </param_name>
                <param_note>
                    <para>国家或地区名，不允许为空。</para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

<return>
    <table>
	        <th>
	    	    <td>常见返回值 </td>
	    	    <td>描述</td>
	        </th>
	        <tr>
	    	    <td>NOERROR</td>
	    	    <td>成功调用该方法</td>
	        </tr>
	        <tr>
	    	    <td>E_INVALID_ARGUMENT</td>
	    	    <td>参数无效</td>
	        </tr>
	        <tr>
	    	    <td>E_BUFFER_TOO_SMALL</td>
	    	    <td>参数可容纳字符串的长度小于国家或地区名字符串的长度</td>
	        </tr>
	    </table>
    <para>其它参见<link url="../../../car_technology/fundamental/development_environment/car/ECode.htm">ECode</link>相关帮助。</para>
</return>
<remarks>    
</remarks>
