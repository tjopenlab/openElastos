<?xml version="1.0" encoding="GB2312"?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume>CPhoneBook Functions</volume>
<interface>IPhoneBook</interface><method> SearchEx </method>
    <description>
        <para>按多个字段搜索联系人。</para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> <link url="../Enum/StorageLocation.htm">StorageLocation</link> </param_type>
                <param_name> location </param_name>
                <param_note>
                    <para>联系人的存储位置。</para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> BufferOf&lt;<link url="../Enum/SearchMethod.htm">SearchMethod</link>&gt; </param_type>
                <param_name> searchMethod </param_name>
                <param_note>
                    <para>搜索方式。</para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> BufferOf&lt;WString&gt; </param_type>
                <param_name> key </param_name>
                <param_note>
                    <para>关键字列表。关键字与搜索方式一一对应。</para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> <link url="../../../SoftwareDevelopmentManual/CoreServices/ProcessThread/ProcessThread/IObjectEnumerator.htm">IObjectEnumerator</link>** </param_type>
                <param_name> contacts </param_name>
                <param_note>
                    <para>指向联系人(Contact)对象枚举器接口指针的指针，用于获取一组IContact接口指针列表。</para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

<return>
    <table>
	        <th>
	    	    <td>常见返回值 </td>
	    	    <td>描述</td>
	        </th>
	        <tr>
	    	    <td>NOERROR</td>
	    	    <td>成功调用该方法</td>
	        </tr>
	    </table>
    <para>其它参见<link url="../../../car_technology/fundamental/development_environment/car/ECode.htm">ECode</link>相关帮助。</para>
</return>
<remarks>    
