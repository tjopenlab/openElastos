<volume>CPhoneBook Functions</volume>
<interface>IPhoneBookErrands</interface><method> BatchOperation </method>
    <description>
        <para>同步进度反馈事件。批量操作时，如果需要等待框或进度条则注册该事件。</para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> <link url="../Enum/BatchOperationType.htm">BatchOperationType</link> </param_type>
                <param_name> operationType </param_name>
                <param_note>
                    <para>批量操作的类型。</para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> Int32* </param_type>
                <param_name> result </param_name>
                <param_note>
                    <para>client端的操作结果。</para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

<return>
    <table>
	        <th>
	    	    <td>常见返回值 </td>
	    	    <td>描述</td>
	        </th>
	        <tr>
	    	    <td>NOERROR</td>
	    	    <td>成功调用该方法</td>
	        </tr>
	    </table>
    <para>其它参见<link url="../../../car_technology/fundamental/development_environment/car/ECode.htm">ECode</link>相关帮助。</para>
</return>
