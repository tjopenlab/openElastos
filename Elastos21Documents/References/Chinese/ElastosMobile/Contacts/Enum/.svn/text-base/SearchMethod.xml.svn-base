<?xml version="1.0" encoding="GB2312"?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/custom_type.xsl"?>    <manpage>
    <volume>Contacts Enumerations </volume>
    <types> SearchMethod </types>
    <description>
        <para>
            联系人搜索方式枚举类型，只要搜索项为字符串字段的都可以搜索。
        </para>
    </description>
<enum>    <enum_member>    <mem_name> SearchMethod_ByFirstName </mem_name>    <mem_value>0 </mem_value>    <mem_description>通过人名字查找    </mem_description>    </enum_member>  
          <enum_member>    <mem_name> SearchMethod_ByLastName </mem_name>    <mem_value>1  </mem_value>    <mem_description>通过姓氏查找    </mem_description>    </enum_member>    
          <enum_member>    <mem_name> SearchMethod_ByPhoneNumber </mem_name>    <mem_value>2  </mem_value>    <mem_description>通过电话查找    </mem_description>    </enum_member>    
          <enum_member>    <mem_name> SearchMethod_ByEmail </mem_name>    <mem_value>3  </mem_value>    <mem_description>通过email地址查找    </mem_description>    </enum_member>
          <enum_member>    <mem_name> SearchMethod_ByCountryOrRegion </mem_name>    <mem_value>4  </mem_value>    <mem_description>通过国家或地区查找    </mem_description>    </enum_member>
          <enum_member>    <mem_name> SearchMethod_ByStateOrProvince </mem_name>    <mem_value>5  </mem_value>    <mem_description>通过州或省份查找   </mem_description>    </enum_member>
          <enum_member>    <mem_name> SearchMethod_ByCityOrCounty </mem_name>    <mem_value>6  </mem_value>    <mem_description>通过县市查找    </mem_description>    </enum_member>    
          <enum_member>    <mem_name> SearchMethod_ByDistrictOrTownship </mem_name>    <mem_value>7  </mem_value>    <mem_description>通过镇区查找    </mem_description>    </enum_member>    
          <enum_member>    <mem_name> SearchMethod_ByStreet </mem_name>    <mem_value>8  </mem_value>    <mem_description>通过街道查找    </mem_description>    </enum_member>
          <enum_member>    <mem_name> SearchMethod_ByZipCode </mem_name>    <mem_value>9  </mem_value>    <mem_description>通过邮政编码查找    </mem_description>    </enum_member>
          <enum_member>    <mem_name> SearchMethod_ByInstantMessagingService </mem_name>    <mem_value>10  </mem_value>    <mem_description>通过即时通讯地址查找</mem_description>    </enum_member>
          <enum_member>    <mem_name> SearchMethod_ByAffiliation </mem_name>    <mem_value>11  </mem_value>    <mem_description>通过工作单位查找    </mem_description>    </enum_member>
          <enum_member>    <mem_name> SearchMethod_ByBirthday </mem_name>    <mem_value>12  </mem_value>    <mem_description>通过生日查找    </mem_description>    </enum_member>
          <enum_member>    <mem_name> SearchMethod_Any </mem_name>    <mem_value>13  </mem_value>    <mem_description>任何字段   </mem_description>    </enum_member>

</enum>
</manpage>