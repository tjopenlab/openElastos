    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume>CContact Functions</volume>
<interface>IContact</interface><method> SetPostalAddress </method>
    <description>
        <para>设置或更新联系人邮政地址。</para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> <link url="../Enum/PostalAddressType.htm">PostalAddressType</link> </param_type>
                <param_name> addrType </param_name>
                <param_note>
                    <para>邮政地址类型。</para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> <link url="../IPostalAddress.htm">IPostalAddress</link>* </param_type>
                <param_name> address </param_name>
                <param_note>
                    <para>该类型的地址对象。</para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

<return>
    <table>
	        <th>
	    	    <td>常见返回值 </td>
	    	    <td>描述</td>
	        </th>
	        <tr>
	    	    <td>NOERROR</td>
	    	    <td>成功调用该方法</td>
	        </tr>
	        <tr>
	    	    <td>E_ZOMBIE_OBJECT</td>
	    	    <td>该联系人记录对象已被删除</td>
	        </tr>
	        <tr>
	    	    <td>E_INVALID_ARGUMENT</td>
	    	    <td>参数无效</td>
	        </tr>	    
	</table>
    <para>其它参见<link url="../../../car_technology/fundamental/development_environment/car/ECode.htm">ECode</link>相关帮助。</para>
</return>
<remarks>    
</remarks>
