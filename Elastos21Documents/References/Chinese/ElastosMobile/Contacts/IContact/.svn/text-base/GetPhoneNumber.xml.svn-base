<?xml version="1.0" encoding="GB2312"?>
    <?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?><manpage>
<volume>CContact Functions</volume>
<interface>IContact</interface><method> GetPhoneNumber </method>
    <description>
        <para>获取联系人电话号码。</para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> <link url="../Enum/PhoneNumberType.htm">PhoneNumberType</link> </param_type>
                <param_name> numberType </param_name>
                <param_note>
                    <para>电话号码类型。</para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> WStringBuf_&lt;<link url="../Datatype.htm">MAX_PHONENUM</link>&gt; </param_type>
                <param_name> number </param_name>
                <param_note>
                    <para>存放获取的电话号码，不允许传空。</para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[out]</param_inout>
                <param_type> WStringBuf_&lt;<link url="../Datatype.htm">MAX_NAME</link>&gt; </param_type>
                <param_name> alias </param_name>
                <param_note>
                    <para>存放该类型号码的别名，可以传空。</para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

<return>
    <table>
	        <th>
	    	    <td>常见返回值 </td>
	    	    <td>描述</td>
	        </th>
	        <tr>
	    	    <td>NOERROR</td>
	    	    <td>成功调用该方法</td>
	        </tr>
	        <tr>
	    	    <td>E_ZOMBIE_OBJECT</td>
	    	    <td>该联系人记录对象已被删除</td>
	        </tr>
	        <tr>
	    	    <td>E_INVALID_ARGUMENT</td>
	    	    <td>参数无效</td>
	        </tr>
	    </table>
    <para>其它参见<link url="../../../car_technology/fundamental/development_environment/car/ECode.htm">ECode</link>相关帮助。</para>
</return>
<remarks>    
</remarks>
