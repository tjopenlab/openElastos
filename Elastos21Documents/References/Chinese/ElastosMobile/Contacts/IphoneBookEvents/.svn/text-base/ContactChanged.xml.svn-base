<volume>CPhoneBook Functions</volume>
<interface>IphoneBookEvents</interface><method> ContactChanged </method>
    <description>
        <para>联系人记录改变时触发该事件。</para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> <link url="../Enum/ContactStatus.htm">ContactStatus</link> </param_type>
                <param_name> status </param_name>
                <param_note>
                    <para>联系人记录发生改变的类型。</para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> <link url="../IContact.htm">IContact</link>* </param_type>
                <param_name> contact </param_name>
                <param_note>
                    <para>被改变的联系人记录。</para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

<return>
    <table>
	        <th>
	    	    <td>常见返回值 </td>
	    	    <td>描述</td>
	        </th>
	        <tr>
	    	    <td>NOERROR</td>
	    	    <td>成功调用该方法</td>
	        </tr>
	    </table>
    <para>其它参见<link url="../../../car_technology/fundamental/development_environment/car/ECode.htm">ECode</link>相关帮助。</para>
</return>
<remarks>    
