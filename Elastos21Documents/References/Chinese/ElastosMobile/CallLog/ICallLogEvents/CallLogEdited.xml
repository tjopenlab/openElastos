<interface>ICallLogEvents</interface><method> CallLogEdited </method>
    <description>
        <para>通话记录改变时触发该事件。</para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> <link url="../Enum/CallLogEdit.htm">CallLogEdit</link> </param_type>
                <param_name> editType </param_name>
                <param_note>
                    <para>通话记录改变的类型。</para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> <link url="../ICallLogEntry.htm">ICallLogEntry</link> * </param_type>
                <param_name> entry </param_name>
                <param_note>
                    <para>被改变的通话记录。</para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

<return>
    <table>
	        <th>
	    	    <td>常见返回值 </td>
	    	    <td>描述</td>
	        </th>
