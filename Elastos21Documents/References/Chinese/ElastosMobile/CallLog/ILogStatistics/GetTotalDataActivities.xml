<volume>CLogStatistics Functions</volume>
<interface>ILogStatistics</interface><method> GetTotalDataActivities </method>
    <description>
        <para>获取总流量，以Byte为单位。</para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> Int64* </param_type>
                <param_name> dataReceived </param_name>
                <param_note>
                    <para>总的下行流量，如果不需要可以传空。</para>
                </param_note>
            </api_param>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> Int64* </param_type>
                <param_name> dataSent </param_name>
                <param_note>
                    <para>总的上行流量，如果不需要可以传空。</para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

<return>
    <table>
	        <th>
	    	    <td>常见返回值 </td>
	    	    <td>描述</td>
	        </th>
	        <tr>
	    	    <td>NOERROR</td>
	    	    <td>成功调用该方法</td>
	        </tr>
	    </table>
    <para>其它参见<link url="../../../car_technology/fundamental/development_environment/car/ECode.htm">ECode</link>相关帮助。</para>
</return>
