<interface>IProfileManagerEvents</interface><method> ProfileSwitched </method>
    <description>
        <para>情景模式更换时触发该事件。</para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
            <api_param>
                <param_inout>[in]</param_inout>
                <param_type> <link url="../IProfile.htm">IProfile</link>* </param_type>
                <param_name> profile </param_name>
                <param_note>
                    <para>被更换的情景模式。</para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

<return>
