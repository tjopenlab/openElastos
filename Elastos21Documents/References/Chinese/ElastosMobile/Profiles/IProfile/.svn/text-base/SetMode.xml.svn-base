<interface>IProfile</interface><method> SetMode </method>
    <description>
        <para>设置情景模式。</para>
    </description>
    <syntax>
        <return_val> ECode </return_val>
        <parameters>
           <api_param>
                <param_inout>[in]</param_inout>
                <param_type> <link url="../Enum/ProfileMode.htm">ProfileMode</link> </param_type>
                <param_name> profileMode </param_name>
                <param_note>
                    <para>情景模式类型。</para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

