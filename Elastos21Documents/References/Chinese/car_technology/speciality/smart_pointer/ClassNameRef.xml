<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/interfaceman.xsl"?>

<manpage>
    <volume> 类智能指针 </volume>
    <interface> ClassNameRef </interface>

    <description>
        <para>类智能指针类共同拥有的方法。ClassName表示任意类名。</para>
    </description>

    <methods>
        <self>
        <method>
        <method_name>Instantiate</method_name>
        <method_url>ClassNameRef/Instantiate</method_url>
        <m_description>创建ClassName类对象，并使相应的类智能指针指向该对象</m_description>
        </method>
        <method>
        <method_name>IsValid</method_name>
        <method_url>ClassNameRef/IsValid</method_url>
        <m_description> 判断ClassName的类智能指针是否有效</m_description>
        </method>
    </self>
    </methods>

    <operators>
        <operator>
            <operator_name>operator ==</operator_name>
            <operator_url>ClassNameRef/operator</operator_url>
            <operator_description>判断当前类智能指针是否与指定类智能指针指向同一个对象</operator_description>
        </operator>
    </operators>

</manpage>