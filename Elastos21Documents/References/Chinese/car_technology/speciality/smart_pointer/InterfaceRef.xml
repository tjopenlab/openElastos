<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/interfaceman.xsl"?>

<manpage>
    <volume> 接口智能指针 </volume>
    <interface> InterfaceRef </interface>

    <description>
        <para>接口智能指针类共同拥有的方法。Interface表示任意接口名。</para>
    </description>

    <methods>
        <self>
            <method>
                <method_name>Instantiate</method_name>
                <method_url>InterfaceRef/Instantiate</method_url>
                <m_description>创建指定类对象，并使当前接口智能指针指向该对象中的Interface接口</m_description>
            </method>
            <method>
                <method_name>IsValid</method_name>
                <method_url>InterfaceRef/IsValid</method_url>
                <m_description>判断指定接口智能指针是否有效</m_description>
            </method>
            <method>
                <method_name>IsSameObject</method_name>
                <method_url>InterfaceRef/IsSameObject</method_url>
                <m_description>判断当前接口智能指针是否与指定智能指针指向同一对象</m_description>
            </method>
            <method>
                <method_name>IsSameInterface</method_name>
                <method_url>InterfaceRef/IsSameInterface</method_url>
                <m_description> 判断当前接口智能指针是否与指定接口智能指针指向同一对象中的同一接口</m_description>
            </method>
        </self>
    </methods>

</manpage>