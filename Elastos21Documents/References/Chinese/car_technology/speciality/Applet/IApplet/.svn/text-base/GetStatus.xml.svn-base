<manpage>
    <volume> CApplet相关API介绍 </volume>
    <interface>IApplet</interface>
    <method> GetStatus </method>

    <description>
        <para>获取applet的当前运行状态。</para>
    </description>
    <syntax>
        <parameters>
        <api_param>
            <param_type>[out] AppletStatus* </param_type>
            <param_name>status </param_name>
            <param_note>
                <para>applet的运行状态。参见<link url="../Reference/AppletStatus.htm">AppletStatus</link>枚举类型说明。</para>
            </param_note>
        </api_param>
        </parameters>
    </syntax>


</manpage>