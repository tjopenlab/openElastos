<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?>

<manpage>
    <volume> CApplet相关API介绍 </volume>
    <interface>CApplet</interface>
    <method> Finish </method>

    <description>
        <para>结束applet的消息循环，使applet线程结束。</para>
    </description>

    <syntax>
        <return_val>STATIC CARAPI</return_val>
        <parameters>
        <api_param>
            <param_type>AppletFinish </param_type>
            <param_name>Flag </param_name>
            <param_note>
                  <para>根据类型不同，执行不同的退出操作。参见<link url="../Reference/AppletFinish.htm">AppletFinish</link>枚举类型说明。</para>
            </param_note>
        </api_param>
        </parameters>
    </syntax>
</manpage>