    <interface>WStringBuf</interface>
    <method> operator [] </method>

    <description>
        <para>存取数组元素操作符，用来对WStringBuf对象的数组元素进行存取操作。</para>
    </description>

    <syntax>
        <return_val> WChar&amp; </return_val>
        <parameters>
        <api_param>
            <param_type> Int32 </param_type>
            <param_name> index </param_name>
            <param_note>
                <para> WStringBuf对象字符串索引值 </para>
            </param_note>
        </api_param>
        </parameters>
        </syntax>
     <syntax>
        <return_val> const WChar&amp; </return_val>
        <modifier>const</modifier>
        <parameters>
        <api_param>
            <param_type> Int32 </param_type>

            <param_name> index </param_name>
            <param_note>
                <para> </para>
            </param_note>
        </api_param>
        </parameters>
     </syntax>
        
        

    <return>
    <para>返回WStringBuf字符串中对应字符。</para>
    </return>
<example>
<pre>
请参见：<link url="../AStringBuf/operator.htm">AStringBuf::operator []</link>
</pre>
</example>    
