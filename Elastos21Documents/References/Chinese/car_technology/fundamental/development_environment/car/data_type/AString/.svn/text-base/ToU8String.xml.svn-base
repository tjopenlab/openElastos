<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?>

<manpage>
    <volume> 常用数据结构 </volume>
    <interface>AString</interface>
    <method> ToU8String </method>

    <description>
        <para> 将AString中的字符串转为UTF-8编码。 </para>
    </description>
    <syntax>
        <return_val> U8StringBuf &amp; </return_val>
        <parameters>
        <api_param>
            <param_type>U8StringBuf &amp;</param_type>
            <param_name> str </param_name>
            <param_note>
                <para> 保存子字符串的U8StringBuf对象。 </para>
            </param_note>
        </api_param>
        <api_param>
            <param_type>Encoding</param_type>
            <param_name> encoding  </param_name>
            <param_default>Encoding_Default  </param_default>
            <param_note>
                <para> 编码格式。 参见<link url="../../others/Encoding.htm">Encoding</link>枚举类型说明。 </para>
            </param_note>
        </api_param>
        </parameters>        
    </syntax>
    <return>
        <para>返回U8StringBuf类型的引用。 </para>
    </return>
 
   
 
</manpage>