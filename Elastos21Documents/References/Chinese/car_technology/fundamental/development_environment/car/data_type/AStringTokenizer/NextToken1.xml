<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?>

<manpage>
   
    <volume> 常用数据结构 </volume>
    <interface>AStringTokenizer  </interface>
    <method>   NextToken </method>

    <description>
        <para>根据参数修改分隔符，并获取下一个Token。 </para>
    </description>

     <syntax>
        <return_val> AString </return_val>
       
         <parameters>
        <api_param>
            <param_type>AString </param_type>
            <param_name>delim </param_name>          
            <param_note>
                <para>新的分隔符</para>
            </param_note>
        </api_param>
         
       </parameters>
        
    </syntax>
    
    <return>
   <para>返回新的分隔符集后的下一个Token。</para>
   </return>

   

</manpage>