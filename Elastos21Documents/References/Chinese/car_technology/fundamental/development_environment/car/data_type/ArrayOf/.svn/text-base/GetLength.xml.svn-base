<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?>

<manpage>
    <volume> 常用数据结构 </volume>
    <interface>ArrayOf</interface>
    <method>GetLength </method>

    <description>
        <para>获取当前ArrayOf数组长度。</para>
    </description>
    <syntax_nopara>
        <return_val> Int32 </return_val>
        <modifier>const</modifier>
        <NoArguments/>
    </syntax_nopara>

    <return>
	<para>返回当前ArrayOf数组长度。</para>
    </return>
     <remarks>
        <para>如果ArrayOf的m_pBuf 指针为空或m_size &lt;sizeof(T), 则返回0。</para>
    </remarks>


<references>
    <reference>
        <ref_name>ArrayOf</ref_name>
        <ref_url>../ArrayOf</ref_url>
    </reference>
</references>

</manpage>