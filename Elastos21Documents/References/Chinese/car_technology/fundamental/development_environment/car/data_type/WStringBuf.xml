        </operator>
        <operator>
            <operator_name>operator []</operator_name>
            <operator_url>WStringBuf/operator</operator_url>
            <operator_description> 存取数组元素操作符，用来对WStringBuf对象的数组元素进行存取操作。</operator_description>
        </operator>
        <operator>
            <operator_name>operator <![CDATA[<<]]> </operator_name>
            <operator_url>WStringBuf/operator3</operator_url>
            <operator_description> 相当于Append。</operator_description>
        </operator>
    </operators>

    <macros>
        <macro>
        <macro_name> NULL_WStringBuf </macro_name>
        <macro_url>WStringBuf/NULL_WSTRINGBUF</macro_url>
        <macro_description> 构造一个内容为空的WStringBuf对象。</macro_description>
        </macro>
        <macro>
        <macro_name> AUTO_WStringBuf</macro_name>
        <macro_url>WStringBuf/AUTO_WSTRINGBUF</macro_url>
        <macro_description> 构造一个大小为n的WStringBuf对象。</macro_description>
        </macro>
    </macros>
