<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?>

<manpage>

    <volume> 常用数据结构 </volume>
    <class>U8StringBuf</class>
    <method> Append </method>

    <description>
        <para>按一定格式在U8StringBuf对象字符串中追加时间信息。</para>
    </description>


    <syntax>
        <return_val> U8StringBuf &amp; </return_val>
        <parameters>
        <api_param>
            <param_type>const U8String &amp; </param_type>
            <param_name>fmt</param_name>
            <param_note>
                <para> 待转换的字符格式 </para>
            </param_note>
        </api_param>
        </parameters>
        <parameters>
        <api_param>
            <param_type>const DateTime &amp; </param_type>
            <param_name>dateTime</param_name>
            <param_note>
                <para> 待追加时间信息</para>
            </param_note>
        </api_param>
        </parameters>
    </syntax>
   
    <return>
        <para>返回U8StringBuf对象引用。</para>
    </return>
    <remarks>
        <para>超过U8StringBuf对象可用的长度会被省略掉。 </para>
    </remarks>


</manpage>