<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?>

<manpage>
    <volume> 常用数据结构 </volume>
    <interface>DateTime</interface>
    <method> ToUniversalTime </method>

    <description>
        <para> 将DateTime对象的值转换为协调通用时间 (UTC)。</para>
    </description>
    <syntax>
        <return_val> DateTime &amp; </return_val>
        <parameters>
        <api_param>
            <param_type>const DateTime &amp;</param_type>
            <param_name> datetime </param_name>
            <param_note>
                <para> datetime对象 </para>
            </param_note>
        </api_param>       
        </parameters>        
    </syntax>
    <return>
        <para>一个DateTime对象，其值为等效于当前 DateTime 对象的值的 UTC。</para>
    </return>
 
 
<references>
    <reference>
        <ref_name>DateTime</ref_name>
        <ref_url>../DateTime</ref_url>
    </reference>
</references>
</manpage>