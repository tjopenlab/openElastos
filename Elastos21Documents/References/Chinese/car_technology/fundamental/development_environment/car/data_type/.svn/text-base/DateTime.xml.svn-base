<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/interfaceman.xsl"?>

<manpage>
    <volume> 常用数据结构 </volume>
    <interface> <i>DateTime </i></interface>

    <description>       
        <para>是一个包含时间信息的结构体。</para>    
   </description>
    
        
    <methods>
      <self>
          
        <method>
        <method_name>IsValid</method_name>
        <method_url>DateTime/IsValid</method_url>
        <m_description> 判断当前DateTime对象时间信息是否为有效值。</m_description>
        </method>
        
        <method>
        <method_name>Parse</method_name>
        <method_url>DateTime/Parse/index/Parse</method_url>
        <m_description> 已重载。根据特定格式信息，将日期和时间的指定字符串表示形式转换为DateTime。</m_description>
        </method>
        
        
        <method>
        <method_name>ToLocalTime</method_name>
        <method_url>DateTime/ToLocalTime</method_url>
        <m_description> 将Universal时间转换为本地时间。</m_description>
        </method>
        
        <method>
        <method_name>ToUniversalTime</method_name>
        <method_url>DateTime/ToUniversalTime</method_url>
        <m_description> 将本地时间转换为协调通用时间 (UTC)。</m_description>
        </method>
        
      
      </self>
    </methods>
    <requirements>
        <headerfile>
            <file_name> elastos.h  </file_name>
        </headerfile>
        <library>
            <lib_name> elastos.lib </lib_name>
        </library>
    </requirements>        
    
    <references>
    <reference>
        <ref_name> DateTime结构体定义 </ref_name>
        <ref_url> ../data_type/DateTime/Def/Definition</ref_url>
    </reference>
    </references>  
        
</manpage>     
        
        
       
        
        
        
        
        
        
        
        
        
    

    