<manpage>
   
    <volume> 常用数据结构 </volume>
    <interface>MemoryBuf</interface>
    <method>  SetByteVaule </method>

    <description>
        <para>将MemoryBuf对象的整个buffer的字节值设置为特定值。 </para>
    </description>

    <syntax>
        <return_val>MemoryBuf &amp; </return_val>
        <parameters>
            <api_param>
                <param_type>Byte</param_type>
                <param_name>value </param_name>
                <param_note>
                    <para> 待设置的值 </para>
                </param_note>
            </api_param>
            
        </parameters>
    </syntax>

   <return>
   <para>返回MemoryBuf对象引用。</para>
   </return>


