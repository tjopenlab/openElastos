<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?>

<manpage>
    <volume> 常用数据结构 </volume>
    <class>U8String</class>
    <method>operator = </method>

    <description>
        <para>赋值操作符，为U8String对象赋值。 </para>
    </description>
    
    <syntax>
        <return_val> void </return_val>
        <parameters>
        <api_param>
            <param_type> const U8String&amp; </param_type>
            <param_name> str </param_name>
            <param_note>
                <para> U8String对象的引用 </para>
            </param_note>
        </api_param>
        </parameters>
    </syntax>
<example>
<pre>
U8String str("abcd");
U8String as;
as = str;
CConsole::WriteLine(as);  //Display : "abcd"
</pre>
</example>
  

</manpage>