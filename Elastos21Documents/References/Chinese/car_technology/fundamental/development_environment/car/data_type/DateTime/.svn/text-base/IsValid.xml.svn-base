<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?>

<manpage>
    <volume> 常用数据结构 </volume>
    <interface>DateTime</interface>
    <method> IsValid </method>

    <description>
        <para> 判断当前DateTime对象时间信息是否为有效值。</para>
    </description>
     <syntax_nopara>
        <return_val> Boolean </return_val>
    </syntax_nopara>
    <return>
        <para>DateTime对象时间信息为有效值返回True，否则返回False。</para>
    </return>
 
<references>
    <reference>
        <ref_name>DateTime</ref_name>
        <ref_url>../DateTime</ref_url>
    </reference>
</references>
</manpage>