<manpage>
    <volume> 常用数据结构 </volume>
    <interface>WString</interface>
    <method> GetCharCount </method>

    <description>
        <para> 按指定的编码格式返回WString实例所占的字符数。</para>
    </description>
    <syntax>
        <return_val> Int32 </return_val>
        <modifier>const</modifier>
<!--
        <parameters>
        <api_param>
            <param_type>Encoding</param_type>
            <param_name> encoding  </param_name>
            <param_default>Encoding_Default  </param_default>
            <param_note>
                <para> 指定编码格式。 </para>
            </param_note>
        </api_param>               
        </parameters>
-->        
    </syntax>
    <return>
        <para>返回WString对象字符串的长度。</para>
    </return>


<example>
