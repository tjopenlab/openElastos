<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?>

<manpage>
    <volume> 常用数据结构 </volume>
    <interface>AString</interface>
    <method>operator char* </method>

    <description>
        <para>类型转换操作符，把AString对象转换成指向字符串的指针。 </para>
    </description>

    <syntax_nopara>
        <method_name> operator const char* </method_name>
        <modifier>const</modifier>
        <NoArguments/>
    </syntax_nopara>

    <return>
        <para>返回指向字符串的指针，数据的内容不变。 </para>
    </return>

<references>
    <reference>
        <ref_name>AString</ref_name>
        <ref_url>../AString</ref_url>
    </reference>
</references>

</manpage>