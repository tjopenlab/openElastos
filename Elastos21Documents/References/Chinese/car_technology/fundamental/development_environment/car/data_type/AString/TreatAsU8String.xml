<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?>

<manpage>
    <volume> 常用数据结构 </volume>
    <interface>AString</interface>
    <method> TreatAsU8String </method>

    <description>
        <para> 直接返回当前字符串。 </para>
    </description>
    <syntax>
        <return_val> U8String &amp; </return_val>

      
    </syntax>
    <return>
        <para>返回U8String类型的引用。 </para>
    </return>
    


<example>
<pre>
U8String as("HELLOhexIN");

as.TreatAsU8String();  
CConsole::WriteLine(as);  //Display : "HELLOhexIN"
</pre>
</example>
 
   
 
</manpage>