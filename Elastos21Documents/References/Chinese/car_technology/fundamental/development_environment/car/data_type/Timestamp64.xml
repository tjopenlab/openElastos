<manpage>
    <volume> 常用数据结构 </volume>
    <interface> <i>Timestamp64 </i></interface>

    <description>       
        <para>时间标记类。</para>    
   </description>
    
    <constructs>
        <construct>
            <construct_name>Timestamp64</construct_name>
            <construct_url>Timestamp64/structure/Timestamp64</construct_url>
            <construct_description>构造一个Timestamp64对象。</construct_description>
        </construct>
        <construct>
            <construct_name>Timestamp64(Int64)</construct_name>
            <construct_url>Timestamp64/structure/Timestamp64_</construct_url>
            <construct_description>构造一个Timestamp64对象。</construct_description>
        </construct>
    </constructs>
        
    <methods>
      <self>
      
        <method>
        <method_name>FromLocalDateTime</method_name>
        <method_url>Timestamp64/FromLocalDateTime</method_url>
        <m_description> 将本地时间转换为Timestamp64对象。</m_description>
        </method>
        
        <method>
        <method_name>FromUniversalDateTime</method_name>
        <method_url>Timestamp64/FromUniversalDateTime</method_url>
        <m_description>将universal时间转换为Timestamp64对象。</m_description>
        </method>
        
        <method>
        <method_name>ToLocalDateTime</method_name>
        <method_url>Timestamp64/ToLocalDateTime</method_url>
        <m_description> 将Timestamp64对象转换为本地时间。</m_description>
        </method>
        
        <method>
        <method_name>ToUniversalDateTime</method_name>
        <method_url>Timestamp64/ToUniversalDateTime</method_url>
        <m_description>将Timestamp64对象转换为universal时间。</m_description>
        </method>
      </self>
    </methods>
    
    <operators>
        <operator>
            <operator_name>operator =</operator_name>
            <operator_url>Timestamp64/operator/index/operator</operator_url>
            <operator_description>已重载。赋值操作符。</operator_description>
        </operator>

        <operator>
            <operator_name>operator Int64</operator_name>
            <operator_url>Timestamp64/operator_Int64</operator_url>
            <operator_description> 类型转换操作符。</operator_description>
        </operator>
      </operators>  

    <requirements>
        <headerfile>
            <file_name> elastos.h  </file_name>
        </headerfile>
        <library>
            <lib_name> elastos.lib </lib_name>
        </library>
    </requirements>



</manpage>     
        
        
       
        
        
        
        
        
        
        
        
        
    

    