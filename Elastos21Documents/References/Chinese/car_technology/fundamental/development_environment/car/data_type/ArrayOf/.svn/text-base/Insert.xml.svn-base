<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?>

<manpage>
    <volume> 常用数据结构 </volume>
    <interface>ArrayOf</interface>
    <method> Insert </method>

    <description>
        <para>将源数据插入到指定位置。</para>
    </description>

    <syntax>
        <return_val>ArrayOf &amp;</return_val>
        <parameters>
            <api_param>
                <param_type>const <i>T</i>*</param_type>
                <param_name>p </param_name>
                <param_note>
                    <para> 源T类型数组 </para>
                </param_note>
            </api_param>
            <api_param>
                <param_type> Int32  </param_type>
                <param_name> offset </param_name>
                <param_note>
                    <para> 插入位置相对于ArrayOf对象储存区起始位置的偏移量，单位为sizeof(T)</para>
                </param_note>
            </api_param>
            <api_param>
                <param_type> Int32  </param_type>
                <param_name> n </param_name>
                <param_note>
                    <para> 要插入的数组元素个数</para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>
    <return>
        <para>返回当前ArrayOf对象。</para>
    </return>
    <remarks>
        <para>超过ArrayOf对象可用的长度会被省略掉。 </para>
    </remarks>

<references>
    <reference>
        <ref_name>ArrayOf</ref_name>
        <ref_url>../ArrayOf</ref_url>
    </reference>
</references>

</manpage>