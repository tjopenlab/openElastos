<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/interfaceman.xsl"?>

<manpage>
    <volume> AStringTokenizer </volume>
    <interface> <i>AStringTokenizer/WStringTokenizer </i></interface>

    <description>       
        <para>AStringTokenizer/WStringTokenize查找字符串中的标记，是一个辅助类。</para>
   </description>
    <constructs>
        <construct>
            <construct_name>AStringTokenizer</construct_name>
            <construct_url>AStringTokenizer/structure/AStringTokenizer</construct_url>
            <construct_description>构造一个AStringTokenizer对象，并指定分隔字符。默认分隔字符为 " \t\n\r\f“ 参数returnDelims设置分隔字符是否当Token返回。 </construct_description>
        </construct>       
     </constructs>   
        
    <methods>
      <self>
        <method>
        <method_name>GetCount</method_name>
        <method_url>AStringTokenizer/GetCount</method_url>
        <m_description>计算在生成异常之前可以调用此AStringTokenizer的NextToken 方法的次数。</m_description>
        </method>
        
        <method>
        <method_name> HasMoreTokens</method_name>
        <method_url>AStringTokenizer/HasMoreTokens</method_url>
        <m_description>测试是否还有Token可取。 </m_description>
        </method>
        
        <method>
        <method_name> NextToken</method_name>
        <method_url>AStringTokenizer/NextToken/index/NextToken</method_url>
        <m_description>已重载。获取下一个Token。 </m_description>
        </method>
        
      </self>
    </methods>

    <operators>
        <operator>
            <operator_name> operator<![CDATA[>>]]></operator_name>
            <operator_url>AStringTokenizer/operator/index/operator</operator_url>
            <operator_description>已重载。获取相应的Token并把Token转为目标类型值。</operator_description>
        </operator>
    </operators>
   
    <requirements>
        <headerfile>
            <file_name> elastos.h  </file_name>
        </headerfile>
        <library>
            <lib_name> elastos.lib </lib_name>
        </library>
    </requirements>        
        
</manpage>     
        
        
       
        
        
        
        
        
        
        
        
        
    

    