<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?>

<manpage>
    <volume> MakeNumberFormat </volume>
    <method> MakeNumberFormat </method>

    <description>
        <para>设置字符格式。</para>
    </description>

    <syntax>
        <return_val> Flags32 </return_val>
        <parameters>
        <api_param>
            <param_type>Flags32</param_type>
            <param_name> fmt</param_name>
            <param_note>
                <para> 字符输出格式，参见<link url="../NumberFormatFlag.htm">_NumberFormat</link>枚举类型。 </para>
            </param_note>
        </api_param>
        </parameters>
        <parameters>
        <api_param>
            <param_type>Int32</param_type>
            <param_name>width</param_name>
            <param_default> 0 </param_default>
            <param_note>
                <para> 设置输出字符宽度。</para>
            </param_note>
        </api_param>
        </parameters>
        <parameters>
        <api_param>
            <param_type>Int32</param_type>
            <param_name>precision</param_name>
            <param_default> 0 </param_default>
            <param_note>
                <para> 设置输出字符精度。</para>
            </param_note>
        </api_param>
        </parameters>
    </syntax>
   
    <return>
        <para>返回设置的字符格式。</para>
    </return>
    <requirements>
        <headerfile>
            <file_name> elstrbuf.h </file_name>
        </headerfile>
        <library>
            <lib_name> elastos.lib </lib_name>
        </library>
    </requirements>



</manpage>