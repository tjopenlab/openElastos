<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?>

<manpage>
    <volume> 常用数据结构 </volume>
    <interface> BufferOf   </interface>
    <method> Insert </method>

    <description>
        <para>将源数据插入到指定位置。</para>
    </description>

    <syntax>
        <return_val>BufferOf &amp;</return_val>
        <parameters>
            <api_param>
                <param_type>const <i>T</i>*</param_type>
                <param_name>p </param_name>
                <param_note>
                    <para> 源T类型数组 </para>
                </param_note>
            </api_param>
            <api_param>
                <param_type> Int32  </param_type>
                <param_name> offset </param_name>
                <param_note>
                    <para> 插入位置相对于BufferOf对象储存区起始位置的偏移量，单位为sizeof(T)</para>
                </param_note>
            </api_param>
            <api_param>
                <param_type> Int32  </param_type>
                <param_name> n </param_name>
                <param_note>
                    <para> 要插入的数组元素个数</para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>

    <return>
        <para>当前BufferOf对象引用。</para>
    </return>

</manpage>