<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?>

<manpage>
    <!-- <sample url="sdk\eztypes\ezarray\IsNull">IsNull</sample>  -->
    <volume> 常用数据结构 </volume>
    <interface>BufferOf </interface>
    <method> IsNull </method>

    <description>
        <para>判断当前BufferOf对象的指向数据区的指针是否为空。</para>
    </description>
    <syntax_nopara>
        <return_val>Boolean</return_val>
        <modifier>const</modifier>
        <NoArguments/>
    </syntax_nopara>

    <return>
        <para>如果当前BufferOf指针为NULL，返回值为TRUE；否则，返回值为FALSE。</para>
    </return>

    

</manpage>