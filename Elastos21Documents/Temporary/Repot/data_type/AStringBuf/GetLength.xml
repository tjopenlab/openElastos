<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?>

<manpage>
    <sample url="sdk\eztypes\AStringBuf\GetLength">GetLength</sample>
    <volume> 常用数据结构 </volume>
    <interface> AStringBuf </interface>
    <method>GetLength</method>

    <description>
        <para> 获取当前字符串的长度，以字符数为单位。</para>
    </description>

    <syntax_nopara>
        <return_val> Int32 </return_val>
        <modifier>const</modifier>
        <NoArguments/>
    </syntax_nopara>

    <return>
        <para>返回字符串的字符数，不包含"\0"结束符。</para>
    </return>

</manpage>