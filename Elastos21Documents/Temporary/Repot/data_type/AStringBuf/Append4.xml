

<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?>

<manpage>
    <sample url="sdk\eztypes\AStringBuf\Append">Append</sample>
    <volume> 常用数据结构 </volume>
    <interface> AStringBuf  </interface>
    <method> Append </method>

    <description>
        <para>按一定格式在AStringBuf对象中原字符串末尾追加Int8数值。</para>
    </description>

    <syntax>
        <return_val> AStringBuf &amp; </return_val>
        <parameters>
        <api_param>
            <param_type>Int8</param_type>
            <param_name>value</param_name>
            <param_note>
                <para> 待添加的数值。 </para>
            </param_note>
        </api_param>
        </parameters>
        <parameters>
        <api_param>
            <param_type>UInt32</param_type>
            <param_name>fmt</param_name>
            <param_default> 0 </param_default>
            <param_note>
                <para> 设置要追加的字符的格式。</para>
            </param_note>
        </api_param>
        </parameters>
    </syntax>
   
    <return>
        <para>返回AStringBuf对象引用。</para>
    </return>

   

</manpage>