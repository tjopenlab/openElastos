
<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?>

<manpage>
    <volume> 常用数据结构 </volume>
    <interface> AStringBuf  </interface>
    <method> GetChar </method>

    <description>
        <para>按指定编码取Index位置的字符。</para>
    </description>

    <syntax>
        <return_val> Int32 </return_val>
        <parameters>
        <api_param>
            <param_type>Int32 </param_type>
            <param_name> idx </param_name>
            <param_note>
                <para> 索引位置。 </para>
            </param_note>
        </api_param>
        </parameters>
        <parameters>
        <api_param>
            <param_type>AString</param_type>
            <param_name>encoding </param_name>
            <param_note>
                <para> 指定编码 </para>
            </param_note>
        </api_param>
        </parameters>
        
    </syntax>

    <return>
        <para>Index位置的字符。</para>
    </return>

    
    

</manpage>