
<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?>

<manpage>
    <volume> 常用数据结构 </volume>
    <interface> AStringBuf  </interface>
    <method> GetCharCount </method>

    <description>
        <para>获得指定编码的字符串长度。</para>
    </description>

    <syntax>
        <return_val> Int32 </return_val>
        
        <parameters>
        <api_param>
            <param_type>AString</param_type>
            <param_name>encoding </param_name>
            <param_note>
                <para> 指定编码 </para>
            </param_note>
        </api_param>
        </parameters>
        
    </syntax>

    <return>
        <para>返回指定编码的字符串长度。</para>
    </return>

    
    

</manpage>