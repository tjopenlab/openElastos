<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?>

<manpage>
    <sample url="sdk\eztypes\AStringBuf\CompareNoCase">CompareNoCase</sample>
    <volume> 常用数据结构 </volume>
    <interface> AStringBuf  </interface>
    <method>CompareNoCase</method>

    <description>
        <para>把AStringBuf对象中原字符串与另一个给定的AStringBuf对象中的字符串作比较,不考虑大小写。</para>
    </description>

    <syntax>
        <return_val> AStringBuf &amp; </return_val>
        <parameters>
        <api_param>
            <param_type>const <i> AStringBuf</i>&amp;</param_type>
            <param_name>asb</param_name>
            <param_note>
                <para> 一个指定的AStringBuf对象。 </para>
            </param_note>
        </api_param>
        </parameters>
    </syntax>

    <return>
        <para>返回AStringBuf对象引用。</para>
    </return>
     
</manpage>