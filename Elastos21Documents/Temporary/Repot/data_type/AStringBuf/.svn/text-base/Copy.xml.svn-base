

<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?>

<manpage>
   
    <volume> 常用数据结构 </volume>
    <interface> AStringBuf  </interface>
    <method> Copy </method>

    <description>
        <para> 将源AString对象的数组元素拷贝到当前AStringBuf对象的数组中。</para>
    </description>

   
    <syntax>
        <return_val> AStringBuf &amp; </return_val>
        <parameters>
        <api_param>
            <param_type>const <i> AStringBuf</i>&amp;</param_type>
            <param_name>asb</param_name>
            <param_note>
                <para> 源AStringBuf对象。 </para>
            </param_note>
        </api_param>
        </parameters>
    </syntax>

    <return>
        <para>返回AStringBuf对象引用。</para>
    </return>
     
</manpage>