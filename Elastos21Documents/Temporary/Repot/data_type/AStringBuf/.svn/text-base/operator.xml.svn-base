<manpage>
    <sample url="sdk\eztypes\AStringBuf\opt_eval">opertor=</sample>
    <volume> 常用数据结构 </volume>
    <interface> AStringBuf </interface>
    <method> operator [] </method>

    <description>
        <para>存取数组元素操作符，用来对AStringBuf对象的数组元素进行存取操作。</para>
    </description>

    <syntax>
        <return_val> AChar&amp; </return_val>
        <parameters>
        <api_param>
            <param_type> Int32 </param_type>
            <param_name> index </param_name>
            <param_note>
                <para> AStringBuf对象的数据区的数组元素下标 </para>
            </param_note>
        </api_param>
        </parameters>
        </syntax>
        <syntax>
        <return_val> const AChar&amp; </return_val>
        <parameters>
        <api_param>
            <param_type> Int32 </param_type>
            <param_name> index </param_name>
            <param_note>
                <para> </para>
            </param_note>
        </api_param>
        </parameters>
        </syntax>
        <syntax>
        <return_val> AChar&amp; </return_val>
        <parameters>
        <api_param>
            <param_type> UInt32 </param_type>
            <param_name> index </param_name>
            <param_note>
                <para></para>
            </param_note>
        </api_param>
        </parameters>
        </syntax>
        <syntax>
        <return_val> const AChar&amp; </return_val>
        <parameters>
        <api_param>
            <param_type> UInt32 </param_type>
            <param_name> index </param_name>
            <param_note>
                <para></para>
            </param_note>
        </api_param>
        </parameters>
     </syntax>

    <return>
    <para>返回AStringBuf对象的数据区的数组对应元素。</para>
    </return>
 
</manpage>