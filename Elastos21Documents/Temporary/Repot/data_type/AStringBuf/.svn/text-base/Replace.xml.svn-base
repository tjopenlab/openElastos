<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?>

<manpage>
    <sample url="sdk\eztypes\AStringBuf\Replace">Replace</sample>
    <volume> 常用数据结构 </volume>
    <interface> AStringBuf  </interface>
    <method>Replace</method>

    <description>
        <para>将缓冲区中从指定位置开始替换为源数据前指定个字符。</para>
    </description>

    <syntax>
        <return_val> AStringBuf &amp; </return_val>
        <parameters>
        <api_param>
            <param_type>Int32</param_type>
            <param_name>offset</param_name>
            <param_note>
                <para> 相对于AStringBuf对象缓冲区起始位置的偏移量，单位为sizeof(AChar) </para>
            </param_note>
        </api_param>
        </parameters>
        <parameters>
        <api_param>
            <param_type>Int32</param_type>
            <param_name>count</param_name>
            <param_note>
                <para>替换的字符个数</para>
            </param_note>
        </api_param>
        </parameters>
        <parameters>
        <api_param>
            <param_type>AString</param_type>
            <param_name>as</param_name>
            <param_note>
                <para>源字符串</para>
            </param_note>
        </api_param>
        </parameters>
    </syntax>

    <return>
        <para>返回AStringBuf对象引用。</para>
    </return>

    
 

</manpage>