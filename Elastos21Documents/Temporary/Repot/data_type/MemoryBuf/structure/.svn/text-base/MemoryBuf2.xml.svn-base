<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/structuremanual.xsl"?>

<manpage>
    
    <volume> 常用数据结构 </volume>
    <class> MemoryBuf </class>
    <function>MemoryBuf</function>

    <description>
        <para> 在栈中构造一个MemoryBuf对象,将m_pBuf指向用户指定的内存块，并分别其长度和已使用长度。 </para>
    </description>

    <syntax>
        <apiname>MemoryBuf</apiname>
        <parameters>
        <api_param>
            <param_type><i>T</i> *</param_type>
            <param_name>pBuf </param_name>          
            <param_note>
                <para>源T类型数组 </para>
            </param_note>
        </api_param>
        </parameters>
        <parameters>
        <api_param>
            <param_type>Int32</param_type>
            <param_name>capacity </param_name>
            <param_note>
                <para>待设置的MemoryBuf数组的数据空间总长度，单位为sizeof(T) </para>
            </param_note>
        </api_param>
        </parameters>
        <parameters>
        <api_param>
            <param_type>Int32</param_type>
            <param_name>used </param_name>
            <param_note>
                <para>待设置的MemoryBuf数组的使用数据空间长度，单位为sizeof(T)</para>
            </param_note>
        </api_param>
        </parameters>
    </syntax>

    <remarks>
     
    </remarks>

   


</manpage>