
<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?>

<manpage>
   
    <volume> 常用数据结构 </volume>
    <interface>MemoryBuf   </interface>
    <method>  SetByteVaule </method>

    <description>
        <para>设置内存块的字节值，相当于memset </para>
    </description>

    <syntax>
        <return_val>MemoryBuf &amp; </return_val>
        <parameters>
            <api_param>
                <param_type>Byte</param_type>
                <param_name>value </param_name>
                <param_note>
                    <para> 内存块的字节值 </para>
                </param_note>
            </api_param>
            
        </parameters>
    </syntax>

   <return>
   <para>返回MemoryBuf对象引用。</para>
   </return>

</manpage>