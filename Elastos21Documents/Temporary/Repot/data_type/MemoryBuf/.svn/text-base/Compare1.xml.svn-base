
<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?>

<manpage>
    <volume> 常用数据结构 </volume>
    <interface>MemoryBuf </interface>
    <method>Compare</method>

    <description>
        <para>与指定内存块比较n个字节，相当于memcmp。</para>
    </description>
    <syntax>
        <return_val>Int32 </return_val>
        <parameters>
            <api_param>
                <param_type>const byte *</param_type>
                <param_name>pBuf </param_name>
                <param_note>
                    <para>  要比较的内存块 </para>
                </param_note>
            </api_param>
            <api_param>
                <param_type> Int32  </param_type>
                <param_name> n </param_name>
                <param_note>
                    <para> 比较的内存块的大小</para>
                </param_note>
            </api_param>
        </parameters>
    </syntax>
  
  
   <return>
   <para>返回比较的结果值。</para>
   </return>
    

    
</manpage>