<?xml version = "1.0" encoding="GB2312" ?>
<?xml:stylesheet type="text/xsl" href="/xsl/xsl_c/methodmanual.xsl"?>

<manpage>
    <volume> 常用数据结构 </volume>
    <interface> AString </interface>
    <method>IsEmpty </method>

    <description>
        <para>判断当前AString对象的字符串内容是否为空。 </para>
    </description>
    
    <syntax_nopara>
        <return_val>Boolean </return_val>
        <NoArguments/>
    </syntax_nopara>

    <return>
        <para>AString对象的字符串内容为空时，返回TRUE，否则返回FALSE。 </para>
    </return>

</manpage>