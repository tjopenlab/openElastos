package org.jab.docsearch.constants;

/**
 * This class contains all OS Type constants
 * 
 * @version $Id: OSType.java,v 1.1 2005/10/25 20:02:50 henschel Exp $
 */
public final class OSType {
    /**
     * unknown
     */
    public final static int UNKNOWN = -1;
    /**
     * WIN 32
     */
    public final static int WIN_32 = 0;
    /**
     * Linux
     */
    public final static int LINUX = 1;
    /**
     * Unix
     */
    public final static int UNIX = 2;
    /**
     * MAC
     */
    public final static int MAC = 3;
}
