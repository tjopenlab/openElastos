/* _scalb.cpp: This program scales argument by a power of 2.
 */

#include <float.h>
#include <stdio.h>

int main()
{
    double x, z;
    long y;

    x = 123.456789;
    y = 16;
    z = _scalb(x, y);
    printf("_scalb(%f, %ld) = %f\n", x, y, z);

    return 0;
}

/*
Output

    _scalb(123.456789, 16) = 8090864.123904
*/
