/* time.cpp: This program uses time to get the system time.
 */

#include <stdio.h>
#include <time.h>

int main()
{
    time_t timer;

    time(&timer);
    printf("Long_time:%d\n", timer);

    return 0;
}

/*
Output

    Long_time:    100129494
*/
