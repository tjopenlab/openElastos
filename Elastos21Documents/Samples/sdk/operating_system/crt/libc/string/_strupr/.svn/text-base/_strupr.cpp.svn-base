/* _strupr.cpp: This program uses _strupr to create
 * uppercase copies of a mixed-case string.
 */

#include <string.h>
#include <stdio.h>
#include <stdlib.h>


int main()
{
    char string[100] = "The String to End All Strings!";

     printf("Mixed: %s\n", string);      

     _strupr(string);
      
     printf("Upper: %s\n", string);


    return 0;
}

//==========================================================================
//Output
//
//    Mixed: The String to End All Strings!
//    Upper: THE STRING TO END ALL STRINGS!
//==========================================================================
