/* log10.cpp: This program uses log and log10
 * to calculate the base-10 logarithm of 9,000.
 */

#include <math.h>
#include <stdio.h>

int main()
{
    double x = 9000.0;
    double y;

    y = log10(x);
    printf("log10(%.2f) = %f\n", x, y);

    return 0;
}


/*
Output

    log10(9000.00) = 3.954243
*/
