/* _wunlink.cpp: This program uses _wunlink to delete the file. */

#include <io.h>
#include <stdio.h>

int main()
{
    // Delete the file
   if( _wunlink( L"wunlink.obj" ) == -1 )
      printf( "Could not delete 'WUNLINK.OBJ'" );
   else
      printf( "Deleted 'WUNLINK.OBJ'\n" );
   return 0;
}

