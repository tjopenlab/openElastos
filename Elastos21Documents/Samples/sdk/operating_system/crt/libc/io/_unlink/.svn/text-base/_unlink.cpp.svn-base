/* _unlink.cpp: This program uses _unlink to delete UNLINK.OBJ. */

#include <io.h>
#include <stdio.h>

int main()
{
    if (_unlink("unlink.obj") == -1)
        printf("Could not delete 'UNLINK.OBJ'\n");
    else
        printf("Deleted 'UNLINK.OBJ'\n");

    return 0;
}


/*
Output

     Deleted 'UNLINK.OBJ'
*/
