#include <elastos.h>
using namespace Elastos;

Int32 main()
{
    WString as(L"Hello,world!");
    const wchar_t *str = as;
    CConsole::Write("This str is: ");
    CConsole::WriteLine(str);

    return 0;
}

//==========================================================================
//Output:This str is: Hello,world!
//==========================================================================
