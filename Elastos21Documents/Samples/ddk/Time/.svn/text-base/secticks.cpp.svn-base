#include <ddk.h>

void SampleFunc()
{
    uint_t uMilliseconds = 1000;
    uint_t uTicks;

    // Converts 1000 ms to ticks.
    uTicks = DzMillisecondsToTicks(uMilliseconds);

    // Converts back.
    uMilliseconds = DzTicksToMilliseconds(uTicks);
}
